{
  return new MeasuresFilter<Collection<Measure>>(){
    @Override public Collection<Measure> filter(    Collection<Measure> measures){
      Collection<Measure> all=new ArrayList<Measure>();
      for (      Measure measure : measures) {
        if (measure != null) {
          all.add(measure);
        }
      }
      return all;
    }
  }
;
}
