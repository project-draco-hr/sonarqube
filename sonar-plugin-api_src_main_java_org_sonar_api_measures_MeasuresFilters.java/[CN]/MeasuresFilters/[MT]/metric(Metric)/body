{
  return new MetricFilter<Measure>(metric){
    public Measure filter(    Collection<Measure> measures){
      if (measures == null) {
        return null;
      }
      for (      Measure measure : measures) {
        if (measure.getClass().equals(Measure.class) && measure.getMetricKey().equals(metric.getKey()) && measure.getCharacteristic() == null) {
          return measure;
        }
      }
      return null;
    }
  }
;
}
