{
  ProjectRepository ref=projectReferentialsLoader.load(ProjectRepositoryQuery.create().setModuleKey(request.mandatoryParam(PARAM_KEY)).setProfileName(request.param(PARAM_PROFILE)).setPreview(request.mandatoryParamAsBoolean(PARAM_PREVIEW)));
  response.stream().setMediaType(MimeTypes.JSON);
  IOUtils.write(ref.toJson(),response.stream().output());
}
