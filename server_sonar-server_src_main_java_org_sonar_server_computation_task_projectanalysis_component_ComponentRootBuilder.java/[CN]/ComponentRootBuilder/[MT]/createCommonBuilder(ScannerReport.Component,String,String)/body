{
  return ComponentImpl.builder(convertType(reportComponent.getType())).setUuid(uuidSupplier.apply(componentKey)).setKey(componentKey).setDescription(trimToNull(reportComponent.getDescription())).setFileAttributes(createFileAttributes(reportComponent)).setReportAttributes(createReportAttributes(reportComponent)).addChildren(toArray(buildChildren(reportComponent,latestModuleKey),Component.class));
}
