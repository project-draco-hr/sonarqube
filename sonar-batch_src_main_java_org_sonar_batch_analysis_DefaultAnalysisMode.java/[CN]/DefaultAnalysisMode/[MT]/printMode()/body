{
  if (preview) {
    LOG.info("Preview mode");
  }
 else   if (issues) {
    LOG.info("Issues mode");
  }
 else {
    LOG.info("Publish mode");
  }
  if (mediumTestMode) {
    LOG.info("Medium test mode");
  }
  if (notAssociated) {
    LOG.info("Local analysis");
  }
  if (!scanAllFiles) {
    LOG.info("Scanning only changed files");
  }
}
