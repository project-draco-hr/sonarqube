{
  if (preview) {
    LOG.info("Preview mode");
  }
 else   if (issues) {
    LOG.info("Issues mode");
  }
 else {
    LOG.info("Publish mode");
  }
  if (mediumTestMode) {
    LOG.info("Medium test mode");
  }
  if (notAssociated) {
    LOG.info("Project is not associated with the server");
  }
}
