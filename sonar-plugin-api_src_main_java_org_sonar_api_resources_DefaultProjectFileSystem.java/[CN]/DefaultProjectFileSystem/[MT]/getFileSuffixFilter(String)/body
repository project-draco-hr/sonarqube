{
  IOFileFilter suffixFilter=FileFilterUtils.trueFileFilter();
  if (langKeys != null && langKeys.length > 0) {
    List<String> suffixes=Arrays.asList(languages.getSuffixes(langKeys));
    if (!suffixes.isEmpty()) {
      suffixFilter=new SuffixFileFilter(suffixes);
    }
  }
  return suffixFilter;
}
