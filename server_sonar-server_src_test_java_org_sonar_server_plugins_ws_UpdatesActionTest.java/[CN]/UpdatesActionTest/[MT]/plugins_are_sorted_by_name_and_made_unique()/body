{
  loggedAsSystemAdmin();
  when(updateCenter.findPluginUpdates()).thenReturn(of(pluginUpdate("key2","name2"),pluginUpdate("key2","name2"),pluginUpdate("key0","name0"),pluginUpdate("key1","name1")));
  underTest.handle(request,response);
  assertJson(response.outputAsString()).withStrictArrayOrder().isSimilarTo("{" + "  \"plugins\": [" + "    {"+ "      \"key\": \"key0\","+ "      \"name\": \"name0\""+ "    },"+ "    {"+ "      \"key\": \"key1\","+ "      \"name\": \"name1\""+ "    },"+ "    {"+ "      \"key\": \"key2\","+ "      \"name\": \"name2\""+ "    },"+ "  ]"+ "}");
}
