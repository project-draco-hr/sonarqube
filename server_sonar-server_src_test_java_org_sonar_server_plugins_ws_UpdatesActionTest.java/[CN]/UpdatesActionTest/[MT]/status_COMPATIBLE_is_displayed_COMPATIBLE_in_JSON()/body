{
  loggedAsSystemAdmin();
  when(updateCenter.findPluginUpdates()).thenReturn(of(pluginUpdate(release(PLUGIN_1,"1.0.0"),COMPATIBLE)));
  underTest.handle(request,response);
  assertJson(response.outputAsString()).isSimilarTo("{" + "  \"plugins\": [" + "   {"+ "      \"updates\": ["+ "        {"+ "          \"status\": \"COMPATIBLE\""+ "        }"+ "      ]"+ "    }"+ "  ]"+ "}");
}
