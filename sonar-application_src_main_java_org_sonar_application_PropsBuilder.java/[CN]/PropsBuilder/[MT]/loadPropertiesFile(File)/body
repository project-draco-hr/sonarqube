{
  Properties p=new Properties();
  File propsFile=new File(homeDir,"conf/sonar.properties");
  if (propsFile.exists()) {
    Reader reader=new InputStreamReader(new FileInputStream(propsFile),StandardCharsets.UTF_8);
    try {
      p.load(reader);
    }
  finally {
      IOUtils.closeQuietly(reader);
    }
  }
  return p;
}
