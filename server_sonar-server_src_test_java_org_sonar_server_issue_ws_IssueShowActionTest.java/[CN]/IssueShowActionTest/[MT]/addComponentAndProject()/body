{
  ComponentDto file=new ComponentDto().setId(10L).setKey("org.sonar.server.issue.IssueClient").setLongName("SonarQube :: Issue Client").setName("SonarQube :: Issue Client").setQualifier("FIL").setSubProjectId(1L).setProjectId(1L);
  when(componentDao.getNullableByKey(session,file.key())).thenReturn(file);
  ComponentDto project=new ComponentDto().setId(1L).setKey("org.sonar.Sonar").setLongName("SonarQube").setName("SonarQube").setProjectId(1L);
  when(componentDao.getNullableById(file.projectId(),session)).thenReturn(project);
}
