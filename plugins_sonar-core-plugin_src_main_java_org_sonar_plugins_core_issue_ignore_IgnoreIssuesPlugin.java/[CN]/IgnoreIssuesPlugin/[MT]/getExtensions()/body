{
  ImmutableList.Builder<Object> extensions=ImmutableList.builder();
  extensions.addAll(IgnoreIssuesConfiguration.getPropertyDefinitions());
  extensions.add(InclusionPatternInitializer.class,ExclusionPatternInitializer.class,RegexpScanner.class,SourceScanner.class,EnforceIssuesFilter.class,IgnoreIssuesFilter.class);
  return extensions.build();
}
