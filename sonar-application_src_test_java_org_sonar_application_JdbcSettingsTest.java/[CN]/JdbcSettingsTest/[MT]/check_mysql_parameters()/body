{
  settings.checkUrlParameters(Provider.MYSQL,"jdbc:mysql://localhost:3306/sonar?useUnicode=true&characterEncoding=utf8");
  settings.checkUrlParameters(Provider.MYSQL,"jdbc:mysql://localhost:3306/sonar?useUnicode=true&characterEncoding=utf8&rewriteBatchedStatements=true&useConfigs=maxPerformance");
  expectedException.expect(MessageException.class);
  expectedException.expectMessage("JDBC URL must have the property 'useUnicode=true'");
  settings.checkUrlParameters(Provider.MYSQL,"jdbc:mysql://localhost:3306/sonar?characterEncoding=utf8");
}
