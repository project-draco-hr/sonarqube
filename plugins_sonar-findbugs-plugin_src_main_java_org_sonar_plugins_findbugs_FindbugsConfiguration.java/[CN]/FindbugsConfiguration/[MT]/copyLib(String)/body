{
  try {
    InputStream is=getClass().getResourceAsStream(name);
    File temp=File.createTempFile("findbugs",".jar");
    FileUtils.forceDeleteOnExit(temp);
    OutputStream os=FileUtils.openOutputStream(temp);
    IOUtils.copy(is,os);
    return temp;
  }
 catch (  IOException e) {
    throw new SonarException(e);
  }
}
