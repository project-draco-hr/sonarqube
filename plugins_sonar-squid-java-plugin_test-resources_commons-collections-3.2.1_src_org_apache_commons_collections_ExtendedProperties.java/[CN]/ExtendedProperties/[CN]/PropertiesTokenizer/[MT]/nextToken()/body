{
  StringBuffer buffer=new StringBuffer();
  while (hasMoreTokens()) {
    String token=super.nextToken();
    if (endsWithSlash(token)) {
      buffer.append(token.substring(0,token.length() - 1));
      buffer.append(DELIMITER);
    }
 else {
      buffer.append(token);
      break;
    }
  }
  return buffer.toString().trim();
}
