{
  Object current=this.get(key);
  if (current instanceof String) {
    List values=new Vector(2);
    values.add(current);
    values.add(value);
    put(key,values);
  }
 else   if (current instanceof List) {
    ((List)current).add(value);
  }
 else {
    if (!containsKey(key)) {
      keysAsListed.add(key);
    }
    put(key,value);
  }
}
