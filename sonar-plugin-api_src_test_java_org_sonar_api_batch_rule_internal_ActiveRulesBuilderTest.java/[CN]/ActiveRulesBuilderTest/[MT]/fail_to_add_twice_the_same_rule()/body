{
  ActiveRulesBuilder builder=new ActiveRulesBuilder();
  builder.create(RuleKey.of("squid","S0001")).activate();
  try {
    builder.create(RuleKey.of("squid","S0001")).activate();
    fail();
  }
 catch (  IllegalStateException e) {
    assertThat(e).hasMessage("Rule 'squid:S0001' is already activated");
  }
}
