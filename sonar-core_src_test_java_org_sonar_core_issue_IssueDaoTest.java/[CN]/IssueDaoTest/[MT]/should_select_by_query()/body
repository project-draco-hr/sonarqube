{
  setupData("shared","should_select_by_query");
  IssueQuery query=IssueQuery.builder().keys(newArrayList("ABCDE")).userLogins(newArrayList("arthur","otherguy")).assignees(newArrayList("perceval","otherguy")).components(newArrayList("Action.java")).resolutions(newArrayList("FIXED")).severities(newArrayList("BLOCKER")).rules(newArrayList(RuleKey.of("squid","AvoidCycle"))).build();
  assertThat(dao.select(query)).hasSize(1);
  assertThat(dao.select(query).get(0).getId()).isEqualTo(100);
}
