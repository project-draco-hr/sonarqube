{
  String status=statuses.next();
  String resolution=null;
  if (status.equals(Issue.STATUS_CLOSED)) {
    resolution=closedResolutions.next();
  }
 else   if (status.equals(Issue.STATUS_RESOLVED)) {
    resolution=resolvedResolutions.next();
  }
  return IssueTesting.newDto(rule,file,project).setMessage("Message from rule " + rule.getKey().toString() + " on line "+ index).setLine(index).setAssignee(users.next()).setReporter(users.next()).setAuthorLogin(users.next()).setSeverity(severities.next()).setStatus(status).setResolution(resolution);
}
