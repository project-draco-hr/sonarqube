{
  byte[] cached=get(obj,new PersistentCacheLoader<byte[]>(){
    @Override public byte[] get() throws IOException {
      String s=valueLoader.get();
      if (s != null) {
        return s.getBytes(ENCODING);
      }
      return null;
    }
  }
);
  if (cached == null) {
    return null;
  }
  return new String(cached,ENCODING);
}
