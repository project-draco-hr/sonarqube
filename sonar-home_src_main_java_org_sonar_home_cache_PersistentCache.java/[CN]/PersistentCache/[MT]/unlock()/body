{
  if (lock != null) {
    try {
      lock.release();
    }
 catch (    IOException e) {
      logger.error("Error releasing lock",e);
    }
  }
  if (lock_fc != null) {
    try {
      lock_fc.close();
    }
 catch (    IOException e) {
      logger.error("Error closing file channel",e);
    }
  }
  if (lock_raf != null) {
    try {
      lock_raf.close();
    }
 catch (    IOException e) {
      logger.error("Error closing file",e);
    }
  }
  lock=null;
  lock_raf=null;
  lock_fc=null;
}
