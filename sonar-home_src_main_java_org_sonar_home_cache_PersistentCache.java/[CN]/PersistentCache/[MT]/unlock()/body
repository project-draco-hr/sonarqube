{
  if (lockFile != null) {
    try {
      lockFile.release();
    }
 catch (    IOException e) {
      logger.error("Error releasing lock",e);
    }
  }
  if (lockChannel != null) {
    try {
      lockChannel.close();
    }
 catch (    IOException e) {
      logger.error("Error closing file channel",e);
    }
  }
  if (lockRandomAccessFile != null) {
    try {
      lockRandomAccessFile.close();
    }
 catch (    IOException e) {
      logger.error("Error closing file",e);
    }
  }
  lockFile=null;
  lockRandomAccessFile=null;
  lockChannel=null;
}
