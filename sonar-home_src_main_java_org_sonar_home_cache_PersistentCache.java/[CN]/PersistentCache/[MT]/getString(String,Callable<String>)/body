{
  byte[] cached=get(obj,new Callable<byte[]>(){
    @Override public byte[] call() throws Exception {
      String s=valueLoader.call();
      if (s != null) {
        return s.getBytes(ENCODING);
      }
      return null;
    }
  }
);
  if (cached == null) {
    return null;
  }
  return new String(cached,ENCODING);
}
