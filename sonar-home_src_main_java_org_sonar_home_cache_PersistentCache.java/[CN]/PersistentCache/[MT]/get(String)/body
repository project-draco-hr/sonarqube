{
  String key=getKey(obj);
  try {
    lock();
    byte[] cached=getCache(key);
    if (cached != null) {
      logger.debug("cache hit for " + obj + " -> "+ key);
      return cached;
    }
    logger.debug("cache miss for " + obj + " -> "+ key);
  }
  finally {
    unlock();
  }
  return null;
}
