{
  StringBuilder sb=new StringBuilder();
  if (set != null) {
    boolean first=true;
    for (    Multiset.Entry<?> entry : set.entrySet()) {
      if (!first) {
        sb.append(";");
      }
      sb.append(entry.getElement().toString());
      sb.append("=");
      sb.append(entry.getCount());
      first=false;
    }
  }
  return sb.toString();
}
