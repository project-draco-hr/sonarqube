{
  StringBuilder sb=new StringBuilder();
  boolean first=true;
  for (  Map.Entry<?,?> entry : map.entrySet()) {
    if (!first) {
      sb.append(";");
    }
    sb.append(entry.getKey().toString());
    sb.append("=");
    if (entry.getValue() != null) {
      sb.append(entry.getValue());
    }
    first=false;
  }
  return sb.toString();
}
