{
  StringBuilder sb=new StringBuilder();
  if (bag != null) {
    boolean first=true;
    for (    Object obj : bag.uniqueSet()) {
      if (!first) {
        sb.append(PAIR_SEPARATOR);
      }
      sb.append(obj.toString());
      sb.append(FIELD_SEPARATOR);
      sb.append(bag.getCount(obj) + var);
      first=false;
    }
  }
  return sb.toString();
}
