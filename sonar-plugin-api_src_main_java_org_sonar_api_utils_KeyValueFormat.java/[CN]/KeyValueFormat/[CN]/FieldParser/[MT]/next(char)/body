{
  if (position >= csv.length()) {
    return null;
  }
  Context context=new Context();
  context.firstChar=csv.charAt(position);
  checkEscaped(context);
  boolean isEnd=false;
  while (position < csv.length() && !isEnd) {
    isEnd=advance(separator,context);
  }
  return context.result.toString();
}
