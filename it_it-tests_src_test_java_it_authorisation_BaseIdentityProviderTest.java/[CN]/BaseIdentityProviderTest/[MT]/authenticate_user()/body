{
  enablePlugin();
  setUserCreatedByAuthPlugin(USER_LOGIN,USER_PROVIDER_ID,USER_NAME,USER_EMAIL);
  ORCHESTRATOR.executeSelenese(Selenese.builder().setHtmlTestsInClasspath("authenticate_through_ui","/authorisation/BaseIdentityProviderTest/authenticate_user.html").build());
