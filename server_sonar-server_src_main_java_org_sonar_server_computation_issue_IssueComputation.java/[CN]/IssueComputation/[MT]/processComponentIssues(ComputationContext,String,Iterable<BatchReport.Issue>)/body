{
  linesCache.init(componentUuid);
  for (  BatchReport.Issue reportIssue : issues) {
    DefaultIssue issue=toDefaultIssue(context,componentUuid,reportIssue);
    if (issue.isNew()) {
      guessAuthor(issue);
      autoAssign(issue);
      copyRuleTags(issue);
    }
    diskIssuesAppender.append(issue);
  }
  linesCache.clear();
}
