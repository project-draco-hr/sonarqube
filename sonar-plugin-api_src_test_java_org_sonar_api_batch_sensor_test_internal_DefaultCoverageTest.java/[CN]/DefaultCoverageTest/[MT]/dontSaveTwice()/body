{
  SensorStorage storage=mock(SensorStorage.class);
  DefaultCoverage coverage=new DefaultCoverage(storage).onFile(main).ofType(CoverageType.UNIT).lineHits(1,2).lineHits(2,5).lineHits(3,0).lineHits(4,0);
  coverage.save();
  thrown.expect(IllegalStateException.class);
  thrown.expectMessage("This object was already saved");
  coverage.save();
}
