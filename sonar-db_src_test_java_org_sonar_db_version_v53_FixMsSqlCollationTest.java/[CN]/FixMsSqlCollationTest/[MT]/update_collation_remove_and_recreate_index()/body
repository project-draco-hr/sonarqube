{
  useMssql();
  new FixMsSqlCollation.UpdateTableCollation(context,db,"rules").addIndex("rules_repo_key","plugin_name","plugin_rule_key").addVarcharColumn("plugin_rule_key",200).execute();
  verify(context).execute("DROP INDEX rules_repo_key ON rules");
  verify(context).execute("CREATE INDEX rules_repo_key ON rules(plugin_name,plugin_rule_key)");
}
