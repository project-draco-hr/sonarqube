{
  File projectDir=new File(XooMediumTest.class.getResource("/mediumtest/xoo/sample").toURI());
  TaskResult result=tester.newScanTask(new File(projectDir,"sonar-project.properties")).property("sonar.issue.ignore.allfile","1").property("sonar.issue.ignore.allfile.1.fileRegexp","object").start();
  assertThat(result.measures()).hasSize(19);
  assertThat(result.issues()).hasSize(19);
}
