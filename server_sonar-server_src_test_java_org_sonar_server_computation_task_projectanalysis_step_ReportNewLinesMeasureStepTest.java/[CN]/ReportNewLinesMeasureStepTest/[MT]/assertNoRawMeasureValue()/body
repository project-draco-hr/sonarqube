{
  assertRawMeasureValue(FILE_1_REF,NEW_NCLOC_KEY,0);
  assertRawMeasureValue(FILE_2_REF,NEW_NCLOC_KEY,0);
  assertRawMeasureValue(FILE_3_REF,NEW_NCLOC_KEY,0);
  assertRawMeasureValue(FILE_4_REF,NEW_NCLOC_KEY,0);
