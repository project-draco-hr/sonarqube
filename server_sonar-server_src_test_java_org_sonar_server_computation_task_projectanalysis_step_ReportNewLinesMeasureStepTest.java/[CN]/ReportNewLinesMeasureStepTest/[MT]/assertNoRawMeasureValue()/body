  assertRawMeasureValue(SUB_MODULE_2_REF,NEW_NCLOC_KEY,0);
  assertRawMeasureValue(MODULE_REF,NEW_NCLOC_KEY,0);
  assertRawMeasureValue(ROOT_REF,NEW_NCLOC_KEY,0);
}
