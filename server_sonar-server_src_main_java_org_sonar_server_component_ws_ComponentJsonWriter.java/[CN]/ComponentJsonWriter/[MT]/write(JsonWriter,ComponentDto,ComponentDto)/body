{
  json.beginObject().prop("uuid",component.uuid()).prop("key",component.key()).prop("id",component.getId()).prop("qualifier",component.qualifier()).prop("name",component.name()).prop("longName",component.longName()).prop("enabled",component.isEnabled()).prop("path",component.path()).prop("projectId",(component.projectUuid() != null && component.parentProjectId() != null) ? project.getId() : null).prop("subProjectId",component.parentProjectId()).endObject();
}
