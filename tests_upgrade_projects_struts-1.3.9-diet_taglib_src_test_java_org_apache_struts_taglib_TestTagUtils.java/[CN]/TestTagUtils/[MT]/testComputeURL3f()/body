{
  request.setPathElements("/myapp","/action.do",null,null);
  Map map=new HashMap();
  map.put("foo1","bar1");
  map.put("foo2","bar2");
  String url=null;
  try {
    url=tagutils.computeURL(pageContext,null,null,"/bar",null,null,map,"anchor",false);
  }
 catch (  MalformedURLException e) {
    fail("MalformedURLException: " + e);
  }
  assertNotNull("url present",url);
  assertTrue("url value",url.equals("/myapp/bar?foo1=bar1&amp;foo2=bar2#anchor") || url.equals("/myapp/bar?foo2=bar2&amp;foo1=bar1#anchor"));
}
