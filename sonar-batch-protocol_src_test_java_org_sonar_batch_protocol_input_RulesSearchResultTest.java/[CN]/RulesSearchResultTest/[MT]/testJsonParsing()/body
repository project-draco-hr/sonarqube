{
  Rule rule1=new Rule("squid:S1194","squid","S1194","\"java.lang.Error\" should not be extended","MAJOR","java");
  Rule rule2=new Rule("squid:ObjectFinalizeOverridenCallsSuperFinalizeCheck","squid","ObjectFinalizeOverridenCallsSuperFinalizeCheck","super.finalize() should be called at the end of Object.finalize() implementations","BLOCKER","java");
  RulesSearchResult rules=new RulesSearchResult();
  rules.setRules(Lists.newArrayList(rule1,rule2));
  JsonAssert.assertJson(getClass().getResource("RulesSearchTest/expected.json")).isSimilarTo(rules.toJson());
}
