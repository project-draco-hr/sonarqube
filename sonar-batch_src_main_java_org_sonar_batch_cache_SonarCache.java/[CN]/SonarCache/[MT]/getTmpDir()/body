{
  if (!tmpDir.exists()) {
    LOG.debug("Creating temporary cache directory: " + tmpDir.getAbsolutePath());
    try {
      FileUtils.forceMkdir(tmpDir);
    }
 catch (    IOException e) {
      throw new SonarException("Unable to create temporary cache directory " + tmpDir.getAbsolutePath(),e);
    }
  }
  return tmpDir;
}
