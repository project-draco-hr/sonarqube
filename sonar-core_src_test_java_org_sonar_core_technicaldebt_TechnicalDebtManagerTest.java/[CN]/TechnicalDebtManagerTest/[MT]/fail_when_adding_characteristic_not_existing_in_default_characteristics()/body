{
  setupData("empty");
  addPluginModel("java","fake-default-model-with-addtionnal-characteristic.xml");
  try {
    manager.initAndMergePlugins(ValidationMessages.create(),defaultRuleCache());
    getSession().commit();
    fail();
  }
 catch (  Exception e) {
