{
  when(technicalDebtModelRepository.getContributingPluginList()).thenReturn(ImmutableList.of(pluginKey));
  when(technicalDebtModelRepository.createReaderForXMLFile(pluginKey)).thenReturn(new FileReader(Resources.getResource(TechnicalDebtManagerTest.class,"TechnicalDebtManagerTest/" + xmlFile).getPath()));
