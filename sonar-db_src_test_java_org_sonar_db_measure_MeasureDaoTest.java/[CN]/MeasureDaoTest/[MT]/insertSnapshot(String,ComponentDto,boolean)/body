{
  SnapshotDto snapshotDto=new SnapshotDto().setUuid(uuid).setComponentUuid(componentDto.uuid()).setRootComponentUuid(componentDto.projectUuid()).setLast(last).setQualifier(componentDto.qualifier()).setScope(componentDto.scope());
  dbClient.snapshotDao().insert(dbSession,snapshotDto);
  dbSession.commit();
  return snapshotDto;
}
