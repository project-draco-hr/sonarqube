{
  db.prepareDbUnit(getClass(),"with_some_measures_for_developer.xml");
  List<MeasureDto> measureDtos=underTest.selectByDeveloperForSnapshotAndMetrics(dbSession,DEVELOPER_ID,SNAPSHOT_ID,ImmutableList.of(AUTHORS_BY_LINE_METRIC_ID,COVERAGE_LINE_HITS_DATA_METRIC_ID,NCLOC_METRIC_ID));
  assertThat(measureDtos).extracting("id").containsOnly(30L,31L,32L);
}
