{
  db.prepareDbUnit(getClass(),"with_some_measures_for_developer.xml");
  List<MeasureDto> measureDtos=underTest.selectBySnapshotAndMetrics(db.getSession(),SNAPSHOT_ID,ImmutableList.of(AUTHORS_BY_LINE_METRIC_ID,COVERAGE_LINE_HITS_DATA_METRIC_ID,NCLOC_METRIC_ID));
  assertThat(measureDtos).extracting("id").containsOnly(20L,21L,22L);
}
