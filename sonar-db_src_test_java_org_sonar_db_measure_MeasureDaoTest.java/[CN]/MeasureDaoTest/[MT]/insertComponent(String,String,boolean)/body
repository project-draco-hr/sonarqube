{
  String uuid=UuidFactoryImpl.INSTANCE.create();
  ComponentDto componentDto=new ComponentDto().setUuid(uuid).setScope(scope).setQualifier(qualifier).setProjectUuid("don't care").setRootUuid("don't care").setUuidPath("don't care").setKey("kee_" + uuid).setEnabled(enabled);
  db.getDbClient().componentDao().insert(db.getSession(),componentDto);
  return uuid;
}
