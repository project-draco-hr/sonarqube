{
  try {
    RangeOffsetHelper.offsetToString(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(1).setStartOffset(10).setEndOffset(11).build(),1,5);
    failBecauseExceptionWasNotThrown(IllegalArgumentException.class);
  }
 catch (  IllegalArgumentException e) {
    assertThat(e).hasMessage("Start offset 10 is defined outside the length (5) of the line 1");
  }
}
