{
  boolean keep=(ClassUtils.isAssignable(extension.getClass(),type) || (Sensor.class.equals(type) && ClassUtils.isAssignable(extension.getClass(),Analyzer.class))) && (matcher == null || matcher.accept(extension));
  if (keep && project != null && ClassUtils.isAssignable(extension.getClass(),CheckProject.class)) {
    keep=((CheckProject)extension).shouldExecuteOnProject(project);
  }
  return keep;
}
