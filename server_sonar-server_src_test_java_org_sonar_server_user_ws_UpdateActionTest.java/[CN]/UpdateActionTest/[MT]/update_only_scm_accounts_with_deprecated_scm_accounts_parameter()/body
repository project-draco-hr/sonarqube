{
  createUser();
  tester.newPostRequest("api/users","update").setParam("login","john").setParam("scm_accounts","jon.snow").execute().assertJson(getClass(),"update_scm_accounts.json");
  UserDto user=dbClient.userDao().selectByLogin(session,"john");
  assertThat(user.getScmAccountsAsList()).containsOnly("jon.snow");
}
