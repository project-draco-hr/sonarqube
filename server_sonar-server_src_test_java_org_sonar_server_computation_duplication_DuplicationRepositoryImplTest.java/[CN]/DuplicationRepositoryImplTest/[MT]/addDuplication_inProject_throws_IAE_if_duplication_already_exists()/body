{
  underTest.addDuplication(COMPONENT_1,ORIGINAL_TEXTBLOCK,COMPONENT_2,DUPLICATE_TEXTBLOCK_1);
  expectedException.expect(IllegalArgumentException.class);
  expectedException.expectMessage(format("In-project duplicate %s in file %s is already associated to original %s in file %s",DUPLICATE_TEXTBLOCK_1,COMPONENT_2.getKey(),ORIGINAL_TEXTBLOCK,COMPONENT_1.getKey()));
  underTest.addDuplication(COMPONENT_1,ORIGINAL_TEXTBLOCK,COMPONENT_2,DUPLICATE_TEXTBLOCK_1);
}
