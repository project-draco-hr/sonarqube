{
  register(new Rules(){
    @Override public void init(    RulesDefinition.NewRepository repository){
      repository.createRule("x1").setName("Name1").setHtmlDescription("Desc1");
    }
  }
);
  register(new Rules(){
    @Override public void init(    RulesDefinition.NewRepository repository){
      repository.createRule(RuleTesting.XOO_X1.rule()).setName("Name1").setHtmlDescription("Desc2");
    }
  }
);
  Rule rule=ruleIndex.getByKey(RuleTesting.XOO_X1);
  assertThat(rule.name()).isEqualTo("Name1");
  assertThat(rule.htmlDescription()).isEqualTo("Desc2");
}
