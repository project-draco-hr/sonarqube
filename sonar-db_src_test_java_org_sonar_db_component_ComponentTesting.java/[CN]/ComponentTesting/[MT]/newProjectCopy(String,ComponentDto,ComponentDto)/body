{
  checkNotNull(project.getId(),"The project need to be persisted before creating this technical project.");
  return newChildComponent(uuid,view).setUuid(uuid).setKey(view.key() + project.key()).setName(project.name()).setLongName(project.longName()).setCopyResourceId(project.getId()).setScope(Scopes.FILE).setQualifier(Qualifiers.PROJECT).setPath(null).setLanguage(null);
}
