{
  checkNotNull(project.getId(),"The project need to be persisted before creating this technical project.");
  return newChildComponent(uuid,view,view).setUuid(uuid).setKey(view.key() + project.key()).setName(project.name()).setLongName(project.longName()).setCopyComponentUuid(project.uuid()).setScope(Scopes.FILE).setQualifier(Qualifiers.PROJECT).setPath(null).setLanguage(null);
}
