{
  return new ComponentDto().setUuid(uuid).setUuidPath(ComponentDto.UUID_PATH_OF_ROOT).setProjectUuid(uuid).setModuleUuidPath(UUID_PATH_SEPARATOR + uuid + UUID_PATH_SEPARATOR).setRootUuid(uuid).setKey(uuid).setName(name).setLongName(name).setScope(Scopes.PROJECT).setQualifier("DEV").setPath(null).setLanguage(null).setEnabled(true);
}
