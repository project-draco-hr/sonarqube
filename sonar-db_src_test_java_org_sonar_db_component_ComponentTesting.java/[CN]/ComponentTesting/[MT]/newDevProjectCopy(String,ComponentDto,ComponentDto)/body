{
  checkNotNull(project.getId(),"The project need to be persisted before creating this technical project.");
  return newChildComponent(uuid,developer).setUuid(uuid).setKey(developer.key() + ":" + project.key()).setName(project.name()).setLongName(project.longName()).setCopyComponentUuid(project.uuid()).setScope(Scopes.PROJECT).setQualifier("DEV_PRJ").setPath(null).setLanguage(null);
}
