{
  String uuid=Uuids.create();
  return new ComponentDto().setUuid(uuid).setProjectUuid(uuid).setModuleUuidPath(MODULE_UUID_PATH_SEP + uuid + MODULE_UUID_PATH_SEP).setRootUuid(uuid).setKey(uuid).setName(name).setLongName(name).setScope(Scopes.PROJECT).setQualifier("DEV").setPath(null).setLanguage(null).setEnabled(true);
}
