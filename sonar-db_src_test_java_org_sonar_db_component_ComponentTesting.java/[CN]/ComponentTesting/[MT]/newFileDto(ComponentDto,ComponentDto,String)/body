{
  String path="src/main/xoo/org/sonar/samples/File.xoo";
  return newChildComponent(fileUuid,module,directory == null ? module : directory).setKey("KEY_" + fileUuid).setName("NAME_" + fileUuid).setLongName(path).setScope(Scopes.FILE).setQualifier(Qualifiers.FILE).setPath(path).setCreatedAt(new Date()).setLanguage("xoo");
}
