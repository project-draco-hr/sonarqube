{
  Reader pmdSonarWayProfile=new InputStreamReader(this.getClass().getResourceAsStream("/org/sonar/plugins/pmd/profile-sun-conventions.xml"));
  RulesProfile profile=importer.importProfile(pmdSonarWayProfile,messages);
  profile.setName(RulesProfile.SUN_CONVENTIONS_NAME);
  profile.setLanguage(Java.KEY);
  return profile;
}
