{
  Cache<String,String> cache=caches.createCache("issues");
  assertThat(cache.get("foo")).isNull();
  cache.put("foo","bar");
  assertThat(cache.get("foo")).isEqualTo("bar");
  assertThat(cache.keySet()).containsOnly("foo");
  cache.remove("foo");
  assertThat(cache.get("foo")).isNull();
  assertThat(cache.keySet()).isEmpty();
}
