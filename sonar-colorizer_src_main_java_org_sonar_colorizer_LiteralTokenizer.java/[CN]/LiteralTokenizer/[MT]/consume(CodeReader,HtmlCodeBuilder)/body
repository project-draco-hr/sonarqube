{
  if (code.peek() == '\'' || code.peek() == '\"') {
    codeBuilder.appendWithoutTransforming(tagBefore);
    int firstChar=code.peek();
    code.popTo(new EndCommentMatcher(firstChar,code),codeBuilder);
    codeBuilder.appendWithoutTransforming(tagAfter);
    return true;
  }
 else {
    return false;
  }
}
