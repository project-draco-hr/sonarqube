{
  ScmInfo scmInfo=new ReportScmInfo(BatchReport.Changesets.newBuilder().setComponentRef(FILE_REF).addChangeset(BatchReport.Changesets.Changeset.newBuilder().setAuthor("john").setDate(123456789L).setRevision("rev-1").build()).addChangeset(BatchReport.Changesets.Changeset.newBuilder().setAuthor("henry").setDate(1234567810L).setRevision("rev-2").build()).addChangesetIndexByLine(0).addChangesetIndexByLine(1).addChangesetIndexByLine(1).addChangesetIndexByLine(0).build());
  assertThat(scmInfo.getAllChangesets()).hasSize(4);
  Changeset changeset=scmInfo.getChangesetForLine(4);
  assertThat(changeset.getAuthor()).isEqualTo("john");
  assertThat(changeset.getDate()).isEqualTo(123456789L);
  assertThat(changeset.getRevision()).isEqualTo("rev-1");
}
