{
  thrown.expect(IllegalStateException.class);
  thrown.expectMessage("Changeset on line 1 must have a date");
  new ReportScmInfo(BatchReport.Changesets.newBuilder().setComponentRef(FILE_REF).addChangeset(BatchReport.Changesets.Changeset.newBuilder().setAuthor("john").setRevision("rev-1").build()).addChangesetIndexByLine(0).build());
}
