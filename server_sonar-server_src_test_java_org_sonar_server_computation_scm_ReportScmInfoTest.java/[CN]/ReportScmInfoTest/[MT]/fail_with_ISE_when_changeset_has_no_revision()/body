{
  thrown.expect(IllegalStateException.class);
  thrown.expectMessage("Changeset on line 1 must have a revision");
  new ReportScmInfo(BatchReport.Changesets.newBuilder().setComponentRef(FILE_REF).addChangeset(BatchReport.Changesets.Changeset.newBuilder().setAuthor("john").setDate(123456789L).build()).addChangesetIndexByLine(0).build());
}
