{
  Future<T> res=new AbstractPartiallyImplementedFuture<T>(){
    @Override public T get() throws InterruptedException, ExecutionException {
      try {
        return task.call();
      }
 catch (      Exception e) {
        throw new ExecutionException(e);
      }
    }
  }
;
  futures.add(res);
  return res;
}
