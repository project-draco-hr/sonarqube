{
  if (dbConnector == null) {
    dbConnector=new MemoryDatabaseConnector();
    dbConnector.start();
    session=new JpaDatabaseSession(dbConnector);
    session.start();
  }
  databaseTester=new JdbcDatabaseTester(MemoryDatabaseConnector.DRIVER,MemoryDatabaseConnector.URL,MemoryDatabaseConnector.USER,MemoryDatabaseConnector.PASSWORD);
  databaseTester.onTearDown();
}
