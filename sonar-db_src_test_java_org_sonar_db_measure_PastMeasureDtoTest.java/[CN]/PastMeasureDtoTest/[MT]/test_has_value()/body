{
  PastMeasureDto measureWithValue=new PastMeasureDto().setId(10L).setValue(1d).setMetricId(2).setPersonId(5);
  assertThat(measureWithValue.hasValue()).isTrue();
  PastMeasureDto measureWithoutValue=new PastMeasureDto().setId(10L).setMetricId(2).setPersonId(5);
  assertThat(measureWithoutValue.hasValue()).isFalse();
}
