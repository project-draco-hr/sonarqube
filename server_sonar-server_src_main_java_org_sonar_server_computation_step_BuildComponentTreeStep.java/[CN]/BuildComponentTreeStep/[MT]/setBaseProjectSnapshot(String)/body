{
  DbSession dbSession=dbClient.openSession(false);
  try {
    SnapshotDto snapshotDto=dbClient.snapshotDao().selectSnapshotByQuery(dbSession,new SnapshotQuery().setComponentUuid(projectUuid).setIsLast(true));
    analysisMetadataHolder.setBaseProjectSnapshot(toSnapshot(snapshotDto));
  }
  finally {
    dbClient.closeSession(dbSession);
  }
}
