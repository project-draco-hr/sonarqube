{
  WebService.NewAction action=newController.createAction("show").setDescription("Detail of a dashboard (name, description, layout and widgets)").setInternal(true).setSince("5.0").setResponseExample(getClass().getResource("show-example.json")).setHandler(this);
  action.createParam(PARAM_KEY).setDescription("Dashboard key").setExampleValue("12345").setRequired(true);
}
