{
  Activity activity=new Activity();
  activity.setType(Activity.Type.ANALYSIS_REPORT);
  activity.setAction("LOG_ANALYSIS_REPORT");
  activity.setData("key",String.valueOf(report.getId())).setData("projectKey",project.key()).setData("projectName",project.name()).setData("projectUuid",project.uuid()).setData("status",String.valueOf(report.getStatus())).setData("submittedAt",formatDateTimeNullSafe(longToDate(report.getCreatedAt()))).setData("startedAt",formatDateTimeNullSafe(longToDate(report.getStartedAt()))).setData("finishedAt",formatDateTimeNullSafe(longToDate(report.getFinishedAt())));
  activityService.save(activity);
}
