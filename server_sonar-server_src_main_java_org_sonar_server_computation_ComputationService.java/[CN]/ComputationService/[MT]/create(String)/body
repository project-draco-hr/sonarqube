{
  AnalysisReportDto report=new AnalysisReportDto().setProjectKey(projectKey).setStatus(PENDING);
  AnalysisReportDao dao=dbClient.analysisReportDao();
  DbSession session=dbClient.openSession(false);
  try {
    dao.insert(session,report);
    session.commit();
  }
  finally {
    session.close();
  }
}
