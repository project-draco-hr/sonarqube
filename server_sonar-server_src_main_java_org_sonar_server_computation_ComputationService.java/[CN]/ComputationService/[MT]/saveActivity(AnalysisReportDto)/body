{
  ComponentDto project=loadProject(report.getProjectKey());
  Activity activity=new Activity();
  activity.setType(Activity.Type.ANALYSIS_REPORT);
  activity.setAction("LOG_ANALYSIS_REPORT");
  activity.setData("key",String.valueOf(report.getId())).setData("projectKey",report.getProjectKey()).setData("status",String.valueOf(report.getStatus())).setData("submittedAt",formatDateTimeNullSafe(longToDate(report.getCreatedAt()))).setData("startedAt",formatDateTimeNullSafe(longToDate(report.getStartedAt()))).setData("finishedAt",formatDateTimeNullSafe(longToDate(report.getFinishedAt())));
  if (project != null) {
    activity.setData("projectName",project.name()).setData("projectUuid",project.uuid());
  }
  activityService.save(activity);
}
