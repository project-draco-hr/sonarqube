{
  return new IssueDto().setKee(issue.key()).setLine(issue.line()).setMessage(issue.message()).setEffortToFix(issue.effortToFix()).setDebt(issue.debt()).setResolution(issue.resolution()).setStatus(issue.status()).setSeverity(issue.severity()).setChecksum(issue.checksum()).setManualSeverity(issue.manualSeverity()).setReporter(issue.reporter()).setAssignee(issue.assignee()).setActionPlanKey(issue.actionPlanKey()).setIssueAttributes(KeyValueFormat.format(issue.attributes())).setAuthorLogin(issue.authorLogin()).setIssueCreationDate(issue.creationDate()).setIssueCloseDate(issue.closeDate()).setIssueUpdateDate(issue.updateDate()).setSelectedAt(issue.selectedAt()).setUpdatedAt(now);
}
