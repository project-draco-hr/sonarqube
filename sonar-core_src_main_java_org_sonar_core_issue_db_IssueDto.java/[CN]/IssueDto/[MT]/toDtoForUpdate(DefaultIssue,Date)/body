{
  return new IssueDto().setKee(issue.key()).setLine(issue.line()).setMessage(issue.message()).setEffortToFix(issue.effortToFix()).setResolution(issue.resolution()).setStatus(issue.status()).setSeverity(issue.severity()).setChecksum(issue.getChecksum()).setManualSeverity(issue.manualSeverity()).setReporter(issue.reporter()).setAssignee(issue.assignee()).setActionPlanKey(issue.actionPlanKey()).setIssueAttributes(issue.attributes() != null ? KeyValueFormat.format(issue.attributes()) : "").setAuthorLogin(issue.authorLogin()).setIssueCreationDate(issue.creationDate()).setIssueCloseDate(issue.closeDate()).setIssueUpdateDate(issue.updateDate()).setUpdatedAt(now);
}
