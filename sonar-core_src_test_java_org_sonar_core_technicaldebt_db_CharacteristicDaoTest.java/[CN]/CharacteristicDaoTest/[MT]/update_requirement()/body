{
  setupData("update_requirement");
  CharacteristicDto dto=new CharacteristicDto().setId(1).setParentId(3).setRootId(1).setRuleId(2).setFunction("linear").setFactorValue(21.0).setFactorUnit("h").setOffsetValue(null).setOffsetUnit(null).setCreatedAt(DateUtils.parseDate("2013-11-22")).setEnabled(false);
  dao.update(dto);
