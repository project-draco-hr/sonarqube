{
  lastChar=-1;
  cursor=new Cursor();
  this.bufferCapacity=bufferCapacity;
  buffer=new char[bufferCapacity];
  Reader reader=initialCodeReader;
  for (  CodeReaderFilter<?> codeReaderFilter : codeReaderFilters) {
    reader=new Filter(reader,codeReaderFilter);
  }
  this.code=reader;
  fillBuffer();
}
