{
  WebService.NewAction action=controller.createAction("raw").setDescription("Get source code as plain text. Require Code viewer permission on file").setSince("5.0").setResponseExample(Resources.getResource(getClass(),"example-raw.txt")).setHandler(this);
  action.createParam("key").setRequired(true).setDescription("File key").setExampleValue("my_project:/src/foo/Bar.php");
}
