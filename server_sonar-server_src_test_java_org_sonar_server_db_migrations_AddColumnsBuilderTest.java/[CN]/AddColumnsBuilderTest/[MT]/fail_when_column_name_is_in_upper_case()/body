{
  try {
    new AddColumnsBuilder.ColumnDef().setName("DATE_IN_MS").setType(AddColumnsBuilder.ColumnDef.Type.BIG_INTEGER).setNullable(true);
    failBecauseExceptionWasNotThrown(IllegalArgumentException.class);
  }
 catch (  IllegalArgumentException e) {
    assertThat(e).hasMessage("Column name should only contains lowercase and _ characters");
  }
}
