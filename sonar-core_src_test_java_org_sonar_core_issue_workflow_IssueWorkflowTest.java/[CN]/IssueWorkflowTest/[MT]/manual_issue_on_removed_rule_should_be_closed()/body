{
  DefaultIssue issue=new DefaultIssue().setKey("ABCDE").setStatus(Issue.STATUS_OPEN).setRuleKey(RuleKey.of("manual","Performance")).setReporter("simon").setEndOfLife(true).setOnDisabledRule(true);
  workflow.start();
  workflow.doAutomaticTransition(issue,mock(IssueChangeContext.class));
  assertThat(issue.resolution()).isEqualTo("REMOVED");
  assertThat(issue.status()).isEqualTo("CLOSED");
}
