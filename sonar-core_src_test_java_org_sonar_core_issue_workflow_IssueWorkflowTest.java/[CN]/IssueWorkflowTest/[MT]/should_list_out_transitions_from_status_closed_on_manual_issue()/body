{
  workflow.start();
  DefaultIssue issue=new DefaultIssue().setKey("ABCDE").setStatus(Issue.STATUS_CLOSED).setRuleKey(RuleKey.of("manual","Performance")).setReporter("simon");
  List<Transition> transitions=workflow.outTransitions(issue);
  assertThat(transitions).hasSize(1);
  assertThat(keys(transitions)).containsOnly("reopen");
}
