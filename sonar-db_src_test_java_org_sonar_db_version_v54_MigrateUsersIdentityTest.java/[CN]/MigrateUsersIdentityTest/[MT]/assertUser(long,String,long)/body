{
  Map<String,Object> result=db.selectFirst("SELECT u.external_identity as \"externalIdentity\", " + "u.external_identity_provider as \"externalIdentityProvider\", " + "u.updated_at as \"updatedAt\" "+ "FROM users u WHERE u.id="+ userId);
  assertThat(result.get("externalIdentity")).isEqualTo(expectedAuthorityId);
  assertThat(result.get("externalIdentityProvider")).isEqualTo("sonarqube");
  assertThat(result.get("updatedAt")).isEqualTo(expectedUpdatedAt);
}
