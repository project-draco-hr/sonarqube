{
  when(dao.selectByRuleId(1)).thenReturn(new CharacteristicDto().setId(3).setRuleId(10).setParentId(2).setRootId(1).setFunction("linear").setFactorValue(30.0).setFactorUnit("mn"));
  when(ruleFinder.findById(1)).thenReturn(null);
  try {
    finder.findRequirementByRuleId(1);
    fail();
  }
 catch (  Exception e) {
    assertThat(e).isInstanceOf(IllegalArgumentException.class);
  }
}
