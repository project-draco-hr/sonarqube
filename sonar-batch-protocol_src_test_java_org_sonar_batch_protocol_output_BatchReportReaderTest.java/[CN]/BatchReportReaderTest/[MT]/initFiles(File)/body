{
  BatchOutputWriter writer=new BatchOutputWriter(dir);
  BatchReport.Metadata.Builder metadata=BatchReport.Metadata.newBuilder().setAnalysisDate(15000000L).setProjectKey("PROJECT_A").setRootComponentRef(1).setDeletedComponentsCount(1);
  writer.writeMetadata(metadata.build());
  BatchReport.Component.Builder component=BatchReport.Component.newBuilder().setRef(1).setUuid("UUID_A");
  writer.writeComponent(component.build());
  BatchReport.Issue issue=BatchReport.Issue.newBuilder().setUuid("ISSUE_A").setLine(50).build();
  writer.writeComponentIssues(1,Arrays.asList(issue));
  writer.writeDeletedComponentIssues(1,"compUuid",Arrays.asList(issue));
}
