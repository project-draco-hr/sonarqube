{
  BatchReportWriter writer=new BatchReportWriter(dir);
  writer.writeMetadata(BatchReport.Metadata.newBuilder().setRootComponentRef(1).build());
  writer.writeComponent(BatchReport.Component.newBuilder().setRef(1).build());
  BatchReport.CpdTextBlock duplicationBlock=BatchReport.CpdTextBlock.newBuilder().setHash("abcdefghijklmnop").setStartLine(1).setEndLine(2).setStartTokenIndex(10).setEndTokenIndex(15).build();
  writer.writeCpdTextBlocks(1,singletonList(duplicationBlock));
  BatchReportReader sut=new BatchReportReader(dir);
  assertThat(sut.readCpdTextBlocks(1)).hasSize(1);
}
