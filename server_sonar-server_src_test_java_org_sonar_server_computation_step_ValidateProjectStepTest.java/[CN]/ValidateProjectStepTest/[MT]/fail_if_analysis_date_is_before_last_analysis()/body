  thrown.expectMessage("Latest analysis: ");
  reportReader.setMetadata(BatchReport.Metadata.newBuilder().setAnalysisDate(1420088400000L).build());
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(1).setType(Constants.ComponentType.PROJECT).setKey(PROJECT_KEY).addChildRef(2).build());
  ComponentDto project=ComponentTesting.newProjectDto("ABCD").setKey(PROJECT_KEY);
  dbClient.componentDao().insert(dbSession,project);
  dbClient.snapshotDao().insert(dbSession,SnapshotTesting.createForProject(project).setCreatedAt(1433131200000L));
  dbSession.commit();
  treeRootHolder.setRoot(DumbComponent.builder(Component.Type.PROJECT,1).setUuid("ABCD").setKey(PROJECT_KEY).build());
  sut.execute();
