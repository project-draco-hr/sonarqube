{
  thrown.expect(IllegalArgumentException.class);
  thrown.expectMessage("Unable to scan non-existing project '" + PROJECT_KEY + "'");
  settings.appendProperty(CoreProperties.CORE_PREVENT_AUTOMATIC_PROJECT_CREATION,"true");
  sut.execute(new ComputationContext(createBasicBatchReportReader(),null,null,null,ComponentTreeBuilders.from(new DumbComponent(Component.Type.PROJECT,1,"ABCD",PROJECT_KEY)),null));
}
