{
  File reportDir=temp.newFolder();
  thrown.expect(IllegalArgumentException.class);
  thrown.expectMessage("Validation of project failed:\n" + "  o \"bran#ch\" is not a valid branch name. Allowed characters are alphanumeric, '-', '_', '.' and '/'.");
  BatchReportWriter writer=new BatchReportWriter(reportDir);
  writer.writeMetadata(BatchReport.Metadata.newBuilder().setBranch("bran#ch").build());
  sut.execute(new ComputationContext(new BatchReportReader(reportDir),null,null,null,ComponentTreeBuilders.from(new DumbComponent(Component.Type.PROJECT,1,"ABCD",PROJECT_KEY)),null));
}
