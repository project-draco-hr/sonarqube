{
  String[] componentUuids={insertComponent("sc1","qu1"),insertComponent("sc2","qu2"),insertComponent("sc3","qu3"),insertComponent("sc4","qu4")};
  Long[] snapshotIds={insertSnapshot(componentUuids[0],"sc1","qu1"),insertSnapshot(componentUuids[1],"sc2","quW"),insertSnapshot(componentUuids[2],"scX","qu3"),insertSnapshot(componentUuids[3],"scY","quZ")};
  underTest.execute();
  assertSnapshot(snapshotIds[0],"sc1","qu1");
  assertSnapshot(snapshotIds[1],"sc2","qu2");
  assertSnapshot(snapshotIds[2],"sc3","qu3");
  assertSnapshot(snapshotIds[3],"sc4","qu4");
}
