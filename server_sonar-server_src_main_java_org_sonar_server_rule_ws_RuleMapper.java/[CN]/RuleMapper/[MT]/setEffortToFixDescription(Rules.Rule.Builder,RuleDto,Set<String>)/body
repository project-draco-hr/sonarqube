{
  if ((shouldReturnField(fieldsToReturn,FIELD_EFFORT_TO_FIX_DESCRIPTION) || shouldReturnField(fieldsToReturn,FIELD_GAP_DESCRIPTION)) && ruleDto.getGapDescription() != null) {
    ruleResponse.setEffortToFixDescription(ruleDto.getGapDescription());
    ruleResponse.setGapDescription(ruleDto.getGapDescription());
  }
}
