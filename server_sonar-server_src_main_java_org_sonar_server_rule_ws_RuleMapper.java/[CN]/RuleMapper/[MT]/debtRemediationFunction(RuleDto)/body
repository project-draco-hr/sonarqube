{
  final String function=ruleDto.getRemediationFunction();
  if (function == null || function.isEmpty()) {
    return defaultDebtRemediationFunction(ruleDto);
  }
 else {
    return new DefaultDebtRemediationFunction(DebtRemediationFunction.Type.valueOf(function.toUpperCase(Locale.ENGLISH)),ruleDto.getRemediationGapMultiplier(),ruleDto.getRemediationBaseEffort());
  }
}
