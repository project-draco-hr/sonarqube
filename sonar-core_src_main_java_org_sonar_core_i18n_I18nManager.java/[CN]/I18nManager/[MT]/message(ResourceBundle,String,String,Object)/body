{
  String value=null;
  if (resourceBundle != null) {
    try {
      value=resourceBundle.getString(key);
    }
 catch (    MissingResourceException e) {
    }
  }
  if (value == null) {
    value=defaultValue;
  }
  if (value != null && parameters.length > 0) {
    return MessageFormat.format(value,parameters);
  }
  return value;
}
