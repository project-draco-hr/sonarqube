{
  DefaultIssue trackedIssue=new org.sonar.core.issue.DefaultIssueBuilder().componentKey(component.key()).projectKey("unused").ruleKey(RuleKey.of(rawIssue.getRuleRepository(),rawIssue.getRuleKey())).effortToFix(rawIssue.hasEffortToFix() ? rawIssue.getEffortToFix() : null).line(rawIssue.hasLine() ? rawIssue.getLine() : null).message(rawIssue.hasMsg() ? rawIssue.getMsg() : null).severity(rawIssue.getSeverity().name()).build();
  return trackedIssue;
}
