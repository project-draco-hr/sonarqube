{
  BatchReportWriter writer=initBasicReport(1);
  writer.writeComponentSyntaxHighlighting(FILE_REF,newArrayList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(1).setStartOffset(4).setEndOffset(2).build()).setType(Constants.HighlightingType.ANNOTATION).build()));
  try {
    sut.execute(new ComputationContext(new BatchReportReader(reportDir),PROJECT_KEY));
    failBecauseExceptionWasNotThrown(IllegalStateException.class);
  }
 catch (  IllegalStateException e) {
    assertThat(e).hasMessage("Cannot persist sources of src/Foo.java").hasCauseInstanceOf(IllegalArgumentException.class);
  }
}
