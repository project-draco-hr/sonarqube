{
  initBasicReport(1);
  reportReader.putSyntaxHighlighting(FILE_REF,newArrayList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.TextRange.newBuilder().setStartLine(1).setEndLine(1).setStartOffset(4).setEndOffset(2).build()).setType(Constants.HighlightingType.ANNOTATION).build()));
  try {
    underTest.execute();
    failBecauseExceptionWasNotThrown(IllegalStateException.class);
  }
 catch (  IllegalStateException e) {
    assertThat(e).hasMessage("Cannot persist sources of MODULE_KEY:src/Foo.java").hasCauseInstanceOf(IllegalArgumentException.class);
  }
}
