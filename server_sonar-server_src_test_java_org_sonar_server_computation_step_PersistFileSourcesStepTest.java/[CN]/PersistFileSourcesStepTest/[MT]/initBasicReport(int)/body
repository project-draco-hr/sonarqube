{
  treeRootHolder.setRoot(new DumbComponent(Component.Type.PROJECT,1,PROJECT_UUID,PROJECT_KEY,new DumbComponent(Component.Type.MODULE,2,"MODULE","MODULE_KEY",new DumbComponent(Component.Type.FILE,FILE_REF,FILE_UUID,"MODULE_KEY:src/Foo.java"))));
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(1).setType(Constants.ComponentType.PROJECT).addChildRef(2).build());
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(2).setType(Constants.ComponentType.MODULE).addChildRef(FILE_REF).build());
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(FILE_REF).setType(Constants.ComponentType.FILE).setLines(numberOfLines).build());
  List<String> lines=newArrayList();
  for (int i=1; i <= numberOfLines; i++) {
    lines.add("line" + i);
  }
  reportReader.putFileSourceLines(FILE_REF,lines);
}
