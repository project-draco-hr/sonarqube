{
  return CreateConditionRequest.builder().setQualityGateId(request.mandatoryParamAsInt(PARAM_GATE_ID)).setMetricKey(request.mandatoryParam(PARAM_METRIC)).setOperator(request.mandatoryParam(PARAM_OPERATOR)).setWarning(request.param(PARAM_WARNING)).setError(request.param(PARAM_ERROR)).setPeriod(request.paramAsInt(PARAM_PERIOD)).build();
}
