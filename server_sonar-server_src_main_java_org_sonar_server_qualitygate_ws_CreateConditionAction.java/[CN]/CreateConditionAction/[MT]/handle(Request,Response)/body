{
  QGatesWs.writeQualityGateCondition(qualityGates.createCondition(QGatesWs.parseId(request,QGatesWs.PARAM_GATE_ID),request.mandatoryParam(QGatesWs.PARAM_METRIC),request.mandatoryParam(QGatesWs.PARAM_OPERATOR),request.param(QGatesWs.PARAM_WARNING),request.param(QGatesWs.PARAM_ERROR),request.paramAsInt(QGatesWs.PARAM_PERIOD)),response.newJsonWriter()).close();
}
