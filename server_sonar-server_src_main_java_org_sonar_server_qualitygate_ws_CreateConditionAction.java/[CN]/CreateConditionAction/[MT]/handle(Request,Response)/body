{
  QualityGatesWs.writeQualityGateCondition(qualityGates.createCondition(QualityGatesWs.parseId(request,QualityGatesWs.PARAM_GATE_ID),request.mandatoryParam(QualityGatesWs.PARAM_METRIC),request.mandatoryParam(QualityGatesWs.PARAM_OPERATOR),request.param(QualityGatesWs.PARAM_WARNING),request.param(QualityGatesWs.PARAM_ERROR),request.paramAsInt(QualityGatesWs.PARAM_PERIOD)),response.newJsonWriter()).close();
}
