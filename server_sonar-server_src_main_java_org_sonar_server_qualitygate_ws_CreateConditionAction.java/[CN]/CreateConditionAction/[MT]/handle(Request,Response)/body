{
  QualityGatesWs.writeQualityGateCondition(qualityGates.createCondition(QualityGatesWs.parseId(request,QualityGatesWsParameters.PARAM_GATE_ID),request.mandatoryParam(QualityGatesWsParameters.PARAM_METRIC),request.mandatoryParam(QualityGatesWsParameters.PARAM_OPERATOR),request.param(QualityGatesWsParameters.PARAM_WARNING),request.param(QualityGatesWsParameters.PARAM_ERROR),request.paramAsInt(QualityGatesWsParameters.PARAM_PERIOD)),response.newJsonWriter()).close();
}
