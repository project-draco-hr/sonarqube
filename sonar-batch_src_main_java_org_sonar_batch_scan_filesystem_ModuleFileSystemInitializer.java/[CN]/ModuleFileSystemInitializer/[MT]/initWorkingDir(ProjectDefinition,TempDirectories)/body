{
  workingDir=module.getWorkDir();
  if (workingDir == null) {
    workingDir=tempDirectories.getDir("work");
  }
 else {
    try {
      FileUtils.forceMkdir(workingDir);
    }
 catch (    Exception e) {
      throw new IllegalStateException("Fail to create working dir: " + workingDir.getAbsolutePath(),e);
    }
  }
}
