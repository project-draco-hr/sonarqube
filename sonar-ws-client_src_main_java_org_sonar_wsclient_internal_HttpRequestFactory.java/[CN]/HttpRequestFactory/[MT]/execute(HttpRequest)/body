{
  try {
    prepare(request);
    if (request.ok()) {
      return request.body("UTF-8");
    }
    throw new HttpException(request.url().toString(),request.code());
  }
 catch (  HttpException e) {
    throw e;
  }
catch (  HttpRequest.HttpRequestException e) {
    throw new IllegalStateException(e.getCause());
  }
}
