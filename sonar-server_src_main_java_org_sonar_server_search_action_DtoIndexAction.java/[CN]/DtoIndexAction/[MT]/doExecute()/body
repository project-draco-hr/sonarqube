{
  try {
    if (this.getMethod().equals(Method.DELETE)) {
      index.deleteByDto(this.item,this.items);
    }
 else     if (this.getMethod().equals(Method.UPSERT)) {
      index.upsertByDto(this.item,this.items);
    }
  }
 catch (  Exception e) {
    throw new IllegalStateException(this.getClass().getSimpleName() + " cannot execute " + this.getMethod()+ " for "+ this.item.getClass().getSimpleName()+ " as "+ this.getIndexType()+ " on key: "+ this.item.getKey(),e);
  }
}
