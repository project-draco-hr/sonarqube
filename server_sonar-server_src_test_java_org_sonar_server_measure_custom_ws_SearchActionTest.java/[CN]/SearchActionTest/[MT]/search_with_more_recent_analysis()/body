{
  long yesterday=DateUtils.addDays(new Date(),-1).getTime();
  MetricDto metric=insertCustomMetric(1);
  dbClient.customMeasureDao().insert(dbSession,newCustomMeasure(1,metric).setCreatedAt(yesterday).setUpdatedAt(yesterday));
  dbClient.snapshotDao().insert(dbSession,SnapshotTesting.createForProject(defaultProject));
  dbSession.commit();
  String response=newRequest().setParam(SearchAction.PARAM_PROJECT_ID,DEFAULT_PROJECT_UUID).execute().outputAsString();
  assertThat(response).matches(nameValuePattern("pending","false"));
}
