{
  BootstrapContainer container=BootstrapContainer.create(Collections.<String,String>emptyMap(),Lists.newArrayList(new BootstrapProperties(Collections.<String,String>emptyMap())));
  TaskContainer taskContainer=new TaskContainer(container,Collections.<String,String>emptyMap());
  taskContainer.installCoreTasks();
  assertThat(taskContainer.getComponentByType(ProjectReactorBuilder.class)).isNotNull().isInstanceOf(ProjectReactorBuilder.class);
  container=BootstrapContainer.create(Collections.<String,String>emptyMap(),Lists.newArrayList(new BootstrapProperties(Collections.<String,String>emptyMap()),new EnvironmentInformation("SonarQubeRunner","2.4")));
  taskContainer=new TaskContainer(container,Collections.<String,String>emptyMap());
  taskContainer.installCoreTasks();
  assertThat(taskContainer.getComponentByType(ProjectReactorBuilder.class)).isNotNull().isInstanceOf(ProjectReactorBuilder.class);
}
