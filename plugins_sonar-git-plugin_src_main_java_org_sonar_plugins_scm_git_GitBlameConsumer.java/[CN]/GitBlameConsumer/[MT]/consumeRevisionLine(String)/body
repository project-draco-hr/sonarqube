{
  String[] parts=line.split("\\s",4);
  if (parts.length >= 1) {
    revision=parts[0];
    BlameLine oldLine=commitInfo.get(revision);
    if (oldLine != null) {
      author=oldLine.getAuthor();
      committer=oldLine.getCommitter();
      time=oldLine.getDate();
    }
    expectRevisionLine=false;
  }
}
