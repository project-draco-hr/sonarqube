{
  if (value == null) {
    for (LinkEntry entry=header.after; entry != header; entry=entry.after) {
      if (entry.getValue() == null) {
        return true;
      }
    }
  }
 else {
    for (LinkEntry entry=header.after; entry != header; entry=entry.after) {
      if (isEqualValue(value,entry.getValue())) {
        return true;
      }
    }
  }
  return false;
}
