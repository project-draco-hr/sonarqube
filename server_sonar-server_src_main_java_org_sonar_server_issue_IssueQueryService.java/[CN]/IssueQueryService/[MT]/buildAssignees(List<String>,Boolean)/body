{
  List<String> assignees=Lists.newArrayList();
  if (assigneesFromParams != null) {
    assignees.addAll(assigneesFromParams);
  }
  if (BooleanUtils.isTrue(assignedToMe)) {
    String login=UserSession.get().login();
    if (login == null) {
      assignees.add(UNKNOWN);
    }
 else {
      assignees.add(login);
    }
  }
  return assignees;
}
