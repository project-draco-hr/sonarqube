{
  String projectKey="sample-scm";
  orchestrator.getServer().provisionProject(projectKey,projectKey);
  setServerProperty(orchestrator,projectKey,"sonar.timemachine.period1","previous_analysis");
  runProjectAnalysis(orchestrator,"scm/xoo-sample-with-scm","sonar.projectDate",formatDate(addDays(new Date(),-60)),"sonar.scm.provider","xoo","sonar.scm.disabled","false");
  runProjectAnalysis(orchestrator,"scm/xoo-sample-with-scm","sonar.projectDate",formatDate(addDays(new Date(),-20)),"sonar.scm.provider","xoo","sonar.scm.disabled","false");
  assertMeasures(projectKey,ImmutableMap.of("new_lines",0,"new_lines_to_cover",0));
}
