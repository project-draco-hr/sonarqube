{
  return new EqualsBuilder().append(rule.getDefaultSubCharacteristicId(),rule.getSubCharacteristicId()).append(rule.getDefaultRemediationFunction(),rule.getRemediationFunction()).append(rule.getDefaultRemediationCoefficient(),rule.getRemediationCoefficient()).append(rule.getDefaultRemediationOffset(),rule.getRemediationOffset()).isEquals();
}
