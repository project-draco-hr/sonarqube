{
  activeRules=new ActiveRulesBuilder().build();
  fs=new DefaultFileSystem(temp.newFolder().toPath());
  MetricFinder metricFinder=mock(MetricFinder.class);
  when(metricFinder.findByKey(CoreMetrics.NCLOC_KEY)).thenReturn(CoreMetrics.NCLOC);
  when(metricFinder.findByKey(CoreMetrics.FUNCTION_COMPLEXITY_DISTRIBUTION_KEY)).thenReturn(CoreMetrics.FUNCTION_COMPLEXITY_DISTRIBUTION);
  settings=new Settings();
  moduleIssues=mock(ModuleIssues.class);
  project=new Project("myProject");
  measureCache=mock(MeasureCache.class);
  CoverageExclusions coverageExclusions=mock(CoverageExclusions.class);
  when(coverageExclusions.accept(any(Resource.class),any(Measure.class))).thenReturn(true);
  resourceCache=new BatchComponentCache();
  sensorStorage=new DefaultSensorStorage(metricFinder,moduleIssues,settings,fs,activeRules,coverageExclusions,resourceCache,mock(ReportPublisher.class),measureCache,mock(SonarCpdBlockIndex.class));
}
