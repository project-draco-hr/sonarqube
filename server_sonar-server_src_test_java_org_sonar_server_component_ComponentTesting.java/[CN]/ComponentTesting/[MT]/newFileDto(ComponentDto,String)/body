{
  return new ComponentDto().setUuid(fileUuid).setProjectUuid(module.projectUuid()).setModuleUuid(module.uuid()).setModuleUuidPath(module.moduleUuidPath() == null ? module.uuid() : module.moduleUuidPath() + "." + module.uuid()).setKey("KEY_" + fileUuid).setName("NAME_" + fileUuid).setLongName("LONG_NAME_" + fileUuid).setParentProjectId(module.getId()).setScope(Scopes.FILE).setQualifier(Qualifiers.FILE).setPath("src/main/xoo/org/sonar/samples/File.xoo").setLanguage("xoo").setEnabled(true);
}
