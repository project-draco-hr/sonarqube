{
  return new ComponentDto().setUuid(Uuids.create()).setProjectUuid(subProjectOrProject.projectUuid()).setModuleUuid(subProjectOrProject.uuid()).setModuleUuidPath(subProjectOrProject.moduleUuidPath() == null ? subProjectOrProject.uuid() : subProjectOrProject.moduleUuidPath() + "." + subProjectOrProject.uuid()).setKey("module").setName("Module").setLongName("Module").setParentProjectId(subProjectOrProject.getId()).setScope(Scopes.PROJECT).setQualifier(Qualifiers.MODULE).setPath("module").setLanguage(null).setEnabled(true);
}
