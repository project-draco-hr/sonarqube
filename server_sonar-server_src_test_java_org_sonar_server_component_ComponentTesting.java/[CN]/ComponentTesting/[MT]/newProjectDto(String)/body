{
  return new ComponentDto().setUuid(uuid).setProjectUuid(uuid).setModuleUuidPath(MODULE_UUID_PATH_SEP + uuid + MODULE_UUID_PATH_SEP).setParentProjectId(null).setKey("KEY_" + uuid).setName("NAME_" + uuid).setLongName("LONG_NAME_" + uuid).setScope(Scopes.PROJECT).setQualifier(Qualifiers.PROJECT).setPath(null).setLanguage(null).setEnabled(true);
}
