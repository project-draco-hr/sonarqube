{
  return new ComponentDto().setUuid(Uuids.create()).setProjectUuid(subProjectOrProject.projectUuid()).setModuleUuid(subProjectOrProject.uuid()).setModuleUuidPath(subProjectOrProject.moduleUuidPath() == null ? subProjectOrProject.uuid() : subProjectOrProject.moduleUuidPath() + "." + subProjectOrProject.uuid()).setKey("file").setName("File").setLongName("File").setParentProjectId(subProjectOrProject.getId()).setScope(Scopes.FILE).setQualifier(Qualifiers.FILE).setPath("src/main/xoo/org/sonar/samples/File.xoo").setLanguage("xoo").setEnabled(true);
}
