{
  CodeReader reader=new CodeReader("word1 word2 word2",10);
  for (int i=0; i < 6; i++) {
    reader.pop();
  }
  StringBuilder result=new StringBuilder();
  reader.peekTo(new EndMatcher(){
    public boolean match(    int endFlag){
      return ' ' == (char)endFlag;
    }
  }
,result);
  assertEquals("word2",result.toString());
}
