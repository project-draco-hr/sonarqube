{
  controller.createAction("available").setDescription("Get the list of all the plugins available for installation on the SonarQube instance, sorted by plugin name." + "<br/>" + "Update status values are: [COMPATIBLE, INCOMPATIBLE, REQUIRES_UPGRADE, DEPS_REQUIRE_UPGRADE]").setSince("5.2").setHandler(this).setResponseExample(Resources.getResource(this.getClass(),"example-available_plugins.json"));
}
