{
  controller.createAction("available").setDescription("Get the list of all the plugins available for installation on the SonarQube instance, sorted by plugin name." + "<br/>" + "Plugin information is retrieved from Update Center. Date and time at which Update Center was last refreshed is provided in the response."+ "<br/>"+ "Update status values are: [COMPATIBLE, INCOMPATIBLE, REQUIRES_UPGRADE, DEPS_REQUIRE_UPGRADE]").setSince("5.2").setHandler(this).setResponseExample(Resources.getResource(this.getClass(),"example-available_plugins.json"));
}
