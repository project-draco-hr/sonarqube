{
  if (this == o) {
    return true;
  }
  if (!(o instanceof Hit)) {
    return false;
  }
  Hit hit=(Hit)o;
  if (fields != null ? !fields.equals(hit.fields) : hit.fields != null) {
    return false;
  }
  if (rank != null ? !rank.equals(hit.rank) : hit.rank != null) {
    return false;
  }
  return !(score != null ? !score.equals(hit.score) : hit.score != null);
}
