{
  setUserAsSystemAdmin();
  addLicenseSetting(LICENSE_KEY_SAMPLE,LICENSE_NAME_SAMPLE,createBase64License(ORGANISATION_SAMPLE,PRODUCT_SAMPLE,SERVER_ID_SAMPLE,EXPIRATION_SAMPLE,TYPE_SAMPLE,Collections.emptyMap()));
  ListWsResponse result=executeRequest();
  assertThat(result.getLicensesList()).hasSize(1);
  Licenses.License license=result.getLicenses(0);
  assertThat(license.getInvalidServerId()).isTrue();
}
