{
  initValidBuilder();
  assertThat(underTest.build(SOME_STATUS_BUT_NO_VALUE,SOME_VALUE).toString()).isEqualTo("Condition{status=OK, metricKey='some metric key', operator=GREATER_THAN, " + "errorThreshold='some error threshold', warningThreshold='some warning threshold', onLeakPeriod=false, value='some value'}");
}
