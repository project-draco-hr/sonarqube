{
  Resource r;
  if (issue.inputFile() != null) {
    r=File.fromIOFile(issue.inputFile().file(),project);
  }
 else {
    r=project;
  }
  Issuable issuable=perspectives.as(Issuable.class,r);
  issuable.addIssue(issuable.newIssueBuilder().ruleKey(RuleKey.of(issue.ruleKey().repository(),issue.ruleKey().rule())).effortToFix(issue.effortToFix()).line(issue.line()).message(issue.message()).build());
}
