{
  RemotePlugin checkstyle=new RemotePlugin("checkstyle",true).addFile("checkstyle-2.8.jar","fakemd51").addFile("ext.xml","fakemd52").addFile("ext.jar","fakemd53");
  String text=checkstyle.marshal();
  assertThat(text,is("checkstyle,true,checkstyle-2.8.jar|fakemd51,ext.xml|fakemd52,ext.jar|fakemd53"));
}
