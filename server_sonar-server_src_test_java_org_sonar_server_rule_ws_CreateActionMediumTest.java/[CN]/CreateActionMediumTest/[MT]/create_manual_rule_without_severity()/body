{
  MockUserSession.set().setGlobalPermissions(GlobalPermissions.QUALITY_PROFILE_ADMIN).setLogin("me");
  WsTester.TestRequest request=wsTester.newGetRequest("api/rules","create").setParam("manual_key","MY_MANUAL").setParam("name","My manual rule").setParam("html_description","Description");
