{
  resource.setId(JsonUtils.getInteger(json,"id")).setKey(JsonUtils.getString(json,"key")).setName(JsonUtils.getString(json,"name")).setLongName(JsonUtils.getString(json,"lname")).setScope(JsonUtils.getString(json,"scope")).setQualifier(JsonUtils.getString(json,"qualifier")).setLanguage(JsonUtils.getString(json,"lang")).setVersion(JsonUtils.getString(json,"version")).setDate(JsonUtils.getDate(json,"date")).setCopy(JsonUtils.getInteger(json,"copy"));
}
