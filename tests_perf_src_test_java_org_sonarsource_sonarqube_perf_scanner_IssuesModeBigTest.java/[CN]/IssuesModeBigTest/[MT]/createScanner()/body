{
  SonarScanner scanner=SonarScanner.create().setProperties("sonar.projectKey","big","sonar.projectName","xoo-big","sonar.projectVersion","1.0","sonar.sources","src","sonar.showProfiling","true");
  scanner.setEnvironmentVariable("SONAR_RUNNER_OPTS","-Xmx128m -server -XX:MaxPermSize=64m").setProjectDir(baseProjectDir);
  return scanner;
}
