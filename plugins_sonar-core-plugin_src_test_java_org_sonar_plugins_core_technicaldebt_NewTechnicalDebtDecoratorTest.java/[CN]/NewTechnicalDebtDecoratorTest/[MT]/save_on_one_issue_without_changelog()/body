{
  when(issuable.issues()).thenReturn(newArrayList((Issue)new DefaultIssue().setKey("A").setCreationDate(nineDaysAgo).setDebt(Duration.create(FIVE_DAYS_IN_MINUTES))));
  decorator.decorate(resource,context);
  verify(context).saveMeasure(argThat(new IsVariationMeasure(CoreMetrics.NEW_TECHNICAL_DEBT,0.0,5.0 * ONE_DAY_IN_MINUTES)));
}
