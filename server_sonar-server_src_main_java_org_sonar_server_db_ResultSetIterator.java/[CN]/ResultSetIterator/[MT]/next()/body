{
  if (!hasNext()) {
    close();
    throw new NoSuchElementException();
  }
  try {
    return read(rs);
  }
 catch (  SQLException e) {
    throw new IllegalStateException("Fail to read result set row",e);
  }
 finally {
    hasNext=doNextQuietly();
    if (!hasNext) {
      close();
    }
  }
}
