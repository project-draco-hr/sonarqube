{
  if (paramValues != null) {
    url.append(paramKey).append('=');
    for (int index=0; index < paramValues.length; index++) {
      if (index > 0) {
        url.append(',');
      }
      if (paramValues[index] != null) {
        url.append(encode(paramValues[index].toString()));
      }
    }
    url.append('&');
  }
}
