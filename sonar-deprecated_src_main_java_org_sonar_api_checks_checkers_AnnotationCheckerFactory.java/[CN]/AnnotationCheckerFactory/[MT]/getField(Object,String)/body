{
  Field[] fields=checker.getClass().getDeclaredFields();
  for (  Field field : fields) {
    CheckProperty annotation=field.getAnnotation(CheckProperty.class);
    if (annotation != null) {
      if (key.equals(field.getName()) || key.equals(annotation.key())) {
        return field;
      }
    }
  }
  return null;
}
