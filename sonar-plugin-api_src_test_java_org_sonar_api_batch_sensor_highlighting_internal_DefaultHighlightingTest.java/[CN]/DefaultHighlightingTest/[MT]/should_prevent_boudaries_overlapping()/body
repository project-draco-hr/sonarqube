{
  throwable.expect(IllegalStateException.class);
  throwable.expectMessage("Cannot register highlighting rule for characters from 8 to 15 as it overlaps at least one existing rule");
  new DefaultHighlighting(mock(SensorStorage.class)).onFile(new DefaultInputFile("foo","src/Foo.java").setLastValidOffset(100)).highlight(0,10,KEYWORD).highlight(8,15,KEYWORD).save();
}
