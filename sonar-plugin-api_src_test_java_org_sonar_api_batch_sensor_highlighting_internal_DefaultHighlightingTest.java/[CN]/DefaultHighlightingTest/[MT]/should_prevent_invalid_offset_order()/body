{
  throwable.expect(IllegalArgumentException.class);
  throwable.expectMessage("startOffset (18) should be < endOffset (15) for file [moduleKey=foo, relative=src/Foo.java, basedir=null].");
  new DefaultHighlighting(mock(SensorStorage.class)).onFile(new DefaultInputFile("foo","src/Foo.java").setLastValidOffset(20)).highlight(0,10,KEYWORD).highlight(18,15,KEYWORD).save();
}
