{
  this.type=type;
  this.options=options;
  StringBuilder sb=new StringBuilder();
  sb.append(type);
  if (options.length > 0) {
    sb.append(FIELD_SEPARATOR);
    for (    String option : options) {
      sb.append(StringEscapeUtils.escapeCsv(option));
      sb.append(OPTION_SEPARATOR);
    }
  }
  this.key=sb.toString();
}
