{
  dashboard.addWidget(ProjectIssueFilterWidget.ID,2).setProperty(ProjectIssueFilterWidget.FILTER_PROPERTY,Long.toString(unresolvedIssues.getId())).setProperty(ProjectIssueFilterWidget.DISTRIBUTION_AXIS_PROPERTY,"assignees");
  dashboard.addWidget(ProjectIssueFilterWidget.ID,2).setProperty("filter",Long.toString(myUnresolvedIssues.getId()));
  dashboard.addWidget(ProjectIssueFilterWidget.ID,2).setProperty(ProjectIssueFilterWidget.FILTER_PROPERTY,Long.toString(hiddenDebt.getId())).setProperty(ProjectIssueFilterWidget.DISTRIBUTION_AXIS_PROPERTY,"resolutions");
}
