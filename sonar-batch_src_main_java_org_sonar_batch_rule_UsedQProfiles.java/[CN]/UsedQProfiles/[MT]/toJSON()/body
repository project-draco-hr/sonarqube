{
  StringWriter json=new StringWriter();
  JsonWriter writer=JsonWriter.of(json);
  writer.beginArray();
  for (  QProfileWithId qProfile : profilesById.values()) {
    writer.beginObject().prop("id",qProfile.id()).prop("name",qProfile.name()).prop("version",qProfile.version()).prop("language",qProfile.language()).endObject();
  }
  writer.endArray();
  writer.close();
  return json.toString();
}
