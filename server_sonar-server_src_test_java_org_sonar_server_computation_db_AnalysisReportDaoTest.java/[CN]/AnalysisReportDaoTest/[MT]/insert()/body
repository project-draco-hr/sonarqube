{
  when(system2.now()).thenReturn(DateUtils.parseDate("2014-09-26").getTime());
  AnalysisReportDto report=new AnalysisReportDto().setProjectKey("123456789-987654321").setData("data-project").setStatus(PENDING);
  report.setCreatedAt(DateUtils.parseDate("2014-09-24")).setUpdatedAt(DateUtils.parseDate("2014-09-25"));
  dao.insert(session,report);
  session.commit();
  db.assertDbUnit(getClass(),"insert-result.xml","analysis_reports");
}
