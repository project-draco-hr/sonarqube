{
  assertThat(LanguageRecognizer.sanitizeExtension(".cbl")).isEqualTo("cbl");
  assertThat(LanguageRecognizer.sanitizeExtension(".CBL")).isEqualTo("cbl");
  assertThat(LanguageRecognizer.sanitizeExtension("CBL")).isEqualTo("cbl");
  assertThat(LanguageRecognizer.sanitizeExtension("cbl")).isEqualTo("cbl");
}
