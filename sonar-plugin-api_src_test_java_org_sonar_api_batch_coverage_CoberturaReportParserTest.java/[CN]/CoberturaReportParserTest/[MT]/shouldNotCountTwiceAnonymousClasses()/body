{
  File coverage=new File(getClass().getResource("/org/sonar/api/utils/CoberturaReportParserUtilsTest/shouldNotCountTwiceAnonymousClasses.xml").toURI());
  InputFile inputFile=mock(InputFile.class);
  when(fs.inputFile(eq(new File("/Users/simon/projects/sonar/trunk/tests/integration/reference-projects/reference/src/main/java","org/sonar/samples/MyFile.java")))).thenReturn(inputFile);
  parser.parseReport(coverage,context);
  verify(context).saveMeasure(eq(inputFile),argThat(new IsMeasure(CoreMetrics.LINES_TO_COVER,5.0)));
}
