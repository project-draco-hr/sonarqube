{
  InputFile inputFile=mock(InputFile.class);
  when(fs.inputFile(eq(new File("/Users/simon/projects/commons-chain/src/java","org/apache/commons/chain/impl/CatalogBase.java")))).thenReturn(inputFile);
  when(context.getResource(any(Resource.class))).thenReturn(new JavaFile("org.sonar.MyClass"));
  parser.parseReport(getCoverageReport(),context);
  verify(context).saveMeasure(eq(inputFile),argThat(new IsMeasure(CoreMetrics.COVERAGE_LINE_HITS_DATA,"48=117;56=234;66=0;67=0;68=0;84=999;86=999;98=318;111=18;121=0;122=0;125=0;126=0;127=0;128=0;131=0;133=0")));
}
