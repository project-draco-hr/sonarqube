{
  NestedReference nr=(NestedReference)request.getAttribute(NESTED_INCLUDES_KEY);
  if (nr != null) {
    return nr.getBeanName();
  }
 else {
    Tag tag=(Tag)nested;
    Tag formTag=null;
    do {
      tag=tag.getParent();
      if ((tag != null) && tag instanceof FormTag) {
        formTag=tag;
      }
    }
 while ((formTag == null) && (tag != null));
    if (formTag == null) {
      return "";
    }
    return ((FormTag)formTag).getBeanName();
  }
}
