{
  for (  Map.Entry<String,String> entry : parentProps.entrySet()) {
    String key=entry.getKey();
    if ((!childProps.containsKey(key) || childProps.get(key).equals(entry.getValue())) && !NON_HERITED_PROPERTIES_FOR_CHILD.contains(key)) {
      childProps.put(entry.getKey(),entry.getValue());
    }
  }
}
