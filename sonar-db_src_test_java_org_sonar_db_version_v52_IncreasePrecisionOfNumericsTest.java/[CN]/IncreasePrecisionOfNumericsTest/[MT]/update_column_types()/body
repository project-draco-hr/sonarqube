{
  when(db.getDialect()).thenReturn(new PostgreSql());
  underTest.execute(context);
  verify(context).execute("ALTER TABLE metrics ALTER COLUMN worst_value TYPE NUMERIC (38,20), ALTER COLUMN best_value TYPE NUMERIC (38,20)");
  verify(context).execute("ALTER TABLE project_measures " + "ALTER COLUMN value TYPE NUMERIC (38,20), " + "ALTER COLUMN variation_value_1 TYPE NUMERIC (38,20), "+ "ALTER COLUMN variation_value_2 TYPE NUMERIC (38,20), "+ "ALTER COLUMN variation_value_3 TYPE NUMERIC (38,20), "+ "ALTER COLUMN variation_value_4 TYPE NUMERIC (38,20), "+ "ALTER COLUMN variation_value_5 TYPE NUMERIC (38,20)");
  verify(context).execute("ALTER TABLE manual_measures ALTER COLUMN value TYPE NUMERIC (38,20)");
}
