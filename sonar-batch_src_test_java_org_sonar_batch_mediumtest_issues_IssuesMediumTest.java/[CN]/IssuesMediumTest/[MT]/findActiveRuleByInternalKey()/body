{
  File projectDir=new File(IssuesMediumTest.class.getResource("/mediumtest/xoo/sample").toURI());
  TaskResult result=tester.newScanTask(new File(projectDir,"sonar-project.properties")).property("sonar.xoo.internalKey","OneIssuePerLine.internal").start();
  assertThat(result.issues()).hasSize(14 + 2);
}
