{
  File projectDir=new File(IssuesMediumTest.class.getResource("/mediumtest/xoo/sample").toURI());
  TaskResult result=tester.newScanTask(new File(projectDir,"sonar-project.properties")).property("sonar.oneIssuePerLine.forceSeverity","CRITICAL").start();
  assertThat(result.issues().iterator().next().severity()).isEqualTo(Severity.CRITICAL.name());
}
