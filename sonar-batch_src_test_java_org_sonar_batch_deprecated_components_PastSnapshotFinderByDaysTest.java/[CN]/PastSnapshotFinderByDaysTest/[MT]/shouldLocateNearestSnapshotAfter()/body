{
  Date current=dateFormat.parse("2010-10-20");
  List<Snapshot> snapshots=Arrays.asList(newSnapshot(1,"2010-09-30"),newSnapshot(2,"2010-10-01"),newSnapshot(3,"2010-10-08"),newSnapshot(4,"2010-10-12"));
  assertThat(PastSnapshotFinderByDays.getNearestToTarget(snapshots,current,15).getId(),is(3));
}
