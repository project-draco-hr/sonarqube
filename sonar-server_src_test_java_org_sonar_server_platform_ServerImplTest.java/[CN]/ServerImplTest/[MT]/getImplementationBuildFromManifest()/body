{
  ServerImpl server=new ServerImpl(new Settings(),"/org/sonar/server/platform/ServerImplTest/build.properties","/org/sonar/server/platform/ServerImplTest/pom-with-version.properties");
  server.start();
  assertThat(server.getImplementationBuild()).isEqualTo("0b9545a8b74aca473cb776275be4dc93a327c363");
}
