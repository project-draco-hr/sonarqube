{
  when(service.listTags("polop",5)).thenReturn(Lists.newArrayList("tag1","tag2","tag3","tag4","tag5"));
  tester.newGetRequest("api/issues","tags").setParam("q","polop").setParam("ps","5").execute().assertJson("{tags:[\"tag1\", \"tag2\", \"tag3\", \"tag4\", \"tag5\"]}");
  verify(service).listTags("polop",5);
}
