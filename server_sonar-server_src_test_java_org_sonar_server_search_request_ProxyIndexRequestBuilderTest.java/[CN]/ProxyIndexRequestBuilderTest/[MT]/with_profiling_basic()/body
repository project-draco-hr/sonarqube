{
  Profiling profiling=new Profiling(new Settings().setProperty(Profiling.CONFIG_PROFILING_LEVEL,Profiling.Level.BASIC.name()));
  SearchClient searchClient=new SearchClient(new Settings(),profiling);
  try {
    searchClient.prepareIndex().setIndex(IndexDefinition.RULE.getIndexName()).setType(IndexDefinition.RULE.getIndexType()).setId("ruleKey").get();
    fail();
  }
 catch (  IllegalStateException e) {
    assertThat(e.getMessage()).isEqualTo("Fail to execute ES index request for key 'ruleKey' on index 'rules' on type 'rule'");
  }
  searchClient.stop();
}
