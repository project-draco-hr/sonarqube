{
  tester.get(RuleDao.class).insert(dbSession,RuleTesting.newXooX1());
  dbSession.commit();
  DeleteByQueryRequestBuilder requestBuilder=searchClient.prepareDeleteByQuery(IndexDefinition.RULE.getIndexName()).setQuery(QueryBuilders.matchAllQuery());
  DeleteByQueryResponse response=requestBuilder.get();
  assertThat(response.iterator()).hasSize(1);
}
