{
  for (int index=line.length() - 1; index >= 0; index--) {
    char character=line.charAt(index);
    for (    char endOfLine : endOfLines) {
      if (character == endOfLine) {
        return 1;
      }
    }
    if (!Character.isWhitespace(character) && character != '*' && character != '/') {
      return 0;
    }
  }
  return 0;
}
