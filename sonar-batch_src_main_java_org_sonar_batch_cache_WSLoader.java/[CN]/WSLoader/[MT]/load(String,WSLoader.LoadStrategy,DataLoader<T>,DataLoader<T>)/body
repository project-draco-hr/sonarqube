{
switch (strategy) {
case CACHE_FIRST:
    return loadFromCacheFirst(id,cacheLoader,serverLoader);
case CACHE_ONLY:
  return loadFromCacheFirst(id,cacheLoader,null);
case SERVER_FIRST:
return loadFromServerFirst(id,serverLoader,cacheLoader);
case SERVER_ONLY:
default :
return loadFromServerFirst(id,serverLoader,null);
}
}
