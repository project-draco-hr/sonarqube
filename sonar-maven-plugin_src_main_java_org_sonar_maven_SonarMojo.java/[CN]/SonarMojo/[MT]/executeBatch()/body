{
  ProjectDefinition def=MavenProjectConverter.convert(session.getSortedProjects(),project);
  Reactor reactor=new Reactor(def);
  Batch batch=new Batch(getInitialConfiguration(),reactor,session,project,getLog(),lifecycleExecutor,pluginManager,artifactFactory,localRepository,artifactMetadataSource,artifactCollector,dependencyTreeBuilder,projectBuilder,getEnvironmentInformation(),Maven2PluginExecutor.class);
  batch.execute();
}
