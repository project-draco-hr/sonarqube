{
  json.append("{");
  json.append("\"k\":");
  json.append(dependency.key());
  json.append(",\"v\":\"");
  json.append(dependency.version());
  json.append("\",\"s\":\"");
  json.append(dependency.scope());
  json.append("\",\"d\":[");
  serializeDeps(json,dependency.dependencies());
  json.append("]");
  json.append("}");
}
