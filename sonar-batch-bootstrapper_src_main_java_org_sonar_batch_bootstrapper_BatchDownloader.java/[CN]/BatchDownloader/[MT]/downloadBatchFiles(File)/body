{
  try {
    List<File> files=new ArrayList<File>();
    String libs=remoteContent(BATCH_PATH);
    for (    String lib : libs.split("\n")) {
      if (!"".equals(lib)) {
        File file=new File(toDir,lib);
        remoteContentToFile(BATCH_PATH + lib,file);
        files.add(file);
      }
    }
    return files;
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
