{
  if (!a.getHash(startA).equals(b.getHash(startB))) {
    return 0;
  }
  int length=0;
  int ai=startA;
  int bi=startB;
  while (ai <= a.length() && bi <= b.length() && a.getHash(ai).equals(b.getHash(bi))) {
    ai++;
    bi++;
    length++;
  }
  ai=startA;
  bi=startB;
  while (ai > 0 && bi > 0 && a.getHash(ai).equals(b.getHash(bi))) {
    ai--;
    bi--;
    length++;
  }
  return length - 1;
}
