{
  lines.add("line-" + line);
  changesetsBuilder.addChangeset(ScannerReport.Changesets.Changeset.newBuilder().setAuthor("author-" + line).setDate(123456789L).setRevision("rev-" + line).build()).addChangesetIndexByLine(line - 1);
  coverages.add(ScannerReport.LineCoverage.newBuilder().setLine(line).setConditions(10).setUtHits(true).setUtCoveredConditions(2).setItHits(true).setItCoveredConditions(3).setOverallCoveredConditions(4).build());
  highlightings.add(ScannerReport.SyntaxHighlightingRule.newBuilder().setRange(ScannerReport.TextRange.newBuilder().setStartLine(line).setEndLine(line).setStartOffset(1).setEndOffset(3).build()).setType(HighlightingType.ANNOTATION).build());
  symbols.add(ScannerReport.Symbol.newBuilder().setDeclaration(ScannerReport.TextRange.newBuilder().setStartLine(line).setEndLine(line).setStartOffset(2).setEndOffset(4).build()).addReference(ScannerReport.TextRange.newBuilder().setStartLine(line + 1).setEndLine(line + 1).setStartOffset(1).setEndOffset(3).build()).build());
}
