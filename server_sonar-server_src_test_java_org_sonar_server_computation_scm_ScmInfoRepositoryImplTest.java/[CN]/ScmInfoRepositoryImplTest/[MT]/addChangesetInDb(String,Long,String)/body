{
  DbFileSources.Data.Builder fileDataBuilder=DbFileSources.Data.newBuilder();
  fileDataBuilder.addLinesBuilder().setLine(1).setScmAuthor(author).setScmDate(date).setScmRevision(revision);
  dbTester.getDbClient().fileSourceDao().insert(new FileSourceDto().setFileUuid(FILE.getUuid()).setProjectUuid("PROJECT_UUID").setSourceData(fileDataBuilder.build()));
}
