{
  if (issue.resolution() == null) {
    unresolved++;
    severityBag.add(issue.severity());
  }
 else   if (Issue.RESOLUTION_FALSE_POSITIVE.equals(issue.resolution())) {
    falsePositives++;
  }
switch (issue.status()) {
case STATUS_OPEN:
    open++;
  break;
case STATUS_REOPENED:
reopened++;
break;
case STATUS_CONFIRMED:
confirmed++;
break;
default :
}
}
