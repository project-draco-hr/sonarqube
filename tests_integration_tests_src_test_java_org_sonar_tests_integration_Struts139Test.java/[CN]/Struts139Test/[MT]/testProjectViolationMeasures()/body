  assertThat(getProjectMeasure(CoreMetrics.INFO_VIOLATIONS_KEY).getValue(),closeTo(5157.0,20.0));
  assertThat(getProjectMeasure(CoreMetrics.VIOLATIONS_DENSITY_KEY).getValue(),closeTo(84.6,5.0));
}
