{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Requirement that=(Requirement)o;
  if (!characteristic.equals(that.characteristic)) {
    return false;
  }
  if (!ruleKey.equals(that.ruleKey)) {
    return false;
  }
  return true;
}
