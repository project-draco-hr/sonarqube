{
  checkNotNull(component.getId(),"The project need to be persisted before creating this snapshot");
  checkNotNull(rootComponentUuid,"Root component uuid is null");
  return new SnapshotDto().setUuid(RandomStringUtils.randomAlphanumeric(40)).setComponentUuid(component.uuid()).setRootComponentUuid(rootComponentUuid).setStatus(SnapshotDto.STATUS_PROCESSED).setQualifier(component.qualifier()).setScope(component.scope()).setCreatedAt(System.currentTimeMillis()).setBuildDate(System.currentTimeMillis()).setLast(true);
}
