{
  if (status == CeActivityDto.Status.FAILED) {
    profiler.stopError("Executed task | project={} | type={} | id={} | submitter='{}'",task.getComponentKey(),task.getType(),task.getUuid(),getSubmitterLoginForLog(task));
  }
 else {
    profiler.stopInfo("Executed task | project={} | type={} | id={} | submitter='{}'",task.getComponentKey(),task.getType(),task.getUuid(),getSubmitterLoginForLog(task));
  }
}
