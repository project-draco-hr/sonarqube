{
  dbTester.prepareDbUnit(getClass(),"update.xml");
  dao.update(dbTester.getSession(),new ComponentLinkDto().setId(1L).setComponentUuid("ABCD").setType("homepage").setName("Home").setHref("http://www.sonarqube.org"));
  dbTester.getSession().commit();
  dbTester.assertDbUnit(getClass(),"update-result.xml","project_links");
}
