{
  this.projectUuid=project.getUuid();
  session.select("org.sonar.db.source.FileSourceMapper.selectHashesForProject",ImmutableMap.of("projectUuid",projectUuid,"dataType",Type.SOURCE),new ResultHandler(){
    @Override public void handleResult(    ResultContext context){
      FileSourceDto dto=(FileSourceDto)context.getResultObject();
      previousFileSourcesByUuid.put(dto.getFileUuid(),dto);
    }
  }
);
}
