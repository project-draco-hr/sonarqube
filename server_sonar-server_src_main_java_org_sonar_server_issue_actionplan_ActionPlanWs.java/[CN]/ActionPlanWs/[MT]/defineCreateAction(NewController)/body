{
  WebService.NewAction action=controller.createAction("create").setDescription("Create an action plan. Requires Administer permission on project").setSince("3.6").setPost(true).setHandler(RailsHandler.INSTANCE);
  addNameParam(action);
  addDescriptionParam(action);
  addDeadLineParam(action);
  addProjectParam(action);
  addFormatParam(action);
}
