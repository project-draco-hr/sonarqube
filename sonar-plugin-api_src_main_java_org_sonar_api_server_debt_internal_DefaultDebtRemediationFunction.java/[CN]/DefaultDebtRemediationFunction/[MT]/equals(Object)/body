{
  if (!(o instanceof DefaultDebtRemediationFunction)) {
    return false;
  }
  if (this == o) {
    return true;
  }
  DefaultDebtRemediationFunction other=(DefaultDebtRemediationFunction)o;
  return new EqualsBuilder().append(coefficient,other.coefficient()).append(offset,other.offset()).append(type,other.type()).isEquals();
}
