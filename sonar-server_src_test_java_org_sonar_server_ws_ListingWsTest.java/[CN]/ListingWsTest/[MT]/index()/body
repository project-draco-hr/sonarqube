{
  WebService.Context context=new WebService.Context();
  ws.define(context);
  new MetricWebService().define(context);
  SimpleResponse response=new SimpleResponse();
  ws.list(context.controllers(),response);
  JSONAssert.assertEquals(IOUtils.toString(getClass().getResource("/org/sonar/server/ws/ListingWebServiceTest/index.json")),response.outputAsString(),true);
}
