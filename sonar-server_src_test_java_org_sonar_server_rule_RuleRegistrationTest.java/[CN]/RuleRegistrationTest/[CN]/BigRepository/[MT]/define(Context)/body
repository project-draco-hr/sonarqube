{
  NewRepository repo=context.createRepository("big","java");
  for (int i=0; i < SIZE; i++) {
    NewRule rule=repo.createRule("rule" + i).setName("name of " + i).setHtmlDescription("description of " + i).setSeverity(Severity.BLOCKER).setInternalKey("config1").setTags("tag1","tag3","tag5");
    for (int j=0; j < 20; j++) {
      rule.createParam("param" + j);
    }
  }
  repo.done();
}
