{
  NewRepository repo=context.newRepository("big","java");
  for (int i=0; i < SIZE; i++) {
    NewRule rule=repo.newRule("rule" + i).setName("name of " + i).setHtmlDescription("description of " + i).setSeverity(Severity.BLOCKER).setMetadata("config1").setTags("tag1","tag3","tag5");
    for (int j=0; j < 20; j++) {
      rule.newParam("param" + j);
    }
  }
  repo.done();
}
