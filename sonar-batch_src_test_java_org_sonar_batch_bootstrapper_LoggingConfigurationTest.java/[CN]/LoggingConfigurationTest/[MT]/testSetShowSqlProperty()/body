{
  Map<String,String> properties=Maps.newHashMap();
  assertThat(LoggingConfiguration.create(null).setProperties(properties).getSubstitutionVariable(LoggingConfiguration.PROPERTY_SQL_LOGGER_LEVEL)).isEqualTo(LoggingConfiguration.LEVEL_SQL_DEFAULT);
  properties.put("sonar.log.profilingLevel","FULL");
  assertThat(LoggingConfiguration.create(null).setProperties(properties).getSubstitutionVariable(LoggingConfiguration.PROPERTY_SQL_LOGGER_LEVEL)).isEqualTo(LoggingConfiguration.LEVEL_SQL_VERBOSE);
  properties.put("sonar.log.profilingLevel","BASIC");
  assertThat(LoggingConfiguration.create(null).setProperties(properties).getSubstitutionVariable(LoggingConfiguration.PROPERTY_SQL_LOGGER_LEVEL)).isEqualTo(LoggingConfiguration.LEVEL_SQL_DEFAULT);
  properties.put("sonar.log.profilingLevel","NONE");
  assertThat(LoggingConfiguration.create(null).setProperties(properties).getSubstitutionVariable(LoggingConfiguration.PROPERTY_SQL_LOGGER_LEVEL)).isEqualTo(LoggingConfiguration.LEVEL_SQL_DEFAULT);
}
