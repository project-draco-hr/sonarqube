{
  assertThat(LoggingConfiguration.create().setFormat(null).getSubstitutionVariable(LoggingConfiguration.PROPERTY_FORMAT),Is.is(LoggingConfiguration.FORMAT_DEFAULT));
  assertThat(LoggingConfiguration.create().setFormat("").getSubstitutionVariable(LoggingConfiguration.PROPERTY_FORMAT),Is.is(LoggingConfiguration.FORMAT_DEFAULT));
  assertThat(LoggingConfiguration.create().setFormat("   ").getSubstitutionVariable(LoggingConfiguration.PROPERTY_FORMAT),Is.is(LoggingConfiguration.FORMAT_DEFAULT));
}
