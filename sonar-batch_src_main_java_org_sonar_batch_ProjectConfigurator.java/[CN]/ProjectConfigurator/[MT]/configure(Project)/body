{
  Date analysisDate=loadAnalysisDate();
  project.setConfiguration(new PropertiesConfiguration()).setExclusionPatterns(loadExclusionPatterns()).setAnalysisDate(analysisDate).setLatestAnalysis(isLatestAnalysis(project.getKey(),analysisDate)).setAnalysisVersion(loadAnalysisVersion()).setAnalysisType(loadAnalysisType()).setLanguageKey(loadLanguageKey());
  return this;
}
