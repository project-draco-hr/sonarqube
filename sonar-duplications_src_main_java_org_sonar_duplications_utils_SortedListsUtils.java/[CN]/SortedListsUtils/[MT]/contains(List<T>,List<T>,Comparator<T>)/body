{
  int j=0;
  for (int i=0; i < list.size(); i++) {
    T e1=list.get(i);
    boolean found=false;
    for (; j < container.size(); j++) {
      T e2=container.get(j);
      int c=comparator.compare(e1,e2);
      if (c == 0) {
        found=true;
        break;
      }
 else       if (c < 0) {
        return false;
      }
    }
    if (!found) {
      return false;
    }
  }
  return true;
}
