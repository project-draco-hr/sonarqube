{
  IssueQuery query=IssueQuery.builder().issueKeys(newArrayList("ABCDE")).createdAfter(null).createdBefore(null).createdAt(null).build();
  assertThat(query.issueKeys()).containsOnly("ABCDE");
  assertThat(query.createdAfter()).isNull();
  assertThat(query.createdBefore()).isNull();
  assertThat(query.createdAt()).isNull();
}
