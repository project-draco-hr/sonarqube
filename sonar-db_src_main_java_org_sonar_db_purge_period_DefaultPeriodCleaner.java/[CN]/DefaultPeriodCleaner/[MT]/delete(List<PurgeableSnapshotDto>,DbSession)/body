{
  for (  PurgeableSnapshotDto snapshot : snapshots) {
    LOG.debug("<- Delete snapshot: {} [{}]",DateUtils.formatDateTime(snapshot.getDate()),snapshot.getSnapshotId());
    purgeDao.deleteSnapshots(session,profiler,PurgeSnapshotQuery.create().setRootSnapshotId(snapshot.getSnapshotId()),PurgeSnapshotQuery.create().setId(snapshot.getSnapshotId()));
  }
}
