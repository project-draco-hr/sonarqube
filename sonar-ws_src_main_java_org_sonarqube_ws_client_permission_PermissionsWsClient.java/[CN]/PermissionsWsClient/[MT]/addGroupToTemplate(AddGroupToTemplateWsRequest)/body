{
  wsClient.execute(newPostRequest(action("add_group_to_template")).setParam(PARAM_GROUP_ID,request.getGroupId()).setParam(PARAM_GROUP_NAME,request.getGroupName()).setParam(PARAM_PERMISSION,request.getPermission()).setParam(PARAM_TEMPLATE_UUID,request.getTemplateId()).setParam(PARAM_TEMPLATE_NAME,request.getTemplateName()));
}
