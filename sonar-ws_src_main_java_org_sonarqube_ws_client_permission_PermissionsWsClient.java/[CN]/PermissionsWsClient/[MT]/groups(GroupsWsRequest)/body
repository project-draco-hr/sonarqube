{
  return wsClient.execute(newGetRequest(action("groups")).setParam("permission",request.getPermission()).setParam("projectId",request.getProjectId()).setParam("projectKey",request.getProjectKey()).setParam("p",request.getPage()).setParam("ps",request.getPageSize()).setParam("selected",request.getSelected()).setParam("q",request.getQuery()),WsPermissions.WsGroupsResponse.parser());
}
