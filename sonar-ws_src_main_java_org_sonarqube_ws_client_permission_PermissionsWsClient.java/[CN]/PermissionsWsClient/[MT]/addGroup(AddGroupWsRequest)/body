{
  wsClient.execute(newPostRequest(action("add_group")).setParam(PARAM_PERMISSION,request.getPermission()).setParam(PARAM_PROJECT_ID,request.getProjectId()).setParam(PARAM_PROJECT_KEY,request.getProjectKey()).setParam(PARAM_GROUP_ID,request.getGroupId()).setParam(PARAM_GROUP_NAME,request.getGroupName()));
}
