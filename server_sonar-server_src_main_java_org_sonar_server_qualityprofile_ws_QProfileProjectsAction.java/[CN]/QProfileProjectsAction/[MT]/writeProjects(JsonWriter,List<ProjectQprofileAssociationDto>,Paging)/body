{
  json.beginObject();
  json.name("results").beginArray();
  for (  ProjectQprofileAssociationDto project : projects) {
    json.beginObject().prop("key",project.getProjectUuid()).prop("name",project.getProjectName()).prop("selected",project.isAssociated()).endObject();
  }
  json.endArray();
  json.prop("more",paging.hasNextPage());
  json.endObject().close();
}
