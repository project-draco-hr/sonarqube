{
  setupData("shared");
  Map<String,Object> params=newHashMap();
  params.put("user","user_not_existing");
  params.put("selected","all");
  try {
    service.find(params);
  }
 catch (  Exception e) {
    assertThat(e).isInstanceOf(NotFoundException.class).hasMessage("User 'user_not_existing' does not exists.");
  }
}
