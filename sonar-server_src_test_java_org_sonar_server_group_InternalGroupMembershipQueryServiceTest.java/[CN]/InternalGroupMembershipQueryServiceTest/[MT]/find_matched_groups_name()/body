{
  setupData("shared");
  List<GroupMembership> result=service.find(ImmutableMap.of("user","user1","selected","all","query","user"));
  assertThat(result).hasSize(1);
  check(result.get(0),"sonar-users",true);
  result=service.find(ImmutableMap.of("user","user1","selected","all","query","sonar"));
  assertThat(result).hasSize(3);
}
