{
  DumbComponent project=DumbComponent.builder(PROJECT,1).addChildren(DumbComponent.builder(MODULE,11).addChildren(DumbComponent.builder(MODULE,111).build()).build()).build();
  treeRootHolder.setRoot(project);
  QualityProfile qp=createQProfile(QP_NAME_1,LANGUAGE_KEY_1);
  addMeasure(111,qp);
  underTest.execute();
  assertThat(measureRepository.getAddedRawMeasures(1).get(QUALITY_PROFILES_KEY)).extracting("data").containsOnly(toJson(qp));
}
