{
  for (  File dir : dirs) {
    if (dir.exists()) {
      context.setRelativeDir(dir);
      Collection<File> files=FileUtils.listFiles(dir,HiddenFileFilter.VISIBLE,DIR_FILTER);
      for (      File file : files) {
        if (accept(file,context,filters)) {
          result.add(file);
        }
      }
    }
  }
}
