{
  String parentKey=profile.parent();
  QProfile parent=parentKey == null ? null : profilesByKey.get(parentKey);
  json.prop(FIELD_PARENT_KEY,nullUnlessNeeded(FIELD_PARENT_KEY,parentKey,fields)).prop(FIELD_PARENT_NAME,nullUnlessNeeded(FIELD_PARENT_NAME,parent == null ? parentKey : parent.name(),fields));
}
