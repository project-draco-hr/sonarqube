{
  dbClient.metricDao().insert(dbSession,newMetricDto().setKey("accessors").setShortName("Accessors").setDomain("Size").setValueType(INT.name()).setHidden(true),newMetricDto().setKey("blocker_remediation_cost").setShortName("Blocker Technical Debt").setDomain("SQALE").setValueType(WORK_DUR.name()).setHidden(false));
  dbSession.commit();
  String result=ws.newRequest().setMediaType(JSON).execute().getInput();
  assertJson(ws.getDef().responseExampleAsString()).isSimilarTo(result);
}
