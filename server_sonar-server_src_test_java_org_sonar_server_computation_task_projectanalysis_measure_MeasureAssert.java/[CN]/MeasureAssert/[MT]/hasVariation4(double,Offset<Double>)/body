{
  isNotNull();
  hasVariations();
  if (!actual.getVariations().hasVariation4()) {
    failWithMessage("Expected Measure to have a Variation 4 but it did not");
  }
  if (abs(expected - actual.getVariations().getVariation4()) > offset.value) {
    failWithMessage("Expected Variation 4 of Measure to be close to <%s> by less than <%s> but was <%s>",expected,offset.value,actual.getVariations().getVariation4());
  }
  return this;
}
