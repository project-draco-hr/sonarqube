{
  try {
    tester.get(QProfileBackuper.class).restore(new StringReader(Resources.toString(getClass().getResource("QProfileBackuperMediumTest/duplicates-xml-backup.xml"),Charsets.UTF_8)),null);
    fail();
  }
 catch (  IllegalArgumentException e) {
    assertThat(e).hasMessage("The quality profile cannot be restored as it contains duplicates for the following rules: xoo:x1, xoo:x2");
  }
}
