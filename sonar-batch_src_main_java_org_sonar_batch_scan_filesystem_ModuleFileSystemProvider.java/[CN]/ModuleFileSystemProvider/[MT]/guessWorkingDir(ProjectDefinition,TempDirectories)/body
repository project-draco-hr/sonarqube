{
  File workDir=module.getWorkDir();
  if (workDir == null) {
    workDir=tempDirectories.getDir("work");
  }
 else {
    try {
      FileUtils.forceMkdir(workDir);
    }
 catch (    Exception e) {
      throw new IllegalStateException("Fail to create working dir: " + workDir.getAbsolutePath(),e);
    }
  }
  return workDir;
}
