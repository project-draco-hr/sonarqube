{
  Statement stmt=null;
  ResultSet rs=null;
  int version=VERSION_UNKNOWN;
  try {
    stmt=connection.createStatement();
    rs=stmt.executeQuery("SELECT version FROM " + SchemaMigration.TABLE_NAME);
    while (rs.next()) {
      int i=Integer.parseInt(rs.getString(1));
      if (i > version) {
        version=i;
      }
    }
  }
 catch (  SQLException e) {
  }
 finally {
    close(rs);
    close(stmt);
  }
  return version;
}
