{
  int touchedRows=mapper(session).updateIfStatus(taskUuid,IN_PROGRESS,system2.now(),system2.now(),PENDING);
  if (touchedRows != 1) {
    session.rollback();
    return Optional.absent();
  }
  CeQueueDto result=mapper(session).selectByUuid(taskUuid);
  session.commit();
  return Optional.of(result);
}
