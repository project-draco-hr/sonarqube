{
  ProjectReferentials ref=new ProjectReferentials();
  ref.metrics().add(new Metric("ncloc","INT"));
  ref.addQProfile(new QProfile("squid-java","Java","java",new SimpleDateFormat("dd/MM/yyyy").parse("14/03/1984")));
  System.out.println(ref.toJson());
  JSONAssert.assertEquals("{timestamp:0,metrics:[{key:ncloc,valueType:INT}]," + "qprofilesByLanguage:{java:{key:\"squid-java\"," + "name:Java,"+ "language:java,rulesUpdatedAt:\"Mar 14, 1984 12:00:00 AM\"}},"+ "activeRules:[]}",ref.toJson(),true);
}
