{
  ConditionToCondition underTest=new ConditionToCondition(of(SOME_CONDITION,status == ConditionStatus.EvaluationStatus.NO_VALUE ? ConditionStatus.NO_VALUE_STATUS : ConditionStatus.create(status,SOME_VALUE)));
  assertThat(underTest.apply(SOME_CONDITION).getStatus().name()).isEqualTo(status.name());
}
