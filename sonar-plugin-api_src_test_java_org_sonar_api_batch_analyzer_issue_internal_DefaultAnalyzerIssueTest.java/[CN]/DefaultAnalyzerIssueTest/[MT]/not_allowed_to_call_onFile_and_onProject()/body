{
  thrown.expect(IllegalStateException.class);
  thrown.expectMessage("onFile or onProject can be called only once");
  new DefaultAnalyzerIssueBuilder().onProject().onFile(new DefaultInputFile("src/Foo.php")).ruleKey(RuleKey.of("repo","rule")).atLine(1).effortToFix(10.0).message("Wrong way!").build();
}
