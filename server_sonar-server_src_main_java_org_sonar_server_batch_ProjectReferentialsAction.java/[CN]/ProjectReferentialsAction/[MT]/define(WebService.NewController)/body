{
  WebService.NewAction action=controller.createAction("project").setDescription("Return project referentials").setSince("4.5").setInternal(true).setHandler(this);
  action.createParam(PARAM_KEY).setRequired(true).setDescription("Project key").setExampleValue("org.codehaus.sonar:sonar");
  action.createParam(PARAM_PROFILE).setDescription("Profile name").setExampleValue("SonarQube Way");
}
