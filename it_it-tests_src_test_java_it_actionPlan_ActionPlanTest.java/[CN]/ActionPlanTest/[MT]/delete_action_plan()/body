{
  ActionPlan newActionPlan=adminActionPlanClient().create(NewActionPlan.create().name("Short term").project(PROJECT_KEY).description("Short term issues").deadLine(toDate("2113-01-31")));
  adminActionPlanClient().delete(newActionPlan.key());
  List<ActionPlan> results=adminActionPlanClient().find(PROJECT_KEY);
  assertThat(results).isEmpty();
}
