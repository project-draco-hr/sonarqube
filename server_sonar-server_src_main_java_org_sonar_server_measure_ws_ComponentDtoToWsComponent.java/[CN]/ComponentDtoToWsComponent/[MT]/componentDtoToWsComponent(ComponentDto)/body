{
  WsMeasures.Component.Builder wsComponent=WsMeasures.Component.newBuilder().setId(component.uuid()).setKey(component.key()).setName(component.name()).setQualifier(component.qualifier());
  if (component.path() != null) {
    wsComponent.setPath(component.path());
  }
  if (component.description() != null) {
    wsComponent.setDescription(component.description());
  }
  return wsComponent;
}
