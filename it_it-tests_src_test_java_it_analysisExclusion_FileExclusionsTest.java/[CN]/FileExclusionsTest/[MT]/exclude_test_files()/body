{
  scan("sonar.global.exclusions","**/*Ignore*.xoo","sonar.exclusions","**/*Exclude*.xoo,org/sonar/tests/packageToExclude/**","sonar.test.exclusions","**/ClassTwoTest.xoo");
  List<Resource> testFiles=orchestrator.getServer().getWsClient().findAll(new ResourceQuery(PROJECT).setQualifiers("UTS").setDepth(-1));
  assertThat(testFiles).hasSize(2);
  assertThat(testFiles).extracting("name").doesNotContain("ClassTwoTest.xoo");
}
