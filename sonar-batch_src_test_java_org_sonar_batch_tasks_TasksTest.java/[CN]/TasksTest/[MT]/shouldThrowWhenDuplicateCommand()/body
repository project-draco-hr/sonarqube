{
  Tasks tasks=new Tasks(settings,new TaskDefinition[]{TaskDefinition.create().setName("foo1").setTask(FakeTask1.class).setCommand("cmd"),TaskDefinition.create().setName("foo2").setTask(FakeTask2.class).setCommand("cmd")});
  thrown.expect(SonarException.class);
  thrown.expectMessage("Task 'foo2' uses the same command than task 'foo1'");
  tasks.start();
}
