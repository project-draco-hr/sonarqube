{
  NewController controller=context.createController("api/qualitygates").setSince("4.3").setDescription("This service can be used to manage quality gates, including requirements and project association.");
  defineQualityGateActions(controller);
  defineConditionActions(controller);
  controller.createAction("app").setInternal(true).setDescription("Get initialization items for the admin UI").setSince("4.3").setHandler(appHandler);
  controller.done();
}
