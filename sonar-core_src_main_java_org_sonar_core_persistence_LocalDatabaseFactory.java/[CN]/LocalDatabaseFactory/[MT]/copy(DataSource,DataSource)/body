  new DbTemplate().copyTable(source,dest,"PROPERTIES","SELECT * FROM PROPERTIES WHERE (USER_ID IS NULL) AND (RESOURCE_ID IS NULL) AND NOT (PROP_KEY LIKE '%.secured')").copyTable(source,dest,"RULES_PROFILES","SELECT * FROM RULES_PROFILES").copyTable(source,dest,"RULES","SELECT * FROM RULES").copyTable(source,dest,"RULES_PARAMETERS","SELECT * FROM RULES_PARAMETERS").copyTable(source,dest,"ACTIVE_RULES","SELECT * FROM ACTIVE_RULES").copyTable(source,dest,"ACTIVE_RULE_PARAMETERS","SELECT * FROM ACTIVE_RULE_PARAMETERS").copyTable(source,dest,"METRICS","SELECT * FROM METRICS");
