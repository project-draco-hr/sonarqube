{
  if (component.isFile()) {
    InputFile inputFile=(InputFile)component.inputPath();
    if (inputFile.type() == Type.TEST) {
      if (!testPlanByFile.containsKey(inputFile)) {
        testPlanByFile.put(inputFile,new DefaultTestPlan());
      }
      return testPlanByFile.get(inputFile);
    }
  }
  return null;
}
