{
  when(permissionDao.selectGroups(any(PermissionQuery.class),anyLong())).thenReturn(newArrayList(new GroupWithPermissionDto().setName("users").setPermission("user")));
  GroupWithPermissionQueryResult result=finder.findGroupsWithPermission(PermissionQuery.builder().permission("user").membership(PermissionQuery.IN).build());
  assertThat(result.groups()).hasSize(1);
  assertThat(result.hasMoreResults()).isFalse();
}
