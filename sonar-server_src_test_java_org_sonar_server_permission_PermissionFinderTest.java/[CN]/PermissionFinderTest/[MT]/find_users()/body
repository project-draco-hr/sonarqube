{
  when(permissionDao.selectUsers(any(WithPermissionQuery.class),anyLong(),anyInt(),anyInt())).thenReturn(newArrayList(new UserWithPermissionDto().setName("user1").setPermission("user")));
  UserWithPermissionQueryResult result=finder.findUsersWithPermission(WithPermissionQuery.builder().permission("user").build());
  assertThat(result.users()).hasSize(1);
  assertThat(result.hasMoreResults()).isFalse();
}
