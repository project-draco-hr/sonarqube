{
  WorkflowEngine engine=new WorkflowEngine(new Workflow(),mock(ReviewStore.class),new Settings());
  ListMultimap<Long,Screen> screens=engine.listAvailableScreens(new Review[]{new DefaultReview().setViolationId(1000L),new DefaultReview().setViolationId(2000L)},new DefaultWorkflowContext(),true);
  assertThat(screens.size()).isEqualTo(0);
}
