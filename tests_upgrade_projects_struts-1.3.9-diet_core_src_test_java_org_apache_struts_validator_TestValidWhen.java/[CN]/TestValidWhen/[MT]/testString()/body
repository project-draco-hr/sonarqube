{
  doParse("(*this* != '--')","XX",0,"stringValue1",true);
  doParse("(*this* == '--')","--",0,"stringValue1",true);
  String testValue="dsgUOLMdLsdL";
  doParse("(*this* == '" + testValue + "')",testValue,0,"stringValue1",true);
  doParse("(*this* == \"" + testValue + "\")",testValue,0,"stringValue1",true);
  doParse("(*this* == \":\")",":",0,"stringValue1",true);
  doParse("(*this* == \"foo:bar\")","foo:bar",0,"stringValue1",true);
}
