{
switch (level) {
case TRACE:
    logTrace(msg,args);
  break;
case DEBUG:
logDebug(msg,args);
break;
case INFO:
logInfo(msg,args);
break;
case WARN:
logWarn(msg,args);
break;
case ERROR:
logError(msg,args);
break;
default :
throw new IllegalArgumentException("Unsupported LoggerLevel value: " + level);
}
}
