{
switch (level) {
case TRACE:
    if (args == null) {
      logger.trace(msg);
    }
 else {
      logger.trace(msg,args);
    }
  break;
case DEBUG:
if (args == null) {
  logger.debug(msg);
}
 else {
  logger.debug(msg,args);
}
break;
case INFO:
if (args == null) {
logger.info(msg);
}
 else {
logger.info(msg,args);
}
break;
case WARN:
if (args == null) {
logger.warn(msg);
}
 else {
logger.warn(msg,args);
}
break;
case ERROR:
if (args == null) {
logger.error(msg);
}
 else {
logger.error(msg,args);
}
break;
default :
throw new IllegalArgumentException("Unsupported LoggerLevel value: " + level);
}
}
