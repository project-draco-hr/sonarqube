{
switch (level) {
case TRACE:
    logger.trace(msg);
  break;
case DEBUG:
logger.debug(msg);
break;
case INFO:
logger.info(msg);
break;
case WARN:
logger.warn(msg);
break;
case ERROR:
logger.error(msg);
break;
default :
throw new IllegalArgumentException("Unsupported LoggerLevel value: " + level);
}
}
