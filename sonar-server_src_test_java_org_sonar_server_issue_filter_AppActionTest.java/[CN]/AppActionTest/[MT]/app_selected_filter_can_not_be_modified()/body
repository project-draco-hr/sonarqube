{
  MockUserSession session=MockUserSession.set().setLogin("eric").setUserId(123).setGlobalPermissions("none");
  when(service.find(13L,session)).thenReturn(new DefaultIssueFilter().setId(13L).setName("Blocker issues").setData("severity=BLOCKER").setUser("simon").setShared(true));
  tester.newGetRequest("api/issue_filters","app").setParam("id","13").execute().assertJson(getClass(),"selected_filter_can_not_be_modified.json");
}
