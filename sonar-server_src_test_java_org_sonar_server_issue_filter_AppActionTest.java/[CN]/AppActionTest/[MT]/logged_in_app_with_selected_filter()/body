{
  MockUserSession session=MockUserSession.set().setLogin("eric").setUserId(123);
  when(service.find(13L,session)).thenReturn(new DefaultIssueFilter().setId(13L).setName("Blocker issues").setData("severity=BLOCKER").setUser("eric"));
  tester.newRequest("app").setParam("id","13").execute().assertJson(getClass(),"logged_in_page_with_selected_filter.json");
}
