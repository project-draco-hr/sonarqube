{
  checkModCount();
  if (!hasNext()) {
    throw new NoSuchElementException("No element at index " + nextIndex + ".");
  }
  if (next == null) {
    next=parent.root.get(nextIndex);
  }
  Object value=next.getValue();
  current=next;
  currentIndex=nextIndex++;
  next=next.next();
  return value;
}
