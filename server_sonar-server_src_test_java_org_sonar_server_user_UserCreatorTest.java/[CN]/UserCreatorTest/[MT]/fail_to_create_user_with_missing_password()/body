{
  try {
    userCreator.create(NewUser.create().setLogin("marius").setName("Marius").setEmail("marius@mail.com").setPassword(null).setPasswordConfirmation("password"));
    fail();
  }
 catch (  BadRequestException e) {
    assertThat(e.errors().messages()).containsOnly(Message.of(Validation.CANT_BE_EMPTY_MESSAGE,"Password"));
  }
}
