{
  try {
    userCreator.create(NewUser.create().setLogin("/marius/").setName("Marius").setEmail("marius@mail.com").setPassword("password").setPasswordConfirmation("password"));
    fail();
  }
 catch (  BadRequestException e) {
    assertThat(e.errors().messages()).containsOnly(Message.of("user.bad_login"));
  }
}
