{
  if ("1".equals(level)) {
    return RulePriority.BLOCKER;
  }
  if ("2".equals(level)) {
    return RulePriority.CRITICAL;
  }
  if ("3".equals(level)) {
    return RulePriority.MAJOR;
  }
  if ("4".equals(level)) {
    return RulePriority.MINOR;
  }
  if ("5".equals(level)) {
    return RulePriority.INFO;
  }
  return null;
}
