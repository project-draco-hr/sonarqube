{
  String uuid="abcd";
  ComponentDto component=ComponentTesting.newProjectDto(uuid).setKey("org.codehaus.sonar:sonar");
  UserSession userSession=MockUserSession.set().setLogin("obiwan").addProjectUuidPermissions(UserRole.ADMIN,uuid);
  Collection<ConfigPage> pages=new ComponentConfigurationPages(i18n,resourceTypes).getConfigPages(component,userSession);
  assertThat(pages).extracting("name").containsExactly("project_quality_profiles.page","project_quality_gate.page","manual_measures.page","action_plans.page","project_links.page");
  assertThat(pages).extracting("url").containsExactly("/project/profile?id=org.codehaus.sonar%3Asonar","/project/qualitygate?id=org.codehaus.sonar%3Asonar","/manual_measures/index?id=org.codehaus.sonar%3Asonar","/action_plans/index?id=org.codehaus.sonar%3Asonar","/project/links?id=org.codehaus.sonar%3Asonar");
}
