{
  scanSampleWithDate("2012-01-01");
  Selenese selenese=Selenese.builder().setHtmlTestsInClasspath("project-settings","/projectAdministration/ProjectAdministrationTest/project-settings/override-global-settings.html","/projectAdministration/ProjectAdministrationTest/project-settings/only-on-project-settings.html").build();
  new SeleneseTest(selenese).runOn(orchestrator);
  assertThat(orchestrator.getServer().getAdminWsClient().find(PropertyQuery.createForResource("sonar.exclusions","sample")).getValue()).isEqualTo("my-exclusions");
  assertThat(orchestrator.getServer().getAdminWsClient().findAll(PropertyQuery.createForResource(null,"sample"))).isNotEmpty();
}
