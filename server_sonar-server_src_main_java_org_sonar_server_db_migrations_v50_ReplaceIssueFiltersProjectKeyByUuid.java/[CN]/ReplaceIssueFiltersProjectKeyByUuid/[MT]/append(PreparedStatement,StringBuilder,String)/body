{
  if (projectKey != null) {
    pstmt.setString(1,projectKey);
    ResultSet rs=null;
    try {
      rs=pstmt.executeQuery();
      if (rs.next()) {
        String projectUuid=SqlUtil.getString(rs,"uuid");
        if (projectUuid != null) {
          newFields.append(NEW_COMPONENT_ROOTS_FIELDS).append("=").append(projectUuid);
        }
      }
    }
  finally {
      DbUtils.closeQuietly(rs);
    }
  }
}
