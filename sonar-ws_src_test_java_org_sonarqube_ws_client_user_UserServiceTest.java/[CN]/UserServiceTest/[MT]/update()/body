{
  underTest.update(UpdateRequest.builder().setLogin("john").setName("John").setEmail("john@doo.com").setScmAccounts(asList("jo","hn")).build());
  serviceTester.assertThat(serviceTester.getPostRequest()).hasParam(PARAM_LOGIN,"john").hasParam(PARAM_NAME,"John").hasParam(PARAM_EMAIL,"john@doo.com").hasParam(PARAM_SCM_ACCOUNT,asList("jo","hn")).andNoOtherParam();
}
