{
  ActiveRule rule=context.activeRules().findByInternalKey(XooConstants.REPOSITORY_KEY,context.settings().getString(INTERNAL_KEY_PROPERTY));
  if (rule != null) {
    context.addIssue(context.issueBuilder().ruleKey(rule.ruleKey()).onFile(file).message("This issue is generated on each file").build());
  }
}
