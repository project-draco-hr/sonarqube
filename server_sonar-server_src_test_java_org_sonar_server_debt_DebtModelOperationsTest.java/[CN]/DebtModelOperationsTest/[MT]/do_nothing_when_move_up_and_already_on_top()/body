{
  when(dao.selectById(session,10)).thenReturn(new CharacteristicDto().setId(10).setKey("MEMORY_EFFICIENCY").setOrder(1));
  when(dao.selectEnabledRootCharacteristics(session)).thenReturn(newArrayList(new CharacteristicDto().setId(10).setKey("MEMORY_EFFICIENCY").setOrder(1),new CharacteristicDto().setId(2).setKey("PORTABILITY").setOrder(2)));
  service.moveUp(10);
  verify(dao,never()).update(any(CharacteristicDto.class),eq(session));
}
