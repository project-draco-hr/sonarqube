{
  StringBuffer sb=new StringBuffer();
  if (contextPath != null) {
    sb.append(contextPath);
  }
  if (servletPath != null) {
    sb.append(servletPath);
  }
  if (pathInfo != null) {
    sb.append(pathInfo);
  }
  if (sb.length() > 0) {
    return (sb.toString());
  }
  throw new UnsupportedOperationException();
}
