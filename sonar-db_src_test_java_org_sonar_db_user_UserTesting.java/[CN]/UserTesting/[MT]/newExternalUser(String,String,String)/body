{
  return new UserDto().setActive(true).setLocal(false).setName(name).setEmail(email).setLogin(login).setScmAccounts(singletonList(randomAlphanumeric(40))).setExternalIdentity(randomAlphanumeric(40)).setExternalIdentityProvider(randomAlphanumeric(40)).setCreatedAt(nextLong()).setUpdatedAt(nextLong());
}
