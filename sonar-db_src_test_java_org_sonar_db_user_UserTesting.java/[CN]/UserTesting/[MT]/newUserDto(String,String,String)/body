{
  return new UserDto().setActive(true).setLocal(true).setName(name).setEmail(email).setLogin(login).setScmAccounts(singletonList(randomAlphanumeric(40))).setExternalIdentity(login).setExternalIdentityProvider("sonarqube").setSalt(randomAlphanumeric(40)).setCryptedPassword(randomAlphanumeric(40)).setCreatedAt(nextLong()).setUpdatedAt(nextLong());
}
