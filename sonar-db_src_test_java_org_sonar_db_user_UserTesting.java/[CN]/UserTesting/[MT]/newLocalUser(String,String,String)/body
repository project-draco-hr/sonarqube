{
  return new UserDto().setActive(true).setLocal(true).setName(name).setEmail(email).setLogin(login).setScmAccounts(randomAlphanumeric(40)).setExternalIdentity(login).setExternalIdentityProvider("sonarqube").setRememberToken(randomAlphanumeric(500)).setRememberTokenExpiresAt(new Date((long)nextInt())).setSalt(randomAlphanumeric(40)).setCryptedPassword(randomAlphanumeric(40)).setCreatedAt(nextLong()).setUpdatedAt(nextLong());
}
