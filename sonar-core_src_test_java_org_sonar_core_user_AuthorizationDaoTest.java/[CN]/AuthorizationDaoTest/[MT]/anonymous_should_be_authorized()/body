{
  setupData("anonymous_should_be_authorized");
  AuthorizationDao authorization=new AuthorizationDao(getMyBatis());
  Set<Integer> componentIds=authorization.keepAuthorizedComponentIds(Sets.<Integer>newHashSet(PROJECT,PACKAGE,FILE,FILE_IN_OTHER_PROJECT),null,"user");
  assertThat(componentIds).containsOnly(PROJECT,PACKAGE,FILE);
  componentIds=authorization.keepAuthorizedComponentIds(Sets.<Integer>newHashSet(PROJECT,PACKAGE,FILE,FILE_IN_OTHER_PROJECT),null,"admin");
  assertThat(componentIds).isEmpty();
}
