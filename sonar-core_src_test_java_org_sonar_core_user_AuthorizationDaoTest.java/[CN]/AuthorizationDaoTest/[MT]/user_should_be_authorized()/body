{
  setupData("user_should_be_authorized");
  AuthorizationDao authorization=new AuthorizationDao(getMyBatis());
  Set<Integer> componentIds=authorization.keepAuthorizedComponentIds(Sets.<Integer>newHashSet(PROJECT,PACKAGE,FILE,FILE_IN_OTHER_PROJECT),USER,"user");
  assertThat(componentIds).containsOnly(PROJECT,PACKAGE,FILE);
  componentIds=authorization.keepAuthorizedComponentIds(Sets.<Integer>newHashSet(PROJECT,PACKAGE,FILE),USER,"admin");
  assertThat(componentIds).isEmpty();
}
