{
  setupData("user_should_be_authorized");
  AuthorizationDao authorization=new AuthorizationDao(getMyBatis());
  Collection<Long> componentIds=authorization.keepAuthorizedProjectIds(session,Sets.newHashSet(PROJECT_ID,EMPTY_PROJECT_ID),USER,"user");
  assertThat(componentIds).containsOnly(PROJECT_ID,EMPTY_PROJECT_ID);
  componentIds=authorization.keepAuthorizedProjectIds(session,Sets.newHashSet(PROJECT_ID),USER,"admin");
  assertThat(componentIds).isEmpty();
}
