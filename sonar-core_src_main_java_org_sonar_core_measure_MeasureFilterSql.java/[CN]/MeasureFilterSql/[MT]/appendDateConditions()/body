{
  if (filter.getFromDate() != null) {
    sql.append(" AND s.created_at >= ? ");
    dateParameters.add(new Date(filter.getFromDate().getTime()));
  }
  if (filter.getToDate() != null) {
    sql.append(" AND s.created_at <= ? ");
    dateParameters.add(new Date(filter.getToDate().getTime()));
  }
}
