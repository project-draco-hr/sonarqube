{
  if (rs != null) {
    try {
      rs.close();
    }
 catch (    SQLException e) {
      LoggerFactory.getLogger(MeasureFilterSql.class).warn("Fail to close result set",e);
    }
  }
  if (stmt != null) {
    try {
      stmt.close();
    }
 catch (    SQLException e) {
      LoggerFactory.getLogger(MeasureFilterSql.class).warn("Fail to close statement",e);
    }
  }
}
