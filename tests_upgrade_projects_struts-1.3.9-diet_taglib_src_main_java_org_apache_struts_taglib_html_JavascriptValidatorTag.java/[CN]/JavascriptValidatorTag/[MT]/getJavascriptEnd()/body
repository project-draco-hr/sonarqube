{
  StringBuffer sb=new StringBuffer();
  sb.append("\n");
  if (!this.isXhtml() && "true".equals(htmlComment)) {
    sb.append(HTML_END_COMMENT);
  }
  if (this.isXhtml() && "true".equalsIgnoreCase(this.cdata)) {
    sb.append("//]]>\r\n");
  }
  sb.append("</script>\n\n");
  return sb.toString();
}
