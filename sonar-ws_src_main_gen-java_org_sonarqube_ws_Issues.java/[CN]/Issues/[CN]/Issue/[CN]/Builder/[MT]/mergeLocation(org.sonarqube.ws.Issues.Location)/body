{
  if (locationBuilder_ == null) {
    if (((bitField0_ & 0x00000100) == 0x00000100) && location_ != org.sonarqube.ws.Issues.Location.getDefaultInstance()) {
      location_=org.sonarqube.ws.Issues.Location.newBuilder(location_).mergeFrom(value).buildPartial();
    }
