{
  if (textRangeBuilder_ == null) {
    if (((bitField0_ & 0x00000100) == 0x00000100) && textRange_ != org.sonarqube.ws.Common.TextRange.getDefaultInstance()) {
      textRange_=org.sonarqube.ws.Common.TextRange.newBuilder(textRange_).mergeFrom(value).buildPartial();
    }
 else {
      textRange_=value;
    }
    onChanged();
  }
 else {
    textRangeBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00000100;
  return this;
}
