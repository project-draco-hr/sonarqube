{
  if (commentsBuilder_ == null) {
    if (((bitField0_ & 0x00200000) == 0x00200000) && comments_ != null && comments_ != org.sonarqube.ws.Issues.Comments.getDefaultInstance()) {
      comments_=org.sonarqube.ws.Issues.Comments.newBuilder(comments_).mergeFrom(value).buildPartial();
    }
 else {
      comments_=value;
    }
    onChanged();
  }
 else {
    commentsBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00200000;
  return this;
}
