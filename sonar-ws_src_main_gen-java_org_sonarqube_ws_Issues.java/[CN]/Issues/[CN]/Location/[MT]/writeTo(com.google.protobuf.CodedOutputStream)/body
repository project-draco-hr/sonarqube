{
  if (((bitField0_ & 0x00000001) == 0x00000001)) {
    output.writeBytes(1,getComponentIdBytes());
  }
  if (((bitField0_ & 0x00000002) == 0x00000002)) {
    output.writeMessage(2,getTextRange());
