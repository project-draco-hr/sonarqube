{
  if (languagesBuilder_ == null) {
    if (((bitField0_ & 0x00000400) == 0x00000400) && languages_ != null && languages_ != org.sonarqube.ws.Issues.Languages.getDefaultInstance()) {
      languages_=org.sonarqube.ws.Issues.Languages.newBuilder(languages_).mergeFrom(value).buildPartial();
    }
 else {
      languages_=value;
    }
    onChanged();
  }
 else {
    languagesBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00000400;
  return this;
}
