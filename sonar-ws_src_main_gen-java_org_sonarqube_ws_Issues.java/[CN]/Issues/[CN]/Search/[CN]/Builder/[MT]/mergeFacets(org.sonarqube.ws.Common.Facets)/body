{
  if (facetsBuilder_ == null) {
    if (((bitField0_ & 0x00000800) == 0x00000800) && facets_ != null && facets_ != org.sonarqube.ws.Common.Facets.getDefaultInstance()) {
      facets_=org.sonarqube.ws.Common.Facets.newBuilder(facets_).mergeFrom(value).buildPartial();
    }
 else {
      facets_=value;
    }
    onChanged();
  }
 else {
    facetsBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00000800;
  return this;
}
