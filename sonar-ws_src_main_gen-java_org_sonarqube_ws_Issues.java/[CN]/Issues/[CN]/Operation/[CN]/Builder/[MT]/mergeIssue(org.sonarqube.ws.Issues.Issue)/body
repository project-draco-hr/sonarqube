{
  if (issueBuilder_ == null) {
    if (((bitField0_ & 0x00000001) == 0x00000001) && issue_ != org.sonarqube.ws.Issues.Issue.getDefaultInstance()) {
      issue_=org.sonarqube.ws.Issues.Issue.newBuilder(issue_).mergeFrom(value).buildPartial();
    }
 else {
      issue_=value;
    }
    onChanged();
  }
 else {
    issueBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00000001;
  return this;
}
