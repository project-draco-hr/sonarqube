{
  addSomeContext(underTest);
  underTest.logTimeLast(true);
  underTest.start().stopError("Rules registered");
  assertThat(tester.logs()).hasSize(1);
  assertThat(tester.logs(LoggerLevel.ERROR).get(0)).startsWith("Rules registered | a_string=bar | an_int=42 | after_start=true | time=").endsWith("ms");
}
