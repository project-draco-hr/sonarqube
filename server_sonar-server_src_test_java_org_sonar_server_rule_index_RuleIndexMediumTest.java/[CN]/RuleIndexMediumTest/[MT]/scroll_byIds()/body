{
  Set<Integer> ids=new HashSet<>();
  for (int i=0; i < 150; i++) {
    RuleDto rule=RuleTesting.newDto(RuleKey.of("scroll","r_" + i));
    dao.insert(dbSession,rule);
    dbSession.commit();
    ids.add(rule.getId());
  }
  List<Rule> rules=index.getByIds(ids);
  assertThat(rules).hasSize(ids.size());
}
