{
  Issue issue=createStandardIssue().setAssignee("john").setReporter("steven").setAuthorLogin("Henry");
  issues.add(issue);
  result.addUsers(Lists.<User>newArrayList(new DefaultUser().setName("John").setLogin("john").setActive(true).setEmail("john@email.com")));
  WsTester.TestRequest request=tester.newGetRequest("api/issues","search");
  request.execute().assertJson(getClass(),"issues_with_users.json");
}
