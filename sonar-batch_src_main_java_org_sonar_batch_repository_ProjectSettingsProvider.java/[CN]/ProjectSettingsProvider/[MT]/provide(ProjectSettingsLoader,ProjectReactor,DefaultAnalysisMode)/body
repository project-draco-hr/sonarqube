{
  if (settings == null) {
    if (mode.isNotAssociated()) {
      settings=createNonAssociatedProjectSettings();
    }
 else {
      MutableBoolean fromCache=new MutableBoolean();
      settings=loader.load(projectReactor.getRoot().getKeyWithBranch(),fromCache);
      checkProject(mode);
    }
  }
  return settings;
}
