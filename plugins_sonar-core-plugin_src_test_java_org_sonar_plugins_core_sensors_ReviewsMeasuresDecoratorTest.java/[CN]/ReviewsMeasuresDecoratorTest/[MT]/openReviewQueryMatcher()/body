{
  return new BaseMatcher<ReviewQuery>(){
    public boolean matches(    Object o){
      ReviewQuery query=(ReviewQuery)o;
      if (query == null) {
        return false;
      }
      return Lists.newArrayList(ReviewDto.STATUS_OPEN,ReviewDto.STATUS_REOPENED).equals(query.getStatuses()) && query.getNoAssignee() == null && query.getPlanned() == null;
    }
    public void describeTo(    Description description){
    }
  }
;
}
