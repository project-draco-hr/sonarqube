{
  WebService.NewParam newParam=action.createParam(WebService.Param.FIELDS).setDescription("Comma-separated list of the fields to be returned in response. All the fields are returned by default.").setPossibleValues(possibleFields);
  if (possibleFields != null && possibleFields.size() > 1) {
    Iterator<String> it=possibleFields.iterator();
    newParam.setExampleValue(String.format("%s,%s",it.next(),it.next()));
  }
}
