{
  ruleDao.insert(dbSession,RuleTesting.newXooX1().setDefaultRemediationFunction(DebtRemediationFunction.Type.LINEAR_OFFSET.name()).setDefaultRemediationCoefficient("1h").setDefaultRemediationOffset("15min").setRemediationFunction(DebtRemediationFunction.Type.LINEAR_OFFSET.name()).setRemediationCoefficient("2h").setRemediationOffset("25min"));
  dbSession.commit();
  ruleIndexer.index();
  WsTester.TestRequest request=tester.wsTester().newGetRequest(API_ENDPOINT,API_SEARCH_METHOD);
  request.setParam(WebService.Param.FIELDS,"debtRemFn,debtOverloaded,defaultDebtRemFn");
  WsTester.Result result=request.execute();
  result.assertJson(this.getClass(),"search_debt_rules_with_default_and_overridden_debt_values.json");
}
