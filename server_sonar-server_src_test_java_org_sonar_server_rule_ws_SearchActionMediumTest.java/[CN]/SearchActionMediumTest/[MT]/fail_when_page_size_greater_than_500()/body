{
  expectedException.expect(BadRequestException.class);
  expectedException.expectMessage("Page size must be less than 500");
  WsTester.TestRequest request=tester.wsTester().newGetRequest(API_ENDPOINT,API_SEARCH_METHOD).setParam(WebService.Param.PAGE_SIZE,String.valueOf(501));
  request.setParam(WebService.Param.FIELDS,"actives");
  request.execute();
}
