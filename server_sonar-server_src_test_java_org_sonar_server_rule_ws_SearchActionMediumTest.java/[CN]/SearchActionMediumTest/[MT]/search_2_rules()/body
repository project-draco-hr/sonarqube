{
  ruleDao.insert(dbSession,RuleTesting.newXooX1().setType(RuleType.BUG));
  ruleDao.insert(dbSession,RuleTesting.newXooX2().setType(RuleType.VULNERABILITY));
  dbSession.commit();
  ruleIndexer.index();
  WsTester.TestRequest request=tester.wsTester().newGetRequest(API_ENDPOINT,API_SEARCH_METHOD);
  WsTester.Result result=request.execute();
  result.assertJson(getClass(),"search_2_rules.json");
}
