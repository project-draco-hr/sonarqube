{
  if (file.isDirectory()) {
    for (    File f : file.listFiles()) {
      analyzeAll(analyzer,f);
    }
  }
 else {
    try {
      analyzer.analyzeAll(file);
    }
 catch (    Exception e) {
      JaCoCoUtils.LOG.warn("Exception during analysis of file " + file.getAbsolutePath(),e);
    }
  }
}
