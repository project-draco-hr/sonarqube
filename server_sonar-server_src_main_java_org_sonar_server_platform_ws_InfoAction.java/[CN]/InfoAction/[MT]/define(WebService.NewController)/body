{
  controller.createAction("info").setDescription("Detailed information about system configuration.<br/>" + "Requires 'Administer' permissions.<br/>" + "Since 5.5, this web service becomes internal in order to more easily update result.").setSince("5.1").setInternal(true).setResponseExample(getClass().getResource("/org/sonar/server/platform/ws/example-system-info.json")).setHandler(this);
}
