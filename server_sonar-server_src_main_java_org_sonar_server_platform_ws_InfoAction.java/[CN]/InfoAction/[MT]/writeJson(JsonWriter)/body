{
  json.beginObject();
  for (  Monitor monitor : monitors) {
    json.name(monitor.name());
    json.beginObject();
    for (    Map.Entry<String,Object> attribute : monitor.attributes().entrySet()) {
      json.name(attribute.getKey()).valueObject(attribute.getValue());
    }
    json.endObject();
  }
  json.endObject();
}
