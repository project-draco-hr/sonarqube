{
  setupData("update_requirement");
  CharacteristicDto dto=new CharacteristicDto().setId(1L).setParentId(3).setRuleId(2).setFunction("linear").setFactorValue(21.0).setFactorUnit("h").setOffsetValue(null).setOffsetUnit(null).setCreatedAt(DateUtils.parseDate("2013-11-22")).setUpdatedAt(DateUtils.parseDate("2013-11-22")).setEnabled(false);
