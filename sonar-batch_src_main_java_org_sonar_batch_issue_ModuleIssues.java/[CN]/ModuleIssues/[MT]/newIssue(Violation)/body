{
  return (DefaultIssue)new DefaultIssueBuilder().componentKey(violation.getResource().getEffectiveKey()).projectKey(project.getRoot().getEffectiveKey()).ruleKey(RuleKey.of(violation.getRule().getRepositoryKey(),violation.getRule().getKey())).effortToFix(violation.getCost()).line(violation.getLineId()).message(violation.getMessage()).severity(violation.getSeverity() != null ? violation.getSeverity().name() : null).build();
}
