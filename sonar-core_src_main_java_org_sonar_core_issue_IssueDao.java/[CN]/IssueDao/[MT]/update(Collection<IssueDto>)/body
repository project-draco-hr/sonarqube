{
  Preconditions.checkNotNull(issues);
  SqlSession session=mybatis.openBatchSession();
  try {
    IssueMapper mapper=session.getMapper(IssueMapper.class);
    for (    IssueDto issue : issues) {
      mapper.update(issue);
    }
    session.commit();
    return this;
  }
  finally {
    MyBatis.closeQuietly(session);
  }
}
