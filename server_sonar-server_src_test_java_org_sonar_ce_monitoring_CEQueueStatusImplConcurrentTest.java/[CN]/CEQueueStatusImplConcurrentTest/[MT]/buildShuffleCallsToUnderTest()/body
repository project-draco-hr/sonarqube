{
  List<Runnable> res=new ArrayList<>();
  for (int i=0; i < 100; i++) {
    res.add(new AddReceivedRunnable());
  }
  for (int i=0; i < 98; i++) {
    res.add(new AddInProgressRunnable());
  }
  for (int i=0; i < 80; i++) {
    res.add(new AddSuccessRunnable());
  }
  for (int i=0; i < 17; i++) {
    res.add(new AddErrorRunnable());
  }
  Collections.shuffle(res);
  return res;
}
