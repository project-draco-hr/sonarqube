{
  final Object element=m_elements[index];
  int hole=index;
  while ((hole * 2) <= m_size) {
    int child=hole * 2;
    if (child != m_size && compare(m_elements[child + 1],m_elements[child]) < 0) {
      child++;
    }
    if (compare(m_elements[child],element) >= 0) {
      break;
    }
    m_elements[hole]=m_elements[child];
    hole=child;
  }
  m_elements[hole]=element;
}
