{
  if (isZero())   if (anObject instanceof IMoney)   return ((IMoney)anObject).isZero();
  if (anObject instanceof MoneyBag) {
    MoneyBag aMoneyBag=(MoneyBag)anObject;
    if (aMoneyBag.fMonies.size() != fMonies.size())     return false;
    for (Enumeration e=fMonies.elements(); e.hasMoreElements(); ) {
      Money m=(Money)e.nextElement();
      if (!aMoneyBag.contains(m))       return false;
    }
    return true;
  }
  return false;
}
