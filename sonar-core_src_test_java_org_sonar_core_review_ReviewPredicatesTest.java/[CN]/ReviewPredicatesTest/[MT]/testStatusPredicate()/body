{
  Predicate<ReviewDto> predicate=ReviewPredicates.status(ReviewDto.STATUS_REOPENED);
  Collection<ReviewDto> filtered=Collections2.filter(Lists.<ReviewDto>newArrayList(new ReviewDto().setStatus(ReviewDto.STATUS_OPEN),new ReviewDto().setStatus(ReviewDto.STATUS_REOPENED),new ReviewDto().setStatus(ReviewDto.STATUS_REOPENED)),predicate);
  assertThat(filtered.size(),is(2));
}
