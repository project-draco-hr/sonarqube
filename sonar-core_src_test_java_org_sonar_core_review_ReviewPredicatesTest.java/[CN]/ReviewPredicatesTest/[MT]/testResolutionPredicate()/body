{
  Predicate<ReviewDto> predicate=ReviewPredicates.resolution(ReviewDto.RESOLUTION_FALSE_POSITIVE);
  Collection<ReviewDto> filtered=Collections2.filter(Lists.<ReviewDto>newArrayList(new ReviewDto().setResolution(null),new ReviewDto().setResolution(ReviewDto.RESOLUTION_FALSE_POSITIVE),new ReviewDto().setResolution(ReviewDto.RESOLUTION_FIXED)),predicate);
  assertThat(filtered.size(),is(1));
}
