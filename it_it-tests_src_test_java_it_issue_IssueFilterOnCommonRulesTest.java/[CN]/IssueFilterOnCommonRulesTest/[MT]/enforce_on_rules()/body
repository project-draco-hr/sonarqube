{
  executeAnalysis("sonar.issue.enforce.multicriteria","1,2","sonar.issue.enforce.multicriteria.1.ruleKey","common-xoo:DuplicatedBlocks","sonar.issue.enforce.multicriteria.1.resourceKey","**/Sample.xoo","sonar.issue.enforce.multicriteria.2.ruleKey","common-xoo:InsufficientCommentDensity","sonar.issue.enforce.multicriteria.2.resourceKey","**/OtherFile.xoo");
  assertThat(findAllIssues()).hasSize(5);
  assertThat(findIssuesByRuleKey("common-xoo:DuplicatedBlocks")).hasSize(1);
  assertThat(findIssuesByRuleKey("common-xoo:InsufficientCommentDensity")).isEmpty();
}
