{
  String qGateName="Sonar way";
  when(settings.getString("sonar.qualitygate")).thenReturn(qGateName);
  when(client.request("/api/qualitygates/show?name=Sonar way")).thenThrow(new HttpDownloader.HttpException(URI.create("/api/qualitygates/show?name=Sonar%20way"),HttpURLConnection.HTTP_NOT_ACCEPTABLE));
  new QualityGateProvider().provide(settings,client,metricFinder);
}
