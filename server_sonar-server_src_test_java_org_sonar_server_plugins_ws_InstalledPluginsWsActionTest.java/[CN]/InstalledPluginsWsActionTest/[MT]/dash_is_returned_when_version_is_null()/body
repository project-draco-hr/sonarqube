{
  when(pluginRepository.getMetadata()).thenReturn(of((PluginMetadata)DefaultPluginMetadata.create("key").setCore(false).setVersion(null)));
  underTest.handle(request,response);
  assertJson(response.outputAsString()).isSimilarTo("{" + "  \"plugins\":" + "  ["+ "    {\"version\": \"-\"}"+ "  ]"+ "}");
}
