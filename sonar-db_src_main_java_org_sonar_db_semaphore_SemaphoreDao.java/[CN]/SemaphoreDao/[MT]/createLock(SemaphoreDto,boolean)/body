{
  Semaphores.Semaphore semaphore=new Semaphores.Semaphore().setName(dto.getName()).setLocked(acquired).setLockedAt(longToDate(dto.getLockedAt())).setCreatedAt(longToDate(dto.getCreatedAt())).setUpdatedAt(longToDate(dto.getUpdatedAt()));
  if (!acquired) {
    semaphore.setDurationSinceLocked(lockedSince(dto));
  }
  return semaphore;
}
