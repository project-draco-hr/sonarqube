{
  Date fromDate=filter.getFromDate();
  if (fromDate != null) {
    sb.append(" and s.created_at >= ? ");
    dateParameters.add(fromDate.getTime());
  }
  Date toDate=filter.getToDate();
  if (toDate != null) {
    sb.append(" and s.created_at <= ? ");
    dateParameters.add(toDate.getTime());
  }
}
