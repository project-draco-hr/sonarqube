{
  resource.setId(JsonUtils.getInteger(json,"id")).setKey(JsonUtils.getString(json,"key")).setName(JsonUtils.getString(json,"name")).setLongName(JsonUtils.getString(json,"lname")).setScope(JsonUtils.getString(json,"scope")).setQualifier(JsonUtils.getString(json,"qualifier")).setLanguage(JsonUtils.getString(json,"lang")).setDescription(JsonUtils.getString(json,"description")).setDate(JsonUtils.getDateTime(json,"date")).setVersion(JsonUtils.getString(json,"version"));
}
