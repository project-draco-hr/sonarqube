{
  DbSession dbSession=db.openSession(false);
  List<ActiveRuleChange> changes=Lists.newArrayList();
  try {
    changes.addAll(this.deactivate(key,dbSession));
    if (!changes.isEmpty()) {
      log.write(dbSession,changes);
      dbSession.commit();
    }
  }
  finally {
    dbSession.close();
  }
  return changes;
}
