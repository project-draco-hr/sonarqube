{
  List<ActiveRuleChange> changes=Lists.newArrayList();
  RuleActivationContext context=contextFactory.create(activation.getKey(),dbSession);
  activation.isReset(true).setParameters(context.parentActiveRuleParamsAsStringMap()).setSeverity(context.defaultSeverity());
  changes.addAll(this.activate(activation,dbSession));
  return changes;
}
