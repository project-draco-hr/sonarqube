{
  Source source=new Source();
  for (  String key : json.keySet()) {
    JSONValue val=json.get(key);
    if (val.isString() != null) {
      source.addLine(Integer.parseInt(key),val.isString().stringValue());
    }
 else {
      source.addLine(Integer.parseInt(key),"");
    }
  }
  return source;
}
