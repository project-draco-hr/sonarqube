{
  Issue issue=new DefaultIssue().setKey("A").setCreationDate(tenDaysAgo).setTechnicalDebt(oneDaysDebt).setChanges(newArrayList(new FieldDiffs().setDiff("technicalDebt",fromWorkDayDuration(twoDaysDebt),fromWorkDayDuration(oneDaysDebt)).setCreationDate(null)));
  assertThat(issueChangelogDebtCalculator.calculateNewTechnicalDebt(issue,rightNow)).isNull();
}
