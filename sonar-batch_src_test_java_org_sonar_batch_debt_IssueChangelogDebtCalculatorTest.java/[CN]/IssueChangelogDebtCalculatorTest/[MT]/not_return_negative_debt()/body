{
  Issue issue=new DefaultIssue().setKey("A").setCreationDate(tenDaysAgo).setDebt(oneDaysDebtInSec).setChanges(newArrayList(new FieldDiffs().setDiff("technicalDebt",fromWorkDayDuration(twoDaysDebt),fromWorkDayDuration(oneDaysDebt)).setCreationDate(null)));
  assertThat(issueChangelogDebtCalculator.calculateNewTechnicalDebt(issue,rightNow)).isNull();
}
