{
  Issue issue=new DefaultIssue().setKey("A").setCreationDate(tenDaysAgo).setDebt(twoDaysDebt).setChanges(newArrayList(new FieldDiffs().setDiff("technicalDebt",oneDay,twoDays).setCreationDate(null)));
  assertThat(issueChangelogDebtCalculator.calculateNewTechnicalDebt(issue,null)).isEqualTo(2 * oneDay);
}
