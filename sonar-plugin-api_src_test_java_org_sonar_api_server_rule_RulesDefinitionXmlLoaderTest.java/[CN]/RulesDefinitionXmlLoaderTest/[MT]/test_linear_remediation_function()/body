{
  String xml="" + "<rules>" + "  <rule>"+ "    <key>1</key>"+ "    <name>One</name>"+ "    <description>Desc</description>"+ "    <effortToFixDescription>lines</effortToFixDescription>"+ "    <debtSubCharacteristic>BUG</debtSubCharacteristic>"+ "    <debtRemediationFunction>LINEAR</debtRemediationFunction>"+ "    <debtRemediationFunctionCoefficient>2d 3h</debtRemediationFunctionCoefficient>"+ "  </rule>"+ "</rules>";
  RulesDefinition.Rule rule=load(xml).rule("1");
  assertThat(rule.debtSubCharacteristic()).isEqualTo("BUG");
  assertThat(rule.effortToFixDescription()).isEqualTo("lines");
  DebtRemediationFunction function=rule.debtRemediationFunction();
  assertThat(function).isNotNull();
  assertThat(function.type()).isEqualTo(DebtRemediationFunction.Type.LINEAR);
  assertThat(function.coefficient()).isEqualTo("2d3h");
  assertThat(function.offset()).isNull();
}
