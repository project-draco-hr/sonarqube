{
  String xml="" + "<rules>" + "  <rule>"+ "    <key>1</key>"+ "    <name>One</name>"+ "    <description>Desc</description>"+ "    <effortToFixDescription>lines</effortToFixDescription>"+ "    <debtSubCharacteristic>BUG</debtSubCharacteristic>"+ "    <debtRemediationFunction>LINEAR_OFFSET</debtRemediationFunction>"+ "    <debtRemediationFunctionCoefficient>2d 3h</debtRemediationFunctionCoefficient>"+ "    <debtRemediationFunctionOffset>5min</debtRemediationFunctionOffset>"+ "  </rule>"+ "</rules>";
  RulesDefinition.Rule rule=load(xml).rule("1");
  assertThat(rule.debtSubCharacteristic()).isNull();
}
