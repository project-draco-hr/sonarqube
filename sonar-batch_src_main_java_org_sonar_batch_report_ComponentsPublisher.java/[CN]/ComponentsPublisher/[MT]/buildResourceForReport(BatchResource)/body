{
  Resource r=batchResource.resource();
  ReportComponent result=new ReportComponent().setBatchId(batchResource.batchId()).setSnapshotId(batchResource.snapshotId()).setId(r.getId()).setName(getName(r)).setPath(r.getPath()).setUuid(r.getUuid()).setType(getType(r)).setLanguageKey(getLanguageKey(r)).setTest(isTest(r));
  for (  BatchResource child : batchResource.children()) {
    result.addChild(buildResourceForReport(child));
  }
  return result;
}
