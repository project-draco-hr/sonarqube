{
  Resource r=batchResource.resource();
  ReportComponent result=new ReportComponent().setBatchId(batchResource.batchId()).setSnapshotId(batchResource.snapshotId()).setId(r.getId()).setName(r.getName()).setPath(r.getPath()).setType(getType(r));
  for (  BatchResource child : batchResource.children()) {
    result.addChild(buildResourceForReport(child));
  }
  return result;
}
