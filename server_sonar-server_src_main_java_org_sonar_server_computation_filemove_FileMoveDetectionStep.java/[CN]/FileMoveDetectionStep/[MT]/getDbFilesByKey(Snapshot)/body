{
  try (DbSession dbSession=dbClient.openSession(false)){
    return from(dbClient.componentDao().selectAllChildren(dbSession,ComponentTreeQuery.builder().setBaseSnapshot(new SnapshotDto().setId(baseProjectSnapshot.getId()).setRootId(baseProjectSnapshot.getId())).setQualifiers(FILE_QUALIFIERS).setSortFields(SORT_FIELDS).setPageSize(Integer.MAX_VALUE).setPage(1).build())).uniqueIndex(ComponentDtoFunctions.toKey());
  }
 }
