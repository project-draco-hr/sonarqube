{
  InputFile file=new DefaultInputFile("foo","src/main/java/Foo.java").setLines(1);
  thrown.expect(IllegalArgumentException.class);
  thrown.expectMessage("It was not expected to blame file src/main/java/Foo.java");
  new DefaultBlameOutput(null,Arrays.<InputFile>asList(new DefaultInputFile("foo","src/main/java/Foo2.java"))).blameResult(file,Arrays.asList(new BlameLine().revision("1").author("guy")));
}
