{
  verify(context).saveMeasure(eq(new JavaPackage("org.apache.struts.config")),argThat(new IsMeasure(CoreMetrics.FUNCTION_COMPLEXITY_DISTRIBUTION,"1=186;2=88;4=11;6=12;8=7;10=2;12=8")));
