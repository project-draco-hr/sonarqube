{
  logTester.setLevel(LoggerLevel.TRACE);
  PutMappingRequestBuilder requestBuilder=esTester.client().preparePutMapping(FakeIndexDefinition.INDEX).setType(FakeIndexDefinition.TYPE).setIgnoreConflicts(true).setSource(mapDomain());
  requestBuilder.get();
  assertThat(logTester.logs(LoggerLevel.TRACE)).hasSize(1);
}
