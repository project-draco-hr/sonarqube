{
  if (size() == 0) {
    return "{}";
  }
  StringBuffer buf=new StringBuffer(32 * size());
  buf.append('{');
  MapIterator it=mapIterator();
  boolean hasNext=it.hasNext();
  while (hasNext) {
    Object key=it.next();
    Object value=it.getValue();
    buf.append(key == this ? "(this Map)" : key).append('=').append(value == this ? "(this Map)" : value);
    hasNext=it.hasNext();
    if (hasNext) {
      buf.append(',').append(' ');
    }
  }
  buf.append('}');
  return buf.toString();
}
