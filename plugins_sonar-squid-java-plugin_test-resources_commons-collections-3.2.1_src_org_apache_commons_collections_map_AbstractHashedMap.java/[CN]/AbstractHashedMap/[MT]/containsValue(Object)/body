{
  if (value == null) {
    for (int i=0, isize=data.length; i < isize; i++) {
      HashEntry entry=data[i];
      while (entry != null) {
        if (entry.getValue() == null) {
          return true;
        }
        entry=entry.next;
      }
    }
  }
 else {
    for (int i=0, isize=data.length; i < isize; i++) {
      HashEntry entry=data[i];
      while (entry != null) {
        if (isEqualValue(value,entry.getValue())) {
          return true;
        }
        entry=entry.next;
      }
    }
  }
  return false;
}
