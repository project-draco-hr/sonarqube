{
  if (s == null) {
    return null;
  }
  try {
    DateFormat sdf=new SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ss.SSS'Z'");
    sdf.setTimeZone(TimeZone.getTimeZone("UTC"));
    return sdf.parse(s);
  }
 catch (  ParseException e) {
    throw new IllegalArgumentException("Cannot parse ES date: " + s,e);
  }
}
