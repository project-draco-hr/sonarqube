{
  if (settings.getBoolean(CoreProperties.DRY_RUN)) {
    return;
  }
  String url="/batch_bootstrap/evict?project=" + project.getId();
  try {
    LOG.debug("Evict dryRun database");
    server.request(url);
  }
 catch (  Exception e) {
    throw new SonarException("Unable to evict dryRun database: " + url,e);
  }
}
