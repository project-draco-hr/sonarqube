{
  String user="user-measures-filter-with-no-share-perm";
  createUser(user,"User Measure Filters without sharing permission");
  try {
    new SeleneseTest(Selenese.builder().setHtmlTestsInClasspath("should_not_share_filter_when_user_have_no_sharing_permissions","/measure/suite/measure_filters/should-not-share-filter-when-user-have-no-sharing-permissions.html").build()).runOn(orchestrator);
  }
  finally {
    deactivateUser(user);
  }
}
