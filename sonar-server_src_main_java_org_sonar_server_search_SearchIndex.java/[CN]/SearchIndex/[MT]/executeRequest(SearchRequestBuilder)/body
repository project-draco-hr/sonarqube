{
  StopWatch watch=createWatch();
  try {
    return builder.execute().actionGet().getHits();
  }
  finally {
    SearchRequest request=builder.request();
    watch.stop("Executed search on ind(ex|ices) '%s' and type(s) '%s' with request: %s",Arrays.toString(request.indices()),Arrays.toString(request.types()),builderToString(builder));
  }
}
