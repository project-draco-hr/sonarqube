{
  SearchOptions SearchOptions=fixtureForCreatedAtFacet();
  Map<String,Long> createdAt=index.search(IssueQuery.builder(userSessionRule).createdBefore(DateUtils.parseDateTime("2016-01-01T00:00:00+0100")).build(),SearchOptions).getFacets().get("createdAt");
  assertThat(createdAt).containsOnly(entry("2011-01-01T00:00:00+0000",1L),entry("2012-01-01T00:00:00+0000",0L),entry("2013-01-01T00:00:00+0000",0L),entry("2014-01-01T00:00:00+0000",5L),entry("2015-01-01T00:00:00+0000",1L));
}
