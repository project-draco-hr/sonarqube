{
  PluginReferential updateCenterPluginReferential=centerClient.getPlugins(refreshUpdateCenter);
  if (updateCenterPluginReferential != null) {
    return PluginCenter.create(updateCenterPluginReferential,installedPluginReferentialFactory.getInstalledPluginReferential(),sonarVersion).setDate(centerClient.getLastRefreshDate());
  }
 else {
    return null;
  }
}
