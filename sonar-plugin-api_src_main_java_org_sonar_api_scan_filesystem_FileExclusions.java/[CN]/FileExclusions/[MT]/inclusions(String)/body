{
  String[] patterns=sanitize(settings.getStringArray(propertyKey));
  List<String> list=Lists.newArrayList();
  for (  String pattern : patterns) {
    if (!"**/*".equals(pattern) && !"file:**/*".equals(pattern)) {
      list.add(pattern);
    }
  }
  return list.toArray(new String[list.size()]);
}
