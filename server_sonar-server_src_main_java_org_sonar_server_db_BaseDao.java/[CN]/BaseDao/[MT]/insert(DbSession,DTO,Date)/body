{
  if (item.getCreatedAt() == null) {
    item.setCreatedAt(now);
  }
  item.setUpdatedAt(now);
  try {
    doInsert(session,item);
    if (hasIndex()) {
      session.enqueue(new UpsertDto<>(getIndexType(),item));
    }
  }
 catch (  Exception e) {
    throw new IllegalStateException("Fail to insert item in db: " + item,e);
  }
}
