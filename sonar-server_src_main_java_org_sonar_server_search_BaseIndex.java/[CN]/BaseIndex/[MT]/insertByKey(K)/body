{
  try {
    UpdateRequest doc=normalizer.normalize(key);
    insertDocument(doc,key);
  }
 catch (  Exception e) {
    throw new IllegalStateException(this.getClass().getSimpleName() + "cannot execute INSERT_BY_KEY for " + key.getClass().getSimpleName()+ " as "+ this.getIndexType()+ " on key: "+ key,e);
  }
}
