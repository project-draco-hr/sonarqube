{
  try {
    UpdateRequest doc=normalizer.normalize(item);
    insertDocument(doc,item.getKey());
  }
 catch (  Exception e) {
    throw new IllegalStateException(this.getClass().getSimpleName() + "cannot execute INSERT_BY_DTO for " + item.getClass().getSimpleName()+ " as "+ this.getIndexType()+ " on key: "+ item.getKey(),e);
  }
}
