{
  try {
    XContentBuilder doc=normalizer.normalize(item);
    String keyValue=getKeyValue(item.getKey());
    if (doc != null && keyValue != null && !keyValue.isEmpty()) {
      insertDocument(doc,keyValue);
    }
 else {
      LOG.error("Could not normalize document {} for insert in {}",keyValue,getIndexName());
    }
  }
 catch (  Exception e) {
    LOG.error("Could not update document for index {}: {}",getIndexName(),e.getMessage());
  }
}
