{
  try {
    LOG.info("Update document with key {}",key);
    UpdateResponse result=getClient().update(new UpdateRequest(this.getIndexName(),this.getType(),this.getKeyValue(key)).doc(this.normalize(key))).get();
  }
 catch (  Exception e) {
    LOG.error("Could not update documet for index {}: {}",this.getIndexName(),e.getMessage());
  }
}
