{
  if (this.normalizer.canNormalize(obj.getClass(),key.getClass())) {
    this.updateDocument(this.normalizer.normalizeOther(obj,key),this.getKeyValue(key));
  }
 else {
    throw new IllegalStateException("Index " + this.getIndexName() + " cannot execute INSERT for class: "+ obj.getClass());
  }
}
