{
  try {
    this.deleteDocument(key);
    for (    KEY additionalKey : keys) {
      this.deleteDocument(additionalKey);
    }
  }
 catch (  Exception e) {
    throw new IllegalStateException("Could not DELETE _id = '" + this.getKeyValue(key) + "' "+ "for index '"+ this.getIndexName()+ "': "+ e.getMessage());
  }
}
