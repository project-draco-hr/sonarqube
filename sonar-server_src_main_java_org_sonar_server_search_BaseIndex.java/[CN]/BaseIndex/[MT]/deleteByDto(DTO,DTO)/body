{
  try {
    this.deleteDocument(item.getKey());
    for (    DTO additionalItem : items) {
      this.deleteDocument(additionalItem.getKey());
    }
  }
 catch (  Exception e) {
    LOG.error("Could not DELETE _id:{} for index {}: {}",this.getKeyValue(item.getKey()),this.getIndexName(),e.getMessage());
  }
}
