{
  try {
    XContentBuilder doc=normalizer.normalize(key);
    String keyValue=getKeyValue(key);
    if (doc != null && keyValue != null && !keyValue.isEmpty()) {
      LOG.debug("Insert document with key {}",key);
      getClient().index(new IndexRequest(this.getIndexName(),getType(),keyValue).source(doc)).get();
    }
 else {
      LOG.error("Could not normalize document {} for insert in {}",key,getIndexName());
    }
  }
 catch (  Exception e) {
    LOG.error("Could not update document for index {}: {}",getIndexName(),e.getMessage());
  }
}
