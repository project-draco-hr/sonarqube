{
  task=new RegisterRules(getSessionFactory(),new RuleRepository[]{new VolumeRepository()},null,profilesManager,ruleRegistry);
  setupData("shared");
  task.start();
  List<Rule> result=getSession().getResults(Rule.class,"status",Rule.STATUS_READY);
  assertThat(result.size(),is(VolumeRepository.SIZE));
}
