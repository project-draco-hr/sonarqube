{
  setupData("shared");
  task.start();
  List<Rule> rules=getSession().createQuery("from " + Rule.class.getSimpleName() + " where pluginName<>'fake'").getResultList();
  assertThat(rules.size()).isGreaterThan(0);
  for (  Rule rule : rules) {
    assertThat(rule.isEnabled()).isEqualTo(false);
    assertThat(rule.getUpdatedAt()).isNotNull();
  }
}
