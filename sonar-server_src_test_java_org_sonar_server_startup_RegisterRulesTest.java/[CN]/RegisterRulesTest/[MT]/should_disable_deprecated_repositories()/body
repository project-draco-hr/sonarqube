{
  setupData("shared");
  task.start();
  List<Rule> rules=getSession().createQuery("from " + Rule.class.getSimpleName() + " where pluginName<>'fake'").getResultList();
  assertThat(rules.size(),greaterThan(0));
  for (  Rule rule : rules) {
    assertThat(rule.isEnabled(),is(false));
    assertThat(rule.getUpdatedAt(),notNullValue());
  }
}
