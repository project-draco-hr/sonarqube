{
  List<Rule> rules=new ArrayList<Rule>();
  for (int i=0; i < SIZE; i++) {
    Rule rule=Rule.create("volume","rule" + i,"description of " + i);
    rule.setPriority(RulePriority.BLOCKER);
    for (int j=0; j < 20; j++) {
      rule.createParameter("param" + j);
    }
    rules.add(rule);
  }
  return rules;
}
