{
  when(jmxConnectionFactory.create(ProcessId.ELASTICSEARCH).getSystemState()).thenReturn(ImmutableSortedMap.<String,Object>of("foo","foo_val","bar","bar_val"));
  Optional<Map<String,Object>> attributes=underTest.attributes();
  assertThat(attributes.get()).containsExactly(MapEntry.entry("bar","bar_val"),MapEntry.entry("foo","foo_val"));
}
