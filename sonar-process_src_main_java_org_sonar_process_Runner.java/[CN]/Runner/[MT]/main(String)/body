{
  Runner process=new Runner(args[0],Integer.parseInt(args[1])){
    @Override public void execute(){
      while (true) {
        LOGGER.info("pseudo running for process {}",args[0]);
        try {
          Thread.sleep(1000);
        }
 catch (        InterruptedException e) {
          e.printStackTrace();
        }
      }
    }
  }
;
  Thread monitor=new Thread(process);
  monitor.start();
  process.execute();
  monitor.interrupt();
}
