{
  Project project=mock(Project.class);
  when(project.getAnalysisType()).thenReturn(Project.AnalysisType.DYNAMIC);
  assertThat(new CoberturaSensor(null,new CoberturaMavenPluginHandler()).getMavenPluginHandler(project),not(nullValue()));
  assertThat(new CoberturaSensor(null,new CoberturaMavenPluginHandler()).getMavenPluginHandler(project).getArtifactId(),is("cobertura-maven-plugin"));
}
