{
  List<String> licenses=new ArrayList<String>();
  if (getProject().getLicenses() != null) {
    for (    Object license : getProject().getLicenses()) {
      License l=(License)license;
      if (l.getName() != null) {
        licenses.add(l.getName());
      }
    }
  }
  return StringUtils.join(licenses," ");
}
