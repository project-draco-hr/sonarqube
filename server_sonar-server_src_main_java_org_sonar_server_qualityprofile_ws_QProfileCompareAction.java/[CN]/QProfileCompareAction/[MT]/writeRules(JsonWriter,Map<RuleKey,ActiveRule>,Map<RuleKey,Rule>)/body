{
  json.beginArray();
  for (  Entry<RuleKey,ActiveRule> activeRule : activeRules.entrySet()) {
    RuleKey key=activeRule.getKey();
    ActiveRule value=activeRule.getValue();
    json.beginObject();
    writeRule(json,key,rulesByKey);
    json.prop("severity",value.severity());
    json.endObject();
  }
  json.endArray();
}
