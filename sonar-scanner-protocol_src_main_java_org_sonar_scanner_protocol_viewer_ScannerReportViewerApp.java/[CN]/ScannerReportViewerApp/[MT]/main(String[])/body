{
  EventQueue.invokeLater(new Runnable(){
    @Override public void run(){
      try {
        ScannerReportViewerApp window=new ScannerReportViewerApp();
        window.frame.setVisible(true);
        window.loadReport();
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
);
}
