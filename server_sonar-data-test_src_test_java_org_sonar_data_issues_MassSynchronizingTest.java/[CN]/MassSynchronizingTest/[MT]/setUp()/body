{
  myIssueDao=new MyIssueDao();
  DbSession setupSession=db.openSession(false);
  generateRules(setupSession);
  generateProjects(setupSession);
  for (int i=0; i < ISSUE_COUNT; i++) {
    myIssueDao.insert(setupSession,getIssue(i));
    if (i % 100 == 0) {
      setupSession.commit();
    }
  }
  setupSession.commit();
  setupSession.close();
}
