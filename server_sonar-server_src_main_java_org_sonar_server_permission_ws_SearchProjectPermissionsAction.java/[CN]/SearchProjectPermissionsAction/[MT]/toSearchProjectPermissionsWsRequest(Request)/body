{
  return new SearchProjectPermissionsWsRequest().setProjectId(request.param(PARAM_PROJECT_ID)).setProjectKey(request.param(PARAM_PROJECT_KEY)).setQualifier(request.param(PARAM_QUALIFIER)).setPage(request.mandatoryParamAsInt(Param.PAGE)).setPageSize(request.mandatoryParamAsInt(Param.PAGE_SIZE)).setQuery(request.param(Param.TEXT_QUERY));
}
