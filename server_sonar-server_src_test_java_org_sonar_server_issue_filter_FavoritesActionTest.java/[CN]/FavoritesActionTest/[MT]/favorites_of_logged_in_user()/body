{
  MockUserSession session=MockUserSession.set().setLogin("eric").setUserId(123);
  when(service.findFavoriteFilters(session)).thenReturn(Arrays.asList(new DefaultIssueFilter().setId(13L).setName("Blocker issues").setData("severity=BLOCKER").setUser("simon").setShared(true)));
  tester.newGetRequest("api/issue_filters","favorites").execute().assertJson("{'favoriteFilters': [{'id': 13, 'name': 'Blocker issues', 'user': 'simon', 'shared': true}]}");
}
