{
  ReviewQuery query=new ReviewQuery();
  query.setIds(10L,11L).setReviewType("FALSE_POSITIVE").setStatuses("OPEN").setSeverities("MINOR","INFO").setProjectKeysOrIds("com.sonar.foo:bar").setResourceKeysOrIds("2","3").setAuthorLogins("foo").setAssigneeLogins("admin").setOutput("html");
  assertThat(query.getUrl(),is("/api/reviews?ids=10,11&statuses=OPEN&severities=MINOR,INFO&projects=com.sonar.foo%3Abar&resources=2,3&authors=foo&assignees=admin&output=html&review_type=FALSE_POSITIVE&"));
}
