{
  Notification notification=new Notification("review-changed").setFieldValue("author","olivier").setFieldValue("creator","simon").setFieldValue("old.assignee","godin").setFieldValue("assignee","freddy");
  dispatcher.performDispatch(notification,context);
  verify(context).addUser("simon");
  verify(context).addUser("godin");
  verify(context).addUser("freddy");
  verifyNoMoreInteractions(context);
}
