{
  for (  View view : views) {
    ViewProxy proxy=new ViewProxy(view);
    if (view instanceof Widget) {
      widgets.add(proxy);
      widgetsPerId.put(proxy.getId(),proxy);
    }
 else     if (view instanceof Page) {
      pagesPerId.put(proxy.getId(),proxy);
      pages.add(proxy);
    }
  }
}
