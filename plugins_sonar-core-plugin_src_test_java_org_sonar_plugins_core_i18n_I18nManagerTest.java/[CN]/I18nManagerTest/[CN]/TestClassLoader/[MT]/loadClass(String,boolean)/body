{
  Class c=findLoadedClass(name);
  if (c == null) {
    if (name.equals(ENGLISH_PACK_CLASS_NAME) || name.equals(QUEBEC_PACK_CLASS_NAME) || name.equals(FRENCH_PACK_CLASS_NAME)) {
      c=findClass(name);
    }
 else {
      return super.loadClass(name,resolve);
    }
  }
  if (resolve) {
    resolveClass(c);
  }
  return c;
}
