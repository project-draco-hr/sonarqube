{
  assertThat(manager.computeHtmlFilePath("org/sonar/i18n/test","rule.test.fakerule.description",Locale.FRENCH),is("org/sonar/i18n/test_fr/fakerule.html"));
  assertThat(manager.computeHtmlFilePath("org/sonar/i18n/test","rule.test.fakerule.description",Locale.ENGLISH),is("org/sonar/i18n/test/fakerule.html"));
}
