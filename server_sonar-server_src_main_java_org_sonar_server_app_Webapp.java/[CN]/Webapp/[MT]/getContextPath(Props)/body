{
  String context=props.value(PROPERTY_CONTEXT,"");
  if ("/".equals(context)) {
    context="";
  }
 else   if (!"".equals(context) && !context.startsWith("/")) {
    throw new IllegalStateException(String.format("Value of '%s' must start with a forward slash: '%s'",PROPERTY_CONTEXT,context));
  }
  return context;
}
