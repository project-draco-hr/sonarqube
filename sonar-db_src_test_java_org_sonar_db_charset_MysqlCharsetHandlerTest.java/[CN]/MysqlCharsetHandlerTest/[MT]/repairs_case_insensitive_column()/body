{
  answerColumnDef(asList(new ColumnDef(TABLE_ISSUES,COLUMN_KEE,"utf8","utf8_bin","varchar",10,false),new ColumnDef(TABLE_PROJECTS,COLUMN_NAME,"latin1","latin1_swedish_ci","varchar",10,false)));
  Connection connection=mock(Connection.class);
  underTest.handle(connection,false);
  verify(selectExecutor).executeUpdate(connection,"ALTER TABLE projects MODIFY name varchar(10) CHARACTER SET 'latin1' COLLATE 'latin1_bin' NOT NULL");
}
