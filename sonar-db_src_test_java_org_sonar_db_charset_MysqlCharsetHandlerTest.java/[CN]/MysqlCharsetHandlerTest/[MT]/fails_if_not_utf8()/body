{
  answerColumnDef(asList(new ColumnDef(TABLE_ISSUES,COLUMN_KEE,"utf8","utf8_bin","varchar",10,false),new ColumnDef(TABLE_PROJECTS,COLUMN_KEE,"latin1","latin1_german1_ci","varchar",10,false),new ColumnDef(TABLE_PROJECTS,COLUMN_NAME,"latin1","latin1_swedish_ci","varchar",20,false)));
  expectedException.expect(MessageException.class);
  expectedException.expectMessage("UTF8 case-sensitive collation is required for database columns [projects.kee, projects.name]");
  underTest.handle(mock(Connection.class),true);
}
