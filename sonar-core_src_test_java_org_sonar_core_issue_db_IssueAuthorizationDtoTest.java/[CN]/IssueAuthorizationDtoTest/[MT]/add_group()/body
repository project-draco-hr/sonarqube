{
  IssueAuthorizationDto dto=new IssueAuthorizationDto().setProject("Sample").setPermission("user").setGroups(newArrayList("sonar-users")).setUsers(newArrayList("john"));
  assertThat(dto.getGroups()).containsExactly("sonar-users");
  dto.addGroup("sonar-admins");
  assertThat(dto.getGroups()).containsExactly("sonar-users","sonar-admins");
}
