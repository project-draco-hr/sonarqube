{
  expectedException.expect(BadRequestException.class);
  expectedException.expectMessage("Project id or project key can be provided, not both.");
  insertComponent(newProjectDto("project-uuid").setKey("project-key"));
  ws.newPostRequest(ENDPOINT,ACTION).setParam(PARAM_PERMISSION,SYSTEM_ADMIN).setParam(PARAM_USER_LOGIN,"ray.bradbury").setParam(PARAM_PROJECT_UUID,"project-uuid").setParam(PARAM_PROJECT_KEY,"project-key").execute();
}
