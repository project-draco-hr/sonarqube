{
  Class<?> c=findLoadedClass(name);
  if (c == null) {
    try {
      if ((getParent() != null) && canLoadFromParent(name)) {
        c=getParent().loadClass(name);
      }
 else {
        c=getSystemClassLoader().loadClass(name);
      }
    }
 catch (    ClassNotFoundException e) {
      c=findClass(name);
    }
  }
  if (resolve) {
    resolveClass(c);
  }
  return c;
}
