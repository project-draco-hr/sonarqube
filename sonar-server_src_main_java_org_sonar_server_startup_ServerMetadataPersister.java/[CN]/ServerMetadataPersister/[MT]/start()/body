{
  LoggerFactory.getLogger(getClass()).debug("Persisting server metadata");
  persistentSettings.saveProperties(ImmutableMap.of(CoreProperties.SERVER_ID,server.getId(),CoreProperties.SERVER_VERSION,server.getVersion(),CoreProperties.SERVER_STARTTIME,new SimpleDateFormat("yyyy-MM-dd'T'HH:mm:ssZ").format(server.getStartedAt())));
}
