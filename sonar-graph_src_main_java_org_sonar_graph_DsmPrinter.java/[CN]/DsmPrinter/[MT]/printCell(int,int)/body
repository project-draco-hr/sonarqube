{
  DsmCell cell=dsm.cell(x,y);
  if (cell == null || cell.getWeight() == 0) {
    writer.append(" ");
  }
 else {
    writer.append("").append(String.valueOf(cell.getWeight()));
  }
  if (cell != null && cell.isFeedbackEdge()) {
    writer.append(FEEDBACK_EDGE_FLAG);
  }
 else {
    writer.append(' ');
  }
  writer.append(CELL_SEPARATOR);
}
