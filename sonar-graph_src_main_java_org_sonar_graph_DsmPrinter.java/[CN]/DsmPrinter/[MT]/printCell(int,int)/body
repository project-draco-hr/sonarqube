{
  if (dsm.getCell(x,y).getWeight() == 0) {
    writer.append(" ");
  }
 else {
    writer.append("").append(String.valueOf(dsm.getCell(x,y).getWeight()));
  }
  if (dsm.getCell(x,y).isFeedbackEdge()) {
    writer.append(FEEDBACK_EDGE_FLAG);
  }
 else {
    writer.append(' ');
  }
  writer.append(CELL_SEPARATOR);
}
