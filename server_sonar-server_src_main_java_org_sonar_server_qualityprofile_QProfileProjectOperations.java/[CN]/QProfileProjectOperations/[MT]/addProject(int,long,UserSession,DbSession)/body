{
  AuthorizedComponentDto project=db.componentDao().getAuthorizedComponentById(projectId,session);
  checkPermission(userSession,project.key());
  QualityProfileDto qualityProfile=findNotNull(profileId,session);
  db.propertiesDao().setProperty(new PropertyDto().setKey(QProfileProjectLookup.PROFILE_PROPERTY_PREFIX + qualityProfile.getLanguage()).setValue(qualityProfile.getName()).setResourceId(project.getId()),session);
  session.commit();
}
