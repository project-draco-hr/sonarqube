{
switch (LA(1)) {
case DECIMAL_LITERAL:
case HEX_LITERAL:
case OCTAL_LITERAL:
{
      integer();
      break;
    }
case STRING_LITERAL:
{
    string();
    break;
  }
case LITERAL_null:
{
  match(LITERAL_null);
  argStack.push(null);
  break;
}
case THIS:
{
match(THIS);
argStack.push(value);
break;
}
default :
{
throw new NoViableAltException(LT(1),getFilename());
}
}
}
