}
case LESSEQUALSIGN:
{
match(LESSEQUALSIGN);
argStack.push(new Integer(LESS_EQUAL));
break;
}
case NOTEQUALSIGN:
{
match(NOTEQUALSIGN);
argStack.push(new Integer(NOT_EQUAL));
break;
}
default :
{
throw new NoViableAltException(LT(1),getFilename());
}
}
