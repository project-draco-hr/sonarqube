{
  expr();
  join();
  expr();
  Boolean v1=(Boolean)argStack.pop();
  Integer join=(Integer)argStack.pop();
  Boolean v2=(Boolean)argStack.pop();
  if (join.intValue() == AND) {
    argStack.push(new Boolean(v1.booleanValue() && v2.booleanValue()));
  }
 else {
    argStack.push(new Boolean(v1.booleanValue() || v2.booleanValue()));
  }
}
