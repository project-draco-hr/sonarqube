{
  MockUserSession.set().addProjectUuidPermissions(UserRole.CODEVIEWER,TestFile1.PROJECT_UUID);
  dbClient.componentDao().insert(dbSession,TestFile1.newDto());
  dbSession.commit();
  es.putDocuments(TestIndexDefinition.INDEX,TestIndexDefinition.TYPE,new TestDoc().setUuid(TestFile1.UUID).setName(TestFile1.NAME).setFileUuid(TestFile1.FILE_UUID).setDurationInMs(TestFile1.DURATION_IN_MS).setStatus(TestFile1.STATUS).setMessage(TestFile1.MESSAGE).setCoveredFiles(TestFile1.COVERED_FILES).setStackTrace(TestFile1.STACKTRACE));
  WsTester.TestRequest request=ws.newGetRequest("api/tests","list").setParam(TestsListAction.TEST_UUID,TestFile1.UUID);
  request.execute().assertJson(getClass(),"list-test-uuid.json");
}
