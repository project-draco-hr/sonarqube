{
  userSessionRule.setGlobalPermissions(GlobalPermissions.SCAN_EXECUTION);
  RuleKey key=RuleKey.of("java","S001");
  dao.insert(dbSession,RuleTesting.newDto(key).setTags(Sets.newHashSet("security")).setSystemTags(Sets.newHashSet("java8","javadoc")));
  dbSession.commit();
  RuleUpdate update=RuleUpdate.createForPluginRule(key).setMarkdownNote("my *note*");
  service.update(update);
}
