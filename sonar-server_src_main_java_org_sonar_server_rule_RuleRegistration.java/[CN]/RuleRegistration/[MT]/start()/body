{
  SqlSession sqlSession=myBatis.openSession();
  try {
    Buffer buffer=new Buffer(system.now());
    selectRulesFromDb(buffer,sqlSession);
    enableRuleDefinitions(buffer,sqlSession);
    processRemainingDbRules(buffer,sqlSession);
    index(buffer);
  }
  finally {
    sqlSession.close();
  }
}
