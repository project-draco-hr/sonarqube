{
  for (  RuleDto ruleDto : ruleDao.selectNonManual(sqlSession)) {
    buffer.add(ruleDto);
    buffer.markUnprocessed(ruleDto);
  }
  for (  RuleParamDto paramDto : ruleDao.selectParameters(sqlSession)) {
    buffer.add(paramDto);
  }
  for (  RuleTagDto tagDto : ruleDao.selectTags(sqlSession)) {
    buffer.add(tagDto);
  }
}
