{
  TestPathAwareCrawler underTest=new TestPathAwareCrawler(PROJECT,PRE_ORDER);
  underTest.visit(SOME_TREE_ROOT);
  Iterator<CallRecord> expected=of(newCallRecord("visitAny",1,null,of(1)),newCallRecord("visitProject",1,null,of(1))).iterator();
