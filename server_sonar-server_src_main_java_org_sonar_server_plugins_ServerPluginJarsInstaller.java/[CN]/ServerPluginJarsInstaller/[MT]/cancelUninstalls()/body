{
  if (fs.getTrashPluginsDir().exists()) {
    for (    File file : listJarFiles(fs.getTrashPluginsDir())) {
      try {
        moveFileToDirectory(file,fs.getUserPluginsDir(),false);
      }
 catch (      IOException e) {
        throw new IllegalStateException("Fail to cancel plugin uninstalls",e);
      }
    }
  }
}
