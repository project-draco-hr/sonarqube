{
  checkArgument(measureComputer.getOutputMetrics().contains(metricKey),"Only metrics in %s can be used to add measures. Metric '%s' is not allowed.",measureComputer.getOutputMetrics(),metricKey);
  if (componentMeasureByMetricKey.get(metricKey) != null) {
    throw new UnsupportedOperationException(String.format("A measure on metric '%s' already exists",metricKey));
  }
}
