{
  if (rend < rpos) {
    throw new IllegalArgumentException("rend < rpos");
  }
  if (rend == rpos) {
    return (d.length);
  }
  if ((rend - rpos) == 1) {
    for (int x=dpos; x < d.length; x++) {
      if (r[rpos] == d[x]) {
        return (x);
      }
    }
  }
  while (((dpos + rend) - rpos) <= d.length) {
    int y=dpos;
    for (int x=rpos; x <= rend; x++) {
      if (x == rend) {
        return (dpos);
      }
      if (r[x] != d[y++]) {
        break;
      }
    }
    dpos++;
  }
  return (-1);
}
