{
  String user="user-measures-filter-with-sharing-perm";
  createUser(user,"User Measure Filters with sharing permission","shareDashboard");
  try {
    Selenese selenese=Selenese.builder().setHtmlTestsInClasspath("share_measure_filters","/measure/measure_filters/should-unshare-filter-remove-other-filters-favourite.html").build();
    orchestrator.executeSelenese(selenese);
  }
  finally {
    deactivateUser(user);
  }
}
