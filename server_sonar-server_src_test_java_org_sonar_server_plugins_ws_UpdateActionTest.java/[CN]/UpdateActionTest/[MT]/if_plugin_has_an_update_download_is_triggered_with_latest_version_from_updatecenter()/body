{
  Version version=Version.create("1.0");
  when(updateCenter.findPluginUpdates()).thenReturn(ImmutableList.of(PluginUpdate.createWithStatus(new Release(new Plugin(PLUGIN_KEY),version),Status.COMPATIBLE)));
  underTest.handle(validRequest,response);
  verify(pluginDownloader).download(PLUGIN_KEY,version);
  assertThat(response.outputAsString()).isEmpty();
}
