{
  IssueDto issue=newIssue();
  tester.get(IssueDao.class).insert(session,issue);
  session.commit();
  index();
  try {
    service.assign(issue.getKey(),"unknown");
    fail();
  }
 catch (  Exception e) {
    assertThat(e).isInstanceOf(NotFoundException.class).hasMessage("Unknown user: unknown");
  }
}
