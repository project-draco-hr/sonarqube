{
  MockUserSession.set().setLogin(connectedUser.getLogin()).setUserId(connectedUser.getId().intValue()).setGlobalPermissions(GlobalPermissions.SYSTEM_ADMIN).addProjectPermissions(UserRole.CODEVIEWER,project.key());
