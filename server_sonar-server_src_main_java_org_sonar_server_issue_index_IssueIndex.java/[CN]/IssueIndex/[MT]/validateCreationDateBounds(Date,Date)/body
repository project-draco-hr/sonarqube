{
  Preconditions.checkArgument(createdAfter == null || createdAfter.before(system.newDate()),"Start bound cannot be in the future");
  Preconditions.checkArgument(createdAfter == null || createdAfter.equals(createdBefore) || createdBefore == null || createdAfter.before(createdBefore),"Start bound cannot be larger than end bound");
}
