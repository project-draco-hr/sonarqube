{
  if (text != null && !text.matches("^.*$")) {
    JSONObject obj=new JSONObject(parseJSON(text));
    if (obj.isObject() != null) {
      if (obj.containsKey("err_code")) {
        handler.onError(new Double(obj.get("err_code").isNumber().doubleValue()).intValue(),obj.get("err_msg").isString().stringValue());
        return true;
      }
    }
  }
  return false;
}
