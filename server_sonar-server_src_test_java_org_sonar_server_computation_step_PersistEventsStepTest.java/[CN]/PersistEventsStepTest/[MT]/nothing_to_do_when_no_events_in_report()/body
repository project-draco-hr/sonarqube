{
  dbTester.prepareDbUnit(getClass(),"nothing_to_do_when_no_events_in_report.xml");
  treeRootHolder.setRoot(new DumbComponent(Component.Type.PROJECT,1,"ABCD",null));
  reportReader.setMetadata(BatchReport.Metadata.newBuilder().setRootComponentRef(1).setAnalysisDate(150000000L).build());
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(1).setType(Constants.ComponentType.PROJECT).build());
  step.execute();
  dbTester.assertDbUnit(getClass(),"nothing_to_do_when_no_events_in_report.xml","events");
}
