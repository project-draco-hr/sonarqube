{
  String duplicationXml=createXmlDuplications(component.getKey(),duplications);
  MeasureDto measureDto=new MeasureDto().setMetricId(duplicationMetric.getId()).setData(duplicationXml).setComponentId(dbIdsRepository.getComponentId(component)).setSnapshotId(batchComponent.getSnapshotId());
  dbClient.measureDao().insert(session,measureDto);
}
