{
  if (!(obj.getClass().equals(RuleMeasure.class))) {
    return false;
  }
  if (this == obj) {
    return true;
  }
  RuleMeasure other=(RuleMeasure)obj;
  return new EqualsBuilder().append(getMetric(),other.getMetric()).append(rule,other.rule).append(rulePriority,other.rulePriority).append(ruleCategory,other.ruleCategory).isEquals();
}
