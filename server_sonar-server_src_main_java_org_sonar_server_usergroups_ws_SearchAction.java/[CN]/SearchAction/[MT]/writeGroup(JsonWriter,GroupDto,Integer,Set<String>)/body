{
  json.beginObject().prop(FIELD_ID,group.getId().toString()).prop(FIELD_NAME,fields.contains(FIELD_NAME) ? group.getName() : null).prop(FIELD_DESCRIPTION,fields.contains(FIELD_DESCRIPTION) ? group.getDescription() : null).prop(FIELD_MEMBERS_COUNT,fields.contains(FIELD_MEMBERS_COUNT) ? memberCount : null).endObject();
}
