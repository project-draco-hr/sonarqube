{
  GetRequestBuilder request=getClient().prepareGet().setType(this.getIndexType()).setIndex(this.getIndexName()).setId(this.getKeyValue(key)).setRouting(this.getKeyValue(key));
  GetResponse response=node.execute(request);
  if (response.isExists()) {
    return toDoc(response.getSource());
  }
  return null;
}
