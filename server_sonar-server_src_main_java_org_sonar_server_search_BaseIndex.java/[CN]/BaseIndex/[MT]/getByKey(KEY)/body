{
  GetRequestBuilder request=client.prepareGet().setType(this.getIndexType()).setIndex(this.getIndexName()).setId(this.getKeyValue(key)).setFetchSource(true).setRouting(this.getKeyValue(key));
  GetResponse response=client.execute(request);
  if (response.isExists()) {
    return toDoc(response.getSource());
  }
  return null;
}
