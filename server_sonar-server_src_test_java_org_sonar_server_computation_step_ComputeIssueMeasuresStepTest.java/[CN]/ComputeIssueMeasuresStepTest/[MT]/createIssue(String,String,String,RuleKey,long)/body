{
  BatchReport.Issue.Builder issueBuilder=Issue.newBuilder().setUuid(Uuids.create()).setStatus(status).setRuleKey(ruleKey.rule()).setRuleRepository(ruleKey.repository()).setSeverity(Constants.Severity.valueOf(severity)).setCreationDate(creationDate);
  if (resolution != null) {
    issueBuilder.setResolution(resolution);
  }
  return issueBuilder.build();
}
