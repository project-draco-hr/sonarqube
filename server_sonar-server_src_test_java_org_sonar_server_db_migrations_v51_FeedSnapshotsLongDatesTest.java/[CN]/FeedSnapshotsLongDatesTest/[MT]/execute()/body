{
  db.prepareDbUnit(getClass(),"before.xml");
  DatabaseMigration migration=new FeedSnapshotsLongDates(db.database(),System2.INSTANCE);
  migration.execute();
  int count=db.countSql("select count(*) from snapshots where created_at_ms is not null " + "and build_date_ms is not null " + "and period1_date_ms is not null "+ "and period2_date_ms is not null "+ "and period3_date_ms is not null "+ "and period4_date_ms is not null "+ "and period5_date_ms is not null");
  assertThat(count).isEqualTo(2);
}
