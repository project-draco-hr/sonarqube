{
  SyntaxHighlighter highlighter=new SyntaxHighlighter(getMyBatis());
  String highlightedSource=highlighter.getHighlightedSourceAsHtml(11L);
  assertThat(highlightedSource).isEqualTo("<tr><td><span class=\"cppd\">/*</span></td></tr>" + LF_END_OF_LINE + "<tr><td><span class=\"cppd\"> * Header</span></td></tr>"+ LF_END_OF_LINE+ "<tr><td><span class=\"cppd\"> */</span></td></tr>"+ LF_END_OF_LINE+ "<tr><td></td></tr>"+ LF_END_OF_LINE+ "<tr><td><span class=\"k\">public </span><span class=\"k\">class </span>HelloWorld {</td></tr>"+ LF_END_OF_LINE+ "<tr><td>}</td></tr>");
}
