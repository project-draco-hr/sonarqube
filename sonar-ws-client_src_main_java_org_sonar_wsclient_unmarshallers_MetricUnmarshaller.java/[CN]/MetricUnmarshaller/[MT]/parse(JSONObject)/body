{
  return new Metric().setKey(JsonUtils.getString(json,"key")).setName(JsonUtils.getString(json,"name")).setDomain(JsonUtils.getString(json,"domain")).setDescription(JsonUtils.getString(json,"description")).setDirection(JsonUtils.getInteger(json,"direction")).setType(JsonUtils.getString(json,"val_type")).setUserManaged(JsonUtils.getBoolean(json,"user_managed")).setHidden(JsonUtils.getBoolean(json,"hidden"));
}
