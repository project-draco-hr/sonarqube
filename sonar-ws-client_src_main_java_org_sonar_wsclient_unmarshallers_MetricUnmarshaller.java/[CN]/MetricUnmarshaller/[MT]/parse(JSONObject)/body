{
  return new Metric().setKey((String)json.get("key")).setName((String)json.get("name")).setDomain((String)json.get("domain")).setDescription((String)json.get("description")).setDirection(JsonUtils.getInteger(json,"direction")).setType(JsonUtils.getString(json,"val_type")).setUserManaged(JsonUtils.getBoolean(json,"user_managed")).setHidden(JsonUtils.getBoolean(json,"hidden"));
}
