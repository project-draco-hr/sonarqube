{
  when(batchExecutorService.scheduleAtFixedRate(any(Runnable.class),eq(0L),eq(1L),eq(TimeUnit.SECONDS))).thenAnswer(new ExecuteFirstArgAsRunnable());
  underTest.schedule();
  assertThat(processingQueue.getTasks()).hasSize(1);
  assertThat(processingQueue.getTasks().iterator().next()).isInstanceOf(CeWorker.class);
}
