{
  if (index == 0) {
    _cur=new Listable(null,_head.next(),null);
    _nextIndex=0;
  }
 else   if (index == _size) {
    _cur=new Listable(_head.prev(),null,null);
    _nextIndex=_size;
  }
 else {
    Listable temp=getListableAt(index);
    _cur=new Listable(temp.prev(),temp,null);
    _nextIndex=index;
  }
}
