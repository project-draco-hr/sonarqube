{
  for (Listable elt=_head.next(), past=null; null != elt && past != _head.prev(); elt=(past=elt).next()) {
    if (null == o && null == elt.value()) {
      removeListable(elt);
      return true;
    }
 else     if (o != null && o.equals(elt.value())) {
      removeListable(elt);
      return true;
    }
  }
  return false;
}
