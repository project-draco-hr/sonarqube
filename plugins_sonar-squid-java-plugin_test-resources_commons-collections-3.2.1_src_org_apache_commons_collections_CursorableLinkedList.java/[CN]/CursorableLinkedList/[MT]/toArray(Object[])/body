{
  if (a.length < _size) {
    a=(Object[])Array.newInstance(a.getClass().getComponentType(),_size);
  }
  int i=0;
  for (Listable elt=_head.next(), past=null; null != elt && past != _head.prev(); elt=(past=elt).next()) {
    a[i++]=elt.value();
  }
  if (a.length > _size) {
    a[_size]=null;
  }
  return a;
}
