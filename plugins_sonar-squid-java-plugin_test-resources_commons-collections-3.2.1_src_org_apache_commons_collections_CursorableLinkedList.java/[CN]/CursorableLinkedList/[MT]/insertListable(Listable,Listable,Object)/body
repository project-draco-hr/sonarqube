{
  _modCount++;
  _size++;
  Listable elt=new Listable(before,after,value);
  if (null != before) {
    before.setNext(elt);
  }
 else {
    _head.setNext(elt);
  }
  if (null != after) {
    after.setPrev(elt);
  }
 else {
    _head.setPrev(elt);
  }
  broadcastListableInserted(elt);
  return elt;
}
