{
  StringBuffer buf=new StringBuffer();
  buf.append("[");
  for (Listable elt=_head.next(), past=null; null != elt && past != _head.prev(); elt=(past=elt).next()) {
    if (_head.next() != elt) {
      buf.append(", ");
    }
    buf.append(elt.value());
  }
  buf.append("]");
  return buf.toString();
}
