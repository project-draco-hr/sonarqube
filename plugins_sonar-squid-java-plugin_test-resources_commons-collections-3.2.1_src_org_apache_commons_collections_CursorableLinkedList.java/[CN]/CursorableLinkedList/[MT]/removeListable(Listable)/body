{
  _modCount++;
  _size--;
  if (_head.next() == elt) {
    _head.setNext(elt.next());
  }
  if (null != elt.next()) {
    elt.next().setPrev(elt.prev());
  }
  if (_head.prev() == elt) {
    _head.setPrev(elt.prev());
  }
  if (null != elt.prev()) {
    elt.prev().setNext(elt.next());
  }
  broadcastListableRemoved(elt);
}
