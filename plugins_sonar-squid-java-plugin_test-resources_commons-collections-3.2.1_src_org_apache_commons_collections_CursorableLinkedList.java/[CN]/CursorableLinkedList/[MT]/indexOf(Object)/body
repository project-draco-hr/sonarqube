{
  int ndx=0;
  if (null == o) {
    for (Listable elt=_head.next(), past=null; null != elt && past != _head.prev(); elt=(past=elt).next()) {
      if (null == elt.value()) {
        return ndx;
      }
      ndx++;
    }
  }
 else {
    for (Listable elt=_head.next(), past=null; null != elt && past != _head.prev(); elt=(past=elt).next()) {
      if (o.equals(elt.value())) {
        return ndx;
      }
      ndx++;
    }
  }
  return -1;
}
