{
  if (c.isEmpty()) {
    return false;
  }
 else   if (_size == index || _size == 0) {
    return addAll(c);
  }
 else {
    Listable succ=getListableAt(index);
    Listable pred=(null == succ) ? null : succ.prev();
    Iterator it=c.iterator();
    while (it.hasNext()) {
      pred=insertListable(pred,succ,it.next());
    }
    return true;
  }
}
