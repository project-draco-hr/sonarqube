{
  int ndx=_size - 1;
  if (null == o) {
    for (Listable elt=_head.prev(), past=null; null != elt && past != _head.next(); elt=(past=elt).prev()) {
      if (null == elt.value()) {
        return ndx;
      }
      ndx--;
    }
  }
 else {
    for (Listable elt=_head.prev(), past=null; null != elt && past != _head.next(); elt=(past=elt).prev()) {
      if (o.equals(elt.value())) {
        return ndx;
      }
      ndx--;
    }
  }
  return -1;
}
