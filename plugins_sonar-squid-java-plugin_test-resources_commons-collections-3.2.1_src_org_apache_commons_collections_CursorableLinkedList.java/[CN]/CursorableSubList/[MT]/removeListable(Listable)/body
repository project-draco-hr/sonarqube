{
  _modCount++;
  _size--;
  if (_head.next() == elt && _head.prev() == elt) {
    _head.setNext(null);
    _head.setPrev(null);
  }
  if (_head.next() == elt) {
    _head.setNext(elt.next());
  }
  if (_head.prev() == elt) {
    _head.setPrev(elt.prev());
  }
  _list.removeListable(elt);
  broadcastListableRemoved(elt);
}
