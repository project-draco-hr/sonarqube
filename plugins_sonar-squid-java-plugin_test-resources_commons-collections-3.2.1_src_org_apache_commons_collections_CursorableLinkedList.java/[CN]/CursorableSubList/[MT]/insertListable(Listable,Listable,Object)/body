{
  _modCount++;
  _size++;
  Listable elt=_list.insertListable((null == before ? _pre : before),(null == after ? _post : after),value);
  if (null == _head.next()) {
    _head.setNext(elt);
    _head.setPrev(elt);
  }
  if (before == _head.prev()) {
    _head.setPrev(elt);
  }
  if (after == _head.next()) {
    _head.setNext(elt);
  }
  broadcastListableInserted(elt);
  return elt;
}
