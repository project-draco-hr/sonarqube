{
  this.treeRootHolder=treeRootHolder;
  this.periodsHolder=periodsHolder;
  this.batchReportReader=batchReportReader;
  this.measureRepository=measureRepository;
  this.newCoverageMetricsList=from(asList(newCoverageMetricsList)).transform(new Function<NewCoverageMetricKeys,NewCoverageMetrics>(){
    @Nullable @Override public NewCoverageMetrics apply(    @Nullable NewCoverageMetricKeys input){
      return new NewCoverageMetrics(metricRepository,input);
    }
  }
).toList();
}
