{
  return ImmutableList.<Formula<?>>of(new VariationSumFormula(outputMetricKeys.getNewLinesToCover(),viewsRestrictedPeriods()),new VariationSumFormula(outputMetricKeys.getNewUncoveredLines(),viewsRestrictedPeriods()),new VariationSumFormula(outputMetricKeys.getNewConditionsToCover(),viewsRestrictedPeriods()),new VariationSumFormula(outputMetricKeys.getNewUncoveredConditions(),viewsRestrictedPeriods()));
}
