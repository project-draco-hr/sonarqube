{
  FormulaExecutorComponentVisitor.newBuilder(metricRepository,measureRepository).withVariationSupport(periodsHolder).buildFor(ImmutableList.<Formula>of(new NewCoverageOnFileFormula(batchReportReader),new NewCoverageOnITFileFormula(batchReportReader),new NewOverallCoverageFormula(batchReportReader))).visit(treeRootHolder.getRoot());
}
