{
  List<Rule> rules=repository.createRules();
  assertThat(rules).onProperty("key").containsOnly("JUnitStaticSuite","JUnitSpelling","JUnitAssertionsShouldIncludeMessage","JUnitTestsShouldIncludeAssert","TestClassWithoutTestCases","UnnecessaryBooleanAssertion","UseAssertEqualsInsteadOfAssertTrue","UseAssertSameInsteadOfAssertTrue","UseAssertNullInsteadOfAssertTrue","SimplifyBooleanAssertion");
}
