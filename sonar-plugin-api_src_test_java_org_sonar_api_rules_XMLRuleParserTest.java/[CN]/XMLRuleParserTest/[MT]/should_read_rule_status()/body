{
  List<Rule> rules=new XMLRuleParser().parse(new StringReader("<rules>" + "<rule><key>foo</key><status>READY</status></rule>" + "<rule><key>foo</key><status>BETA</status></rule>"+ "<rule><key>foo</key><status>DEPRECATED</status></rule>"+ "</rules>"));
  assertThat(rules.get(0).getStatus(),is("READY"));
  assertThat(rules.get(1).getStatus(),is("BETA"));
  assertThat(rules.get(2).getStatus(),is("DEPRECATED"));
}
