{
  reset(ruleDao);
  underTest.getByKey(AB_RULE.getKey());
  assertNoCallToDb();
  reset(ruleDao);
  underTest.findByKey(AB_RULE.getKey());
  assertNoCallToDb();
  reset(ruleDao);
  underTest.getById(AB_RULE.getId());
  assertNoCallToDb();
  reset(ruleDao);
  underTest.findById(AB_RULE.getId());
  assertNoCallToDb();
}
