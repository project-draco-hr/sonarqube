{
  return (DefaultIssue)new DefaultIssueBuilder().createdDate(creationDate).componentKey(violation.getResource().getEffectiveKey()).ruleKey(RuleKey.of(violation.getRule().getRepositoryKey(),violation.getRule().getKey())).effortToFix(violation.getCost()).line(violation.getLineId()).description(violation.getMessage()).severity(violation.getSeverity() != null ? violation.getSeverity().name() : Severity.MAJOR).build();
}
