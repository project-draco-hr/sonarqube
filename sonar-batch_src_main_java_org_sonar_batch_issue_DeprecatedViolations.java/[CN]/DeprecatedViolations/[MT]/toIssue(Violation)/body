{
  DefaultIssue issue=new DefaultIssue().setComponentKey(violation.getResource().getEffectiveKey()).setKey(UUID.randomUUID().toString()).setRuleRepositoryKey(violation.getRule().getRepositoryKey()).setRuleKey(violation.getRule().getKey()).setCost(violation.getCost()).setLine(violation.getLineId()).setMessage(violation.getMessage()).setStatus(Issue.STATUS_OPEN).setSeverity(violation.getSeverity() != null ? violation.getSeverity().name() : null);
  return issue;
}
