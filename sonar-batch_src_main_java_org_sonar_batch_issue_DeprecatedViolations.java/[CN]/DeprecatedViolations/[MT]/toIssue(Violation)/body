{
  DefaultIssue issue=new DefaultIssue().setComponentKey(violation.getResource().getEffectiveKey()).setKey(UUID.randomUUID().toString()).setRuleKey(RuleKey.of(violation.getRule().getRepositoryKey(),violation.getRule().getKey())).setCost(violation.getCost()).setLine(violation.getLineId()).setDescription(violation.getMessage()).setResolution(Issue.RESOLUTION_OPEN).setStatus(Issue.STATUS_OPEN).setManualSeverity(false).setManual(false).setSeverity(violation.getSeverity() != null ? violation.getSeverity().name() : null);
  return issue;
}
