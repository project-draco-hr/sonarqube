{
  return (DefaultIssue)new DefaultIssueBuilder(violation.getResource().getEffectiveKey()).ruleKey(RuleKey.of(violation.getRule().getRepositoryKey(),violation.getRule().getKey())).cost(violation.getCost()).line(violation.getLineId()).description(violation.getMessage()).severity(violation.getSeverity() != null ? violation.getSeverity().name() : null).build();
}
