{
  List<String> ids=new ArrayList<String>();
  for (  Artifact dep : getIncludedArtifacts()) {
    if (ArrayUtils.contains(LOG_GROUP_IDS,dep.getGroupId())) {
      ids.add(dep.getDependencyConflictId());
    }
  }
  if (!ids.isEmpty()) {
    StringBuilder message=new StringBuilder();
    message.append("Dependencies on the following log libraries should be excluded or declared with scope 'provided':").append("\n\t").append(StringUtils.join(ids,", ")).append('\n');
    getLog().warn(message.toString());
  }
}
