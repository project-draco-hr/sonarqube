{
  dbTester.prepareDbUnit(getClass(),"insert_new_issue.xml");
  issueCache.newAppender().append(new DefaultIssue().setKey("ISSUE").setRuleKey(RuleKey.of("xoo","S01")).setComponentUuid("COMPONENT").setProjectUuid("PROJECT").setSeverity(Severity.BLOCKER).setStatus(Issue.STATUS_OPEN).setNew(true)).close();
  step.execute();
  dbTester.assertDbUnit(getClass(),"insert_new_issue-result.xml",new String[]{"id"},"issues");
}
