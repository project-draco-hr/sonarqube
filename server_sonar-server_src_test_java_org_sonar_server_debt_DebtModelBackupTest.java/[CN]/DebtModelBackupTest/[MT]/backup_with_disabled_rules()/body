{
  when(ruleDao.selectEnabledAndNonManual(session)).thenReturn(newArrayList(new RuleDto().setRepositoryKey("squid").setRuleKey("UselessImportCheck"),new RuleDto().setRepositoryKey("squid").setRuleKey("AvoidNPE")));
  underTest.backup();
  verify(debtModelXMLExporter).export(ruleDebtListCaptor.capture());
  assertThat(ruleDebtListCaptor.getValue()).isEmpty();
}
