{
  DebtModel debtModel=new DebtModel().addRootCharacteristic(new DefaultDebtCharacteristic().setId(1).setKey("USABILITY").setName("Usability").setOrder(1)).addRootCharacteristic(new DefaultDebtCharacteristic().setId(2).setKey("EFFICIENCY").setName("Efficiency").setOrder(2)).addSubCharacteristic(new DefaultDebtCharacteristic().setId(3).setKey("MEMORY_USE").setName("Memory use").setParentId(2),"EFFICIENCY");
  List<RuleDebt> rules=newArrayList(new RuleDebt().setRuleKey(RuleKey.of("checkstyle","Regexp")).setCharacteristicKey("MEMORY_USE").setFunction(DebtRemediationFunction.Type.LINEAR_OFFSET).setFactor("3d").setOffset("15min"));
  TestUtils.assertSimilarXml(getFileContent("export_xml.xml"),xmlExporter.export(debtModel,rules));
}
