{
  when(fileCache.byModule("struts")).thenReturn(Lists.<InputFile>newArrayList(DefaultInputFile.create(temp.newFile(),Charsets.UTF_8,"src/Foo.java",ImmutableMap.of(DefaultInputFile.ATTRIBUTE_HASH,"ABC")),DefaultInputFile.create(temp.newFile(),Charsets.UTF_8,"src/Bar.java",ImmutableMap.of(DefaultInputFile.ATTRIBUTE_HASH,"DEF"))));
  SensorContext sensorContext=mock(SensorContext.class);
  sensor.analyse(project,sensorContext);
  verify(componentDataCache).setStringData("struts",SnapshotDataTypes.FILE_HASHES,"src/Foo.java=ABC;src/Bar.java=DEF");
  verifyZeroInteractions(sensorContext);
}
