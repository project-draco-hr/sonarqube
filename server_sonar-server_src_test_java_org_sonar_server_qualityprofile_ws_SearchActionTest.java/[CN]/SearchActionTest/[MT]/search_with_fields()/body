{
  qualityProfileDao.insert(dbSession,QualityProfileDto.createFor("sonar-way-xoo1-12345").setLanguage(xoo1.getKey()).setName("Sonar way").setDefault(true),QualityProfileDto.createFor("sonar-way-xoo2-23456").setLanguage(xoo2.getKey()).setName("Sonar way"),QualityProfileDto.createFor("my-sonar-way-xoo2-34567").setLanguage(xoo2.getKey()).setName("My Sonar way").setParentKee("sonar-way-xoo2-23456"));
  commit();
  String result=ws.newRequest().setParam(Param.FIELDS,"key,language").execute().getInput();
  assertJson(result).isSimilarTo(getClass().getResource("SearchActionTest/search_fields.json"));
}
