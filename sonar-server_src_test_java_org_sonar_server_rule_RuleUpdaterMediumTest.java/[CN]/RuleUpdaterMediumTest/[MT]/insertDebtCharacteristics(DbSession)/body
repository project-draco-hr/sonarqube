{
  CharacteristicDto reliability=new CharacteristicDto().setKey("RELIABILITY").setName("Reliability").setEnabled(true);
  db.debtCharacteristicDao().insert(reliability,dbSession);
  reliabilityId=reliability.getId();
  CharacteristicDto softReliability=new CharacteristicDto().setKey("SOFT_RELIABILITY").setName("Software reliability").setParentId(reliability.getId()).setEnabled(true);
  db.debtCharacteristicDao().insert(softReliability,dbSession);
  softReliabilityId=softReliability.getId();
  CharacteristicDto hardReliability=new CharacteristicDto().setKey("HARD_RELIABILITY").setName("Hardware reliability").setParentId(reliability.getId()).setEnabled(true);
  db.debtCharacteristicDao().insert(hardReliability,dbSession);
  hardReliabilityId=hardReliability.getId();
}
