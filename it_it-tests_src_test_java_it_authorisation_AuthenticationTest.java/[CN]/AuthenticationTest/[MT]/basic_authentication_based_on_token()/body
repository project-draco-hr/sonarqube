{
  WsUserTokens.GenerateWsResponse generateWsResponse=userTokensWsClient.generate(new GenerateWsRequest().setLogin(LOGIN).setName("Validate token based authentication"));
  WsClient wsClient=new HttpWsClient(new HttpConnector.Builder().url(ORCHESTRATOR.getServer().getUrl()).token(generateWsResponse.getToken()).build());
  WsResponse response=wsClient.wsConnector().call(new GetRequest("api/authentication/validate"));
  assertThat(response.content()).isEqualTo("{\"valid\":true}");
}
