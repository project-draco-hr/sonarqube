{
  WsUserTokens.GenerateWsResponse generateWsResponse=userTokensWsClient.generate(new GenerateWsRequest().setLogin(LOGIN).setName("Validate token based authentication"));
  WsClient wsClient=new WsClient(newHttpConnector().url(ORCHESTRATOR.getServer().getUrl()).login(generateWsResponse.getToken()).password("").build());
  String response=wsClient.execute(newGetRequest("api/authentication/validate"));
  assertThat(response).isEqualTo("{\"valid\":true}");
}
