{
  DefaultCharacteristic requirement=new DefaultCharacteristic().setId(1).setRuleKey(RuleKey.of("repo","rule")).setFunction("linear_offset").setFactor(new WorkUnit.Builder().setMinutes(2).build()).setOffset(new WorkUnit.Builder().setHours(1).build()).setRootId(3).setParentId(2);
  assertThat(requirement.isRequirement()).isTrue();
}
