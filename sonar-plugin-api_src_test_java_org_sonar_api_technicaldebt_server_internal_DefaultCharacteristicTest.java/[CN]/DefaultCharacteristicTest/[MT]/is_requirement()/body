{
  DefaultCharacteristic requirement=new DefaultCharacteristic().setId(1).setRuleKey(RuleKey.of("repo","rule")).setFunction("linear_offset").setFactor(WorkUnit.create(2d,WorkUnit.MINUTES)).setOffset(WorkUnit.create(1d,WorkUnit.HOURS)).setRootId(3).setParentId(2);
  assertThat(requirement.isRequirement()).isTrue();
}
