{
  DefaultCharacteristic requirement=new DefaultCharacteristic().setId(1).setRuleKey(RuleKey.of("repo","rule")).setFunction("linear_offset").setFactorValue(2).setFactorUnit(WorkDuration.UNIT.MINUTES).setOffsetValue(1).setOffsetUnit(WorkDuration.UNIT.HOURS).setRootId(3).setParentId(2);
  assertThat(requirement.isRequirement()).isTrue();
}
