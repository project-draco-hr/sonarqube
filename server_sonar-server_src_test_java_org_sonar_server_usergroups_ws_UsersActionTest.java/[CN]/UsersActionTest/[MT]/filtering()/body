{
  GroupDto group=createGroup();
  UserDto groupUser=createUser("ada","Ada Lovelace");
  createUser("grace","Grace Hopper");
  addUserToGroup(groupUser,group);
  session.commit();
  tester.newGetRequest("api/usergroups","users").setParam("id",group.getId().toString()).setParam("q","ace").execute().assertJson(getClass(),"all.json");
  tester.newGetRequest("api/usergroups","users").setParam("id",group.getId().toString()).setParam("q","love").execute().assertJson(getClass(),"all_ada.json");
}
