{
  GroupDto group=insertGroup();
  UserDto groupUser=insertUser("ada","Ada Lovelace");
  insertUser("grace","Grace Hopper");
  addUserToGroup(groupUser,group);
  session.commit();
  newUsersRequest().setParam("id",group.getId().toString()).setParam("selected","selected").execute().assertJson(getClass(),"selected.json");
}
