{
  GroupDto group=insertGroup();
  UserDto groupUser=insertUser("ada","Ada Lovelace");
  insertUser("grace","Grace Hopper");
  addUserToGroup(groupUser,group);
  dbSession.commit();
  newUsersRequest().setParam("id",group.getId().toString()).setParam("ps","1").setParam(Param.SELECTED,SelectionMode.ALL.value()).execute().assertJson(getClass(),"all_page1.json");
  newUsersRequest().setParam("id",group.getId().toString()).setParam("ps","1").setParam("p","2").setParam(Param.SELECTED,SelectionMode.ALL.value()).execute().assertJson(getClass(),"all_page2.json");
}
