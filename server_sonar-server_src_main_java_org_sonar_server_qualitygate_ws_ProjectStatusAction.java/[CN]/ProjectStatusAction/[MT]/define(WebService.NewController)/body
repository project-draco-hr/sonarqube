{
  WebService.NewAction action=controller.createAction("project_status").setDescription("Quality gate status for a given Compute Engine task. <br />" + "Returns a http code 404 if the analysis associated with the task is not found or does not exist.<br />" + "Requires 'Administer System' or 'Execute Analysis' permission.").setResponseExample(getClass().getResource("project_status-example.json")).setSince("5.3").setHandler(this);
  action.createParam("analysisId").setDescription("Analysis id").setRequired(true).setExampleValue("2963");
}
