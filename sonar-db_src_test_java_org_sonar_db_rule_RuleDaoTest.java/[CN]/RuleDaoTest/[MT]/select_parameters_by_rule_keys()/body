{
  dbTester.prepareDbUnit(getClass(),"select_parameters_by_rule_key.xml");
  assertThat(underTest.selectRuleParamsByRuleKeys(dbTester.getSession(),Arrays.asList(RuleKey.of("checkstyle","AvoidNull"),RuleKey.of("unused","Unused")))).hasSize(2);
  assertThat(underTest.selectRuleParamsByRuleKeys(dbTester.getSession(),singletonList(RuleKey.of("unknown","Unknown")))).isEmpty();
}
