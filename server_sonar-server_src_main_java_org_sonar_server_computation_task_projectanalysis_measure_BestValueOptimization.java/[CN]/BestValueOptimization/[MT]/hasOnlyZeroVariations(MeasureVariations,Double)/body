{
  return (!variations.hasVariation1() || isVariationEmptyOrBestValue(variations.getVariation1(),bestValue)) && (!variations.hasVariation2() || isVariationEmptyOrBestValue(variations.getVariation2(),bestValue)) && (!variations.hasVariation3() || isVariationEmptyOrBestValue(variations.getVariation3(),bestValue))&& (!variations.hasVariation4() || isVariationEmptyOrBestValue(variations.getVariation4(),bestValue))&& (!variations.hasVariation5() || isVariationEmptyOrBestValue(variations.getVariation5(),bestValue));
}
