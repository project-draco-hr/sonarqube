{
  LOGGER.debug("Monitoring thread is terminating");
  if (!monitorExecutionService.isShutdown()) {
    monitorExecutionService.shutdownNow();
  }
  if (!watch.isCancelled()) {
    watch.cancel(true);
  }
  for (int i=processes.size() - 1; i >= 0; i--) {
    processes.get(i).terminate();
  }
  processes.clear();
  interruptAndWait();
}
