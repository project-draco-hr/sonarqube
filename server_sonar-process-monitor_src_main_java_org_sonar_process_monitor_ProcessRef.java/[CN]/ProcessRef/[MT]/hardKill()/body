{
  boolean killed=false;
  terminated=true;
  pingEnabled=false;
  if (ProcessUtils.isAlive(process)) {
    ProcessUtils.destroyQuietly(process);
    killed=true;
  }
  for (  StreamGobbler gobbler : gobblers) {
    StreamGobbler.waitUntilFinish(gobbler);
  }
  ProcessUtils.closeStreams(process);
  return killed;
}
