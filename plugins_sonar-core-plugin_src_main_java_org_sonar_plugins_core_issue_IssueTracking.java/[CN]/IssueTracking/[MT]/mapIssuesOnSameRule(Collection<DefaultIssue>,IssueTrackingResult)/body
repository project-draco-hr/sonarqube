{
  for (  DefaultIssue newIssue : newIssues) {
    if (isNotAlreadyMapped(newIssue,result)) {
      mapIssue(newIssue,findLastIssueWithSameChecksumAndMessage(newIssue,result.unmatchedForRule(newIssue.ruleKey())),result);
    }
  }
  for (  DefaultIssue newIssue : newIssues) {
    if (isNotAlreadyMapped(newIssue,result)) {
      mapIssue(newIssue,findLastIssueWithSameLineAndMessage(newIssue,result.unmatchedForRule(newIssue.ruleKey())),result);
    }
  }
  for (  DefaultIssue newIssue : newIssues) {
    if (isNotAlreadyMapped(newIssue,result)) {
      mapIssue(newIssue,findLastIssueWithSameChecksum(newIssue,result.unmatchedForRule(newIssue.ruleKey())),result);
    }
  }
}
