{
  for (  DefaultIssue newIssue : newIssues) {
    if (isNotAlreadyMapped(newIssue,result)) {
      mapIssue(newIssue,findLastIssueWithSameChecksumAndMessage(newIssue,result.unmatchedByKeyForRule(newIssue.ruleKey()).values()),result);
    }
  }
  for (  DefaultIssue newIssue : newIssues) {
    if (isNotAlreadyMapped(newIssue,result)) {
      mapIssue(newIssue,findLastIssueWithSameLineAndMessage(newIssue,result.unmatchedByKeyForRule(newIssue.ruleKey()).values()),result);
    }
  }
  for (  DefaultIssue newIssue : newIssues) {
    if (isNotAlreadyMapped(newIssue,result)) {
      mapIssue(newIssue,findLastIssueWithSameChecksum(newIssue,result.unmatchedByKeyForRule(newIssue.ruleKey()).values()),result);
    }
  }
}
