{
  orchestrator.executeBuild(SonarRunner.create(projectDir("shared/xoo-sample")));
  Selenese selenese=Selenese.builder().setHtmlTestsInClasspath("ui-i18n","/i18n/default-locale-is-english.html","/i18n/french-locale.html","/i18n/french-pack.html","/i18n/locale-with-france-country.html","/i18n/locale-with-swiss-country.html").build();
  new SeleneseTest(selenese).runOn(orchestrator);
}
