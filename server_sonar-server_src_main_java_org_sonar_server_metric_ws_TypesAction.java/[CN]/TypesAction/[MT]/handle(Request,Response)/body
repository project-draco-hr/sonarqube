{
  JsonWriter json=response.newJsonWriter();
  json.beginObject();
  json.name("types");
  json.beginArray();
  for (  Metric.ValueType metricType : Metric.ValueType.values()) {
    json.beginObject();
    json.prop("key",metricType.name());
    json.prop("name",metricType.description());
    json.endObject();
  }
  json.endArray();
  json.endObject();
  json.close();
}
