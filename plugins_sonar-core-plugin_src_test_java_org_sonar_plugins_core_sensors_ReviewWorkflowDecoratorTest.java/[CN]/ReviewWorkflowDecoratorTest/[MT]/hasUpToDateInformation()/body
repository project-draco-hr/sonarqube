{
  assertThat(ReviewWorkflowDecorator.hasUpToDateInformation(new ReviewDto().setTitle("Design").setLine(30),new Violation(new Rule()).setMessage("Design").setLineId(30))).isTrue();
  assertThat(ReviewWorkflowDecorator.hasUpToDateInformation(new ReviewDto().setTitle("Design").setLine(30),new Violation(new Rule()).setMessage("Other").setLineId(30))).isFalse();
  assertThat(ReviewWorkflowDecorator.hasUpToDateInformation(new ReviewDto().setTitle("Design").setLine(300),new Violation(new Rule()).setMessage("Design").setLineId(200))).isFalse();
  assertThat(ReviewWorkflowDecorator.hasUpToDateInformation(new ReviewDto().setTitle("Design").setLine(300),new Violation(new Rule()).setMessage("Design").setLineId(null))).isFalse();
}
