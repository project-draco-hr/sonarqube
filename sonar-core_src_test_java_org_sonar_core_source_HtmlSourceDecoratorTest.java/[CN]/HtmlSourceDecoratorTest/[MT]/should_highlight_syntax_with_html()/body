{
  HtmlSourceDecorator highlighter=new HtmlSourceDecorator(getMyBatis());
  List<String> highlightedSource=(List<String>)highlighter.getDecoratedSourceAsHtml(11L);
  assertThat(highlightedSource).containsExactly("<span class=\"cppd\">/*</span>","<span class=\"cppd\"> * Header</span>","<span class=\"cppd\"> */</span>","","<span class=\"k\">public </span><span class=\"k\">class </span>HelloWorld {","}");
}
