{
  HtmlSourceDecorator sourceDecorator=new HtmlSourceDecorator(getMyBatis());
  List<String> decoratedSource=(List<String>)sourceDecorator.getDecoratedSourceAsHtml(11L);
  assertThat(decoratedSource).containsExactly("<span class=\"cppd\">/*</span>","<span class=\"cppd\"> * Header</span>","<span class=\"cppd\"> */</span>","","<span class=\"k\">public </span><span class=\"k\">class </span>HelloWorld {","}");
}
