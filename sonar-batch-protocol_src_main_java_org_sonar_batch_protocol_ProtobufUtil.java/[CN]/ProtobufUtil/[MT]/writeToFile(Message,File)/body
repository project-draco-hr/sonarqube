{
  OutputStream out=null;
  try {
    out=new BufferedOutputStream(new FileOutputStream(toFile,false));
    message.writeTo(out);
  }
 catch (  IOException e) {
    throw new IllegalStateException("Unable to write protocol buffer data to file " + toFile,e);
  }
 finally {
    IOUtils.closeQuietly(out);
  }
}
