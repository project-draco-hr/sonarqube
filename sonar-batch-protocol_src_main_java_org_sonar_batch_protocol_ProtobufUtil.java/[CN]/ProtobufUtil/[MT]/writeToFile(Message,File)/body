{
  OutputStream out=null;
  try {
    out=new BufferedOutputStream(new FileOutputStream(toFile,false));
    message.writeTo(out);
  }
 catch (  IOException e) {
    throw new IllegalStateException(format("Unable to write protobuf message to file %s",toFile),e);
  }
 finally {
    IOUtils.closeQuietly(out);
  }
}
