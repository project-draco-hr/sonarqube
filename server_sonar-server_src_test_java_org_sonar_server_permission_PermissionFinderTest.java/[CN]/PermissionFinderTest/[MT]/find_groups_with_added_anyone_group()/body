{
  when(permissionDao.selectGroups(any(PermissionQuery.class),anyLong())).thenReturn(newArrayList(new GroupWithPermissionDto().setName("users").setPermission("user")));
  GroupWithPermissionQueryResult result=finder.findGroupsWithPermission(PermissionQuery.builder().permission("user").pageIndex(1).membership(PermissionQuery.ANY).build());
  assertThat(result.groups()).hasSize(2);
  GroupWithPermission first=result.groups().get(0);
  assertThat(first.name()).isEqualTo("Anyone");
  assertThat(first.hasPermission()).isFalse();
}
