{
  when(permissionDao.selectGroups(any(DbSession.class),any(PermissionQuery.class),anyLong())).thenReturn(newArrayList(new GroupWithPermissionDto().setName("users").setPermission("user")));
  GroupWithPermissionQueryResult result=underTest.findGroupsWithPermission(dbSession,PermissionQuery.builder().permission("user").pageIndex(1).membership(PermissionQuery.OUT).build());
  assertThat(result.groups()).hasSize(1);
}
