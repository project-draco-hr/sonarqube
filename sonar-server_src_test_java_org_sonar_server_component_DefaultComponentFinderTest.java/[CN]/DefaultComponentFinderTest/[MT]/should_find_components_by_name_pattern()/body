{
  List<ResourceDto> dtoList=newArrayList(new ResourceDto().setKey("org.codehaus.sonar").setName("Sonar").setQualifier("TRK"),new ResourceDto().setKey("org.apache.tika:tika").setName("Apache Tika").setQualifier("TRK"),new ResourceDto().setKey("org.apache.jackrabbit:jackrabbit").setName("Apache Jackrabbit").setQualifier("TRK"));
  when(dao.getResources(any(ResourceQuery.class))).thenReturn(dtoList);
  ComponentQuery query=ComponentQuery.builder().names(newArrayList("Apache")).build();
  assertThat(finder.find(query).components()).hasSize(2);
}
