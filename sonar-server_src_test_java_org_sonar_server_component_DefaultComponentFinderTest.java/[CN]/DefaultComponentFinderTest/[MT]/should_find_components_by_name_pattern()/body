{
  when(dao.selectComponentsByQualifiers(anyCollection())).thenReturn(newArrayList(createProject("org.codehaus.sonar","Sonar"),createProject("org.apache.tika:tika","Apache Tika"),createProject("org.apache.jackrabbit:jackrabbit","Apache Jackrabbit")));
  ComponentQuery query=ComponentQuery.builder().names(newArrayList("Apache")).build();
  assertThat(finder.find(query).components()).hasSize(2);
}
