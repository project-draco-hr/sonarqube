{
  List<Component> components=newArrayList(createProject("org.codehaus.sonar","Sonar"),createProject("org.apache.tika:tika","Apache Tika"),createProject("org.apache.jackrabbit:jackrabbit","Apache Jackrabbit"));
  ComponentQuery query=ComponentQuery.builder().keys(newArrayList("org.apache")).build();
  assertThat(finder.find(query,components).components()).hasSize(2);
}
