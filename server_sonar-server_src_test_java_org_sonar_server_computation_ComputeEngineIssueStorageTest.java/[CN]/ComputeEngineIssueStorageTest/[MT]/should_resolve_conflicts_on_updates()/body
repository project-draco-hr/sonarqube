{
  setupData("should_resolve_conflicts_on_updates");
  project.setId(10L).setKey("struts");
  Date date=DateUtils.parseDate("2013-05-18");
  DefaultIssue issue=new DefaultIssue().setKey("ABCDE").setNew(false).setChanged(true).setCreationDate(DateUtils.parseDate("2005-05-12")).setUpdateDate(date).setRuleKey(RuleKey.of("squid","AvoidCycles")).setComponentKey("struts:Action").setSelectedAt(1400000000000L).setLine(444).setSeverity("BLOCKER").setChecksum("FFFFF").setAttribute("JIRA","http://jira.com").setAssignee("looser").setResolution(null).setStatus("REOPEN");
  sut.save(issue);
  checkTables("should_resolve_conflicts_on_updates",new String[]{"id","created_at","updated_at","issue_change_creation_date"},"issues");
