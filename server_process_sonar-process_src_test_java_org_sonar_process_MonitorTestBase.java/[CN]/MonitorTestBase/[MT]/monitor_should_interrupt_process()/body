{
  ProcessWrapper process=new ProcessWrapper("DummyOkProcess").addProperty(MonitoredProcess.NAME_PROPERTY,"DummyOkProcess").addClasspath(dummyAppJar.getAbsolutePath()).setWorkDir(temp.getRoot()).setTempDirectory(temp.getRoot()).setJmxPort(freePort).setClassName(DUMMY_OK_APP);
  assertThat(process.execute());
  monitor.start();
  monitor.monitor(process);
  monitor.terminate();
  assertThat(monitor.isAlive()).isFalse();
  assertThat(process.isAlive()).isFalse();
}
