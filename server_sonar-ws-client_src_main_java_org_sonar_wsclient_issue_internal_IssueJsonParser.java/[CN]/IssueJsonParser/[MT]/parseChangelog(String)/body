{
  List<IssueChange> changes=new ArrayList<IssueChange>();
  Map jRoot=(Map)JSONValue.parse(json);
  List<Map> jChanges=(List<Map>)jRoot.get("changelog");
  if (jChanges != null) {
    for (    Map jChange : jChanges) {
      changes.add(new DefaultIssueChange(jChange));
    }
  }
  return changes;
}
