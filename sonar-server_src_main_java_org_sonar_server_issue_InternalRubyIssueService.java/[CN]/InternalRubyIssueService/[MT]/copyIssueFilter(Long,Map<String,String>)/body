{
  Result<DefaultIssueFilter> result=createIssueFilterResultForCopy(parameters);
  if (result.ok()) {
    try {
      result.set(issueFilterService.copy(issueFilterIdToCopy,result.get(),UserSession.get()));
    }
 catch (    Exception e) {
      result.addError(e.getMessage());
    }
  }
  return result;
}
