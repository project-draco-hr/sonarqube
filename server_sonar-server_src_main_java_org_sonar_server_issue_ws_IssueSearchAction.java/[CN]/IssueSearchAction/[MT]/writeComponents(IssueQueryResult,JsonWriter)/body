{
  json.name("components").beginArray();
  for (  Component component : result.components()) {
    ComponentDto componentDto=(ComponentDto)component;
    json.beginObject().prop("key",component.key()).prop("id",componentDto.getId()).prop("qualifier",component.qualifier()).prop("name",component.name()).prop("longName",component.longName()).prop("path",component.path()).prop("projectId",(componentDto.projectId() != null && componentDto.subProjectId() != null) ? componentDto.projectId() : null).prop("subProjectId",componentDto.subProjectId()).endObject();
  }
  json.endArray();
}
