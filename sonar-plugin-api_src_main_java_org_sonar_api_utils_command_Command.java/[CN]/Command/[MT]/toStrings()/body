{
  List<String> command=Lists.newArrayList();
  if (newShell) {
    if (SystemUtils.IS_OS_WINDOWS) {
      command.add("cmd");
      command.add("/C");
    }
 else {
      command.add("sh");
    }
  }
  command.add(executable);
  command.addAll(arguments);
  return command.toArray(new String[command.size()]);
}
