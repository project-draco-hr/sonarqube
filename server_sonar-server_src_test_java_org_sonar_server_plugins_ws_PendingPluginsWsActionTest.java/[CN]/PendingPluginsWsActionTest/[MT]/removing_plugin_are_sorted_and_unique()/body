{
  when(serverPluginJarsInstaller.getUninstalledPluginFilenames()).thenReturn(of("file2.jar","file0.jar","file0.jar","file1.jar"));
  underTest.handle(request,response);
  assertJson(response.outputAsString()).setStrictArrayOrder(true).isSimilarTo("{" + "  \"installing\": []," + "  \"removing\": "+ "  ["+ "    {"+ "      \"artifact\": {"+ "        \"name\": \"file0.jar\""+ "      }"+ "    },"+ "    {"+ "      \"artifact\": {"+ "        \"name\": \"file1.jar\""+ "      }"+ "    },"+ "    {"+ "      \"artifact\": {"+ "        \"name\": \"file2.jar\""+ "      }"+ "    }"+ "  ]"+ "}");
}
