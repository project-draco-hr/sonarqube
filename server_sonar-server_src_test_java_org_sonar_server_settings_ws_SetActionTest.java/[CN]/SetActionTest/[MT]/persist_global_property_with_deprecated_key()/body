{
  propertyDefinitions.addComponent(PropertyDefinition.builder("my.key").deprecatedKey("my.old.key").name("foo").description("desc").category("cat").subCategory("subCat").type(PropertyType.STRING).defaultValue("default").build());
  callForGlobalSetting("my.old.key","My Value");
  assertGlobalSetting("my.key","My Value");
}
