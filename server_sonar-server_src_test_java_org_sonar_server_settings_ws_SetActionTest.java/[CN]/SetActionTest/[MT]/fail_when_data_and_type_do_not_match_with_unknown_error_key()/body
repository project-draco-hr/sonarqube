{
  propertyDefinitions.addComponent(PropertyDefinition.builder("my.key").name("foo").description("desc").category("cat").subCategory("subCat").type(PropertyType.INTEGER).defaultValue("default").build());
  expectedException.expect(BadRequestException.class);
  expectedException.expectMessage("Error when validating setting with key 'my.key' and value 'My Value'");
  callForGlobalSetting("my.key","My Value");
}
