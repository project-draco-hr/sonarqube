{
  SearchRequestBuilder searchRequest=getClient().prepareSearch(TestIndexDefinition.INDEX).setTypes(TestIndexDefinition.TYPE).setSize(searchOptions.getLimit()).setFrom(searchOptions.getOffset()).setQuery(QueryBuilders.nestedQuery(FIELD_COVERED_FILES,FilterBuilders.boolFilter().must(FilterBuilders.termFilter(FIELD_COVERED_FILES + "." + FIELD_COVERED_FILE_UUID,sourceFileUuid).cache(false)).must(FilterBuilders.termFilter(FIELD_COVERED_FILES + "." + FIELD_COVERED_FILE_LINES,lineNumber).cache(false))));
  return new SearchResult<>(searchRequest.get(),CONVERTER);
}
