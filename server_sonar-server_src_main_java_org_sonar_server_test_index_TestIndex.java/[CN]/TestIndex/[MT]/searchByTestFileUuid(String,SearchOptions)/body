{
  SearchRequestBuilder searchRequest=getClient().prepareSearch(TestIndexDefinition.INDEX).setTypes(TestIndexDefinition.TYPE).setSize(searchOptions.getLimit()).setFrom(searchOptions.getOffset()).setQuery(QueryBuilders.filteredQuery(QueryBuilders.matchAllQuery(),FilterBuilders.termFilter(FIELD_FILE_UUID,testFileUuid)));
  return new SearchResult<>(searchRequest.get(),CONVERTER);
}
