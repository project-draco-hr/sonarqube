{
  List<Map<String,Object>> coverageBlocks=new ArrayList<>();
  for (  SearchHit hit : getClient().prepareSearch(TestIndexDefinition.INDEX).setTypes(TestIndexDefinition.TYPE).setSize(1).setQuery(QueryBuilders.filteredQuery(QueryBuilders.matchAllQuery(),FilterBuilders.boolFilter().must(FilterBuilders.termFilter(FIELD_UUID,testFileUuid).cache(false)).must(FilterBuilders.termFilter(TestIndexDefinition.FIELD_NAME,methodName).cache(false)))).get().getHits().getHits()) {
    coverageBlocks.addAll(new TestDoc(hit.sourceAsMap()).coverageBlocks());
  }
  return coverageBlocks;
}
