{
  this.settings=settings;
  this.coverageMetrics=ImmutableSet.<Metric>builder().addAll(coverageDecorator.generatedMetrics()).addAll(coverageDecorator.usedMetrics()).addAll(lineCoverageDecorator.generatedMetrics()).addAll(lineCoverageDecorator.dependsUponMetrics()).addAll(branchCoverageDecorator.generatedMetrics()).addAll(branchCoverageDecorator.dependsUponMetrics()).build();
  initPatterns();
}
