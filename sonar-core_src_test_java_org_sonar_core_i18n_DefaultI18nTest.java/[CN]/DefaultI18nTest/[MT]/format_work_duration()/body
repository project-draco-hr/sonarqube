{
  when(workDurationFormatter.format(10)).thenReturn(newArrayList(new WorkDurationFormatter.Result("work_duration.x_days",5),new WorkDurationFormatter.Result(" ",null),new WorkDurationFormatter.Result("work_duration.x_hours",2),new WorkDurationFormatter.Result(" ",null),new WorkDurationFormatter.Result("work_duration.x_minutes",1)));
  assertThat(manager.formatWorkDuration(Locale.ENGLISH,10)).isEqualTo("5d 2h 1min");
}
