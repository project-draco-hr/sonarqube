{
  CeTaskProcessorRepositoryImpl underTest=new CeTaskProcessorRepositoryImpl(new CeTaskProcessor[]{createCeTaskProcessor(SOME_CE_TASK_TYPE + "_1"),createCeTaskProcessor(SOME_CE_TASK_TYPE + "_2",SOME_CE_TASK_TYPE + "_3")});
  assertThat(underTest.getForCeTask(createCeTask(SOME_CE_TASK_TYPE,SOME_COMPONENT_KEY))).isAbsent();
}
