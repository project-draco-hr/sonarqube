{
  WebService.NewAction action=controller.createAction("upload_report").setDescription("Update analysis report").setSince("5.0").setPost(true).setInternal(true).setHandler(this);
  action.createParam(PARAM_PROJECT).setRequired(true).setDescription("Project key").setExampleValue("org.codehaus.sonar:sonar");
  action.createParam(PARAM_FIRST_ANALYSIS).setDescription("Is it the first analysis of this project ?").setDefaultValue(false).setBooleanPossibleValues();
}
