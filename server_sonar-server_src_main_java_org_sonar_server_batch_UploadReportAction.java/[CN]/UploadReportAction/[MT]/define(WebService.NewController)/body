{
  WebService.NewAction action=controller.createAction(UPLOAD_REPORT_ACTION).setDescription("Update analysis report").setSince("5.0").setPost(true).setInternal(true).setHandler(this);
  action.createParam(PARAM_PROJECT_KEY).setRequired(true).setDescription("Project key").setExampleValue("org.codehaus.sonar:sonar");
  action.createParam(PARAM_SNAPSHOT).setRequired(true).setDescription("Snapshot id").setExampleValue("123");
  action.createParam(PARAM_REPORT_DATA).setRequired(false).setDescription("Report file");
}
