{
  String sql="SELECT m.name as metricName, pm.value as value, pm.text_value as textValue, pm.variation_value_1, pm.variation_value_2, pm.variation_value_3 " + "FROM project_measures pm, snapshots s, metrics m " + "WHERE pm.snapshot_id=s.id and pm.metric_id=m.id and s.qualifier='" + qualifier + "'";
  List<Map<String,String>> rows=orchestrator.getDatabase().executeSql(sql);
  System.out.println("---- " + title + " - measures on qualifier "+ qualifier);
  for (  Map<String,String> row : rows) {
    System.out.println("  " + row);
  }
}
