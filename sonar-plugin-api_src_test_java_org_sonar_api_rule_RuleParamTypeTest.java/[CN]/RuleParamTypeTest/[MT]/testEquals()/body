{
  RuleParamType noOptions=RuleParamType.INTEGER;
  RuleParamType withOptions1=RuleParamType.ofValues("one","two");
  RuleParamType withOptions2=RuleParamType.ofValues("three","four");
  assertThat(RuleParamType.INTEGER).isEqualTo(RuleParamType.INTEGER).isNotEqualTo(RuleParamType.STRING).isNotEqualTo("INTEGER").isNotEqualTo(withOptions1).isNotEqualTo(null);
  assertThat(withOptions1).isEqualTo(withOptions1).isNotEqualTo(noOptions).isNotEqualTo(withOptions2).isNotEqualTo("SINGLE_SELECT_LIST|one,two,").isNotEqualTo(null);
}
