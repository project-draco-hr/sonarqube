{
  String value=request.param(PARAM_DEBT_REMEDIATION_FN_TYPE);
  if (value != null) {
    if (StringUtils.isBlank(value)) {
      update.setDebtRemediationFunction(null);
    }
 else {
      DebtRemediationFunction fn=new DefaultDebtRemediationFunction(DebtRemediationFunction.Type.valueOf(value),request.param(PARAM_DEBT_REMEDIATION_FY_COEFF),request.param(PARAM_DEBT_REMEDIATION_FN_OFFSET));
      update.setDebtRemediationFunction(fn);
    }
  }
}
