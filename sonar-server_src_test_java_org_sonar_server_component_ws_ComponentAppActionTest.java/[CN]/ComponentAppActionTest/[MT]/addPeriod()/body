{
  Date periodDate=DateUtils.parseDate("2014-05-08");
  when(resourceDao.getLastSnapshotByResourceId(eq(1L),eq(session))).thenReturn(new SnapshotDto().setPeriod1Mode("previous_analysis").setPeriod1Date(periodDate));
  when(periods.label(anyString(),anyString(),any(Date.class))).thenReturn("since previous analysis (May 08 2014)");
}
