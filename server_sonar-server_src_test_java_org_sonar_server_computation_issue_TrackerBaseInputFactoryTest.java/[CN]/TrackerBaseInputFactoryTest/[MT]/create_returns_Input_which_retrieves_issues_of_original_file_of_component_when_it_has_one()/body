{
  String originalUuid="original uuid";
  when(movedFilesRepository.getOriginalFile(FILE)).thenReturn(Optional.of(new MovedFilesRepository.OriginalFile(6542,originalUuid,"original key")));
  underTest.create(FILE).getIssues();
