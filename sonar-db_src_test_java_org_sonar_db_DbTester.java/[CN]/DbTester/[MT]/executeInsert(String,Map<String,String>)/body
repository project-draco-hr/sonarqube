{
  if (valuesByColumn.isEmpty()) {
    throw new IllegalArgumentException("Values cannot be empty");
  }
  String sql="insert into " + table.toLowerCase() + " ("+ Joiner.on(", ").join(valuesByColumn.keySet())+ ") values ('"+ Joiner.on("', '").join(valuesByColumn.values())+ "')";
  executeUpdateSql(sql);
}
