{
  if (valuesByColumn.isEmpty()) {
    throw new IllegalArgumentException("Values cannot be empty");
  }
  String sql="insert into " + table.toLowerCase(Locale.ENGLISH) + " ("+ COMMA_JOINER.join(valuesByColumn.keySet())+ ") values ("+ COMMA_JOINER.join(Collections.nCopies(valuesByColumn.size(),'?'))+ ")";
  executeUpdateSql(sql,valuesByColumn.values().toArray(new String[valuesByColumn.size()]));
}
