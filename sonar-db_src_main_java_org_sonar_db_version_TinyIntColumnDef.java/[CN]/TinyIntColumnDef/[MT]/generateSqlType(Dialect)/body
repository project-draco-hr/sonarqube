{
switch (dialect.getId()) {
case PostgreSql.ID:
    return "SMALLINT";
case Oracle.ID:
  return "NUMBER(3)";
case MySql.ID:
return "TINYINT(1)";
case MsSql.ID:
case H2.ID:
return "TINYINT";
default :
throw new UnsupportedOperationException(String.format("Unknown dialect '%s'",dialect.getId()));
}
}
