{
  String sql=createSQL(dataType,afterDate,projectUuid);
  PreparedStatement stmt=dbClient.newScrollingSingleRowSelectStatement(connection,sql);
  int index=1;
  if (afterDate > 0L) {
    stmt.setLong(index,afterDate);
    index++;
  }
  if (projectUuid != null) {
    stmt.setString(index,projectUuid);
  }
  return stmt;
}
