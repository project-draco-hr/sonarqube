{
  IssueDto issue=IssueTesting.newDto(rule,file,project).setKee("82fd47d4-b650-4037-80bc-7b112bd4eac2").setIssueAttributes(KeyValueFormat.format(ImmutableMap.of("jira-issue-key","SONAR-1234")));
  db.issueDao().insert(session,issue);
  session.commit();
  WsTester.Result result=wsTester.newGetRequest(IssuesWs.API_ENDPOINT,SearchAction.SEARCH_ACTION).execute();
  result.assertJson(this.getClass(),"issue_with_attributes.json",false);
}
