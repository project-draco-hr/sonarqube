{
  underTest.set(SetRequest.builder().setKey("sonar.debt").setValue("8h").setComponentId("UUID").setComponentKey("KEY").build());
  serviceTester.assertThat(serviceTester.getPostRequest()).hasParam(PARAM_KEY,"sonar.debt").hasParam(PARAM_VALUE,"8h").hasParam(PARAM_COMPONENT_ID,"UUID").hasParam(PARAM_COMPONENT_KEY,"KEY").andNoOtherParam();
}
