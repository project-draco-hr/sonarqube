{
  int total=0;
  while (rs.next()) {
    long violationId=rs.getLong(1);
    violationIds[cursor++]=violationId;
    if (cursor == GROUP_SIZE) {
      convert();
      Arrays.fill(violationIds,-1L);
      cursor=0;
    }
    total++;
  }
  if (cursor > 0) {
    convert();
  }
  LoggerFactory.getLogger(getClass()).info(String.format("%d violations migrated to issues",total));
  return null;
}
