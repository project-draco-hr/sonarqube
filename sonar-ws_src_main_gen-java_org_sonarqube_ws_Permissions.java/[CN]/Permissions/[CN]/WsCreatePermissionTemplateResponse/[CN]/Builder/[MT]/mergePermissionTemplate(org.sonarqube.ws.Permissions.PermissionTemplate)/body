{
  if (permissionTemplateBuilder_ == null) {
    if (((bitField0_ & 0x00000001) == 0x00000001) && permissionTemplate_ != org.sonarqube.ws.Permissions.PermissionTemplate.getDefaultInstance()) {
      permissionTemplate_=org.sonarqube.ws.Permissions.PermissionTemplate.newBuilder(permissionTemplate_).mergeFrom(value).buildPartial();
    }
