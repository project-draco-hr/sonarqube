{
  if (globalPermissionsBuilder_ == null) {
    if (value == null) {
      throw new NullPointerException();
    }
    ensureGlobalPermissionsIsMutable();
    globalPermissions_.set(index,value);
    onChanged();
  }
 else {
    globalPermissionsBuilder_.setMessage(index,value);
  }
  return this;
}
