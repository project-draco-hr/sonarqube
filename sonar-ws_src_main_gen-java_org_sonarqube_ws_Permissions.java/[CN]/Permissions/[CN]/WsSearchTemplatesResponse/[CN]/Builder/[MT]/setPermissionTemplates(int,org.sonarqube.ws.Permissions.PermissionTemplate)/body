{
  if (permissionTemplatesBuilder_ == null) {
    if (value == null) {
      throw new NullPointerException();
    }
    ensurePermissionTemplatesIsMutable();
    permissionTemplates_.set(index,value);
    onChanged();
  }
 else {
    permissionTemplatesBuilder_.setMessage(index,value);
  }
  return this;
}
