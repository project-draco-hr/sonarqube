{
  if (lastReturned == null) {
    throw new IllegalStateException();
  }
  if (fast) {
synchronized (FastHashMap.this) {
      if (expected != map) {
        throw new ConcurrentModificationException();
      }
      FastHashMap.this.remove(lastReturned.getKey());
      lastReturned=null;
      expected=map;
    }
  }
 else {
    iterator.remove();
    lastReturned=null;
  }
}
