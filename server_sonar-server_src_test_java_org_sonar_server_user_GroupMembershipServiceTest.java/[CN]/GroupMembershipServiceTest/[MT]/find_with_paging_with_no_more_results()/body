{
  dbTester.prepareDbUnit(getClass(),"shared.xml");
  GroupMembershipFinder.Membership queryResult=service.find(ImmutableMap.<String,Object>of("user","user1","selected","all","page",3,"pageSize",1));
  List<GroupMembership> result=queryResult.groups();
  assertThat(result).hasSize(1);
  assertThat(queryResult.hasMoreResults()).isFalse();
}
