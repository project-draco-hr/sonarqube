{
  GetRequest get=new GetRequest(path("groups")).setParam(PARAM_PERMISSION,request.getPermission()).setParam(PARAM_PROJECT_ID,request.getProjectId()).setParam(PARAM_PROJECT_KEY,request.getProjectKey()).setParam("p",request.getPage()).setParam("ps",request.getPageSize()).setParam("selected",request.getSelected()).setParam("q",request.getQuery());
  return call(get,WsPermissions.WsGroupsResponse.parser());
}
