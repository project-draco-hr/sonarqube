{
switch (periodIndex) {
case 1:
    period1Date=date;
  break;
case 2:
period2Date=date;
break;
case 3:
period3Date=date;
break;
case 4:
period4Date=date;
break;
case 5:
period5Date=date;
break;
default :
throw new IndexOutOfBoundsException("Index of Snapshot.periodDate is between 1 and 5");
}
return this;
}
