{
switch (periodIndex) {
case 1:
    return longToDate(period1Date);
case 2:
  return longToDate(period2Date);
case 3:
return longToDate(period3Date);
case 4:
return longToDate(period4Date);
case 5:
return longToDate(period5Date);
default :
throw newPeriodIndexOutOfBoundsException("periodDate");
}
}
