{
switch (periodIndex) {
case 1:
    period1Param=s;
  break;
case 2:
period2Param=s;
break;
case 3:
period3Param=s;
break;
case 4:
period4Param=s;
break;
case 5:
period5Param=s;
break;
default :
throw new IndexOutOfBoundsException("Index of Snapshot.periodModeParameter is between 1 and 5");
}
return this;
}
