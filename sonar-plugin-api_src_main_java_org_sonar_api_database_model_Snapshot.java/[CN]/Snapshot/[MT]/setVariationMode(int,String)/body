{
switch (index) {
case 1:
    variationMode1=s;
  break;
case 2:
variationMode2=s;
break;
case 3:
variationMode3=s;
break;
case 4:
variationMode4=s;
break;
case 5:
variationMode5=s;
break;
default :
throw new IndexOutOfBoundsException("Index of Snapshot.variationMode is between 1 and 5");
}
return this;
}
