{
  if (coll1.size() < coll2.size()) {
    for (Iterator it=coll1.iterator(); it.hasNext(); ) {
      if (coll2.contains(it.next())) {
        return true;
      }
    }
  }
 else {
    for (Iterator it=coll2.iterator(); it.hasNext(); ) {
      if (coll1.contains(it.next())) {
        return true;
      }
    }
  }
  return false;
}
