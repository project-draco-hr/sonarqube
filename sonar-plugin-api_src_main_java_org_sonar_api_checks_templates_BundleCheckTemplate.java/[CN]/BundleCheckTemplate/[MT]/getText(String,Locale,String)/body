{
  String result=null;
  ResourceBundle bundle=getBundle(locale);
  if (bundle != null) {
    try {
      result=bundle.getString(key);
    }
 catch (    MissingResourceException e) {
      LOG.debug(e.getMessage());
    }
  }
  if (result == null) {
    result=defaultValue;
  }
  return result;
}
