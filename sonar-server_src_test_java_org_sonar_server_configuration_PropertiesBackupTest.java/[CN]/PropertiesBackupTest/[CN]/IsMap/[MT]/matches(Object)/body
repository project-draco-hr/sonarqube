{
  if (argument != null && argument instanceof Map) {
    Map<String,String> argAsMap=(Map<String,String>)argument;
    for (    String key : argAsMap.keySet()) {
      if (!referenceMap.containsKey(key) || !referenceMap.get(key).equals(argAsMap.get(key))) {
        return false;
      }
    }
    return true;
  }
  return false;
}
