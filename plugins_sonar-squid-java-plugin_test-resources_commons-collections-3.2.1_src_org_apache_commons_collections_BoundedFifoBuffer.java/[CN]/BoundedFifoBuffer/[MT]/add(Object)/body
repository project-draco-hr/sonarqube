{
  if (null == element) {
    throw new NullPointerException("Attempted to add null object to buffer");
  }
  if (m_full) {
    throw new BufferOverflowException("The buffer cannot hold more than " + maxElements + " objects.");
  }
  m_elements[m_end++]=element;
  if (m_end >= maxElements) {
    m_end=0;
  }
  if (m_end == m_start) {
    m_full=true;
  }
  return true;
}
