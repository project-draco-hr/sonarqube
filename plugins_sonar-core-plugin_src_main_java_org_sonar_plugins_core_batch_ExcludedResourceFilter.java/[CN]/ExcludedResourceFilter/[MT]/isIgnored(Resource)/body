{
  String[] patterns=ResourceUtils.isUnitTestClass(resource) ? project.getTestExclusionPatterns() : project.getExclusionPatterns();
  if (patterns == null) {
    return false;
  }
  for (  String pattern : patterns) {
    if (resource.matchFilePattern(pattern)) {
      return true;
    }
  }
  return false;
}
