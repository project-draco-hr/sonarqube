{
  DbSession session=dbClient.openSession(false);
  try {
    if (hasAlreadyBeenExecuted(session)) {
      return;
    }
    if (!isSqalePluginInstalled(session)) {
      clearDebt(session);
    }
    markAsExecuted(session);
    session.commit();
  }
  finally {
    dbClient.closeSession(session);
  }
}
