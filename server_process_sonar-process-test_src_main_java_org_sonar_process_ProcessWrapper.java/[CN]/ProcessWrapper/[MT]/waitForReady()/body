{
  if (processMXBean == null) {
    return false;
  }
  long now=0;
  long wait=500L;
  while (now < READY_TIMEOUT_MS) {
    try {
      if (processMXBean == null) {
        return false;
      }
      if (processMXBean.isReady()) {
        return true;
      }
    }
 catch (    Exception e) {
    }
    Thread.sleep(wait);
    now+=wait;
  }
  return false;
}
