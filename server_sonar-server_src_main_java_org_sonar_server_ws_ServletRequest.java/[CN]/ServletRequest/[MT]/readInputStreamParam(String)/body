{
  try {
    if (!isMultipartContent()) {
      return null;
    }
    Part part=source.getPart(key);
    if (part == null || part.getSize() == 0) {
      return null;
    }
    return part.getInputStream();
  }
 catch (  Exception e) {
    throw new IllegalStateException("Can't read file part",e);
  }
}
