{
  WebService.NewAction action=controller.createAction("project").setDescription("Get the pending and last executed tasks of a given project").setInternal(true).setResponseExample(getClass().getResource("project-example.json")).setHandler(this);
  action.createParam(PARAM_COMPONENT_UUID).setRequired(true).setExampleValue(Uuids.UUID_EXAMPLE_01);
}
