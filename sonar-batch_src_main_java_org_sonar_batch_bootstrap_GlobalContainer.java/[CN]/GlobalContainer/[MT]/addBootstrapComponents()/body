{
  add(BatchPluginRepository.class,BatchPluginJarInstaller.class,GlobalSettings.class,ServerClient.class,ExtensionInstaller.class,Logback.class,DefaultServer.class,new TempFolderProvider(),TempFolderCleaner.class,HttpDownloader.class,UriReader.class,new FileCacheProvider(),System2.INSTANCE,DefaultI18n.class,new GlobalRepositoriesProvider(),UserRepository.class);
  if (getComponentByType(PluginsRepository.class) == null) {
    add(DefaultPluginsReferential.class);
  }
  if (getComponentByType(GlobalRepositoriesLoader.class) == null) {
    add(DefaultGlobalRepositoriesLoader.class);
  }
  if (getComponentByType(ProjectRepositoriesLoader.class) == null) {
    add(DefaultProjectRepositoriesLoader.class);
  }
  if (getComponentByType(ServerIssuesLoader.class) == null) {
    add(DefaultServerIssuesLoader.class);
  }
}
