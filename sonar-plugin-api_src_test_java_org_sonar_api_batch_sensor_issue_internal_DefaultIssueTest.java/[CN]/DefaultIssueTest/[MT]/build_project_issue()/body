{
  Issue issue=new DefaultIssueBuilder().onProject().ruleKey(RuleKey.of("repo","rule")).atLine(1).effortToFix(10.0).message("Wrong way!").build();
  assertThat(issue.inputFile()).isNull();
  assertThat(issue.ruleKey()).isEqualTo(RuleKey.of("repo","rule"));
  assertThat(issue.line()).isEqualTo(1);
  assertThat(issue.effortToFix()).isEqualTo(10.0);
  assertThat(issue.message()).isEqualTo("Wrong way!");
}
