{
  WebService.NewAction action=controller.createAction("update").setDescription("Update an action plan. Requires Administer permission on project").setSince("3.6").setPost(true).setHandler(RailsHandler.INSTANCE);
  addKeyParam(action);
  addNameParam(action);
  addDescriptionParam(action);
  addDeadLineParam(action);
  addFormatParam(action);
}
