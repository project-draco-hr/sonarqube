{
  assertThat(getProjectMeasure(CoreMetrics.VIOLATIONS_KEY).getValue(),closeTo(7565.0,500.0));
  assertThat(getProjectMeasure(CoreMetrics.BLOCKER_VIOLATIONS_KEY).getValue(),closeTo(0.0,20.0));
