{
  DecoratorContext context=mock(DecoratorContext.class);
  when(context.getMeasure(CoreMetrics.SCM_LAST_COMMIT_DATETIMES_BY_LINE)).thenReturn(new Measure(CoreMetrics.SCM_LAST_COMMIT_DATETIMES_BY_LINE,"10=2008-05-18T00:00:00+0000"));
  NewCoverageFileAnalyzer decorator=newDecorator();
  decorator.doDecorate(context);
  verify(context,never()).saveMeasure(any(Measure.class));
}
