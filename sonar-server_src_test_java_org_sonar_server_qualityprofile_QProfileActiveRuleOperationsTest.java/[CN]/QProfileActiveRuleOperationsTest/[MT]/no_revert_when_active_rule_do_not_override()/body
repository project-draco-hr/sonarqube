{
  ActiveRuleDto activeRule=new ActiveRuleDto().setId(5).setProfileId(1).setRuleId(10).setSeverity(Severity.MINOR).setInheritance(null);
  when(activeRuleDao.selectById(5,session)).thenReturn(activeRule);
  when(activeRuleDao.selectById(5,session)).thenReturn(activeRule);
