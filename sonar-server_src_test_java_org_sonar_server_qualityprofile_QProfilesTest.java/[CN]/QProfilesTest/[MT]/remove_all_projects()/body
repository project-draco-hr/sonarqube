{
  QualityProfileDto qualityProfile=new QualityProfileDto().setId(1).setName("My profile").setLanguage("java");
  when(qualityProfileDao.selectById(1)).thenReturn(qualityProfile);
  qProfiles.removeAllProjects(1);
