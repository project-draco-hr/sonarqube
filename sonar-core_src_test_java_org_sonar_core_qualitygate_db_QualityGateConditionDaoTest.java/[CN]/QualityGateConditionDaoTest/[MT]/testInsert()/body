{
  setupData("insert");
  QualityGateConditionDto newCondition=new QualityGateConditionDto().setQualityGateId(1L).setMetricId(2L).setOperator(">").setWarningThreshold("10").setErrorThreshold("20").setPeriod(3);
  dao.insert(newCondition);
  checkTable("insert","quality_gate_conditions","name");
  assertThat(newCondition.getId()).isNotNull();
}
