{
  QueryContext queryContext=fixtureForCreatedAtFacet();
  Collection<FacetValue> createdAt=index.search(IssueQuery.builder().createdAfter(DateUtils.parseDate("2014-09-01")).createdBefore(DateUtils.parseDate("2015-01-19")).build(),queryContext).getFacets().get("createdAt");
  assertThat(createdAt).hasSize(6).containsOnly(new FacetValue("2014-08-01T01:00:00+0000",0),new FacetValue("2014-09-01T01:00:00+0000",5),new FacetValue("2014-10-01T01:00:00+0000",0),new FacetValue("2014-11-01T01:00:00+0000",0),new FacetValue("2014-12-01T01:00:00+0000",0),new FacetValue("2015-01-01T01:00:00+0000",1));
}
