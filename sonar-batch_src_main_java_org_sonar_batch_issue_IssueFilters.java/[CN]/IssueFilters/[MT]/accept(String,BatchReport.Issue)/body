{
  Issue issue=toIssueForIssueFilter(componentKey,rawIssue);
  if (new DefaultIssueFilterChain(filters).accept(issue)) {
    for (    org.sonar.api.issue.IssueFilter filter : exclusionFilters) {
      if (!filter.accept(issue)) {
        return false;
      }
    }
    return true;
  }
 else {
    return false;
  }
}
