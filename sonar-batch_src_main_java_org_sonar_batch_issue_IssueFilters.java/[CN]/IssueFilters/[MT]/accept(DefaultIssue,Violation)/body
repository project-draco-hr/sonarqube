{
  for (  IssueFilter filter : filters) {
    if (!filter.accept(issue)) {
      return false;
    }
  }
  if (!deprecatedFilters.isEmpty()) {
    Violation v=(violation != null ? violation : deprecatedViolations.toViolation(issue));
    return !deprecatedFilters.isIgnored(v);
  }
  return true;
}
