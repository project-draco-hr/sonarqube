{
  setupData("update");
  RuleDto ruleToUpdate=new RuleDto().setId(1).setRuleKey("NewRuleKey").setRepositoryKey("plugin").setName("new name").setDescription("new description").setStatus(Rule.STATUS_DEPRECATED).setConfigKey("NewConfigKey").setSeverity(0).setCardinality(Cardinality.MULTIPLE).setLanguage("dart").setParentId(3).setUpdatedAt(DateUtils.parseDate("2013-12-17")).setNoteData("My note").setNoteUserLogin("admin").setNoteCreatedAt(DateUtils.parseDate("2013-12-19")).setNoteUpdatedAt(DateUtils.parseDate("2013-12-20"));
