{
  orchestrator.executeBuild(SonarScanner.create(projectDir("shared/xoo-sample")));
  Selenese selenese=Selenese.builder().setHtmlTestsInClasspath("subcategories","/settings/subcategories/project-subcategories.html","/settings/subcategories/project-subcategories-no-default.html").build();
  new SeleneseTest(selenese).runOn(orchestrator);
  assertThat(getProperty("prop3","sample")).isEqualTo("myValue2");
}
