{
  regexpScanner.scan(javaFile,new File(Resources.getResource("org/sonar/batch/issue/ignore/scanner/IssueExclusionsRegexpScannerTest/file-with-single-regexp.txt").toURI()),UTF_8);
  verify(patternsInitializer).getPatternMatcher();
  verify(patternMatcher,times(1)).addPatternToExcludeResource(javaFile);
  verifyNoMoreInteractions(patternsInitializer);
}
