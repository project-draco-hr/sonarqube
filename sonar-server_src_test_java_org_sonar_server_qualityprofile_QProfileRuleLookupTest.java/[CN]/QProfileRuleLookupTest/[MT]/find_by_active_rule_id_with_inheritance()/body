{
  esSetup.client().prepareBulk().add(Requests.indexRequest().index("rules").type("active_rule").parent("25").source(testFileAsString("find_active_rules_with_inheritance/active_rule25.json"))).add(Requests.indexRequest().index("rules").type("active_rule").parent("759").source(testFileAsString("find_active_rules_with_inheritance/active_rule391.json"))).setRefresh(true).execute().actionGet();
  QProfileRule child=profileRules.findByActiveRuleId(391);
  assertThat(child.inheritance()).isEqualTo("INHERITED");
  assertThat(child.activeRuleParentId()).isEqualTo(25);
}
