{
  if (packageKey != null) {
    String prefix=StringUtils.substringBeforeLast(packageKey,":") + ":";
    String key=StringUtils.substringAfterLast(packageKey,":");
    if ("[default]".equals(key)) {
      return prefix + "[root]";
    }
    return prefix + StringUtils.replace(key,".","/");
  }
  return null;
}
