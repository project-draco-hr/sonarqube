{
  answerSql(asList(new String[]{TABLE_ISSUES,COLUMN_KEE,"Latin1_General_CS_AS"},new String[]{TABLE_PROJECTS,COLUMN_KEE,"Latin1_General_CI_AI"},new String[]{TABLE_PROJECTS,COLUMN_NAME,"Latin1_General_CI_AI"}));
  expectedException.expect(MessageException.class);
  expectedException.expectMessage("Case-sensitive and accent-sensitive collation (CS_AS) is required for database columns [projects.kee, projects.name]");
  underTest.handle(mock(Connection.class),true);
}
