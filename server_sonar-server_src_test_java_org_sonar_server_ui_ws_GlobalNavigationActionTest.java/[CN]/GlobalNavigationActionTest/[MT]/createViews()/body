{
  Page page=new Page(){
    @Override public String getTitle(){
      return "My Plugin Page";
    }
    @Override public String getId(){
      return "my_plugin_page";
    }
  }
;
  Page controller=new Page(){
    @Override public String getTitle(){
      return "My Rails App";
    }
    @Override public String getId(){
      return "/my_rails_app";
    }
  }
;
  return new Views(new View[]{page,controller});
}
