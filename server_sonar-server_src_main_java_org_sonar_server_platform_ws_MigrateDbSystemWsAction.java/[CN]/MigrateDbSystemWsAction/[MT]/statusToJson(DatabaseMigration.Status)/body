{
switch (status) {
case NONE:
    return "NO_MIGRATION";
case RUNNING:
  return "MIGRATION_RUNNING";
case FAILED:
return "MIGRATION_FAILED";
case SUCCEEDED:
return "MIGRATION_SUCCEEDED";
default :
throw new IllegalArgumentException("Unsupported DatabaseMigration.Status " + status + " can not be converted to JSON value");
}
}
