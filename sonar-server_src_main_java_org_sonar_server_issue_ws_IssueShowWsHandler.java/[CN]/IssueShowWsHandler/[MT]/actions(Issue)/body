{
  List<String> actions=newArrayList();
  if (UserSession.get().isLoggedIn()) {
    actions.add("comment");
    if (issue.resolution() == null) {
      actions.add("assign");
      actions.add("plan");
      for (      Action action : actionService.listAvailableActions(issue)) {
        actions.add(action.key());
      }
    }
  }
  return actions;
}
