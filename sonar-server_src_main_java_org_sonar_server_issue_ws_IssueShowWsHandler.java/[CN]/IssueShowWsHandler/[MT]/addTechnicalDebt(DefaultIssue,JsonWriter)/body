{
  WorkDayDuration technicalDebt=issue.technicalDebt();
  if (technicalDebt != null) {
    json.prop("fTechnicalDebt",technicalDebtService.format(technicalDebt));
    json.name("technicalDebt").beginObject().prop("days",technicalDebt.days()).prop("hours",technicalDebt.hours()).prop("minutes",technicalDebt.minutes()).endObject();
  }
}
