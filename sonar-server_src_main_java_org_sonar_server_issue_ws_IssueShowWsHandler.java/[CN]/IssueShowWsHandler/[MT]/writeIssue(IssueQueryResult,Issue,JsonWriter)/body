{
  json.prop("key",issue.key()).prop("line",issue.line()).prop("message",issue.message()).prop("resolution",issue.resolution()).prop("status",issue.status()).prop("status",issue.status()).prop("severity",issue.severity());
  if (issue.assignee() != null) {
    json.prop("assignee",issue.assignee()).prop("assigneeName",result.user(issue.assignee()).name());
  }
}
