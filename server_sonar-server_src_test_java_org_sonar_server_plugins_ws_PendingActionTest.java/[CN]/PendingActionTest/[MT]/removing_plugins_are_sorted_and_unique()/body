{
  loggedAsSystemAdmin();
  when(serverPluginRepository.getUninstalledPlugins()).thenReturn(of(newPluginInfo(0).setName("Foo"),newPluginInfo(3).setName("Bar"),newPluginInfo(2).setName("Bar")));
  underTest.handle(request,response);
  assertJson(response.outputAsString()).withStrictArrayOrder().isSimilarTo("{" + "  \"installing\": []," + "  \"updating\": [],"+ "  \"removing\": "+ "  ["+ "    {"+ "      \"key\": \"key2\","+ "      \"name\": \"Bar\","+ "    },"+ "    {"+ "      \"key\": \"key3\","+ "      \"name\": \"Bar\","+ "    },"+ "    {"+ "      \"key\": \"key0\","+ "      \"name\": \"Foo\","+ "    }"+ "  ]"+ "}");
}
