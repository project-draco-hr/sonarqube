{
  thrown.expect(IllegalStateException.class);
  thrown.expectMessage("The properties file of the module 'module1' does not exist: " + TestUtils.getResource(this.getClass(),"multi-module-with-unexisting-file").getAbsolutePath() + File.separator+ "any-folder"+ File.separator+ "any-file.properties");
  loadProjectDefinition("multi-module-with-unexisting-file");
}
