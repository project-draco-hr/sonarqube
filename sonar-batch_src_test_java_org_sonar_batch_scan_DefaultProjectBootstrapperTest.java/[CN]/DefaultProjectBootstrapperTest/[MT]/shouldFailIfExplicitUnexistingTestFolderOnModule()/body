{
  thrown.expect(IllegalStateException.class);
  thrown.expectMessage("The folder 'tests' does not exist for 'module1' (base directory = " + TestUtils.getResource(this.getClass(),"multi-module-with-explicit-unexisting-test-dir").getAbsolutePath() + File.separator+ "module1)");
