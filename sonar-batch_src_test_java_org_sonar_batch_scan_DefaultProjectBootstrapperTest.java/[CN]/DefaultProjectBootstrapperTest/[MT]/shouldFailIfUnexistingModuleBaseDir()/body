{
  thrown.expect(IllegalStateException.class);
  thrown.expectMessage("The base directory of the module 'module1' does not exist: " + TestUtils.getResource(this.getClass(),"multi-module-with-unexisting-basedir").getAbsolutePath() + File.separator+ "module1");
  loadProjectDefinition("multi-module-with-unexisting-basedir");
}
