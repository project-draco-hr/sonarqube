{
  Collection<Metric> metrics=decorator.usedMetrics();
  assertThat(metrics).containsOnly(CoreMetrics.LINES_TO_COVER,CoreMetrics.UNCOVERED_LINES,CoreMetrics.NEW_LINES_TO_COVER,CoreMetrics.NEW_UNCOVERED_LINES,CoreMetrics.CONDITIONS_TO_COVER,CoreMetrics.UNCOVERED_CONDITIONS,CoreMetrics.NEW_CONDITIONS_TO_COVER,CoreMetrics.NEW_UNCOVERED_CONDITIONS);
}
