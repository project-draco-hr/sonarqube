{
  String componentUuid=component.getUuid();
  return new SnapshotDto().setUuid(snapshotUuid).setVersion(setVersion ? component.getReportAttributes().getVersion() : null).setComponentUuid(componentUuid).setLast(false).setStatus(SnapshotDto.STATUS_UNPROCESSED).setCreatedAt(analysisDate).setBuildDate(system2.now());
}
