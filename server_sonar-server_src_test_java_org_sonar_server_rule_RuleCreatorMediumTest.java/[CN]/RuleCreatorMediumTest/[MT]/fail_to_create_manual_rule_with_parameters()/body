{
  try {
    NewRule.createForManualRule("MANUAL_RULE").setName("My manual").setHtmlDescription("Some description").setParameters(ImmutableMap.of("regex","a.*"));
    fail();
  }
 catch (  Exception e) {
    assertThat(e).isInstanceOf(IllegalStateException.class).hasMessage("Not a custom rule");
  }
}
