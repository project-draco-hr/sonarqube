{
  NewRule newRule=NewRule.createForManualRule("*INVALID*").setName("My custom").setHtmlDescription("Some description");
  try {
    creator.create(newRule);
    fail();
  }
 catch (  Exception e) {
    assertThat(e).isInstanceOf(BadRequestException.class).hasMessage("coding_rules.validation.invalid_rule_key");
  }
}
