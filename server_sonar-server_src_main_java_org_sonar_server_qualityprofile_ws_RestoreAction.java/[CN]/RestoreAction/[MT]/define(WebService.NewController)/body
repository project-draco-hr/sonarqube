{
  controller.createAction("restore").setSince("5.2").setDescription("Restore a quality profile using an XML file. The restored profile name is taken from the backup file, " + "so if a profile with the same name and language already exists, it will be overwritten.").setPost(true).setHandler(this).createParam(PARAM_BACKUP).setDescription("A profile backup file in XML format, as generated by api/qualityprofiles/backup " + "or the former api/profiles/backup.").setRequired(true);
}
