{
  QualityProfileDto profile=result.profile();
  if (profile != null) {
    String language=profile.getLanguage();
    json.beginObject().name("profile").beginObject().prop("key",profile.getKey()).prop("name",profile.getName()).prop("language",language).prop("languageName",languages.get(profile.getLanguage()).getName()).prop("isDefault",false).prop("isInherited",false).endObject();
  }
  json.prop("ruleSuccesses",result.countSucceeded());
  json.prop("ruleFailures",result.countFailed());
  json.endObject().close();
}
