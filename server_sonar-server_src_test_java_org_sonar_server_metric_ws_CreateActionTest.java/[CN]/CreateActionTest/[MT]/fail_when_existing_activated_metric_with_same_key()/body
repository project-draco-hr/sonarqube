{
  expectedException.expect(ServerException.class);
  dbClient.metricDao().insert(dbSession,MetricTesting.newMetricDto().setKey(DEFAULT_KEY).setValueType(DEFAULT_TYPE).setUserManaged(true).setEnabled(true));
