{
  WsTester.Result result=newRequest().setParam(PARAM_KEY,DEFAULT_KEY).setParam(PARAM_NAME,DEFAULT_NAME).setParam(PARAM_TYPE,DEFAULT_TYPE).setParam(PARAM_DOMAIN,DEFAULT_DOMAIN).setParam(PARAM_DESCRIPTION,DEFAULT_DESCRIPTION).execute();
  result.assertJson(getClass(),"metric.json");
  result.outputAsString().matches("\"id\"\\s*:\\s*\"\\w+\"");
}
