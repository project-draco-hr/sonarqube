{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  DebtRemediationFunction that=(DebtRemediationFunction)o;
  return new EqualsBuilder().append(type,that.type()).append(coefficient,that.coefficient()).append(offset,that.offset()).isEquals();
}
