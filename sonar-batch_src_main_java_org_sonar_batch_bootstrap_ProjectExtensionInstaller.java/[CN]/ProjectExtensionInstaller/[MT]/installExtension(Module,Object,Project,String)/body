{
  if (ExtensionUtils.isBatchExtension(extension) && ExtensionUtils.isSupportedEnvironment(extension,environment) && ExtensionUtils.isInstantiationStrategy(extension,InstanciationStrategy.PER_PROJECT)&& !isDeactivatedCoverageExtension(extension,project,pluginKey)) {
    module.addComponent(extension);
    return extension;
  }
  return null;
}
