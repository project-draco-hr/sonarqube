{
  if (!verifyDepth(component)) {
    return;
  }
  stack.add(new PathElementImpl<>(component,createForComponent(component)));
  if (this.visitor.getOrder() == PRE_ORDER) {
    visitNode(component);
  }
  visitChildren(component);
  if (this.visitor.getOrder() == POST_ORDER) {
    visitNode(component);
  }
  stack.pop();
}
