{
  visitAny(component,stack);
switch (component.getType()) {
case PROJECT:
    visitProject(component,stack);
  break;
case MODULE:
visitModule(component,stack);
break;
case DIRECTORY:
visitDirectory(component,stack);
break;
case FILE:
visitFile(component,stack);
break;
default :
visitUnknown(component,stack);
}
}
