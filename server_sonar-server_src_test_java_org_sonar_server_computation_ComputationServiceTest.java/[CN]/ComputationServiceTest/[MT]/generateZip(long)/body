{
  File dir=tempFolder.newDir();
  BatchReportWriter writer=new BatchReportWriter(dir);
  writer.writeMetadata(BatchReport.Metadata.newBuilder().setRootComponentRef(1).setProjectKey("PROJECT_KEY").setAnalysisDate(150000000L).setSnapshotId(snapshotId).build());
  File zip=tempFolder.newFile();
  ZipUtils.zipDir(dir,zip);
  return zip;
}
