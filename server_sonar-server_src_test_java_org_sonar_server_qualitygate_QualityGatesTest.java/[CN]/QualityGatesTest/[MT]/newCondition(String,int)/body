{
  return new QualityGateConditionDto().setId(RandomUtils.nextLong()).setMetricKey(metricKey).setMetricId(metricId).setQualityGateId(QUALITY_GATE_ID).setOperator("GT").setWarningThreshold(RandomStringUtils.randomAlphanumeric(15)).setErrorThreshold(RandomStringUtils.randomAlphanumeric(15)).setPeriod(RandomUtils.nextBoolean() ? 1 : null);
}
