{
  if (bytecodeFilesOrDirectories == null) {
    return false;
  }
  for (  File bytecodeFilesOrDirectory : bytecodeFilesOrDirectories) {
    if (bytecodeFilesOrDirectory.exists() && (bytecodeFilesOrDirectory.isFile() || !FileUtils.listFiles(bytecodeFilesOrDirectory,new String[]{"class"},true).isEmpty())) {
      return true;
    }
  }
  return false;
}
