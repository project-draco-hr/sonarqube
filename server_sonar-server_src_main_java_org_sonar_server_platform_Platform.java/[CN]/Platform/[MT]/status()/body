{
  if (!started) {
    return Status.BOOTING;
  }
  PlatformLevel current=this.currentLevel;
  PlatformLevel levelSafe=this.levelSafeMode;
  if (levelSafe != null && current == levelSafe) {
    return Status.SAFEMODE;
  }
  if (current == level4) {
    return Status.UP;
  }
  return Status.BOOTING;
}
