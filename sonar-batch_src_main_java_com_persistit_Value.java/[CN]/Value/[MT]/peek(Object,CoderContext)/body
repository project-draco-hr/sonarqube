{
  final Object object;
  final int saveDepth=_depth;
  final int saveLevel=_level;
  final int saveNext=_next;
  final int saveEnd=_end;
  try {
    object=get(target,context);
  }
  finally {
    _end=saveEnd;
    _next=saveNext;
    _level=saveLevel;
    _depth=saveDepth;
  }
  return object;
}
