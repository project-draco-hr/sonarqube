{
  if (secure) {
    Util.clearBytes(_bytes,0,_bytes.length);
    if (_longBytes != null) {
      Util.clearBytes(_longBytes,0,_longBytes.length);
    }
    _longSize=0;
    if (_stringAssemblyBufferSoftRef != null) {
