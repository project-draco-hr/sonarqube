{
  int result=base & 0x0F;
  base&=0x30;
switch (base) {
case BASE5:
    result=result << 8 | (_bytes[_next++] & 0xFF);
  result=result << 8 | (_bytes[_next++] & 0xFF);
case BASE3:
result=result << 8 | (_bytes[_next++] & 0xFF);
case BASE2:
result=result << 8 | (_bytes[_next++] & 0xFF);
case BASE1:
break;
}
return result;
}
