{
  permissionUpdater=mock(PermissionUpdater.class);
  dbClient=db.getDbClient();
  dbSession=db.getSession();
  ComponentFinder componentFinder=new ComponentFinder(dbClient);
  ws=new WsTester(new PermissionsWs(new AddUserAction(dbClient,permissionUpdater,new PermissionChangeBuilder(new PermissionDependenciesFinder(dbClient,componentFinder,new UserGroupFinder(dbClient),resourceTypes)))));
  userSession.login("admin").setGlobalPermissions(SYSTEM_ADMIN);
}
