{
  if (tagsBuilder_ == null) {
    if (((bitField0_ & 0x00004000) == 0x00004000) && tags_ != null && tags_ != org.sonarqube.ws.Rules.Tags.getDefaultInstance()) {
      tags_=org.sonarqube.ws.Rules.Tags.newBuilder(tags_).mergeFrom(value).buildPartial();
    }
