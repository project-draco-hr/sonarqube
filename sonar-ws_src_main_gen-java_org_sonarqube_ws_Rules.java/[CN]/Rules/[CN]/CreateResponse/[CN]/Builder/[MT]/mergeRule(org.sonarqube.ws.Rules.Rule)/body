{
  if (ruleBuilder_ == null) {
    if (((bitField0_ & 0x00000001) == 0x00000001) && rule_ != null && rule_ != org.sonarqube.ws.Rules.Rule.getDefaultInstance()) {
      rule_=org.sonarqube.ws.Rules.Rule.newBuilder(rule_).mergeFrom(value).buildPartial();
    }
 else {
      rule_=value;
    }
    onChanged();
  }
 else {
    ruleBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00000001;
  return this;
}
