{
  when(system2.now()).thenReturn((long)uuid.hashCode());
  OrganizationDto dto=new OrganizationDto().setUuid(uuid).setKey(uuid + "_key").setName(uuid + "_name").setDescription(uuid + "_description").setUrl(uuid + "_url").setAvatarUrl(uuid + "_avatar_url");
  dbTester.getDbClient().organizationDao().insert(dbTester.getSession(),dto);
  dbTester.commit();
  return dto;
}
