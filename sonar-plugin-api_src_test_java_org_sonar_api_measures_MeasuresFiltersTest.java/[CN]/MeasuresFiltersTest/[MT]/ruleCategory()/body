{
  MeasuresFilter<RuleMeasure> filter=MeasuresFilters.ruleCategory(CoreMetrics.VIOLATIONS,2);
  List<Measure> measures=Arrays.asList(RuleMeasure.createForPriority(CoreMetrics.VIOLATIONS,RulePriority.CRITICAL,50.0),RuleMeasure.createForCategory(CoreMetrics.VIOLATIONS,2,10.0),RuleMeasure.createForCategory(CoreMetrics.VIOLATIONS,3,15.0),new Measure(CoreMetrics.VIOLATIONS,500.0));
  assertThat(filter.filter(measures).getValue(),is(10.0));
}
