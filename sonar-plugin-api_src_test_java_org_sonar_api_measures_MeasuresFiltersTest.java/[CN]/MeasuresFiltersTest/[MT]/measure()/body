{
  MeasuresFilter<Measure> filter=MeasuresFilters.measure(new Measure(CoreMetrics.VIOLATIONS));
  List<Measure> measures=Arrays.asList(new Measure(CoreMetrics.COMMENT_LINES,50.0),new Measure(CoreMetrics.VIOLATIONS,10.0),RuleMeasure.createForCategory(CoreMetrics.VIOLATIONS,2,12.0),new Measure(CoreMetrics.COVERAGE,15.0));
  assertThat(filter.filter(measures).getValue(),is(10.0));
}
