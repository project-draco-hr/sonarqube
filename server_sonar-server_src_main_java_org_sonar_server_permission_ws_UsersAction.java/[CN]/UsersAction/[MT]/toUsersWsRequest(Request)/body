{
  return new UsersWsRequest().setPermission(request.mandatoryParam(PARAM_PERMISSION)).setProjectId(request.param(PARAM_PROJECT_ID)).setProjectKey(request.param(PARAM_PROJECT_KEY)).setQuery(request.param(Param.TEXT_QUERY)).setPage(request.mandatoryParamAsInt(Param.PAGE)).setPageSize(request.mandatoryParamAsInt(Param.PAGE_SIZE));
}
