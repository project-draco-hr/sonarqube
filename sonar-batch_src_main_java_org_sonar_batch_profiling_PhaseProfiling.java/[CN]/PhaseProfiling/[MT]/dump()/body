{
  double percent=this.totalTime() / 100.0;
  for (  ItemProfiling itemProfiling : truncate(sortByDescendingTotalTime(profilingPerItem.values()))) {
    StringBuilder sb=new StringBuilder();
    sb.append("   o ").append(itemProfiling.itemName()).append(": ").append(itemProfiling.totalTimeAsString()).append(" (").append((int)(itemProfiling.totalTime() / percent)).append("%)");
    println(sb.toString());
  }
}
