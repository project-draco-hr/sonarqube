{
  int resourceSnapshotId=getSnapshotIdFor(resource);
  for (  Block block : blocks) {
    CloneBlock dbBlock=new CloneBlock(currentProjectSnapshotId,resourceSnapshotId,block.getBlockHash().toString(),block.getIndexInFile(),block.getFirstLineNumber(),block.getLastLineNumber());
    session.save(dbBlock);
  }
  session.commit();
}
