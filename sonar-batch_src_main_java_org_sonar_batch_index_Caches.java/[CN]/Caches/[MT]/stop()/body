{
  if (persistit != null) {
    try {
      persistit.close(false);
      persistit=null;
      volume=null;
    }
 catch (    PersistitException e) {
      throw new IllegalStateException("Fail to close caches",e);
    }
  }
  FileUtils.deleteQuietly(tempDir);
  tempDir=null;
  cacheNames.clear();
}
