{
  ruleTags.putAllTags(ImmutableList.of(new RuleTagDto().setTag("tag1"),new RuleTagDto().setTag("tag2"),new RuleTagDto().setTag("tag3"),new RuleTagDto().setTag("tag4"),new RuleTagDto().setTag("tag5")));
  assertThat(esSetup.client().prepareCount("rules").setTypes(ESRuleTags.TYPE_TAG).execute().actionGet().getCount()).isEqualTo(5L);
}
