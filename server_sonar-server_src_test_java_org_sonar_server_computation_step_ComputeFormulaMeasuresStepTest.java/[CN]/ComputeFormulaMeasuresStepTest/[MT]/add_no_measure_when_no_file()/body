{
  DumbComponent project=DumbComponent.builder(PROJECT,1).addChildren(DumbComponent.builder(MODULE,11).addChildren(DumbComponent.builder(DIRECTORY,111).build()).build()).build();
  treeRootHolder.setRoot(project);
  sut.execute();
  assertThat(measureRepository.getNewRawMeasures(1)).isEmpty();
  assertThat(measureRepository.getNewRawMeasures(11)).isEmpty();
  assertThat(measureRepository.getNewRawMeasures(111)).isEmpty();
}
