{
  ComponentDto project=ComponentTesting.newProjectDto("ghost-uuid-" + id).setName("ghost-name-" + id).setKey("ghost-key-" + id);
  dbClient.componentDao().insert(dbSession,project);
  SnapshotDto snapshot=SnapshotTesting.createForProject(project).setStatus(SnapshotDto.STATUS_UNPROCESSED);
  dbClient.snapshotDao().insert(dbSession,snapshot);
  dbSession.commit();
}
