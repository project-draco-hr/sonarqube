{
  PermissionTemplateUserDto permissionTemplateUser=new PermissionTemplateUserDto().setTemplateId(templateId).setPermission(permission).setUserId(userId);
  SqlSession session=myBatis.openSession(false);
  try {
    PermissionTemplateMapper mapper=session.getMapper(PermissionTemplateMapper.class);
    mapper.deleteUserPermission(permissionTemplateUser);
    session.commit();
  }
  finally {
    MyBatis.closeQuietly(session);
  }
}
