{
  RuleDto rule=RuleTesting.newXooX1();
  tester.get(RuleDao.class).insert(dbSession,rule);
  dbSession.commit();
  GetResponse response=searchClient.prepareGet().setIndex(IndexDefinition.RULE.getIndexName()).setType(IndexDefinition.RULE.getIndexType()).setId(rule.getKey().toString()).get();
  assertThat(response.getSource()).isNotEmpty();
}
