{
  LoggerFactory.getLogger(App.class).info("Stopping");
  if (monitor != null && monitor.isAlive()) {
    monitor.terminate();
    monitor.interrupt();
    monitor=null;
  }
  if (server != null) {
    server.terminate();
    server=null;
  }
  if (elasticsearch != null) {
    elasticsearch.terminate();
    elasticsearch=null;
  }
}
