{
  WebService.NewAction deactivate=controller.createAction(DEACTIVATE_ACTION).setDescription("Deactivate a rule on a Quality profile").setHandler(new RequestHandler(){
    @Override public void handle(    Request request,    Response response) throws Exception {
      deactivate(request,response);
    }
  }
).setPost(true).setSince("4.4");
  defineActiveRuleKeyParameters(deactivate);
}
