{
  db.prepareDbUnit(getClass(),"persons.xml");
  new BaseDataChange(db.database()){
    @Override public void execute(    Context context) throws SQLException {
      context.prepareUpsert("insert into persons(id,login,age,enabled) values (?,?,?,?)").setLong(1,10L).setString(2,"kurt").setInt(3,27).setBoolean(4,true).execute().commit().close();
    }
  }
.execute();
  db.assertDbUnit(getClass(),"insert-result.xml","persons");
}
