{
  InputStream responseStream=ws.newRequest().setMediaType(MimeTypes.PROTOBUF).setParam(PARAM_PERMISSION,UserRole.USER).setParam(PARAM_TEMPLATE_NAME,template1.getName()).setParam(PAGE,"2").setParam(PAGE_SIZE,"1").execute().getInputStream();
  WsGroupsResponse response=WsGroupsResponse.parseFrom(responseStream);
  assertThat(response.getGroupsList()).extracting("name").containsExactly("group-1-name");
}
