{
  logAsSysAdminUser();
  PermissionTemplateDto template1=dbClient.permissionTemplateDao().insert(dbSession,newPermissionTemplateDto().setUuid("template-uuid-1"));
  expectedException.expect(BadRequestException.class);
  ws.newRequest().setParam(PARAM_PERMISSION,USER).setParam(PARAM_TEMPLATE_ID,template1.getUuid()).setParam(PARAM_TEMPLATE_NAME,template1.getName()).execute();
