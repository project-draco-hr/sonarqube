{
  InputStream responseStream=ws.newRequest().setMediaType(MediaTypes.PROTOBUF).setParam(PARAM_PERMISSION,UserRole.ADMIN).setParam(PARAM_TEMPLATE_ID,template1.getUuid()).setParam(Param.SELECTED,SelectionMode.ALL.value()).execute().getInputStream();
  WsGroupsResponse response=WsGroupsResponse.parseFrom(responseStream);
  assertThat(response.getGroupsList()).extracting("name").containsExactly("group-1-name","group-2-name","group-3-name");
}
