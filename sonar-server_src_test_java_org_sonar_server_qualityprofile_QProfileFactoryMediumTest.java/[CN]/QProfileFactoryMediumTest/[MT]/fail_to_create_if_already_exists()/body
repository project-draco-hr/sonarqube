{
  QProfileName name=new QProfileName("xoo","P1");
  factory.create(dbSession,name);
  dbSession.commit();
  dbSession.clearCache();
  try {
    factory.create(dbSession,name);
    fail();
  }
 catch (  BadRequestException e) {
    assertThat(e).hasMessage("Quality profile already exists: QProfile{lang=xoo, name=P1}");
  }
}
