{
  StringBuilder builder=new StringBuilder();
  builder.append("(");
  for (int i=0; i < filters.size(); i++) {
    if (i > 0) {
      builder.append(" ");
      builder.append(getOperator());
      builder.append(" ");
    }
    builder.append(filters.get(i));
  }
  builder.append(")");
  return builder.toString();
}
