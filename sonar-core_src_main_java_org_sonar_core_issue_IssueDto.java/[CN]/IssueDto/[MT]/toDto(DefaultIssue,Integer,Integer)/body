{
  return new IssueDto().setKey(issue.key()).setLine(issue.line()).setTitle(issue.title()).setDescription(issue.description()).setCost(issue.cost()).setResolution(issue.resolution()).setStatus(issue.status()).setSeverity(issue.severity()).setChecksum(issue.getChecksum()).setManualIssue(issue.isManual()).setManualSeverity(issue.isManualSeverity()).setUserLogin(issue.userLogin()).setAssignee(issue.assignee()).setCreatedAt(issue.createdAt()).setUpdatedAt(issue.updatedAt()).setClosedAt(issue.closedAt()).setRuleId(ruleId).setResourceId(componentId).setAttributes(issue.attributes() != null ? KeyValueFormat.format(issue.attributes()) : "").setAuthorLogin(issue.authorLogin());
}
