{
  IssueChange change=IssueChange.create().comment("this is a comment").assignee("lancelot").attribute("JIRA","FOO-1234").attribute("LINK",null).cost(4.2).transition("resolve").severity("BLOCKER");
  assertThat(change.urlParams()).hasSize(7).includes(entry("comment","this is a comment"),entry("newAssignee","lancelot"),entry("newAttr[JIRA]","FOO-1234"),entry("newAttr[LINK]",""),entry("newCost",4.2),entry("transition","resolve"),entry("newSeverity","BLOCKER"));
}
