{
  Project project=new Project("foo");
  AbstractCoverageExtension extension=new AbstractCoverageExtension(){
  }
;
  project.setAnalysisType(Project.AnalysisType.DYNAMIC);
  assertThat(extension.shouldExecuteOnProject(project)).isTrue();
  project.setAnalysisType(Project.AnalysisType.REUSE_REPORTS);
  assertThat(extension.shouldExecuteOnProject(project)).isTrue();
  project.setAnalysisType(Project.AnalysisType.STATIC);
  assertThat(extension.shouldExecuteOnProject(project)).isFalse();
}
