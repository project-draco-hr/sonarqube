{
  for (  Map.Entry<String,String> entry : requiredJavaOptions.entrySet()) {
    String value=System.getProperty(entry.getKey());
    if (!StringUtils.equals(value,entry.getValue())) {
      throw new MessageException(String.format("JVM option '%s' must be set to '%s'. Got '%s'",entry.getKey(),entry.getValue(),StringUtils.defaultString(value)));
    }
  }
}
