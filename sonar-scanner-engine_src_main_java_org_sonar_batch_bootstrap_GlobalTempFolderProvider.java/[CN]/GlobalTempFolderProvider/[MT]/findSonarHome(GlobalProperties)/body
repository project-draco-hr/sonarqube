{
  String home=props.property("sonar.userHome");
  if (home != null) {
    return Paths.get(home).toAbsolutePath();
  }
  home=system.envVariable("SONAR_USER_HOME");
  if (home != null) {
    return Paths.get(home).toAbsolutePath();
  }
  home=system.property("user.home");
  return Paths.get(home,".sonar").toAbsolutePath();
}
