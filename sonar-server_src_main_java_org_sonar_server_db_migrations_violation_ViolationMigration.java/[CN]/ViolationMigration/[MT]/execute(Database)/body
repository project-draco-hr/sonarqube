{
  try {
    truncateIssueTables(db);
    migrate(db);
  }
 catch (  SQLException e) {
    logger.error("Fail to convert violations to issues",e);
    SqlUtil.log(logger,e);
    throw MessageException.of("Fail to convert violations to issues");
  }
catch (  Exception e) {
    logger.error("Fail to convert violations to issues",e);
    throw MessageException.of("Fail to convert violations to issues");
  }
}
