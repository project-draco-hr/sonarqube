{
  String projectUuid=report.getProject().uuid();
  if (index.get(IssueAuthorizationIndex.class).getNullableByKey(projectUuid) == null) {
    dbClient.issueAuthorizationDao().synchronizeAfter(session,null,ImmutableMap.of(IssueAuthorizationDao.PROJECT_UUID,projectUuid));
    session.commit();
  }
}
