{
  String key=getSonarKey(pom);
  definition.setProperties(pom.getModel().getProperties()).setKey(key).setVersion(pom.getVersion()).setName(pom.getName()).setDescription(pom.getDescription()).addContainerExtension(pom);
  guessJavaVersion(pom,definition);
  guessEncoding(pom,definition);
  convertMavenLinksToProperties(definition,pom);
  synchronizeFileSystem(pom,definition);
}
