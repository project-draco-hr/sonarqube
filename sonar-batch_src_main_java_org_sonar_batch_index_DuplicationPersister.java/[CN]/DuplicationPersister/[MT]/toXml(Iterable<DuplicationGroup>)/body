{
  StringBuilder xml=new StringBuilder();
  xml.append("<duplications>");
  for (  DuplicationGroup duplication : duplications) {
    xml.append("<g>");
    toXml(xml,duplication.originBlock());
    for (    DuplicationGroup.Block part : duplication.duplicates()) {
      toXml(xml,part);
    }
    xml.append("</g>");
  }
  xml.append("</duplications>");
  return xml.toString();
}
