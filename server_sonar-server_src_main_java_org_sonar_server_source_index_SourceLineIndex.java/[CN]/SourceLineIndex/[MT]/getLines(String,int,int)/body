{
  Preconditions.checkArgument(from > 0,"Minimum value for 'from' is 1");
  Preconditions.checkArgument(to >= from,"'to' must be larger than or equal to 'from'");
  List<SourceLineDoc> lines=Lists.newArrayList();
  for (  SearchHit hit : esClient.prepareSearch(SourceLineIndexDefinition.INDEX_SOURCE_LINES).setTypes(SourceLineIndexDefinition.TYPE_SOURCE_LINE).setSize(1 + to - from).setQuery(QueryBuilders.boolQuery().must(QueryBuilders.termQuery(SourceLineIndexDefinition.FIELD_FILE_UUID,fileUuid)).must(QueryBuilders.rangeQuery(SourceLineIndexDefinition.FIELD_LINE).gte(from).lte(to))).addSort(SourceLineIndexDefinition.FIELD_LINE,SortOrder.ASC).get().getHits().getHits()) {
    lines.add(new SourceLineDoc(hit.sourceAsMap()));
  }
  return lines;
}
