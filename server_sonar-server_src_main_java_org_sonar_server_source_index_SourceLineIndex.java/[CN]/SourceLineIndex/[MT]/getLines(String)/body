{
  List<SourceLineDoc> lines=Lists.newArrayList();
  for (  SearchHit hit : getClient().prepareSearch(SourceLineIndexDefinition.INDEX).setTypes(SourceLineIndexDefinition.TYPE).setQuery(QueryBuilders.boolQuery().must(QueryBuilders.termQuery(SourceLineIndexDefinition.FIELD_FILE_UUID,fileUuid))).addSort(SourceLineIndexDefinition.FIELD_LINE,SortOrder.ASC).get().getHits().getHits()) {
    lines.add(new SourceLineDoc(hit.sourceAsMap()));
  }
  return lines;
}
