{
  setupData("shared","measures");
  FilterExecutor executor=new FilterExecutor(getSession());
  Filter filter=new Filter().setQualifiers(Sets.newHashSet(Qualifiers.CLASS)).addMeasureCriterion(new MeasureCriterion(2,">",5.0,false)).addMeasureCriterion(new MeasureCriterion(1,">",5.0,false)).setSortedMetricId(2,true,false).setAscendingSort(false);
  FilterResult result=executor.execute(filter);
  assertSnapshotIds(result,5,6);
}
