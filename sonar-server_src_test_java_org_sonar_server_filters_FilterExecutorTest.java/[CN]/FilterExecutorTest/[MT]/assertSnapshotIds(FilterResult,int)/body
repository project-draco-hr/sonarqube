{
  assertThat(result.size(),is(snapshotIds.length));
  for (  int snapshotId : snapshotIds) {
    boolean found=false;
    for (    Object row : result.getRows()) {
      found|=result.getSnapshotId(row) == snapshotId;
    }
    if (!found) {
      fail("Snapshot id not found in results: " + snapshotId);
    }
  }
}
