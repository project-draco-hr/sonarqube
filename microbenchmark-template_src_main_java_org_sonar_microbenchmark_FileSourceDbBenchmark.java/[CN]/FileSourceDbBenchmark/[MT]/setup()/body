{
  FileSourceDb.Data.Builder builder=FileSourceDb.Data.newBuilder();
  for (int i=0; i < linesNumber; i++) {
    FileSourceDb.Line.Builder lineBuilder=builder.addLinesBuilder();
    lines.add(lineBuilder.setLine(i + 1).setScmAuthor("charlie").setScmRevision("ABCDE").setScmDate(15000000000L).setUtLineHits(5).setUtConditions(2).setUtCoveredConditions(1).setSource(RandomStringUtils.randomAlphanumeric(10)).setHighlighting(RandomStringUtils.randomAlphanumeric(20)).setSymbols(RandomStringUtils.randomAlphanumeric(20)).addAllDuplications(Arrays.asList(12,13,15)).build());
  }
  data=builder.build();
}
