{
  NewAction action=controller.createAction("list").setDescription("List supported programming languages").setHandler(this).setResponseExample(Resources.getResource(getClass(),"example-list.json"));
  action.createParam("q").setDescription("A pattern to match language keys/names against").setExampleValue("java");
  action.createParam("ps").setDescription("The size of the list to return, 0 for all languages").setExampleValue("25").setDefaultValue("0");
}
