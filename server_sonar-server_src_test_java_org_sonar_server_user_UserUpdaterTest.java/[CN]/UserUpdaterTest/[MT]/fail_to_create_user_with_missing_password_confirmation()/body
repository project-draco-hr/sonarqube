{
  try {
    userUpdater.create(NewUser.create().setLogin(DEFAULT_LOGIN).setName("Marius").setEmail("marius@mail.com").setPassword("password").setPasswordConfirmation(null));
    fail();
  }
 catch (  BadRequestException e) {
    assertThat(e.errors().messages()).contains(Message.of(Validation.CANT_BE_EMPTY_MESSAGE,"Password confirmation"));
  }
}
