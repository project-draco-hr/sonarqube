{
  try {
    userUpdater.create(NewUser.create().setLogin(null).setName("Marius").setEmail("marius@mail.com").setPassword("password").setPasswordConfirmation("password"));
    fail();
  }
 catch (  BadRequestException e) {
    assertThat(e.errors().messages()).containsOnly(Message.of(Validation.CANT_BE_EMPTY_MESSAGE,"Login"));
  }
}
