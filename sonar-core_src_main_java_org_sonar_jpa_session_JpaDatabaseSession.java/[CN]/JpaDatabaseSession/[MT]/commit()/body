{
  if (entityManager != null && inTransaction) {
    if (entityManager.isOpen()) {
      if (entityManager.getTransaction().getRollbackOnly()) {
        entityManager.getTransaction().rollback();
      }
 else {
        entityManager.getTransaction().commit();
      }
      entityManager.clear();
      index=0;
    }
    inTransaction=false;
  }
}
