{
  InputStreamReader reader=null;
  try {
    reader=new InputStreamReader(new FileInputStream(file),charset);
    return chunk(reader);
  }
 catch (  Exception e) {
    throw new DuplicationsException("Unable to lex file : " + file.getAbsolutePath(),e);
  }
 finally {
    IOUtils.closeQuietly(reader);
  }
}
