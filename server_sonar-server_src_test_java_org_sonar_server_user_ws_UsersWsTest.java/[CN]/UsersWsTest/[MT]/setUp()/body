{
  WsTester tester=new WsTester(new UsersWs(new CreateAction(mock(UserIndex.class),mock(UserUpdater.class),mock(I18n.class),userSessionRule),new UpdateAction(mock(UserIndex.class),mock(UserUpdater.class),userSessionRule),new CurrentUserAction(userSessionRule),new DeactivateAction(mock(UserIndex.class),mock(UserUpdater.class),userSessionRule),new ChangePasswordAction(mock(UserUpdater.class),userSessionRule)));
