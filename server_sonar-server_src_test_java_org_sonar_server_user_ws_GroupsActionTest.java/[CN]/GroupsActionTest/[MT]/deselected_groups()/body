{
  UserDto user=createUser();
  GroupDto usersGroup=createGroup("sonar-users","Sonar Users");
  createGroup("sonar-admins","Sonar Admins");
  addUserToGroup(user,usersGroup);
  session.commit();
  tester.newGetRequest("api/users","groups").setParam("login","john").setParam(Param.SELECTED,SelectionMode.DESELECTED.value()).execute().assertJson(getClass(),"deselected.json");
}
