{
  adminIssueClient().doTransition(issue.key(),"resolve");
  adminIssueClient().doAction(issue.key(),"fake");
  Assertions.assertThat(adminIssueClient().actions(issue.key())).doesNotContain("fake");
  projectAnalysis.run();
  Assertions.assertThat(adminIssueClient().actions(issue.key())).doesNotContain("fake");
}
