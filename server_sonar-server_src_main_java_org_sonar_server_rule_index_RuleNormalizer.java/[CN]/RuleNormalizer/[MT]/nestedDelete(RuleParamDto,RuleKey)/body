{
  return ImmutableList.of(new UpdateRequest().id(key.toString()).script(ProcessConstants.ES_PLUGIN_LISTUPDATE_SCRIPT_NAME).addScriptParam(ProcessConstants.ES_PLUGIN_LISTUPDATE_FIELD,RuleField.PARAMS.field()).addScriptParam(ProcessConstants.ES_PLUGIN_LISTUPDATE_VALUE,null).addScriptParam(ProcessConstants.ES_PLUGIN_LISTUPDATE_ID_FIELD,RuleParamField.NAME.field()).addScriptParam(ProcessConstants.ES_PLUGIN_LISTUPDATE_ID_VALUE,param.getName()));
