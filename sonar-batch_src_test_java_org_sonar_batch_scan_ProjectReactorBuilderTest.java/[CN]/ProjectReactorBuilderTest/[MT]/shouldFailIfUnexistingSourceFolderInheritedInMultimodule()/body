{
  thrown.expect(IllegalStateException.class);
  thrown.expectMessage("The folder 'unexisting-source-dir' does not exist for 'com.foo.project:module1' (base directory = " + TestUtils.getResource(this.getClass(),"multi-module-with-unexisting-source-dir").getAbsolutePath() + File.separator+ "module1)");
  loadProjectDefinition("multi-module-with-unexisting-source-dir");
}
