{
  setDefaultSecretKey();
  underTest.start();
  String token=Jwts.builder().setId("123").setIssuer("sonarqube").setIssuedAt(new Date(system2.now())).setExpiration(new Date(system2.now() + 20 * 60 * 1000)).signWith(SignatureAlgorithm.HS256,decodeSecretKey(SECRET_KEY)).compact();
  expectedException.expect(UnauthorizedException.class);
  expectedException.expectMessage("Token subject hasn't been found");
  underTest.decode(token);
}
