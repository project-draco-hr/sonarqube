{
  setDefaultSecretKey();
  underTest.start();
  String token=Jwts.builder().setId("123").setSubject(USER_LOGIN).setExpiration(new Date(system2.now() + 20 * 60 * 1000)).signWith(SignatureAlgorithm.HS256,decodeSecretKey(SECRET_KEY)).compact();
  expectedException.expect(UnauthorizedException.class);
  expectedException.expectMessage("Token creation date hasn't been found");
  underTest.decode(token);
}
