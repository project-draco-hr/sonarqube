{
  try {
    tester.get(QProfileBackuper.class).restore(new StringReader(Resources.toString(getClass().getResource("QProfileBackuperMediumTest/not-xml-backup.txt"),Charsets.UTF_8)));
    fail();
  }
 catch (  IllegalStateException e) {
    assertThat(e).hasMessage("Fail to restore Quality profile backup");
    assertThat(e.getCause()).isInstanceOf(XMLStreamException.class);
  }
}
