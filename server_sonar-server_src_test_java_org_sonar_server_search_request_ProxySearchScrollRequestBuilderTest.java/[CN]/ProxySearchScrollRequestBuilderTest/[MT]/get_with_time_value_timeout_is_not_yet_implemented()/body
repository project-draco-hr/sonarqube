{
  SearchResponse search=searchClient.prepareSearch(IndexDefinition.RULE.getIndexName()).setSearchType(SearchType.SCAN).setScroll(TimeValue.timeValueSeconds(3L)).get();
  try {
    searchClient.prepareSearchScroll(search.getScrollId()).get(TimeValue.timeValueMinutes(1));
