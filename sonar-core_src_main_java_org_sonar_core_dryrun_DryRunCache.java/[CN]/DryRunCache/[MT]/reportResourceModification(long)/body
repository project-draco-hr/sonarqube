{
  FileUtils.deleteQuietly(getCacheLocation(projectId));
  ResourceDto rootProject=resourceDao.getRootProjectByComponentId(projectId);
  propertiesDao.setProperty(new PropertyDto().setKey(SONAR_DRY_RUN_CACHE_LAST_UPDATE_KEY).setResourceId(rootProject.getId()).setValue(String.valueOf(System.nanoTime())));
}
