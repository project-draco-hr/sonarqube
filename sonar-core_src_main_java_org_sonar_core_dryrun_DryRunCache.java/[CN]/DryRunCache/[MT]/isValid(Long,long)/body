{
  long globalTimestamp=getModificationTimestamp(null);
  if (globalTimestamp > lastTimestampInCache) {
    return false;
  }
  if (projectId != null) {
    long projectTimestamp=getModificationTimestamp(projectId);
    if (projectTimestamp > lastTimestampInCache) {
      return false;
    }
  }
  return true;
}
