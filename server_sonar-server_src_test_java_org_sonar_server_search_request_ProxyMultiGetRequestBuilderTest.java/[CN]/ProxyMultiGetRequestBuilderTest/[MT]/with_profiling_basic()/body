{
  Profiling profiling=new Profiling(new Settings().setProperty(Profiling.CONFIG_PROFILING_LEVEL,Profiling.Level.BASIC.name()));
  SearchClient searchClient=new SearchClient(new Settings(),profiling);
  try {
    MultiGetRequestBuilder request=searchClient.prepareMultiGet();
    request.add(new MultiGetRequest.Item(IndexDefinition.RULE.getIndexName(),IndexDefinition.RULE.getIndexType(),"ruleKey").fetchSourceContext(FetchSourceContext.FETCH_SOURCE));
    request.get();
    fail();
  }
 catch (  Exception e) {
    assertThat(e).isInstanceOf(IllegalStateException.class);
    assertThat(e.getMessage()).contains("Fail to execute ES multi get request");
  }
}
