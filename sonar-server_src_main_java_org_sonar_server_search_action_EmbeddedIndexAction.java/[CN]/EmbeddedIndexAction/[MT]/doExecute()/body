{
  try {
    if (this.getMethod().equals(Method.DELETE)) {
      index.delete(this.key,this.item,this.items);
    }
 else     if (this.getMethod().equals(Method.UPSERT)) {
      index.upsert(this.key,this.item,this.items);
    }
  }
 catch (  Exception e) {
    throw new IllegalStateException(this.getClass().getSimpleName() + "cannot execute " + this.getMethod()+ " for "+ this.item.getClass().getSimpleName()+ " as "+ this.getIndexType()+ " on key: "+ this.key,e);
  }
}
