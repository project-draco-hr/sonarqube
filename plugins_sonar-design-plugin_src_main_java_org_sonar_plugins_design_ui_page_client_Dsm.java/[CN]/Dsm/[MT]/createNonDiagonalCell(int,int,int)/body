{
  Label cell;
  cell=buildCell(row,col,weight,(col > row ? GRID_CELL_TOP_RIGHT : GRID_CELL_BOTTOM_LEFT));
  if (weight > 0) {
    String tooltip=data.get(col).getName() + " -> " + data.get(row).getName()+ " ("+ weight+ "). "+ I18nConstants.INSTANCE.cellTooltip();
    cell.setTitle(tooltip);
  }
  return cell;
}
