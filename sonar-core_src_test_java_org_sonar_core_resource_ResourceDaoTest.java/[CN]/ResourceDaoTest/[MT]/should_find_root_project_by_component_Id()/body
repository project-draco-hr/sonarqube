{
  setupData("fixture");
  ResourceDto resource=dao.getRootProjectByComponentId(4l);
  assertThat(resource.getName()).isEqualTo("Struts");
  resource=dao.getRootProjectByComponentId(3l);
  assertThat(resource.getName()).isEqualTo("Struts");
  resource=dao.getRootProjectByComponentId(2l);
  assertThat(resource.getName()).isEqualTo("Struts");
}
