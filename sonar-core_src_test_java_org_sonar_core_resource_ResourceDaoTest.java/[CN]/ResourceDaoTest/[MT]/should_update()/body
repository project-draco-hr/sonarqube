{
  setupData("update");
  ResourceDto project=new ResourceDto().setKey("org.struts:struts").setScope(Scopes.PROJECT).setQualifier(Qualifiers.PROJECT).setName("Struts").setLongName("Apache Struts").setLanguage("java").setDescription("MVC Framework").setId(1L);
  dao.insertOrUpdate(project);
  assertThat(project.getId()).isNotNull();
  checkTables("update","projects");
}
