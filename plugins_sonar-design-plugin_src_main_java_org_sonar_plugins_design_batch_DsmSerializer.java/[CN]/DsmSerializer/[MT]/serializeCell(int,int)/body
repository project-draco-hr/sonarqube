{
  DsmCell cell=dsm.getCell(x,y);
  json.append('{');
  if (cell.getEdge() != null && cell.getWeight() > 0) {
    Dependency dep=(Dependency)cell.getEdge();
    json.append("\"i\":");
    json.append(dep.getId());
    json.append(",\"w\":");
    json.append(cell.getWeight());
  }
  json.append('}');
}
