{
  return new ComponentUpdateDto().setUuid(from.uuid()).setBChanged(false).setBCopyComponentUuid(from.getCopyResourceUuid()).setBDescription(from.description()).setBEnabled(from.isEnabled()).setBLanguage(from.language()).setBLongName(from.longName()).setBModuleUuid(from.moduleUuid()).setBModuleUuidPath(from.moduleUuidPath()).setBName(from.name()).setBPath(from.path()).setBQualifier(from.qualifier());
}
