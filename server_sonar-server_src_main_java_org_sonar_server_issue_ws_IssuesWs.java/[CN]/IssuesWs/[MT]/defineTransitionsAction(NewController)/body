{
  WebService.NewAction action=controller.createAction(TRANSITIONS_ACTION).setDescription("Get Possible Workflow Transitions for an Issue. Requires Browse permission on project").setSince("3.6").setHandler(RailsHandler.INSTANCE).setResponseExample(Resources.getResource(IssuesWs.class,"example-transitions.json"));
  action.createParam("issue").setDescription("Key of the issue").setRequired(true).setExampleValue("5bccd6e8-f525-43a2-8d76-fcb13dde79ef");
}
