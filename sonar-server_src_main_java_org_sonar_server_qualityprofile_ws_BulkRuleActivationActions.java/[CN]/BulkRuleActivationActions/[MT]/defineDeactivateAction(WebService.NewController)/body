{
  WebService.NewAction deactivate=controller.createAction("deactivate_rules").setDescription("Bulk deactivate rules on Quality profiles").setPost(true).setSince("4.4").setHandler(new RequestHandler(){
    @Override public void handle(    Request request,    Response response) throws Exception {
      bulkDeactivate(request,response);
    }
  }
);
  defineProfileKeyParameters(deactivate);
}
