{
  Collection<Violation> violations=Lists.newArrayList();
  Rule rule=Rule.create("checkstyle","foo","Foo");
  violations.add(new Violation(rule));
  Assert.assertThat(function.calculateCost(requirement,violations),Is.is(3.14));
  violations.add(new Violation(rule));
  Assert.assertThat(function.calculateCost(requirement,violations),Is.is(3.14));
}
