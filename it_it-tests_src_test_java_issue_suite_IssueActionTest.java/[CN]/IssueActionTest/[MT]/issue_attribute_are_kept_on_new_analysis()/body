{
  adminIssueClient().doTransition(issue.key(),"resolve");
  adminIssueClient().doAction(issue.key(),"fake");
  assertThat(adminIssueClient().actions(issue.key())).doesNotContain("fake");
  orchestrator.executeBuild(scan);
  assertThat(adminIssueClient().actions(issue.key())).doesNotContain("fake");
}
