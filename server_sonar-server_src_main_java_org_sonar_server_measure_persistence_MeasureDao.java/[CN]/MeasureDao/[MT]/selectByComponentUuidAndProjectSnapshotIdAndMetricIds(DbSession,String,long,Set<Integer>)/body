{
  return DaoUtils.executeLargeInputs(metricIds,new Function<List<Integer>,List<PastMeasureDto>>(){
    @Override public List<PastMeasureDto> apply(    List<Integer> ids){
      return mapper(session).selectByComponentUuidAndProjectSnapshotIdAndStatusAndMetricIds(componentUuid,projectSnapshotId,ids,SnapshotDto.STATUS_PROCESSED);
    }
  }
);
}
