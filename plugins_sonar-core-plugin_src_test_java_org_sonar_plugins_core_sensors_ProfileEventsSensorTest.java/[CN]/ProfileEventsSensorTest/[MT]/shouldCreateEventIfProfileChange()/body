{
  mockProfileWithVersion(1);
  TimeMachine timeMachine=mockTM(21.0,"Bar",1.0);
  ProfileEventsSensor sensor=new ProfileEventsSensor(wrapper,timeMachine,fs);
  sensor.analyse(project,context);
  verify(context).createEvent(same(project),eq("Foo version 1"),eq("Foo version 1 is used instead of Bar version 1"),same(Event.CATEGORY_PROFILE),any(Date.class));
}
