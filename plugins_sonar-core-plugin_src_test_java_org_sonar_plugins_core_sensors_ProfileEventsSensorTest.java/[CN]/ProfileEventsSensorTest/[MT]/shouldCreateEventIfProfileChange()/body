{
  RulesProfile profile=mockProfile(1);
  TimeMachine timeMachine=mockTM(project,21.0,1.0);
  ProfileEventsSensor sensor=new ProfileEventsSensor(profile,timeMachine);
  sensor.analyse(project,context);
  verify(context).createEvent(same(project),eq("Profile V1"),eq("A different quality profile was used"),same(Event.CATEGORY_PROFILE),(Date)anyObject());
}
