{
  mockProfileWithVersion(2);
  TimeMachine timeMachine=mockTM(22.0,"Foo",1.0);
  ProfileEventsSensor sensor=new ProfileEventsSensor(wrapper,timeMachine,moduleLanguages);
  sensor.analyse(project,context);
  verify(context).createEvent(same(project),eq("Foo version 2"),eq("Foo version 2 is used instead of Foo version 1"),same(Event.CATEGORY_PROFILE),any(Date.class));
}
