{
  setupData("shared");
  DefaultRuleProvider provider=new DefaultRuleProvider(getSessionFactory());
  Collection<Rule> rules=provider.findAll(RuleQuery.create());
  assertNotNull(rules);
  assertThat(rules.size(),is(3));
  for (  Rule rule : rules) {
    assertThat(rule.getId(),anyOf(is(1),is(3),is(4)));
  }
}
