{
  File file=new File(baseDir,path);
  String effectiveKey=module.getKey() + ":" + path;
  String deprecatedEffectiveKey=module.getKey() + ":" + deprecatedKey;
  return new DeprecatedDefaultInputFile(path).setDeprecatedKey(deprecatedEffectiveKey).setFile(file).setKey(effectiveKey).setType(isTest ? InputFile.Type.TEST : InputFile.Type.MAIN);
}
