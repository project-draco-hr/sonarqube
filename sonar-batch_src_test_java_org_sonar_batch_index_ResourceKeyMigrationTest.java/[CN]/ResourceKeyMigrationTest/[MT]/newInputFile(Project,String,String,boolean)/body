{
  File file=new File(baseDir,path);
  String effectiveKey=module.getKey() + ":" + path;
  String deprecatedEffectiveKey=module.getKey() + ":" + deprecatedKey;
  return new DefaultInputFile(path).setFile(file).setKey(effectiveKey).setDeprecatedKey(deprecatedEffectiveKey).setType(isTest ? InputFile.Type.TEST : InputFile.Type.MAIN);
}
