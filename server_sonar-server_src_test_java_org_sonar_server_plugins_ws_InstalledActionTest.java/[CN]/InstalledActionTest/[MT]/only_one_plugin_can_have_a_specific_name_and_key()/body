{
  loggedAsSystemAdmin();
  when(pluginRepository.getPluginInfos()).thenReturn(of(plugin("A","name2"),plugin("A","name2")));
  underTest.handle(request,response);
  assertJson(response.outputAsString()).withStrictArrayOrder().isSimilarTo("{" + "  \"plugins\":" + "  ["+ "    {\"key\": \"A\"}"+ "  ]"+ "}");
  assertThat(response.outputAsString()).containsOnlyOnce("name2");
}
