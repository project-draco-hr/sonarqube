{
  assertThat(toEntries(measureRepository.getAddedRawMeasures(componentRef))).containsOnly(entryOf(DEVELOPMENT_COST_KEY,newMeasureBuilder().create(String.valueOf(devCost))),entryOf(SQALE_DEBT_RATIO_KEY,newMeasureBuilder().create(debt / (double)devCost * 100.0)),entryOf(SQALE_RATING_KEY,createSqaleRatingMeasure(sqaleRating)));
}
