{
  InputStream responseStream=newRequest(UserRole.USER,null).setParam(org.sonarqube.ws.client.permission.PermissionsWsParameters.PARAM_TEMPLATE_NAME,template1.getName()).setParam(WebService.Param.TEXT_QUERY,"ame-1").setMediaType(PROTOBUF).execute().getInputStream();
  OldUsersWsResponse response=parseFrom(responseStream);
  assertThat(response.getUsersList()).extracting("login").containsOnly("login-1");
}
