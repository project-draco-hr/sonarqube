{
  InputStream responseStream=newRequest(UserRole.USER,null).setParam(WsPermissionParameters.PARAM_TEMPLATE_NAME,template1.getName()).setParam(WebService.Param.TEXT_QUERY,"ame-1").setMediaType(PROTOBUF).execute().getInputStream();
  WsUsersResponse response=parseFrom(responseStream);
  assertThat(response.getUsersList()).extracting("login").containsOnly("login-1");
}
