{
  InputStream responseStream=newRequest(UserRole.USER,null).setParam(WsPermissionParameters.PARAM_TEMPLATE_NAME,template1.getName()).setParam(WebService.Param.SELECTED,"all").setMediaType(PROTOBUF).execute().getInputStream();
  WsUsersResponse response=parseFrom(responseStream);
  assertThat(response.getUsersList()).extracting("login").containsExactly("login-1","login-2","login-3");
  assertThat(response.getUsers(2).getSelected()).isFalse();
}
