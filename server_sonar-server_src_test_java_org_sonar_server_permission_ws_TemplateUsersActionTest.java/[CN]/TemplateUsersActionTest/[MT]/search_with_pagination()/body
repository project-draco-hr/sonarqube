{
  InputStream responseStream=newRequest(UserRole.USER,null).setParam(WsPermissionParameters.PARAM_TEMPLATE_NAME,template1.getName()).setParam(WebService.Param.SELECTED,"all").setParam(WebService.Param.PAGE,"2").setParam(WebService.Param.PAGE_SIZE,"1").setMediaType(PROTOBUF).execute().getInputStream();
  WsTemplateUsersResponse response=parseFrom(responseStream);
  assertThat(response.getUsersList()).extracting("login").containsOnly("login-2");
}
