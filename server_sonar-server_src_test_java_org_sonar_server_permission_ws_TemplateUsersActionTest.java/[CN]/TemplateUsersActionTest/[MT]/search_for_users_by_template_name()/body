{
  InputStream responseStream=newRequest(UserRole.USER,null).setParam(org.sonarqube.ws.client.permission.PermissionsWsParameters.PARAM_TEMPLATE_NAME,template1.getName()).setMediaType(PROTOBUF).execute().getInputStream();
  OldUsersWsResponse response=parseFrom(responseStream);
  assertThat(response.getUsersList()).extracting("login").containsExactly("login-1","login-2");
}
