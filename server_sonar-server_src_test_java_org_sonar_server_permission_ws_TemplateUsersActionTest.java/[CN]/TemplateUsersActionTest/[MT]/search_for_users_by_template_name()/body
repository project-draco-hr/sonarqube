{
  InputStream responseStream=newRequest(UserRole.USER,null).setParam(WsPermissionParameters.PARAM_TEMPLATE_NAME,template1.getName()).setMediaType(PROTOBUF).execute().getInputStream();
  WsTemplateUsersResponse response=parseFrom(responseStream);
  assertThat(response.getUsersList()).extracting("login").containsExactly("login-1","login-2");
}
