{
  expectedException.expect(VisitException.class);
  expectedException.expectCause(hasType(IllegalStateException.class).andMessage(format("Unsupported value (%s) in property sonar.qualitygate","10 sds")));
  treeRootHolder.setRoot(PROJECT_ALONE);
  when(settingsRepository.getSettings(PROJECT_ALONE)).thenReturn(new MapSettings().setProperty("sonar.qualitygate","10 sds"));
  underTest.execute();
}
