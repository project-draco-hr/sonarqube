{
  ruleBuilder.add(SQUID_RULE_KEY).setName(SQUID_RULE_NAME);
  activeRulesBuilder.create(SQUID_RULE_KEY).activate();
  initModuleIssues();
  DefaultIssue issue=new DefaultIssue().addLocation(new DefaultIssueLocation().onFile(file).at(file.selectLine(3)).message("Foo")).forRule(SQUID_RULE_KEY);
  boolean added=moduleIssues.initAndAddIssue(issue);
  assertThat(added).isFalse();
  verifyZeroInteractions(reportPublisher);
}
