{
  ruleBuilder.add(SQUID_RULE_KEY).setInternalKey(SQUID_RULE_KEY.rule());
  initModuleIssues();
  DefaultIssue issue=new DefaultIssue().addLocation(new DefaultIssueLocation().onFile(file).at(file.selectLine(3)).message("")).forRule(SQUID_RULE_KEY);
  try {
    moduleIssues.initAndAddIssue(issue);
    fail();
  }
 catch (  Exception e) {
    assertThat(e).isInstanceOf(MessageException.class);
  }
  verifyZeroInteractions(reportPublisher);
}
