{
  Collection<Metric> metrics=decorator.usedMetrics();
  assertThat(metrics).containsOnly(CoreMetrics.IT_LINES_TO_COVER,CoreMetrics.IT_UNCOVERED_LINES,CoreMetrics.NEW_IT_LINES_TO_COVER,CoreMetrics.NEW_IT_UNCOVERED_LINES,CoreMetrics.IT_CONDITIONS_TO_COVER,CoreMetrics.IT_UNCOVERED_CONDITIONS,CoreMetrics.NEW_IT_CONDITIONS_TO_COVER,CoreMetrics.NEW_IT_UNCOVERED_CONDITIONS);
}
