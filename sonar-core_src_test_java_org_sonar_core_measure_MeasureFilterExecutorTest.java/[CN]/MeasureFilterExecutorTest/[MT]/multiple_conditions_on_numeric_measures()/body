{
  MeasureFilter filter=new MeasureFilter().setResourceQualifiers("CLA").setSortOnMetric(METRIC_LINES).addCondition(new MeasureFilterCondition(METRIC_LINES,">",2)).addCondition(new MeasureFilterCondition(METRIC_LINES,"<=",50));
  List<MeasureFilterRow> rows=executor.execute(filter,new MeasureFilterContext());
  assertThat(rows).hasSize(1);
  verifyJavaTinyFile(rows.get(0));
}
