{
  MeasureFilter filter=new MeasureFilter().setResourceQualifiers("TRK").setSortOnMetric(METRIC_LINES).setSortOnPeriod(5).setSortAsc(false);
  List<MeasureFilterRow> rows=executor.execute(filter,null);
  assertThat(rows).hasSize(2);
  verifyPhpProject(rows.get(0));
  verifyJavaProject(rows.get(1));
}
