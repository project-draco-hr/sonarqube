{
  return ImmutableMap.<String,String>builder().put("id",String.valueOf(report.getId())).put("projectKey",report.getProjectKey()).put("projectName",report.getProjectName()).put("status",String.valueOf(report.getStatus())).put("createdAt",DateUtils.formatDateTime(report.getCreatedAt())).put("updatedAt",DateUtils.formatDateTime(report.getUpdatedAt())).build();
}
