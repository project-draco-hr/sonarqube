{
  return ImmutableMap.<String,String>builder().put("id",String.valueOf(report.getId())).put("projectKey",project.key()).put("projectName",project.name()).put("projectUuid",project.uuid()).put("status",String.valueOf(report.getStatus())).put("submittedAt",DateUtils.formatDateTimeNullSafe(report.getCreatedAt())).put("startedAt",DateUtils.formatDateTimeNullSafe(report.getStartedAt())).put("finishedAt",DateUtils.formatDateTimeNullSafe(report.getFinishedAt())).build();
}
