{
  return ImmutableMap.<String,String>builder().put("id",String.valueOf(report.getId())).put("projectKey",report.getProjectKey()).put("projectName",report.getProjectName()).put("status",String.valueOf(report.getStatus())).put("submittedAt",DateUtils.formatDateTimeNullSafe(report.getCreatedAt())).put("updatedAt",DateUtils.formatDateTimeNullSafe(report.getUpdatedAt())).put("startedAt",DateUtils.formatDateTimeNullSafe(report.getStartedAt())).put("finishedAt",DateUtils.formatDateTimeNullSafe(report.getFinishedAt())).build();
}
