{
  QualityProfileDto child=createProfile("xoo","Child");
  session.commit();
  assertThat(db.activeRuleDao().selectByProfileKey(session,child.getKey())).isEmpty();
  wsTester.newPostRequest(QProfilesWs.API_ENDPOINT,"change_parent").setParam(QProfileIdentificationParamUtils.PARAM_PROFILE_KEY,child.getKee()).setParam("parentName","polop").setParam("parentKey","palap").execute();
}
