{
  setupData("should_resolve_conflicts_on_updates");
  FakeSaver saver=new FakeSaver(getMyBatis(),new FakeRuleFinder());
  Date date=DateUtils.parseDate("2013-05-18");
  DefaultIssue issue=new DefaultIssue().setKey("ABCDE").setNew(false).setChanged(true).setCreationDate(DateUtils.parseDate("2005-05-12")).setUpdateDate(date).setRuleKey(RuleKey.of("squid","AvoidCycles")).setComponentKey("struts:Action").setSelectedAt(DateUtils.parseDate("2005-01-01")).setLine(444).setSeverity("BLOCKER").setChecksum("FFFFF").setAssignee("looser").setResolution(null).setStatus("REOPEN");
  saver.save(issue);
  checkTables("should_resolve_conflicts_on_updates",new String[]{"id","created_at","updated_at"},"issues");
}
