{
  int maxScore=92;
  int[][] scores={{maxScore},{8},{85}};
  MatchesByScore matchesByScore=MatchesByScore.create(new ScoreMatrix(of("A","B","C"),ImmutableMap.of("1",fileOf("1")),scores,maxScore));
  assertThat(matchesByScore.getSize()).isEqualTo(2);
  assertThat(Lists.newArrayList(matchesByScore)).isEqualTo(Arrays.asList(ImmutableList.of(new Match("C","1")),NO_MATCH,NO_MATCH,NO_MATCH,NO_MATCH,NO_MATCH,NO_MATCH,ImmutableList.of(new Match("A","1"))));
}
