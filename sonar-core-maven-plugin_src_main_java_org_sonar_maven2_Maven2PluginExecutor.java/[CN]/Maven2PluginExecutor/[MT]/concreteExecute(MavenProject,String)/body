{
  ReactorManager reactor=new ReactorManager(Arrays.asList(pom));
  MavenSession clonedSession=new MavenSession(mavenSession.getContainer(),mavenSession.getSettings(),mavenSession.getLocalRepository(),mavenSession.getEventDispatcher(),reactor,Arrays.asList(goal),mavenSession.getExecutionRootDirectory(),mavenSession.getExecutionProperties(),mavenSession.getStartTime());
  lifecycleExecutor.execute(clonedSession,reactor,clonedSession.getEventDispatcher());
}
