{
  dbTester.prepareDbUnit(getClass(),"index.xml");
  esTester.putDocuments(ViewIndexDefinition.INDEX,ViewIndexDefinition.TYPE_VIEW,new ViewDoc().setUuid("ABCD").setProjects(newArrayList("BCDE")).getFields());
  indexer.index();
  assertThat(esTester.countDocuments(ViewIndexDefinition.INDEX,ViewIndexDefinition.TYPE_VIEW)).isEqualTo(1L);
}
