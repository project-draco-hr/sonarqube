{
  if (size() == 0) {
    return "[]";
  }
  StringBuffer buf=new StringBuffer(16 * size());
  buf.append("[");
  Iterator it=iterator();
  boolean hasNext=it.hasNext();
  while (hasNext) {
    Object value=it.next();
    buf.append(value == this ? "(this Collection)" : value);
    hasNext=it.hasNext();
    if (hasNext) {
      buf.append(", ");
    }
  }
  buf.append("]");
  return buf.toString();
}
