{
  if (obj == this) {
    return true;
  }
  if (obj instanceof List == false) {
    return false;
  }
  List other=(List)obj;
  if (other.size() != size()) {
    return false;
  }
  ListIterator it1=listIterator();
  ListIterator it2=other.listIterator();
  while (it1.hasNext() && it2.hasNext()) {
    Object o1=it1.next();
    Object o2=it2.next();
    if (!(o1 == null ? o2 == null : o1.equals(o2)))     return false;
  }
  return !(it1.hasNext() || it2.hasNext());
}
