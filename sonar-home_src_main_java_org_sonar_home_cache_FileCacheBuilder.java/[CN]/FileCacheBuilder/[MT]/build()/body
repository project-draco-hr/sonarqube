{
  if (userHome == null) {
    String path=System.getenv("SONAR_USER_HOME");
    if (path == null) {
      path=System.getProperty("user.home") + File.separator + ".sonar";
    }
    userHome=new File(path);
  }
  File cacheDir=new File(userHome,"cache");
  return FileCache.create(cacheDir,logger);
}
