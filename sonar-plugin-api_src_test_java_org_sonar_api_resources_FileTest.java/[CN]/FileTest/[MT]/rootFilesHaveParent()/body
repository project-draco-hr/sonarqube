{
  File file=new File((String)null,"toto.sql");
  assertThat(file.getKey(),is("toto.sql"));
  assertThat(file.getName(),is("toto.sql"));
  assertThat(file.getParent().getKey(),is(Directory.ROOT));
  file=new File("","toto.sql");
  assertThat(file.getKey(),is("toto.sql"));
  assertThat(file.getName(),is("toto.sql"));
  assertThat(file.getParent().getKey(),is(Directory.ROOT));
  file=new File("toto.sql");
  assertThat(file.getKey(),is("toto.sql"));
  assertThat(file.getName(),is("toto.sql"));
  assertThat(file.getParent().getKey(),is(Directory.ROOT));
}
