{
  fileSourceRepository.addLine("line 1;").addLine("line 2;");
  BatchReport.Issue reportIssue=BatchReport.Issue.newBuilder().setMsg("the message").setRuleRepository(CommonRuleKeys.commonRepositoryForLang("java")).setRuleKey("S001").setSeverity(Constants.Severity.BLOCKER).build();
  reportReader.putIssues(FILE.getReportAttributes().getRef(),asList(reportIssue));
  Input<DefaultIssue> input=underTest.create(FILE);
  assertThat(input.getIssues()).isEmpty();
}
