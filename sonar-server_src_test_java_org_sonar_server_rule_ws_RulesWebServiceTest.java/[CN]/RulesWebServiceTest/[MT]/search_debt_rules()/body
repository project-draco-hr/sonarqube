{
  ruleDao.insert(session,RuleTesting.newXooX1().setDefaultRemediationCoefficient("DefaultCoef").setDefaultRemediationFunction("DefaultFunction").setDefaultRemediationCoefficient("DefaultCoef").setDefaultSubCharacteristicId(1));
  session.commit();
  MockUserSession.set();
  WsTester.TestRequest request=tester.wsTester().newGetRequest(API_ENDPOINT,API_SEARCH_METHOD);
  request.setParam(SearchOptions.PARAM_FIELDS,"debtRemFn,debtChar");
  WsTester.Result result=request.execute();
  result.assertJson(this.getClass(),"search_debt_rule.json");
}
