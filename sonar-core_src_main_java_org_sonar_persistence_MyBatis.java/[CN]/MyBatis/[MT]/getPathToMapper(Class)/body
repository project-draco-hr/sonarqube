{
  InputStream input=getClass().getResourceAsStream("/" + StringUtils.replace(mapperClass.getName(),".","/") + "-"+ database.getDialect().getId()+ ".xml");
  if (input == null) {
    input=getClass().getResourceAsStream("/" + StringUtils.replace(mapperClass.getName(),".","/") + ".xml");
  }
  return input;
}
