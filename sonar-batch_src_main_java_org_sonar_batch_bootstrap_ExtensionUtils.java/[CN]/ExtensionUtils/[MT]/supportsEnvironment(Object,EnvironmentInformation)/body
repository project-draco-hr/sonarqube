{
  SupportedEnvironment env=AnnotationUtils.getAnnotation(extension,SupportedEnvironment.class);
  if (env == null) {
    return true;
  }
  for (  String supported : env.value()) {
    if (StringUtils.equalsIgnoreCase(environment.getKey(),supported)) {
      return true;
    }
  }
  return false;
}
