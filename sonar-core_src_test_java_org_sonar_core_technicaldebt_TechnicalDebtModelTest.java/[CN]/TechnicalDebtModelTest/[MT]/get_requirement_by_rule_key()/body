{
  Characteristic rootCharacteristic=new Characteristic().setKey("MEMORY_EFFICIENCY").setName("Memory use");
  Characteristic characteristic=new Characteristic().setKey("EFFICIENCY").setName("Efficiency").setParent(rootCharacteristic);
  RuleKey ruleKey=RuleKey.of("checkstyle","Regexp");
  Requirement requirement=new Requirement().setCharacteristic(characteristic).setRuleKey(ruleKey).setFunction("linear").setFactor(WorkUnit.create(2d,WorkUnit.HOURS)).setOffset(WorkUnit.create(0d,WorkUnit.HOURS));
  sqaleModel.addRootCharacteristic(rootCharacteristic);
  assertThat(sqaleModel.requirementsByRule(ruleKey)).isEqualTo(requirement);
  assertThat(sqaleModel.requirementsByRule(RuleKey.of("not","found"))).isNull();
}
