{
  BulkChangeQuery query=BulkChangeQuery.create().issues("ABCD","EFGH").actions("assign").actionParameter("assign","assignee","geoffrey").comment("My bulk comment").sendNotifications(false);
  assertThat(query.urlParams()).hasSize(5).containsEntry("issues","ABCD,EFGH").containsEntry("actions","assign").containsEntry("assign.assignee","geoffrey").containsEntry("comment","My bulk comment").containsEntry("sendNotifications","false");
}
