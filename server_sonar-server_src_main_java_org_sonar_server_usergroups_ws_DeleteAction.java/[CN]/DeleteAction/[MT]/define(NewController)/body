{
  WebService.NewAction action=context.createAction("delete").setDescription(format("Delete a group. The default groups cannot be deleted.<br/>" + "'%s' or '%s' must be provided.<br />" + "Requires System Administrator permission.",PARAM_GROUP_ID,PARAM_GROUP_NAME)).setHandler(this).setSince("5.2").setPost(true);
  UserGroupsWsParameters.createGroupParameters(action);
}
