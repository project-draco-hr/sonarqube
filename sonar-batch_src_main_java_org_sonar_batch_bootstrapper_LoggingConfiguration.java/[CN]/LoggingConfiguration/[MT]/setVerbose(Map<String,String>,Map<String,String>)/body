{
  String logLevel=getFallback("sonar.log.level",props,fallback);
  String deprecatedProfilingLevel=getFallback("sonar.log.profilingLevel",props,fallback);
  boolean verbose="true".equals(getFallback("sonar.verbose",props,fallback)) || "DEBUG".equals(logLevel) || "TRACE".equals(logLevel)|| "BASIC".equals(deprecatedProfilingLevel)|| "FULL".equals(deprecatedProfilingLevel);
  return setVerbose(verbose);
}
