{
  String logLevel=properties.get("sonar.log.level");
  String deprecatedProfilingLevel=properties.get("sonar.log.profilingLevel");
  boolean verbose="true".equals(properties.get("sonar.verbose")) || "DEBUG".equals(logLevel) || "TRACE".equals(logLevel)|| "BASIC".equals(deprecatedProfilingLevel)|| "FULL".equals(deprecatedProfilingLevel);
  boolean sql="TRACE".equals(logLevel) || "FULL".equals(deprecatedProfilingLevel);
  setShowSql(sql);
  setVerbose(verbose);
  return this;
}
