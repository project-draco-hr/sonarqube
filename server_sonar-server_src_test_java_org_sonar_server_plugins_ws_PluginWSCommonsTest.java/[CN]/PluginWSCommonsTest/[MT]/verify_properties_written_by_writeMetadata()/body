{
  jsonWriter.beginObject();
  underTest.writeMetadata(jsonWriter,GIT_PLUGIN_METADATA);
  jsonWriter.endObject();
  jsonWriter.close();
  assertJson(response.outputAsString()).setStrictArrayOrder(true).isSimilarTo("{" + "  \"key\": \"scmgit\"," + "  \"name\": \"Git\","+ "  \"description\": \"Git SCM Provider.\","+ "  \"version\": \"1.0\","+ "  \"license\": \"GNU LGPL 3\","+ "  \"organizationName\": \"SonarSource\","+ "  \"organizationUrl\": \"http://www.sonarsource.com\","+ "}");
}
