{
  jsonWriter.beginObject();
  underTest.writeMetadata(jsonWriter,gitPluginInfo());
  jsonWriter.endObject();
  jsonWriter.close();
  assertJson(response.outputAsString()).setStrictArrayOrder(true).isSimilarTo("{" + "  \"key\": \"scmgit\"," + "  \"name\": \"Git\","+ "  \"description\": \"Git SCM Provider.\","+ "  \"version\": \"1.0\","+ "  \"license\": \"GNU LGPL 3\","+ "  \"organizationName\": \"SonarSource\","+ "  \"organizationUrl\": \"http://www.sonarsource.com\","+ "  \"homepageUrl\": \"http://redirect.sonarsource.com/plugins/scmgit.html\","+ "  \"issueTrackerUrl\": \"http://jira.codehaus.org/browse/SONARSCGIT\","+ "}");
}
