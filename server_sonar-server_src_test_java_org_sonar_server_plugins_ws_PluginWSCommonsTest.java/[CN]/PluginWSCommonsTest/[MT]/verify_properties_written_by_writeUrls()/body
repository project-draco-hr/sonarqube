{
  jsonWriter.beginObject();
  underTest.writeUrls(jsonWriter,GIT_PLUGIN_METADATA);
  jsonWriter.endObject();
  jsonWriter.close();
  assertJson(response.outputAsString()).setStrictArrayOrder(true).isSimilarTo("{" + "  \"urls\": {" + "    \"homepage\": \"http://redirect.sonarsource.com/plugins/scmgit.html\","+ "    \"issueTracker\": \"http://jira.codehaus.org/browse/SONARSCGIT\""+ "  },"+ "}");
}
