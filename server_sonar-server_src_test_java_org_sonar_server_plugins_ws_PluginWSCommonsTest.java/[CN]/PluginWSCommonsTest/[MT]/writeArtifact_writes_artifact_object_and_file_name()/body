{
  jsonWriter.beginObject();
  underTest.writeArtifact(jsonWriter,GIT_PLUGIN_METADATA);
  jsonWriter.endObject();
  jsonWriter.close();
  assertJson(response.outputAsString()).setStrictArrayOrder(true).isSimilarTo("{" + "  \"artifact\": {" + "     \"name\": \"sonar-scm-git-plugin-1.0.jar\""+ "  }"+ "}");
}
