{
  jsonWriter.beginObject();
  underTest.writeRelease(jsonWriter,RELEASE);
  jsonWriter.endObject();
  jsonWriter.close();
  assertJson(response.outputAsString()).setStrictArrayOrder(true).isSimilarTo("{" + "  \"release\": {" + "     \"version\": \"1.0\","+ "     \"date\": \"2015-04-16\""+ "  }"+ "}");
}
