{
  jsonWriter.beginObject();
  underTest.writeArtifact(jsonWriter,release("p_key").setDownloadUrl("http://toto.com/file.jar"));
  jsonWriter.endObject();
  jsonWriter.close();
  assertJson(response.outputAsString()).setStrictArrayOrder(true).isSimilarTo("{" + "  \"artifact\": {" + "     \"name\": \"file.jar\","+ "     \"url\": \"http://toto.com/file.jar\""+ "  }"+ "}");
}
