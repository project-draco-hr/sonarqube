{
  underTest.writePluginMetadata(jsonWriter,GIT_PLUGIN_METADATA);
  jsonWriter.close();
  assertJson(response.outputAsString()).setStrictArrayOrder(true).isSimilarTo("{" + "  \"key\": \"scmgit\"," + "  \"name\": \"Git\","+ "  \"description\": \"Git SCM Provider.\","+ "  \"version\": \"1.0\","+ "  \"license\": \"GNU LGPL 3\","+ "  \"organizationName\": \"SonarSource\","+ "  \"organizationUrl\": \"http://www.sonarsource.com\","+ "  \"urls\": {"+ "    \"homepage\": \"http://redirect.sonarsource.com/plugins/scmgit.html\","+ "    \"issueTracker\": \"http://jira.codehaus.org/browse/SONARSCGIT\""+ "  },"+ "  \"artifact\": {"+ "    \"name\": \"sonar-scm-git-plugin-1.0.jar\""+ "  }"+ "}");
}
