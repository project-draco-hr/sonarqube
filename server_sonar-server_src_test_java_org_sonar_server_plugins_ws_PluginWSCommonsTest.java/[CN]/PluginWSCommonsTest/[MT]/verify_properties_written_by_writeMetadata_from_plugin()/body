{
  jsonWriter.beginObject();
  underTest.writeMetadata(jsonWriter,newPlugin());
  jsonWriter.endObject();
  jsonWriter.close();
  assertJson(response.outputAsString()).isSimilarTo("{" + "  \"key\": \"p_key\"," + "  \"name\": \"p_name\","+ "  \"description\": \"p_description\","+ "  \"category\": \"p_category\","+ "  \"license\": \"p_license\","+ "  \"organizationName\": \"p_orga_name\","+ "  \"organizationUrl\": \"p_orga_url\","+ "  \"termsAndConditionsUrl\": \"p_t_and_c_url\""+ "}");
}
