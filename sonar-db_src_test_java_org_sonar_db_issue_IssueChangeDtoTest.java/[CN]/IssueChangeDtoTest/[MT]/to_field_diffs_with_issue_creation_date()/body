{
  IssueChangeDto changeDto=new IssueChangeDto().setKey("EFGH").setUserLogin("emmerik").setChangeData("Some text").setIssueKey("ABCDE").setIssueChangeCreationDate(System2.INSTANCE.now());
  FieldDiffs diffs=changeDto.toFieldDiffs();
  assertThat(diffs.userLogin()).isEqualTo("emmerik");
  assertThat(diffs.issueKey()).isEqualTo("ABCDE");
  assertThat(diffs.creationDate()).isNotNull();
}
