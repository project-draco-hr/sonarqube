{
  Multimap<String,NotificationChannel> recipients=HashMultimap.create();
  recipients.put("user",emailChannel);
  when(notificationManager.findSubscribedRecipientsForDispatcher(dispatcher,42)).thenReturn(recipients);
  dispatcher.performDispatch(new Notification("review-changed").setFieldValue("projectId","42").setFieldValue("author","user2").setFieldValue("new.assignee","user"),context);
  verify(context,never()).addUser(any(String.class),any(NotificationChannel.class));
}
