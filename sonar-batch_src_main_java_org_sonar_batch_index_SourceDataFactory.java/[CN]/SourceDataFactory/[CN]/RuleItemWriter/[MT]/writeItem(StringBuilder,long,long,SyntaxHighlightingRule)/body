{
  if (currentLineSb.length() > 0) {
    currentLineSb.append(SyntaxHighlightingData.RULE_SEPARATOR);
  }
  currentLineSb.append(startLineOffset).append(SyntaxHighlightingData.FIELD_SEPARATOR).append(endLineOffset).append(SyntaxHighlightingData.FIELD_SEPARATOR).append(item.getTextType().cssClass());
}
