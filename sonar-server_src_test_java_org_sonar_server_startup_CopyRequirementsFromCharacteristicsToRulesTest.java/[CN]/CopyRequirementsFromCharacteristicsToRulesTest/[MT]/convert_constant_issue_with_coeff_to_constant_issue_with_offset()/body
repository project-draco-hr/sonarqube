{
  db.prepareDbUnit(getClass(),"convert_constant_issue_with_coeff_to_constant_issue_with_offset.xml");
  when(status.isUpgraded()).thenReturn(true);
  when(status.getInitialDbVersion()).thenReturn(498);
  service.start();
