{
  if (literal != null) {
    if (key != null) {
      JspException e=new JspException(messages.getMessage("common.both"));
      TagUtils.getInstance().saveException(pageContext,e);
      throw e;
    }
 else {
      return (literal);
    }
  }
 else {
    if (key != null) {
      return TagUtils.getInstance().message(pageContext,getBundle(),getLocale(),key);
    }
 else {
      return null;
    }
  }
}
