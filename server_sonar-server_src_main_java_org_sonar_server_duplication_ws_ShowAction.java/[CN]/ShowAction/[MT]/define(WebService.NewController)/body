{
  WebService.NewAction action=controller.createAction("show").setDescription("Get duplications. Require Browse permission on file's project").setSince("4.4").setHandler(this).setResponseExample(Resources.getResource(this.getClass(),"example-show.json"));
  action.createParam("key").setDescription("File key").setExampleValue("my_project:/src/foo/Bar.php");
  action.createParam("uuid").setDescription("File UUID").setExampleValue("584a89f2-8037-4f7b-b82c-8b45d2d63fb2");
}
