{
  underTest.activateDatabaseSettings(Collections.<String,String>emptyMap());
  assertThat(underTest.getProperties()).isNotEmpty();
  underTest.load();
  assertThat(underTest.getProperties()).isNotEmpty();
  underTest.clear();
  assertThat(underTest.getProperties()).isEmpty();
  underTest.unload();
  assertThat(underTest.getProperties()).isNotEmpty();
  underTest.clear();
  assertThat(underTest.getProperties()).isEmpty();
}
