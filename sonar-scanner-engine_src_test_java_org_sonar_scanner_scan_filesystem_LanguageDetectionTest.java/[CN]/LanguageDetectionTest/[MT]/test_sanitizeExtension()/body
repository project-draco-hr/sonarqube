{
  assertThat(LanguageDetection.sanitizeExtension(".cbl")).isEqualTo("cbl");
  assertThat(LanguageDetection.sanitizeExtension(".CBL")).isEqualTo("cbl");
  assertThat(LanguageDetection.sanitizeExtension("CBL")).isEqualTo("cbl");
  assertThat(LanguageDetection.sanitizeExtension("cbl")).isEqualTo("cbl");
}
