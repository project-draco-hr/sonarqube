{
  ws.newRequest().setParam(PARAM_TEMPLATE_KEY,permissionTemplate.getKee()).setParam(PARAM_PERMISSION,CODEVIEWER).setParam(PARAM_GROUP_ID,String.valueOf(group.getId())).execute();
  assertThat(getGroupNamesInTemplateAndPermission(permissionTemplate.getId(),CODEVIEWER)).containsExactly(GROUP_NAME);
}
