{
  for (int index=0; index < token.length(); index++) {
    if (token.charAt(index) == '*' || Character.isDigit(token.charAt(index))) {
      if (token.charAt(index + 1) == '.') {
        index++;
      }
      while (++index < token.length()) {
        if (token.charAt(index) != ' ') {
          return index;
        }
      }
    }
  }
  return token.length() - 1;
}
