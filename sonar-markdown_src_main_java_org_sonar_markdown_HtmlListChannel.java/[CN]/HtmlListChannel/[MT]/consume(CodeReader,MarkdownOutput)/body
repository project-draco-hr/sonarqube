{
  try {
    if (code.getColumnPosition() == 0 && listElement.consume(code,output)) {
      while (endOfLine.consume(code,output) && listElement.consume(code,output)) {
      }
      output.append("</ul>");
      return true;
    }
    return false;
  }
  finally {
    pendingListConstruction=false;
  }
}
