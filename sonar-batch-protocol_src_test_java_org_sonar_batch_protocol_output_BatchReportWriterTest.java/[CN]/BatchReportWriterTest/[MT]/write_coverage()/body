{
  assertThat(underTest.hasComponentData(FileStructure.Domain.COVERAGES,1)).isFalse();
  underTest.writeComponentCoverage(1,asList(BatchReport.Coverage.newBuilder().setLine(1).setConditions(1).setUtHits(true).setItHits(false).setUtCoveredConditions(1).setItCoveredConditions(1).setOverallCoveredConditions(1).build()));
  assertThat(underTest.hasComponentData(FileStructure.Domain.COVERAGES,1)).isTrue();
}
