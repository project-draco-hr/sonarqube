{
  assertThat(underTest.hasComponentData(FileStructure.Domain.SYNTAX_HIGHLIGHTINGS,1)).isFalse();
  underTest.writeComponentSyntaxHighlighting(1,Arrays.asList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(1).build()).setType(Constants.HighlightingType.ANNOTATION).build()));
  assertThat(underTest.hasComponentData(FileStructure.Domain.SYNTAX_HIGHLIGHTINGS,1)).isTrue();
}
