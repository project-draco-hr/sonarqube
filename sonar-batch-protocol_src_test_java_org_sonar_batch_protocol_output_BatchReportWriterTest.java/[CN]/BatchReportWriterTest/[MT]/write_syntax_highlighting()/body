{
  assertThat(sut.hasComponentData(FileStructure.Domain.SYNTAX_HIGHLIGHTING,1)).isFalse();
  sut.writeComponentSyntaxHighlighting(1,Arrays.asList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(1).build()).setType(Constants.HighlightingType.ANNOTATION).build()));
  assertThat(sut.hasComponentData(FileStructure.Domain.SYNTAX_HIGHLIGHTING,1)).isTrue();
}
