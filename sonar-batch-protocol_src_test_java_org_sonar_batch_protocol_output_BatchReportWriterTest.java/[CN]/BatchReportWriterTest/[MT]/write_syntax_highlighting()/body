{
  File dir=temp.newFolder();
  BatchReportWriter writer=new BatchReportWriter(dir);
  assertThat(writer.hasComponentData(FileStructure.Domain.SYNTAX_HIGHLIGHTING,1)).isFalse();
  writer.writeComponentSyntaxHighlighting(1,Arrays.asList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(1).build()).setType(Constants.HighlightingType.ANNOTATION).build()));
  assertThat(writer.hasComponentData(FileStructure.Domain.SYNTAX_HIGHLIGHTING,1)).isTrue();
}
