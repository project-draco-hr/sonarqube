{
  ProjectRepositories data=projectDataLoader.load(ProjectDataQuery.create().setModuleKey(wsRequest.mandatoryParam(PARAM_KEY)).setProfileName(wsRequest.param(PARAM_PROFILE)).setIssuesMode(wsRequest.mandatoryParamAsBoolean(PARAM_PREVIEW)));
  WsProjectResponse projectResponse=buildResponse(data);
  writeProtobuf(projectResponse,wsRequest,wsResponse);
}
