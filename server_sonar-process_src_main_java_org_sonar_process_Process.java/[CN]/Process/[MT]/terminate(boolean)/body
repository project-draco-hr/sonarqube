{
  LOGGER.trace("Process[{}]::terminate() START",name);
  Runtime.getRuntime().removeShutdownHook(shutdownHook);
  shutdownHook.start();
  if (waitForTermination) {
    try {
      shutdownHook.join();
    }
 catch (    InterruptedException e) {
      System.exit(-1);
    }
  }
  LOGGER.trace("Process[{}]::terminate() END",name);
  System.exit(1);
}
