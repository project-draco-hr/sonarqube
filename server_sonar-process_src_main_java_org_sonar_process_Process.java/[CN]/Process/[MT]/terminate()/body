{
  LOGGER.debug("Process[{}] terminating",name);
  if (monitor != null) {
    this.monitor.shutdownNow();
    this.monitor=null;
    if (this.pingTask != null) {
      this.pingTask.cancel(true);
      this.pingTask=null;
    }
    this.onTerminate();
  }
  LOGGER.debug("Process[{}] terminated",name);
}
