{
  dbTester.prepareDbUnit(getClass(),"nothing_to_do_when_link_already_exists.xml");
  treeRootHolder.setRoot(new DumbComponent(Component.Type.PROJECT,1,"ABCD",null));
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(1).setType(Constants.ComponentType.PROJECT).addLink(BatchReport.ComponentLink.newBuilder().setType(Constants.ComponentLinkType.HOME).setHref("http://www.sonarqube.org").build()).build());
  step.execute();
  dbTester.assertDbUnit(getClass(),"nothing_to_do_when_link_already_exists.xml","project_links");
}
