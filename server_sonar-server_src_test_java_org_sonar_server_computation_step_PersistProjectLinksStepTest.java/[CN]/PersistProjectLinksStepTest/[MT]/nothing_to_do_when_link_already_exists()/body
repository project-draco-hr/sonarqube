{
  dbTester.prepareDbUnit(getClass(),"nothing_to_do_when_link_already_exists.xml");
  treeRootHolder.setRoot(ReportComponent.builder(Component.Type.PROJECT,1).setUuid("ABCD").build());
  reportReader.putComponent(ScannerReport.Component.newBuilder().setRef(1).setType(Constants.ComponentType.PROJECT).addLink(ScannerReport.ComponentLink.newBuilder().setType(Constants.ComponentLinkType.HOME).setHref("http://www.sonarqube.org").build()).build());
  step.execute();
  dbTester.assertDbUnit(getClass(),"nothing_to_do_when_link_already_exists.xml","project_links");
}
