{
  dbTester.prepareDbUnit(getClass(),"delete_link.xml");
  treeRootHolder.setRoot(new DumbComponent(Component.Type.PROJECT,1,"ABCD",null));
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(1).setType(Constants.ComponentType.PROJECT).build());
  step.execute();
  assertThat(dbTester.countRowsOfTable("project_links")).isEqualTo(0);
}
