{
  try {
    return jsonBuilder().startObject().startObject(this.getType()).field("dynamic",true).startObject("properties").startObject("id").field("type","string").field("index","not_analyzed").endObject().startObject("key").field("type","string").field("index","not_analyzed").endObject().startObject("repositoryKey").field("type","string").field("index","not_analyzed").endObject().startObject("severity").field("type","string").field("index","not_analyzed").endObject().endObject().endObject().endObject();
  }
 catch (  IOException e) {
    LOG.error("Could not create mapping for {}",this.getIndexName());
    return null;
  }
}
