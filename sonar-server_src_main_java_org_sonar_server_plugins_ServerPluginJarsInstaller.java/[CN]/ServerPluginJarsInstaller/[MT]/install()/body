{
  TimeProfiler profiler=new TimeProfiler().start("Install plugins");
  deleteUninstalledPlugins();
  loadUserPlugins();
  if (serverUpgradeStatus.isFreshInstall()) {
    copyAndLoadBundledPlugins();
  }
  moveAndLoadDownloadedPlugins();
  loadCorePlugins();
  deployPlugins();
  profiler.stop();
}
