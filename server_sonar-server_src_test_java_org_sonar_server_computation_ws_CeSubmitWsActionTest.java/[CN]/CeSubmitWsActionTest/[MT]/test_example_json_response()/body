{
  CeTask task=new CeTask("TASK_1",CeTaskTypes.REPORT,"PROJECT_1","robert");
  when(reportSubmitter.submit(eq("my_project"),Matchers.isNull(String.class),eq("My Project"),any(InputStream.class))).thenReturn(task);
  TestResponse wsResponse=tester.newRequest().setParam("projectKey","my_project").setParam("projectName","My Project").setParam("report","{binary}").setMediaType(MimeTypes.JSON).setMethod("POST").execute();
  JsonAssert.assertJson(tester.getDef().responseExampleAsString()).isSimilarTo(wsResponse.getInput());
}
