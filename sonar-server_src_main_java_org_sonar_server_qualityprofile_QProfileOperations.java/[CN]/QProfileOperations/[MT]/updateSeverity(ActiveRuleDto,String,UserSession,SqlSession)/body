{
  Integer oldSeverity=activeRule.getSeverity();
  activeRule.setSeverity(Severity.ordinal(newSeverity));
  activeRuleDao.update(activeRule,session);
  session.commit();
  profilesManager.ruleSeverityChanged(activeRule.getProfileId(),activeRule.getId(),RulePriority.valueOfInt(oldSeverity),RulePriority.valueOf(newSeverity),userSession.name());
}
