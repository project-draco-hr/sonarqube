  RuleDto ruleToInsert2=new RuleDto().setId(2).setRuleKey("NewRuleKey2").setRepositoryKey("plugin2").setName("new name2").setDescription("new description2").setStatus(Rule.STATUS_BETA).setConfigKey("NewConfigKey2").setSeverity(Severity.MAJOR).setCardinality(Cardinality.SINGLE).setLanguage("js").setParentId(null).setSubCharacteristicId(102).setDefaultSubCharacteristicId(103).setRemediationFunction("linear_offset").setDefaultRemediationFunction("linear").setRemediationCoefficient("5d").setDefaultRemediationCoefficient("1h").setRemediationOffset("10h").setDefaultRemediationOffset("5min").setEffortToFixDescription("squid.S115.effortToFix2");
