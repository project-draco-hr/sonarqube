{
  Common.Component.Builder builder=Common.Component.newBuilder().setId(dto.uuid()).setKey(dto.key()).setQualifier(dto.qualifier()).setName(nullToEmpty(dto.name())).setLongName(nullToEmpty(dto.longName())).setEnabled(dto.isEnabled());
  String path=dto.path();
  if (!Strings.isNullOrEmpty(path)) {
    builder.setPath(path);
  }
  if (dto.projectUuid() != null && dto.parentProjectId() != null) {
    builder.setProject(dto.projectUuid());
  }
  if (dto.parentProjectId() != null) {
    builder.setSubProject(dto.moduleUuid());
  }
  return builder;
}
