{
  if (component.getType().isDeeperThan(maxDepth)) {
    return;
  }
  stack.add(new PathElementImpl<>(component,createForComponent(component)));
  if (order == PRE_ORDER) {
    visitNode(component);
  }
  visitChildren(component);
  if (order == POST_ORDER) {
    visitNode(component);
  }
  stack.pop();
}
