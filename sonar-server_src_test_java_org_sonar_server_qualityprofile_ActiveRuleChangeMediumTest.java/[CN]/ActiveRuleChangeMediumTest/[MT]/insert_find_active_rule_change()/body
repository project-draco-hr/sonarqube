{
  ActiveRuleChange change=new ActiveRuleChange().setInheritance(ActiveRule.Inheritance.INHERITED).setSeverity("BLOCKER").setParameter("param1","value1");
  service.write(dbSession,Log.Type.ACTIVE_RULE,change);
  dbSession.commit();
  assertThat(index.findAll().getHits()).hasSize(1);
  Log log=Iterables.getFirst(index.findAll().getHits(),null);
  assertThat(log).isNotNull();
}
