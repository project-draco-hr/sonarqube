{
  IssueDto issueDto=new IssueDto().setId(1L).setRuleId(1).setComponentId(1l).setRootComponentId(100l).setComponentKey("Action.java").setRootComponentKey("struts").setRuleKey("squid","AvoidCycle").setStatus("OPEN").setResolution("OPEN");
  when(issueDao.selectByKey("ABCDE")).thenReturn(issueDto);
  Issue issue=finder.findByKey("ABCDE");
  assertThat(issue).isNotNull();
  assertThat(issue.componentKey()).isEqualTo("Action.java");
  assertThat(issue.ruleKey().toString()).isEqualTo("squid:AvoidCycle");
}
