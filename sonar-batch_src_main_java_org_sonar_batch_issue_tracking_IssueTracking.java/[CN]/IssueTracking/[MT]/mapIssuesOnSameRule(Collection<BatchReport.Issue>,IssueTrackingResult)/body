{
  for (  BatchReport.Issue rawIssue : rawIssues) {
    if (isNotAlreadyMapped(rawIssue,result)) {
      mapIssue(rawIssue,findLastIssueWithSameChecksumAndMessage(rawIssue,result.unmatchedByKeyForRule(ruleKey(rawIssue)).values()),result);
    }
    if (isNotAlreadyMapped(rawIssue,result)) {
      mapIssue(rawIssue,findLastIssueWithSameLineAndMessage(rawIssue,result.unmatchedByKeyForRule(ruleKey(rawIssue)).values()),result);
    }
    if (isNotAlreadyMapped(rawIssue,result)) {
      mapIssue(rawIssue,findLastIssueWithSameChecksum(rawIssue,result.unmatchedByKeyForRule(ruleKey(rawIssue)).values()),result);
    }
  }
}
