{
  WebService.NewAction activate=controller.createAction("activate_rule").setDescription("Activate a rule on a Quality profile").setHandler(new RequestHandler(){
    @Override public void handle(    Request request,    Response response) throws Exception {
      activate(request,response);
    }
  }
).setPost(true).setSince("4.4");
  defineActiveRuleKeyParameters(activate);
  activate.createParam("severity").setDescription("Severity").setPossibleValues(Severity.ALL);
  activate.createParam("params").setDescription("Parameters");
}
