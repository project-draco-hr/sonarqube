{
  RuleKey manualRuleKey=TESTER.get(RuleCreator.class).create(NewRule.createForManualRule("MANUAL_RULE").setName("My manual").setHtmlDescription("Some description"));
  dbSession.commit();
  dbSession.clearCache();
  assertThat(index.getByKey(manualRuleKey).status()).isEqualTo(RuleStatus.READY);
  register(null);
  assertThat(index.getByKey(manualRuleKey).status()).isEqualTo(RuleStatus.READY);
}
