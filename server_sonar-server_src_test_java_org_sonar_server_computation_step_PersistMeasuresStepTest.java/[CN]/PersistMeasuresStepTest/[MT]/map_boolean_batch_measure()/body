{
  BatchReport.Measure batchMeasure=BatchReport.Measure.newBuilder().setValueType(Constants.MeasureValueType.BOOLEAN).setBooleanValue(true).setMetricKey("metric-key").build();
  MeasureDto measure=sut.toMeasureDto(batchMeasure,component);
  assertThat(measure.getValue()).isEqualTo(1.0);
  batchMeasure=BatchReport.Measure.newBuilder().setValueType(Constants.MeasureValueType.BOOLEAN).setBooleanValue(false).setMetricKey("metric-key").build();
  measure=sut.toMeasureDto(batchMeasure,component);
  assertThat(measure.getValue()).isEqualTo(0.0);
  batchMeasure=BatchReport.Measure.newBuilder().setValueType(Constants.MeasureValueType.BOOLEAN).setMetricKey("metric-key").build();
  measure=sut.toMeasureDto(batchMeasure,component);
  assertThat(measure.getValue()).isNull();
}
