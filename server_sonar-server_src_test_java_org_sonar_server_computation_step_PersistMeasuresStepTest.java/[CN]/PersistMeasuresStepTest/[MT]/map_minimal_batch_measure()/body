{
  BatchReport.Component component=defaultComponent().build();
  ComponentDto componentDto=addComponent(component.getRef(),"component-key");
  BatchReport.Measure batchMeasure=BatchReport.Measure.newBuilder().setValueType(MeasureValueType.INT).setMetricKey(METRIC_KEY).build();
  MeasureDto measure=sut.toMeasureDto(batchMeasure,component);
  assertThat(measure).isEqualToComparingFieldByField(new MeasureDto().setComponentId(componentDto.getId()).setSnapshotId(3L).setMetricId(metric.getId()));
}
