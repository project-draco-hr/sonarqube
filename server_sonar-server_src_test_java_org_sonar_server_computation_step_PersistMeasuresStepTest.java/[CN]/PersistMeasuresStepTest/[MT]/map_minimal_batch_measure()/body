{
  BatchReport.Measure batchMeasure=BatchReport.Measure.newBuilder().setValueType(Constants.MeasureValueType.INT).setIntValue(123).setSeverity(Constants.Severity.INFO).setMetricKey("metric-key").setRuleKey("repo:rule-key").build();
  BatchReport.Component component=defaultComponent().build();
  MeasureDto measure=sut.toMeasureDto(batchMeasure,component);
  assertThat(measure).isEqualToComparingFieldByField(expectedMinimalistMeasure());
}
