{
  BatchReport.Measure batchMeasure=BatchReport.Measure.newBuilder().setValueType(Constants.MeasureValueType.DOUBLE).setDoubleValue(3.2).setMetricKey("metric-key").build();
  MeasureDto measure=sut.toMeasureDto(batchMeasure,component);
  assertThat(measure.getValue()).isEqualTo(3.2);
  batchMeasure=BatchReport.Measure.newBuilder().setValueType(Constants.MeasureValueType.DOUBLE).setMetricKey("metric-key").build();
  measure=sut.toMeasureDto(batchMeasure,component);
  assertThat(measure.getValue()).isNull();
}
