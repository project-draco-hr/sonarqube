{
  es.putDocuments(TestIndexDefinition.INDEX,TestIndexDefinition.TYPE,newTestDoc("1",newCoverageBlock("3"),newCoverageBlock("4"),newCoverageBlock("5")),newTestDoc("2",newCoverageBlock("5"),newCoverageBlock("6"),newCoverageBlock("7")));
  List<Map<String,Object>> result=sut.coveredLines("uuid-1","name-1");
  assertThat(result).hasSize(3);
  assertThat(result.get(0).get("uuid")).isEqualTo("main-uuid-3");
  assertThat((List<Integer>)result.get(0).get("lines")).containsOnly(25,33,82);
}
