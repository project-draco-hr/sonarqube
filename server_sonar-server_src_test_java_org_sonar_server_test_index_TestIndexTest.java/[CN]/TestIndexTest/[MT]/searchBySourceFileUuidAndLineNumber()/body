{
  es.putDocuments(TestIndexDefinition.INDEX,TestIndexDefinition.TYPE,newTestDoc("1","TESTFILE1",newCoverageBlock("10"),newCoverageBlock("11"),newCoverageBlock("12")),newTestDoc("2","TESTFILE1",newCoverageBlock("3"),newCoverageBlock("4"),newCoverageBlock("5")),newTestDoc("3","TESTFILE1",newCoverageBlock("5"),newCoverageBlock("6"),newCoverageBlock("7")));
  List<TestDoc> result=underTest.searchBySourceFileUuidAndLineNumber("main-uuid-5",82,searchOptions()).getDocs();
  assertThat(result).hasSize(2);
  assertThat(result).extractingResultOf("name").containsOnly("name-2","name-3");
}
