{
  es.putDocuments(TestIndexDefinition.INDEX,TestIndexDefinition.TYPE,newTestDoc("1",newCoverageBlock("3"),newCoverageBlock("4"),newCoverageBlock("5")),newTestDoc("2",newCoverageBlock("5"),newCoverageBlock("6"),newCoverageBlock("7")));
  List<TestDoc> result=sut.searchByTestUuid("uuid-1",searchOptions()).getDocs();
  assertThat(result).hasSize(1);
  assertThat(result.get(0).testUuid()).isEqualTo("uuid-1");
}
