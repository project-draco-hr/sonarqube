{
  ActiveDashboardDto activeDashboardDto=new ActiveDashboardDto();
  activeDashboardDto.setDashboardId(dashboardDto.getId());
  activeDashboardDto.setOrderIndex(index);
  activeDashboardDao.insert(activeDashboardDto);
