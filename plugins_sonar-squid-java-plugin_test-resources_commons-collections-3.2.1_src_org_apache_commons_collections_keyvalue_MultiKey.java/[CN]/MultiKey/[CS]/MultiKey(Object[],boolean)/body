{
  super();
  if (keys == null) {
    throw new IllegalArgumentException("The array of keys must not be null");
  }
  if (makeClone) {
    this.keys=(Object[])keys.clone();
  }
 else {
    this.keys=keys;
  }
  int total=0;
  for (int i=0; i < keys.length; i++) {
    if (keys[i] != null) {
      total^=keys[i].hashCode();
    }
  }
  hashCode=total;
}
