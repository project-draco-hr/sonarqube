{
  MassUpdate massUpdate=context.prepareMassUpdate();
  massUpdate.select("SELECT e.event_date, e.created_at, e.id FROM events e WHERE event_date_ms IS NULL");
  massUpdate.update("UPDATE events SET event_date_ms=?, created_at_ms=? WHERE id=?");
  massUpdate.rowPluralName("events");
  massUpdate.execute(new EventDateHandler(system2.now()));
}
