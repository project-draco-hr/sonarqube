{
  RuleDto ruleDto=RuleDto.createFor(ruleKey).setName(newRule.name()).setDescription(newRule.markdownDescription()).setDescriptionFormat(Format.MARKDOWN).setSeverity(newRule.severity()).setStatus(RuleStatus.READY).setCreatedAtInMs(system2.now()).setUpdatedAtInMs(system2.now());
  dbClient.ruleDao().insert(dbSession,ruleDto);
  return ruleKey;
}
