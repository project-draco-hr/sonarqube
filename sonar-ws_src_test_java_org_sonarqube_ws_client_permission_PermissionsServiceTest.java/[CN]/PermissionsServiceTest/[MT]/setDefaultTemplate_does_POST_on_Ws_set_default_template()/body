{
  underTest.setDefaultTemplate(new SetDefaultTemplateWsRequest().setQualifier(QUALIFIER_VALUE).setTemplateId(TEMPLATE_ID_VALUE).setTemplateName(TEMPLATE_NAME_VALUE));
  assertThat(serviceTester.getPostParser()).isNull();
  PostRequest postRequest=serviceTester.getPostRequest();
  serviceTester.assertThat(postRequest).hasPath("set_default_template").hasParam(PARAM_QUALIFIER,QUALIFIER_VALUE).hasParam(PARAM_TEMPLATE_ID,TEMPLATE_ID_VALUE).hasParam(PARAM_TEMPLATE_NAME,TEMPLATE_NAME_VALUE).andNoOtherParam();
}
