{
  if (parent.modCount != mods) {
    throw new ConcurrentModificationException();
  }
  if (canRemove == false) {
    throw new IllegalStateException();
  }
  MutableInteger mut=(MutableInteger)current.getValue();
  if (mut.value > 1) {
    mut.value--;
  }
 else {
    entryIterator.remove();
  }
  parent.size--;
  canRemove=false;
}
