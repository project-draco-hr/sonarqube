{
  if (object == this) {
    return true;
  }
  if (object instanceof Bag == false) {
    return false;
  }
  Bag other=(Bag)object;
  if (other.size() != size()) {
    return false;
  }
  for (Iterator it=map.keySet().iterator(); it.hasNext(); ) {
    Object element=it.next();
    if (other.getCount(element) != getCount(element)) {
      return false;
    }
  }
  return true;
}
