{
  modCount++;
  if (nCopies > 0) {
    MutableInteger mut=(MutableInteger)map.get(object);
    size+=nCopies;
    if (mut == null) {
      map.put(object,new MutableInteger(nCopies));
      return true;
    }
 else {
      mut.value+=nCopies;
      return false;
    }
  }
 else {
    return false;
  }
}
