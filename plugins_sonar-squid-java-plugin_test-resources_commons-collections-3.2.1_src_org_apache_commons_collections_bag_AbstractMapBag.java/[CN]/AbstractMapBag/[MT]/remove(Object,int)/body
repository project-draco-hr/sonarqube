{
  MutableInteger mut=(MutableInteger)map.get(object);
  if (mut == null) {
    return false;
  }
  if (nCopies <= 0) {
    return false;
  }
  modCount++;
  if (nCopies < mut.value) {
    mut.value-=nCopies;
    size-=nCopies;
  }
 else {
    map.remove(object);
    size-=mut.value;
  }
  return true;
}
