{
  int hash=ref.hashCode();
  int index=hashIndex(hash,data.length);
  HashEntry previous=null;
  HashEntry entry=data[index];
  while (entry != null) {
    if (((ReferenceEntry)entry).purge(ref)) {
      if (previous == null) {
        data[index]=entry.next;
      }
 else {
        previous.next=entry.next;
      }
      this.size--;
      return;
    }
    previous=entry;
    entry=entry.next;
  }
}
