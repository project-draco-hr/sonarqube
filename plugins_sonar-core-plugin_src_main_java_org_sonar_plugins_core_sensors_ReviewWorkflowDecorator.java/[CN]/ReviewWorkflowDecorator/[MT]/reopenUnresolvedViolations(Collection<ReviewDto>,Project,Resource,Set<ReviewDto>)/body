{
  for (  ReviewDto openReview : openReviews) {
    if (ReviewDto.STATUS_RESOLVED.equals(openReview.getStatus()) && !ReviewDto.RESOLUTION_FALSE_POSITIVE.equals(openReview.getResolution()) && !openReview.isManualViolation()) {
      reopen(openReview,project,resource);
      updated.add(openReview);
    }
  }
}
