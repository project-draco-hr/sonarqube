{
  if (projectBuilder == null) {
    return null;
  }
  Method toString;
  try {
    toString=projectBuilder.getClass().getMethod("toString");
  }
 catch (  Exception e) {
    return projectBuilder.toString();
  }
  if (toString.getDeclaringClass() != Object.class) {
    return projectBuilder.toString();
  }
 else {
    return projectBuilder.getClass().getName();
  }
}
