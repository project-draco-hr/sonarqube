{
  System.out.println("received isReady()");
  if (ready) {
    return true;
  }
  System.out.println("checking server.isStarted()");
  if (server.isStarted()) {
    System.out.println("moving to ready");
    ready=true;
    writeTimeToFile("readyAt");
  }
  System.out.println("ready: " + ready);
  return ready;
}
