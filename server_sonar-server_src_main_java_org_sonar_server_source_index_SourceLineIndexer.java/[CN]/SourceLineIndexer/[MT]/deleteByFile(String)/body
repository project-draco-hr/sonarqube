{
  SearchRequestBuilder searchRequest=esClient.prepareSearch(SourceLineIndexDefinition.INDEX).setTypes(SourceLineIndexDefinition.TYPE).setQuery(QueryBuilders.filteredQuery(QueryBuilders.matchAllQuery(),FilterBuilders.termFilter(FIELD_FILE_UUID,fileUuid).cache(false)));
  BulkIndexer.delete(esClient,SourceLineIndexDefinition.INDEX,searchRequest);
}
