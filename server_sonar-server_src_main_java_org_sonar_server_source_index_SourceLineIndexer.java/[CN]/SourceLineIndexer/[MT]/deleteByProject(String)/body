{
  SearchRequestBuilder searchRequest=esClient.prepareSearch(SourceLineIndexDefinition.INDEX).setRouting(projectUuid).setTypes(SourceLineIndexDefinition.TYPE).setQuery(QueryBuilders.filteredQuery(QueryBuilders.matchAllQuery(),FilterBuilders.termFilter(FIELD_PROJECT_UUID,projectUuid).cache(false)));
  BulkIndexer.delete(esClient,SourceLineIndexDefinition.INDEX,searchRequest);
}
