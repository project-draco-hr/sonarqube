{
  int numberOfLines=fileRow.getLineUpdateRequests().size();
  SearchRequestBuilder searchRequest=esClient.prepareSearch(SourceLineIndexDefinition.INDEX).setTypes(SourceLineIndexDefinition.TYPE).setRouting(fileRow.getProjectUuid()).setQuery(QueryBuilders.filteredQuery(QueryBuilders.matchAllQuery(),FilterBuilders.boolFilter().must(FilterBuilders.termFilter(FIELD_FILE_UUID,fileRow.getFileUuid()).cache(false)).must(FilterBuilders.rangeFilter(SourceLineIndexDefinition.FIELD_LINE).gt(numberOfLines).cache(false)).cache(false)));
  bulk.addDeletion(searchRequest);
}
