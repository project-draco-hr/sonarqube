{
  NewController controller=context.newController("api/issue_filters").setSince("4.2").setDescription("Issue Filters");
  NewAction search=controller.newAction("page");
  search.setDescription("Data required for rendering page 'Issues'. Internal use only.").setPrivate(true).setHandler(new RequestHandler(){
    @Override public void handle(    Request request,    Response response) throws Exception {
      page(request,response);
    }
  }
);
  controller.done();
}
