{
  thrown.expect(IllegalArgumentException.class);
  thrown.expectMessage("severity 'unknown' is invalid");
  new TestIssue.Builder().setKey("ABCD").setRuleKey(RuleKey.of("xoo","S01")).setSeverity("unknown").setStatus(org.sonar.api.issue.Issue.STATUS_RESOLVED).setResolution(org.sonar.api.issue.Issue.RESOLUTION_FIXED).build();
}
