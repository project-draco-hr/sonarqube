{
  thrown.expect(NullPointerException.class);
  thrown.expectMessage("key cannot be null");
  new TestIssue.Builder().setRuleKey(RuleKey.of("xoo","S01")).setSeverity(Severity.BLOCKER).setStatus(org.sonar.api.issue.Issue.STATUS_RESOLVED).setResolution(org.sonar.api.issue.Issue.RESOLUTION_FIXED).build();
