{
  thrown.expect(IllegalArgumentException.class);
  thrown.expectMessage("status 'unknown' is invalid");
  new TestIssue.Builder().setKey("ABCD").setRuleKey(RuleKey.of("xoo","S01")).setSeverity(Severity.BLOCKER).setStatus("unknown").setResolution(org.sonar.api.issue.Issue.RESOLUTION_FIXED).build();
}
