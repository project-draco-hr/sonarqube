{
  addProperties(batchSettings.getProperties());
  ProjectDefinition def=moduleDefinition;
  do {
    if (projectRepos.moduleExists(def.getKeyWithBranch())) {
      addProperties(projectRepos.settings(def.getKeyWithBranch()));
      break;
    }
    def=def.getParent();
  }
 while (def != null);
}
