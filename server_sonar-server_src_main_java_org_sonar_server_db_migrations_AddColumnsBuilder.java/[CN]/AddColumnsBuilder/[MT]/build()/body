{
  StringBuilder sql=new StringBuilder().append("ALTER TABLE ").append(tableName).append(" ");
switch (dialect.getId()) {
case PostgreSql.ID:
    addColumns(sql,"ADD COLUMN ");
  break;
case MsSql.ID:
sql.append("ADD ");
addColumns(sql,"");
break;
default :
sql.append("ADD (");
addColumns(sql,"");
sql.append(")");
}
return sql.toString();
}
