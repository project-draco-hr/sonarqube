{
  try {
    return FileUtils.readFileToString(inputFile.getFile(),sourcesEncoding.name());
  }
 catch (  Exception e) {
    throw new SonarException("Unable to read and import the source file : '" + inputFile.getFile().getAbsolutePath() + "' with the charset : '"+ sourcesEncoding.name()+ "'.",e);
  }
}
