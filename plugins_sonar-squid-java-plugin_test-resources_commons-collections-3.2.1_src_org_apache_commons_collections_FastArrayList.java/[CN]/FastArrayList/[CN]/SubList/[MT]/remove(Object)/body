{
  if (fast) {
synchronized (FastArrayList.this) {
      ArrayList temp=(ArrayList)list.clone();
      boolean r=get(temp).remove(o);
      if (r)       last--;
      list=temp;
      expected=temp;
      return r;
    }
  }
 else {
synchronized (list) {
      return get(expected).remove(o);
    }
  }
}
