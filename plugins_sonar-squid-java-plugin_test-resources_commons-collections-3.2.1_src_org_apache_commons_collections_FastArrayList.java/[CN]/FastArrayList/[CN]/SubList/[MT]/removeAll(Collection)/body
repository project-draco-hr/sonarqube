{
  if (fast) {
synchronized (FastArrayList.this) {
      ArrayList temp=(ArrayList)list.clone();
      List sub=get(temp);
      boolean r=sub.removeAll(o);
      if (r)       last=first + sub.size();
      list=temp;
      expected=temp;
      return r;
    }
  }
 else {
synchronized (list) {
      return get(expected).removeAll(o);
    }
  }
}
