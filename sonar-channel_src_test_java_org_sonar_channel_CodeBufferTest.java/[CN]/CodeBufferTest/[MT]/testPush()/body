{
  CodeReader reader=new CodeReader("12",defaulConfiguration);
  assertEquals('1',(char)reader.pop());
  reader.push("a");
  assertEquals('a',(char)reader.peek());
  reader.push("45");
  assertEquals("45a2",new String(reader.peek(4)));
  for (int i=0; i < 4; i++) {
    reader.pop();
  }
  assertEquals(-1,reader.pop());
}
