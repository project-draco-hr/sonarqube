{
  return ResourceIndexerFilter.create().setQualifiers(new String[]{Qualifiers.PROJECT,Qualifiers.MODULE,Qualifiers.VIEW,Qualifiers.SUBVIEW,Qualifiers.DIRECTORY,Qualifiers.PACKAGE,Qualifiers.FILE,Qualifiers.CLASS,Qualifiers.UNIT_TEST_FILE}).setScopes(new String[]{Scopes.PROJECT,Scopes.DIRECTORY,Scopes.FILE});
}
