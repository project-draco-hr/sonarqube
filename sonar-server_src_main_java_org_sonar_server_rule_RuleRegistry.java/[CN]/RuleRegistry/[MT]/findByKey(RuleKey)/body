{
  final SearchHits hits=searchIndex.executeRequest(searchIndex.client().prepareSearch(INDEX_RULES).setTypes(TYPE_RULE).setPostFilter(boolFilter().must(termFilter(RuleDocument.FIELD_REPOSITORY_KEY,key.repository()),termFilter(RuleDocument.FIELD_KEY,key.rule()))));
  if (hits.totalHits() == 0) {
    return null;
  }
 else {
    return RuleDocumentParser.parse(hits.hits()[0].sourceAsMap());
  }
}
