{
  GroupDto group=insertGroup("admins");
  UserDto user=insertUser("my-admin");
  session.commit();
  userSession.login("admin").setGlobalPermissions(GlobalPermissions.SYSTEM_ADMIN);
  tester.newPostRequest("api/usergroups","remove_user").setParam("groupId",group.getId().toString()).setParam("userLogin",user.getLogin()).execute().assertNoContent();
  assertThat(groupMembershipDao.selectGroupsByLogins(session,Arrays.asList(user.getLogin())).get(user.getLogin())).isEmpty();
}
