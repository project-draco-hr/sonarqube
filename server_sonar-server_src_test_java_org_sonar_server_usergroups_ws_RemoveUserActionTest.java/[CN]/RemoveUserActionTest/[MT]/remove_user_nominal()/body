{
  GroupDto users=insertGroup("users");
  UserDto user=insertUser("my-admin");
  insertMember(users.getId(),user.getId());
  session.commit();
  userSession.login("admin").setGlobalPermissions(GlobalPermissions.SYSTEM_ADMIN);
  tester.newPostRequest("api/usergroups","remove_user").setParam("groupId",users.getId().toString()).setParam("userLogin",user.getLogin()).execute().assertNoContent();
  assertThat(groupMembershipDao.selectGroupsByLogins(session,Arrays.asList(user.getLogin())).get(user.getLogin())).isEmpty();
}
