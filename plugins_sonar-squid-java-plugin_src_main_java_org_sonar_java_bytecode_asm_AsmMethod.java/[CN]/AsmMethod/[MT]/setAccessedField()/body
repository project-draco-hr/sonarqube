{
  if (!isConstructor()) {
    for (    AsmEdge edge : getOutgoingEdges()) {
      if (isCallToNonStaticInternalField(edge)) {
        if (accessedField != null && accessedField != edge.getTo()) {
          accessedField=null;
          break;
        }
        accessedField=(AsmField)edge.getTo();
      }
 else       if (isCallToNonStaticInternalMethod(edge)) {
        accessedField=null;
        break;
      }
    }
  }
}
