{
  SetRequest setRequest=SetRequest.builder().setKey(request.mandatoryParam(PARAM_KEY)).setValue(request.param(PARAM_VALUE)).setValues(request.multiParam(PARAM_VALUES)).setComponentId(request.param(PARAM_COMPONENT_ID)).setComponentKey(request.param(PARAM_COMPONENT_KEY)).build();
  checkValueIsSet(setRequest);
  return setRequest;
}
