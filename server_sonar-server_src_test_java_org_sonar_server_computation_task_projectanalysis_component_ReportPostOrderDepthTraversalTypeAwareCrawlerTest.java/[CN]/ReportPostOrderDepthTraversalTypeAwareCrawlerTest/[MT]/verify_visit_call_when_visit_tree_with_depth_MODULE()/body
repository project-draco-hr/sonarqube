{
  moduleCrawler.visit(COMPONENT_TREE);
  assertThat(moduleVisitor.callsRecords).containsExactly(reportCallRecord("visitAny",MODULE_3),reportCallRecord("visitModule",MODULE_3),reportCallRecord("visitAny",MODULE_2),reportCallRecord("visitModule",MODULE_2),reportCallRecord("visitAny",COMPONENT_TREE),reportCallRecord("visitProject",COMPONENT_TREE));
}
