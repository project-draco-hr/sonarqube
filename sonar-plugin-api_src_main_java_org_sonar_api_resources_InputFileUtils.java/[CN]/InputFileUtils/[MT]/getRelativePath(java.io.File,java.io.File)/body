{
  List<String> stack=Lists.newArrayList(file.getName());
  java.io.File cursor=file.getParentFile();
  while (cursor != null) {
    if (basedir.equals(cursor)) {
      return StringUtils.join(stack,"/");
    }
    stack.add(0,cursor.getName());
    cursor=cursor.getParentFile();
  }
  return null;
}
