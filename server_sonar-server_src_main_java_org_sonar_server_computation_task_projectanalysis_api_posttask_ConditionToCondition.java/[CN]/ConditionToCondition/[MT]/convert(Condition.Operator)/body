{
switch (operator) {
case EQUALS:
    return QualityGate.Operator.EQUALS;
case NOT_EQUALS:
  return QualityGate.Operator.NOT_EQUALS;
case GREATER_THAN:
return QualityGate.Operator.GREATER_THAN;
case LESS_THAN:
return QualityGate.Operator.LESS_THAN;
default :
throw new IllegalArgumentException(format("Unsupported value '%s' of Condition.Operation can not be converted to QualityGate.Operator",operator));
}
}
