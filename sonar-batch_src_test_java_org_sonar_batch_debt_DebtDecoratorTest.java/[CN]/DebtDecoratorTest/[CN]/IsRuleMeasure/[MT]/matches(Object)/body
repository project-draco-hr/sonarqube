{
  if (!(o instanceof RuleMeasure)) {
    return false;
  }
  RuleMeasure m=(RuleMeasure)o;
  return ObjectUtils.equals(metric,m.getMetric()) && ObjectUtils.equals(ruleKey.repository(),m.getRule().getRepositoryKey()) && ObjectUtils.equals(ruleKey.rule(),m.getRule().getKey())&& ObjectUtils.equals(value,m.getValue());
}
