{
  JarFile jar=null;
  try {
    jar=new JarFile(file);
    if (jar.getManifest() != null) {
      loadManifest(jar.getManifest());
    }
  }
 catch (  Exception e) {
    throw new RuntimeException("Unable to read plugin manifest from jar : " + file.getAbsolutePath(),e);
  }
 finally {
    if (jar != null) {
      jar.close();
    }
  }
}
