{
  session.select("selectAllKeysAfterTimestamp",new Timestamp(timestamp),new ResultHandler(){
    @Override public void handleResult(    ResultContext context){
      Map<String,Object> fields=(Map<String,Object>)context.getResultObject();
      ActiveRuleKey key=ActiveRuleKey.of(QualityProfileKey.of((String)fields.get("PROFILEFIELD"),(String)fields.get("LANGUAGEFIELD")),RuleKey.of((String)fields.get("REPOSITORYFIELD"),(String)fields.get("RULEFIELD")));
      session.enqueue(new KeyIndexAction<ActiveRuleKey>(getIndexType(),IndexAction.Method.UPSERT,key));
    }
  }
);
}
