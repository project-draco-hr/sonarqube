{
  try {
    JsonWriter writer=new JsonWriter(out);
    writer.setIndent("  ");
    writer.beginArray();
    for (    G issue : issues) {
      gson.toJson(converter.apply(issue),PreviousIssue.class,writer);
    }
    writer.endArray();
    writer.close();
  }
 catch (  IOException e) {
    throw new IllegalStateException("Unable to stream issues",e);
  }
}
