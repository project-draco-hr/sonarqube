{
  json.beginObject();
  json.prop("total",result.getTotal());
  json.prop(Param.PAGE,paging.pageIndex());
  json.prop(Param.PAGE_SIZE,paging.pageSize());
  json.name("events").beginArray();
  for (  QProfileActivity event : result.getHits()) {
    json.beginObject().prop("date",DateUtils.formatDateTime(event.getCreatedAt())).prop("authorLogin",event.getLogin()).prop("authorName",event.authorName()).prop("action",event.getAction()).prop("ruleKey",event.ruleKey().toString()).prop("ruleName",event.ruleName());
    writeParameters(json,event);
    json.endObject();
  }
  json.endArray();
  json.endObject().close();
}
