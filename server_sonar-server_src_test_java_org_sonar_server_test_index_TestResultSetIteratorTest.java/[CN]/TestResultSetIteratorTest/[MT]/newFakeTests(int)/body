{
  List<FileSources.Test> tests=new ArrayList<>();
  for (int i=1; i <= numberOfTests; i++) {
    FileSources.Test.Builder test=FileSources.Test.newBuilder().setUuid("TEST_FILE_UUID_" + i).setName("NAME_" + i).setStatus(FileSources.Test.TestStatus.FAILURE).setStacktrace("STACKTRACE_" + i).setMsg("MESSAGE_" + i).setExecutionTimeMs(i);
    for (int j=1; j <= numberOfTests; j++) {
      test.addCoveredFile(FileSources.Test.CoveredFile.newBuilder().setFileUuid("MAIN_FILE_UUID_" + j).addCoveredLine(j));
    }
    tests.add(test.build());
  }
  return tests;
}
