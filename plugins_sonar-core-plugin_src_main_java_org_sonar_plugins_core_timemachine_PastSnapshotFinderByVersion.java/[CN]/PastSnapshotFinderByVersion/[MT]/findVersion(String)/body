{
  String hql="from " + Snapshot.class.getSimpleName() + " where version=:version AND resourceId=:resourceId AND status=:status order by createdAt desc";
  List<Snapshot> snapshots=session.createQuery(hql).setParameter("version",version).setParameter("resourceId",projectSnapshot.getResourceId()).setParameter("status",Snapshot.STATUS_PROCESSED).setMaxResults(1).getResultList();
  return snapshots.isEmpty() ? null : snapshots.get(0);
}
