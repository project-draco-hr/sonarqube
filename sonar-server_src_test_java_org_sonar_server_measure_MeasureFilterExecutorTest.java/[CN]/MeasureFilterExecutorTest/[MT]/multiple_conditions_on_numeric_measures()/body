{
  db.prepareDbUnit(getClass(),"shared.xml");
  MeasureFilter filter=new MeasureFilter().setResourceQualifiers(Arrays.asList("CLA")).setSortOnMetric(METRIC_LINES).addCondition(new MeasureFilterCondition(METRIC_LINES,MeasureFilterCondition.Operator.GREATER,2)).addCondition(new MeasureFilterCondition(METRIC_LINES,MeasureFilterCondition.Operator.LESS_OR_EQUALS,50));
  List<MeasureFilterRow> rows=executor.execute(filter,new MeasureFilterContext());
  assertThat(rows).hasSize(1);
  verifyJavaTinyFile(rows.get(0));
}
