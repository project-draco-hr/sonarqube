{
  if (current.size() > 0)   return true;
  while (bucket < buckets.length) {
synchronized (locks[bucket]) {
      Node n=buckets[bucket];
      while (n != null) {
        current.add(n);
        n=n.next;
      }
      bucket++;
      if (current.size() > 0)       return true;
    }
  }
  return false;
}
