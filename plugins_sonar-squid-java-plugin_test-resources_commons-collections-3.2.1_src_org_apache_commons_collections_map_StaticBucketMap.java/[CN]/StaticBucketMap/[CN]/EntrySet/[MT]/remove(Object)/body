{
  if (obj instanceof Map.Entry == false) {
    return false;
  }
  Map.Entry entry=(Map.Entry)obj;
  int hash=getHash(entry.getKey());
synchronized (locks[hash]) {
    for (Node n=buckets[hash]; n != null; n=n.next) {
      if (n.equals(entry)) {
        StaticBucketMap.this.remove(n.getKey());
        return true;
      }
    }
  }
  return false;
}
