{
  int hash=getHash(key);
synchronized (locks[hash]) {
    Node n=buckets[hash];
    Node prev=null;
    while (n != null) {
      if (n.key == key || (n.key != null && n.key.equals(key))) {
        if (null == prev) {
          buckets[hash]=n.next;
        }
 else {
          prev.next=n.next;
        }
        locks[hash].size--;
        return n.value;
      }
      prev=n;
      n=n.next;
    }
  }
  return null;
}
