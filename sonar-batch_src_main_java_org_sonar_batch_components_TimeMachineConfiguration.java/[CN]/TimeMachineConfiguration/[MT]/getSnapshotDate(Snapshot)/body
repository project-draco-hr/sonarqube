{
  Query query=session.createNativeQuery("select s.created_at from snapshots s where s.project_id=:projectId and (s.root_snapshot_id=:rootSnapshotId or s.id=:rootSnapshotId) and s.islast=:islast");
  query.setParameter("projectId",project.getId());
  query.setParameter("rootSnapshotId",projectSnapshot.getId());
  query.setParameter("islast",Boolean.TRUE);
  Date date=session.getSingleResult(query,null);
  return date;
}
