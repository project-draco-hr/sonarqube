{
  checkMod();
  while (nextNull()) {
    Entry e=entry;
    int i=index;
    while ((e == null) && (i > 0)) {
      i--;
      e=table[i];
    }
    entry=e;
    index=i;
    if (e == null) {
      currentKey=null;
      currentValue=null;
      return false;
    }
    nextKey=e.getKey();
    nextValue=e.getValue();
    if (nextNull())     entry=entry.next;
  }
  return true;
}
