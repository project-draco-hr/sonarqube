{
  int hash=ref.hashCode();
  int index=indexFor(hash);
  Entry previous=null;
  Entry entry=table[index];
  while (entry != null) {
    if (entry.purge(ref)) {
      if (previous == null)       table[index]=entry.next;
 else       previous.next=entry.next;
      this.size--;
      return;
    }
    previous=entry;
    entry=entry.next;
  }
}
