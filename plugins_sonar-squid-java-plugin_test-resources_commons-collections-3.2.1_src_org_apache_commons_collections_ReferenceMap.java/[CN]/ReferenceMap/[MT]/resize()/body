{
  Entry[] old=table;
  table=new Entry[old.length * 2];
  for (int i=0; i < old.length; i++) {
    Entry next=old[i];
    while (next != null) {
      Entry entry=next;
      next=next.next;
      int index=indexFor(entry.hash);
      entry.next=table[index];
      table[index]=entry;
    }
    old[i]=null;
  }
  threshold=(int)(table.length * loadFactor);
}
