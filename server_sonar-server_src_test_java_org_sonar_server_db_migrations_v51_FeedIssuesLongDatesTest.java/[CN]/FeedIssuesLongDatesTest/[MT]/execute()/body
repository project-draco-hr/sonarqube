{
  db.prepareDbUnit(getClass(),"before.xml");
  DatabaseMigration migration=new FeedIssuesLongDates(db.database(),System2.INSTANCE);
  migration.execute();
  int count=db.countSql("select count(*) from issues where " + "issue_creation_date_ms is not null " + "and issue_update_date_ms is not null "+ "and issue_close_date_ms is not null");
  assertThat(count).isEqualTo(2);
}
