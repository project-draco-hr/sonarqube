{
  for (int x=0; x < dimension; x++) {
    for (int y=0; y < dimension; y++) {
      V from=vertices[x];
      V to=vertices[y];
      Edge<V> edge=graph.getEdge(from,to);
      if (edge != null) {
        boolean isFeedbackEdge=feedbackEdges.contains(edge);
        cells[x][y]=new DsmCell(edge,isFeedbackEdge);
      }
    }
  }
}
