{
  session.select("selectKeysOfRulesUpdatedSince",new Timestamp(timestamp),new ResultHandler(){
    @Override public void handleResult(    ResultContext context){
      Map<String,String> map=(Map)context.getResultObject();
      session.enqueue(new KeyIndexAction<RuleKey>(getIndexType(),IndexAction.Method.UPSERT,RuleKey.of(map.get("repoField"),map.get("ruleField"))));
    }
  }
);
}
