{
  String workingDirectory=env.rootDir().getAbsolutePath();
  elasticsearch=new ProcessWrapper(env.rootDir().getAbsolutePath(),"org.sonar.search.ElasticSearch",ImmutableMap.of("esDebug",properties.containsKey("esDebug") ? properties.get("esDebug") : "false","esPort",esPort,"esHome",env.rootDir().getAbsolutePath()),"ES",env.rootDir().getAbsolutePath() + "/lib/search/sonar-search-4.5-SNAPSHOT.jar");
  monitor.registerProcess(elasticsearch);
  monitor.start();
  try {
    monitor.join();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
  shutdown();
}
