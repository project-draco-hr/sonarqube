{
  elasticsearch=new ProcessWrapper("org.sonar.search.ElasticSearch",new String[]{env.rootDir().getAbsolutePath() + "/lib/search/sonar-search-4.5-SNAPSHOT.jar"},ImmutableMap.of("esPort",esPort,"esHome",env.rootDir().getAbsolutePath()),"ES",monitor.getMonitoringPort());
  registerProcess(elasticsearch);
}
