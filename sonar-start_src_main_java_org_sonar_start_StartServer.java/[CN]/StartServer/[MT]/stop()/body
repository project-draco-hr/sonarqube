{
  LOGGER.info("Shutting down all node services");
  Runtime.getRuntime().removeShutdownHook(shutdownHook);
  if (elasticsearch != null) {
    LOGGER.info("Shutting down ES service");
    elasticsearch.stop();
  }
  if (sonarqube != null) {
    LOGGER.info("Shutting down SQ service");
    sonarqube.stop();
  }
}
