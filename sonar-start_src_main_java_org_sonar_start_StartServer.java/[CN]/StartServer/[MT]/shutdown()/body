{
  LOGGER.info("Shutting down sonar Node");
  if (monitor != null) {
    monitor.interrupt();
    monitor=null;
  }
  if (elasticsearch != null) {
    elasticsearch.shutdown();
    elasticsearch=null;
  }
  if (sonarqube != null) {
    sonarqube.shutdown();
    sonarqube=null;
  }
  if (executor != null) {
    executor.shutdown();
    try {
      executor.awaitTermination(10L,TimeUnit.SECONDS);
    }
 catch (    InterruptedException e) {
      LOGGER.warn("Executing terminated",e);
    }
    executor=null;
  }
}
