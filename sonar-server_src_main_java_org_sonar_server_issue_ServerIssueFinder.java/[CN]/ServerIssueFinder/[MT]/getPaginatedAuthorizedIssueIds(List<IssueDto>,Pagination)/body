{
  Set<Long> issueIds=Sets.newLinkedHashSet();
  int index=0;
  for (  IssueDto issueDto : authorizedIssues) {
    if (index >= pagination.offset() && issueIds.size() < pagination.limit()) {
      issueIds.add(issueDto.getId());
    }
 else     if (issueIds.size() >= pagination.limit()) {
      break;
    }
    index++;
  }
  return issueIds;
}
