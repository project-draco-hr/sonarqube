{
  this.qualityGateDao=qualityGateDao;
  this.conditionDao=conditionDao;
  this.conditionDtoToBean=new Function<QualityGateConditionDto,Condition>(){
    @Override @Nonnull public Condition apply(    @Nonnull QualityGateConditionDto input){
      Metric metric=metricRepository.getById(input.getMetricId());
      return new Condition(metric,input.getPeriod(),input.getOperator(),input.getErrorThreshold(),input.getWarningThreshold());
    }
  }
;
}
