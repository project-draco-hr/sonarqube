{
  int leastIndex=-1;
  Object leastObject=null;
  for (int i=0; i < values.size(); i++) {
    if (valueSet.get(i) == false) {
      set(i);
    }
    if (valueSet.get(i)) {
      if (leastIndex == -1) {
        leastIndex=i;
        leastObject=values.get(i);
      }
 else {
        Object curObject=values.get(i);
        if (comparator.compare(curObject,leastObject) < 0) {
          leastObject=curObject;
          leastIndex=i;
        }
      }
    }
  }
  return leastIndex;
}
