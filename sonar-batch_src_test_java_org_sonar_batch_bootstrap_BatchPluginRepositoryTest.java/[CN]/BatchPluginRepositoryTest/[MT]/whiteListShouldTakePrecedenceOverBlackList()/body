{
  Settings settings=new Settings().setProperty(CoreProperties.BATCH_INCLUDE_PLUGINS,"checkstyle,pmd,findbugs").setProperty(CoreProperties.BATCH_EXCLUDE_PLUGINS,"cobertura,pmd");
  BatchPluginRepository.PluginFilter filter=new BatchPluginRepository.PluginFilter(settings);
  assertThat(filter.accepts("pmd")).isTrue();
}
