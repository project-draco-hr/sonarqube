{
  IndexProperties.ES_TYPE type=settings.hasKey(IndexProperties.TYPE) ? IndexProperties.ES_TYPE.valueOf(settings.getString(IndexProperties.TYPE)) : IndexProperties.ES_TYPE.DATA;
switch (type) {
case MEMORY:
    initMemoryES();
  break;
case TRANSPORT:
initTransportES();
break;
case DATA:
default :
initDataES();
break;
}
}
