{
  PermissionTemplateGroupDto permissionTemplateGroup=new PermissionTemplateGroupDto().setTemplateId(templateId).setPermission(permission).setGroupId(groupId).setCreatedAt(now()).setUpdatedAt(now());
  SqlSession session=myBatis.openSession(false);
  try {
    mapper(session).insertGroupPermission(permissionTemplateGroup);
    session.commit();
  }
  finally {
    MyBatis.closeQuietly(session);
  }
}
