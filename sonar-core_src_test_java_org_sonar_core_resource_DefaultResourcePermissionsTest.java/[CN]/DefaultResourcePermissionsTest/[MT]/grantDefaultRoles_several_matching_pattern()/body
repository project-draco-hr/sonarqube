{
  setupData("grantDefaultRolesSeveralPattern");
  settings.setProperty("sonar.permission.template.default","default");
  throwable.expect(IllegalStateException.class);
  throwable.expectMessage("The \"foo.project\" key matches multiple permission templates: \"Start with foo\", \"Start with foo again\". A system administrator must update these templates so that only one of them matches the key.");
  permissions.grantDefaultRoles(project);
}
