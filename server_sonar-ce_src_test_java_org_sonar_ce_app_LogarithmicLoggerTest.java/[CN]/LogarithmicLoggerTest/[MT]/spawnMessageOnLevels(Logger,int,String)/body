{
  int c=i % 5;
switch (c) {
case 0:
    logarithmicLogger.error(msg);
  break;
case 1:
logarithmicLogger.warn(msg);
break;
case 2:
logarithmicLogger.info(msg);
break;
case 3:
logarithmicLogger.debug(msg);
break;
case 4:
logarithmicLogger.trace(msg);
break;
default :
throw new IllegalArgumentException("Unsupported value " + c);
}
}
