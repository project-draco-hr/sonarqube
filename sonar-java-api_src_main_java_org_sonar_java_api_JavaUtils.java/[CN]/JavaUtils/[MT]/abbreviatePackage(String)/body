{
  String[] parts=StringUtils.split(packageName,PACKAGE_SEPARATOR);
  StringBuilder sb=new StringBuilder();
  if (parts.length >= 1) {
    sb.append(parts[0]);
  }
  for (int index=1; index < parts.length; index++) {
    sb.append(PACKAGE_SEPARATOR).append(parts[index].charAt(0));
  }
  return sb.toString();
}
