{
  try {
    exchange.clear();
    exchange.append(group);
    Key key=new Key(exchange.getKey());
    key.to(Key.AFTER);
    exchange.removeKeyRange(exchange.getKey(),key);
    return this;
  }
 catch (  Exception e) {
    throw new IllegalStateException("Fail to clear cache group: " + group,e);
  }
}
