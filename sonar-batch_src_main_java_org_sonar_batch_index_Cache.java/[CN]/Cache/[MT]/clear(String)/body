{
  try {
    exchange.clear();
    exchange.append(group);
    Key key=new Key(exchange.getKey());
    key.append(Key.AFTER);
    exchange.removeKeyRange(exchange.getKey(),key);
    return this;
  }
 catch (  Exception e) {
    throw new IllegalStateException("Fail to clear group '" + group + "' from cache "+ name,e);
  }
}
