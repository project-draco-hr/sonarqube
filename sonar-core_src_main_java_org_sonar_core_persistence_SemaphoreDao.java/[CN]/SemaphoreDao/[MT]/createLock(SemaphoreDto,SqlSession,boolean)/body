{
  Semaphores.Semaphore semaphore=new Semaphores.Semaphore().setName(dto.getName()).setLocked(acquired).setLockedAt(dto.getLockedAt()).setCreatedAt(dto.getCreatedAt()).setUpdatedAt(dto.getUpdatedAt());
  if (!acquired) {
    semaphore.setDurationSinceLocked(getDurationSinceLocked(dto,session));
  }
  return semaphore;
}
