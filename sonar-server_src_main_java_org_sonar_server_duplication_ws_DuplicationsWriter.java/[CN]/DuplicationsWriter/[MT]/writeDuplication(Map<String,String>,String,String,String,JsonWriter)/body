{
  String ref=refByComponentKey.get(componentKey);
  if (ref == null) {
    ref=Integer.toString(refByComponentKey.size() + 1);
    refByComponentKey.put(componentKey,ref);
  }
  json.beginObject();
  json.prop("from",Integer.valueOf(from));
  json.prop("size",Integer.valueOf(size));
  json.prop("_ref",ref);
  json.endObject();
}
