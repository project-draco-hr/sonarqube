{
  if (totalTime < 1000) {
    return String.format("%sms",totalTime);
  }
 else {
    long sec=totalTime / 1000;
    if (sec < 60) {
      return String.format("%ss",sec);
    }
 else {
      long min=sec / 60;
      long remainingSec=sec - (min * 60);
      if (remainingSec > 0) {
        return String.format("%smin %ss",min,remainingSec);
      }
 else {
        return String.format("%smin",min);
      }
    }
  }
}
