{
  Query query=getSession().createQuery("SELECT m.id FROM " + MeasureModel.class.getSimpleName() + " m, "+ Snapshot.class.getSimpleName()+ " s WHERE s.id = m.snapshotId and "+ "(s.rootId=:rootSid OR s.id=:rootSid) and (m.rule is not null or m.rulesCategoryId is not null or m.rulePriority is not null)");
  query.setParameter("rootSid",sid);
  List<Integer> measureIds=query.getResultList();
  deleteMeasuresById(measureIds);
}
