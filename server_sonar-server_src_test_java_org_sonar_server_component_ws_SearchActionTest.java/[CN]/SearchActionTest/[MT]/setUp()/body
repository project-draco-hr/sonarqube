{
  userSession.login().setGlobalPermissions(GlobalPermissions.SYSTEM_ADMIN);
  resourceTypes.setAllQualifiers(Qualifiers.PROJECT,Qualifiers.MODULE,Qualifiers.DIRECTORY,Qualifiers.FILE);
