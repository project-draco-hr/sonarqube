{
  DsmCell cell=dsm.cell(x,y);
  json.append('{');
  if (cell != null && cell.getEdge() != null && cell.getWeight() > 0) {
    Dependency dep=(Dependency)cell.getEdge();
    json.append("\"i\":");
    json.append(dep.getId());
    json.append(",\"w\":");
    json.append(cell.getWeight());
  }
  json.append('}');
}
