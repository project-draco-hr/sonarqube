{
  StringBuilder content=new StringBuilder();
  for (int i=0; i < lines.length; i++) {
    content.append(lines[i]);
    if (i != (lines.length - 1)) {
switch (endOfLineDelimiter) {
case LF:
        content.append('\n');
      break;
case CR:
    content.append('\r');
  break;
case CR_PLUS_LF:
content.append("\r\n");
break;
}
}
}
return content.toString();
}
