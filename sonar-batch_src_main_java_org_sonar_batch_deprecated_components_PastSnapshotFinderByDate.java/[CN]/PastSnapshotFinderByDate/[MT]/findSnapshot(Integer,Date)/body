{
  String hql="from " + Snapshot.class.getSimpleName() + " where createdAt>=:date AND resourceId=:resourceId AND status=:status order by createdAt asc";
  List<Snapshot> snapshots=session.createQuery(hql).setParameter("date",dateToLong(date)).setParameter("resourceId",projectId).setParameter("status",Snapshot.STATUS_PROCESSED).setMaxResults(1).getResultList();
  return snapshots.isEmpty() ? null : snapshots.get(0);
}
