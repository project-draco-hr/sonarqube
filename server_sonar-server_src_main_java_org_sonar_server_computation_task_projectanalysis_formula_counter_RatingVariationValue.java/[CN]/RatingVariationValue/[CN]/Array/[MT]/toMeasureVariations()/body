{
  if (!isAnySet()) {
    return Optional.empty();
  }
  Double[] variations=new Double[values.length];
  for (int i=0; i < values.length; i++) {
    if (values[i].isSet()) {
      variations[i]=(double)values[i].getValue().getIndex();
    }
  }
  return Optional.of(new MeasureVariations(variations));
}
