{
  SearchResponse response=getClient().prepareSearch(this.getIndexName()).setTypes(this.getIndexType()).setQuery(QueryBuilders.termQuery(RuleNormalizer.RuleField.ID.field(),ids)).setSize(1).get();
  List<Rule> rules=Lists.newArrayList();
  for (  SearchHit hit : response.getHits()) {
    rules.add(toDoc(hit.getSource()));
  }
  return rules;
}
