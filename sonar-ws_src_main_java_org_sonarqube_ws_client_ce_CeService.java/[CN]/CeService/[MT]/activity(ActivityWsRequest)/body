{
  return call(new GetRequest(path("activity")).setParam(PARAM_COMPONENT_ID,request.getComponentId()).setParam("q",request.getQuery()).setParam(PARAM_STATUS,inlineMultipleParamValue(request.getStatus())).setParam(PARAM_TYPE,request.getType()).setParam(PARAM_MAX_EXECUTED_AT,request.getMaxExecutedAt()).setParam(PARAM_MIN_SUBMITTED_AT,request.getMinSubmittedAt()).setParam(PARAM_ONLY_CURRENTS,request.getOnlyCurrents()).setParam("p",request.getPage()).setParam("ps",request.getPageSize()),ActivityResponse.parser());
}
