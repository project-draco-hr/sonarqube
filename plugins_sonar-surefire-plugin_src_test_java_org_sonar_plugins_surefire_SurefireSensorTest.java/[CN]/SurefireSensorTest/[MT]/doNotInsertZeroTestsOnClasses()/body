{
  SensorContext context=mock(SensorContext.class);
  new SurefireSensor().collect(newJarProject(),context,new File(getClass().getResource("/org/sonar/plugins/surefire/SurefireSensorTest/doNotInsertZeroTestsOnClasses/").toURI()));
  verify(context,never()).saveMeasure(argThat(new IsResource(JavaFile.SCOPE_ENTITY,JavaFile.QUALIFIER_UNIT_TEST_CLASS)),(Metric)anyObject(),anyDouble());
}
