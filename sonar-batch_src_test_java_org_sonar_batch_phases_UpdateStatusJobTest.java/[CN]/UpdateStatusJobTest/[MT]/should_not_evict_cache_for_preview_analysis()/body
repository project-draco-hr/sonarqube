{
  Settings settings=new Settings();
  when(mode.isPreview()).thenReturn(true);
  Project project=new Project("struts");
  ServerClient serverClient=mock(ServerClient.class);
  UpdateStatusJob job=new UpdateStatusJob(settings,serverClient,mock(DatabaseSession.class),mock(ResourcePersister.class),project,mock(Snapshot.class),mode);
  job.evictPreviewDB();
  verify(serverClient,never()).request(anyString());
}
