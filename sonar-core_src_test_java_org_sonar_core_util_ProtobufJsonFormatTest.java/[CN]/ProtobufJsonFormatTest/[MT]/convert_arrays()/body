{
  ArrayFieldMsg msg=ArrayFieldMsg.newBuilder().addStrings("one").addStrings("two").addNesteds(org.sonar.core.test.Test.NestedMsg.newBuilder().setLabel("nestedOne")).addNesteds(org.sonar.core.test.Test.NestedMsg.newBuilder().setLabel("nestedTwo")).addNullableArray("nullableOne").addNullableArray("nullableTwo").build();
  assertThat(toJson(msg)).isEqualTo("{\"strings\":[\"one\",\"two\"],\"nesteds\":[{\"label\":\"nestedOne\"},{\"label\":\"nestedTwo\"}],\"nullableArray\":[\"nullableOne\",\"nullableTwo\"]}");
}
