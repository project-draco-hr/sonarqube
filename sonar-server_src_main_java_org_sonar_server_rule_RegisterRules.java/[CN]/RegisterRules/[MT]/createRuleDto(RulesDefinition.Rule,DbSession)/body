{
  RuleDto ruleDto=RuleDto.createFor(RuleKey.of(ruleDef.repository().key(),ruleDef.key())).setCardinality(ruleDef.template() ? Cardinality.MULTIPLE : Cardinality.SINGLE).setConfigKey(ruleDef.internalKey()).setDescription(ruleDef.htmlDescription()).setLanguage(ruleDef.repository().language()).setName(ruleDef.name()).setSeverity(ruleDef.severity()).setStatus(ruleDef.status().name()).setEffortToFixDescription(ruleDef.effortToFixDescription()).setSystemTags(ruleDef.tags());
  return dbClient.ruleDao().insert(ruleDto,session);
}
