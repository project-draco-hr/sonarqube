{
  return ImmutableList.of(new UpdateRequest().id(key.toString()).script(ListUpdate.NAME).addScriptParam(ListUpdate.FIELD,RuleField.PARAMS.field()).addScriptParam(ListUpdate.VALUE,null).addScriptParam(ListUpdate.ID_FIELD,RuleParamField.NAME.field()).addScriptParam(ListUpdate.ID_VALUE,param.getName()));
}
