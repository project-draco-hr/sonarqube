{
  RuleKey ruleKey=RuleKey.of(XooConstants.REPOSITORY_KEY,RULE_KEY);
  InputDir inputDir=context.fileSystem().inputDir(file.file().getParentFile());
  if (inputDir != null) {
    context.addIssue(context.issueBuilder().ruleKey(ruleKey).onDir(inputDir).message("This issue is generated for file " + file.relativePath()).build());
  }
}
