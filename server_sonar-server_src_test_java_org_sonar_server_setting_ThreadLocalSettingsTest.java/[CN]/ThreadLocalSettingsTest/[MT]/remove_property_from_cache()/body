{
  underTest=create(Collections.emptyMap());
  underTest.load();
  underTest.set("foo","bar");
  assertThat(underTest.get("foo").get()).isEqualTo("bar");
  underTest.remove("foo");
  assertThat(underTest.get("foo")).isNotPresent();
  underTest.unload();
  assertThat(underTest.get("foo")).isNotPresent();
}
