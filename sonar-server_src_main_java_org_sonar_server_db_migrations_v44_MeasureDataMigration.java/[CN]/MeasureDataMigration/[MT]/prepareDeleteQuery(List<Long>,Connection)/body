{
  String deleteSql=new StringBuilder().append("DELETE FROM measure_data WHERE id IN (").append(StringUtils.repeat("?",",",ids.size())).append(")").toString();
  PreparedStatement s=connection.prepareStatement(deleteSql);
  int i=1;
  for (  Long id : ids) {
    s.setLong(i,id);
    i++;
  }
  return s;
}
