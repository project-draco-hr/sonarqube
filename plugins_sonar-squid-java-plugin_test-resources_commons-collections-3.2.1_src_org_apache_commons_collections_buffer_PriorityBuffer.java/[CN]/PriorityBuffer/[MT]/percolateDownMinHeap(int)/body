{
  final Object element=elements[index];
  int hole=index;
  while ((hole * 2) <= size) {
    int child=hole * 2;
    if (child != size && compare(elements[child + 1],elements[child]) < 0) {
      child++;
    }
    if (compare(elements[child],element) >= 0) {
      break;
    }
    elements[hole]=elements[child];
    hole=child;
  }
  elements[hole]=element;
}
