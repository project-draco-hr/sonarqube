{
  expectedException.expect(BadRequestException.class);
  expectedException.expectMessage("Project id or project key can be provided, not both.");
  dbClient.componentDao().insert(dbSession,newProjectDto("project-uuid").setKey("project-key"));
  ws.newRequest().setParam(PARAM_PERMISSION,SCAN_EXECUTION).setParam(PARAM_PROJECT_UUID,"project-uuid").setParam(PARAM_PROJECT_KEY,"project-key").execute();
}
