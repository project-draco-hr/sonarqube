{
  dbClient.componentDao().insert(dbSession,newProjectDto("project-uuid").setKey("project-key"));
  ComponentDto project=dbClient.componentDao().selectOrFailByUuid(dbSession,"project-uuid");
  GroupDto group=insertGroup(new GroupDto().setName("project-group-name"));
  insertGroupRole(new GroupRoleDto().setGroupId(group.getId()).setRole(ISSUE_ADMIN).setResourceId(project.getId()));
  userSession.login().addProjectUuidPermissions(UserRole.ADMIN,"project-uuid");
  String result=ws.newRequest().setParam(PARAM_PERMISSION,ISSUE_ADMIN).setParam(PARAM_PROJECT_UUID,"project-uuid").execute().getInput();
  assertThat(result).contains("project-group-name").doesNotContain("group-1").doesNotContain("group-2").doesNotContain("group-3");
}
