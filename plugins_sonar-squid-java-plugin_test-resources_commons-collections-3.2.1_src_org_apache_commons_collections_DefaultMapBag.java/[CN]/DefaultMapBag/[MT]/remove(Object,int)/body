{
  _mods++;
  boolean result=false;
  int count=getCount(object);
  if (nCopies <= 0) {
    result=false;
  }
 else   if (count > nCopies) {
    _map.put(object,new Integer(count - nCopies));
    result=true;
    _total-=nCopies;
  }
 else {
    result=(_map.remove(object) != null);
    _total-=count;
  }
  return result;
}
