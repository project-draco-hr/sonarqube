{
  json.name("qualityprofiles").beginArray();
  for (  QualityProfileDto profile : qualityProfileService.findAll()) {
    json.beginObject().prop("key",profile.getKey().toString()).prop("name",profile.getName()).prop("lang",profile.getLanguage()).prop("parent",profile.getParent()).prop("parentKey",profile.getParentKey().toString()).endObject();
  }
  json.endArray();
}
