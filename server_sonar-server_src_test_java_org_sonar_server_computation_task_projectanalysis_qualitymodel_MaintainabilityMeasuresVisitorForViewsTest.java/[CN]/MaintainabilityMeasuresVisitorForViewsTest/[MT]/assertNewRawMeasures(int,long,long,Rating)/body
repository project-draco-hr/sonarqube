{
  assertThat(toEntries(measureRepository.getAddedRawMeasures(componentRef))).contains(entryOf(DEVELOPMENT_COST_KEY,newMeasureBuilder().create(String.valueOf(devCost))),entryOf(SQALE_DEBT_RATIO_KEY,newMeasureBuilder().create(debt / (double)devCost * 100.0,1)),entryOf(SQALE_RATING_KEY,createMaintainabilityRatingMeasure(rating)));
}
