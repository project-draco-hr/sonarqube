{
  if (propertyKeys == null) {
    return true;
  }
  boolean keySituation=rule == ElementPropertiesRule.INCLUDE;
  if (rule == ElementPropertiesRule.INCLUDE) {
    keySituation=propertyKeys.contains(key);
  }
 else   if (rule == ElementPropertiesRule.EXCLUDE) {
    keySituation=!propertyKeys.contains(key);
  }
  return keySituation;
}
