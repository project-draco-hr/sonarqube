{
  dbTester.truncateTables();
  dbClient=new DbClient(dbTester.database(),dbTester.myBatis(),new ComponentDao());
  session=dbClient.openSession(false);
  when(i18n.message(Locale.getDefault(),"qualifier.TRK","Project")).thenReturn("Project");
  service=new ComponentService(dbClient,new ResourceKeyUpdaterDao(dbTester.myBatis()),i18n,new ResourceIndexerDao(dbTester.myBatis()),userSessionRule,System2.INSTANCE);
}
