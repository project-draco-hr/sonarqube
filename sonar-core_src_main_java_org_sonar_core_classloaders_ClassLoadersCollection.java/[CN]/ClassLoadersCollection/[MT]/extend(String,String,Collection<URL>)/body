{
  try {
    ClassRealm base=world.getRealm(baseKey);
    base.createChildRealm(key);
    for (    URL url : urls) {
      base.addURL(url);
    }
  }
 catch (  NoSuchRealmException e) {
    throw new SonarException(e);
  }
catch (  DuplicateRealmException e) {
    throw new SonarException(e);
  }
}
