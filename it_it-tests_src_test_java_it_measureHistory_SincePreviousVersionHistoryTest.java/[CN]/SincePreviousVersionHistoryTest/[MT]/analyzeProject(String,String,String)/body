{
  SonarRunner build=SonarRunner.create(projectDir("shared/xoo-multi-modules-sample")).setProperties("sonar.projectVersion",version);
  if (exclusions != null) {
    build.setProperties("sonar.exclusions",exclusions);
  }
  if (date != null) {
    build.setProperty("sonar.projectDate",date);
  }
  orchestrator.executeBuild(build);
}
