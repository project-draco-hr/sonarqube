{
  return metric.isOptimizedBestValue() == Boolean.TRUE && metric.getBestValue() != null && (measure.getValue() == null || NumberUtils.compare(metric.getBestValue(),measure.getValue()) == 0) && allNull(measure.getId(),measure.getAlertStatus(),measure.getDescription(),measure.getTendency(),measure.getUrl()) && !measure.hasData() && isZeroVariation(measure.getVariation1()) && isZeroVariation(measure.getVariation2()) && isZeroVariation(measure.getVariation3()) && isZeroVariation(measure.getVariation4()) && isZeroVariation(measure.getVariation5());
}
