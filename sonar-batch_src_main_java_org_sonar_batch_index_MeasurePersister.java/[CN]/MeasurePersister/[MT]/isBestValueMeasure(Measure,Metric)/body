{
  return measure.getId() == null && metric.isOptimizedBestValue() == Boolean.TRUE && metric.getBestValue() != null && (measure.getValue() == null || NumberUtils.compare(metric.getBestValue(),measure.getValue()) == 0) && measure.getAlertStatus() == null && measure.getDescription() == null && measure.getTendency() == null && measure.getUrl() == null && !measure.hasData() && isZeroVariation(measure.getVariation1()) && isZeroVariation(measure.getVariation2()) && isZeroVariation(measure.getVariation3()) && isZeroVariation(measure.getVariation4()) && isZeroVariation(measure.getVariation5());
}
