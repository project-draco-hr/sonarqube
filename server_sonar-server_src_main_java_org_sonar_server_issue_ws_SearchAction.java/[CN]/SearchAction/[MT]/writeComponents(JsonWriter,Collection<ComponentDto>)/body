{
  json.name("components").beginArray();
  for (  ComponentDto component : components) {
    json.beginObject().prop("uuid",component.uuid()).prop("key",component.key()).prop("id",component.getId()).prop("enabled",component.isEnabled()).prop("qualifier",component.qualifier()).prop("name",component.name()).prop("longName",component.longName()).prop("path",component.path()).prop("projectId",(component.projectId() != null && component.subProjectId() != null) ? component.projectId() : null).prop("subProjectId",component.subProjectId()).endObject();
  }
  json.endArray();
}
