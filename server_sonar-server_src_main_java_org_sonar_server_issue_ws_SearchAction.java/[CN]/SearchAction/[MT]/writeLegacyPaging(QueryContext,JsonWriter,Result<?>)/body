{
  json.prop("maxResultsReached",false);
  json.name("paging").beginObject().prop("pageIndex",context.getPage()).prop("pageSize",context.getLimit()).prop("total",result.getTotal()).prop("fTotal",i18n.formatInteger(UserSession.get().locale(),(int)result.getTotal())).prop("pages",Math.ceil(result.getTotal() / (context.getLimit() * 1.0))).endObject();
}
