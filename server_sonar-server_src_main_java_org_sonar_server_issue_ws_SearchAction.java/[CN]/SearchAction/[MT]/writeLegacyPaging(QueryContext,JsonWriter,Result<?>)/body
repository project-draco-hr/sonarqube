{
  json.prop("maxResultsReached",false);
  long pages=context.getLimit();
  if (pages > 0) {
    pages=result.getTotal() / context.getLimit();
    if (result.getTotal() % context.getLimit() > 0) {
      pages++;
    }
  }
  json.name("paging").beginObject().prop("pageIndex",context.getPage()).prop("pageSize",context.getLimit()).prop("total",result.getTotal()).prop("fTotal",i18n.formatInteger(UserSession.get().locale(),(int)result.getTotal())).prop("pages",pages).endObject();
}
