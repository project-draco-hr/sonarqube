{
  Date from=previousProfile.getRulesUpdatedAt();
  String data=KeyValueFormat.format(ImmutableSortedMap.of("key",profile.getKey(),"from",UtcDateUtils.formatDateTime(fixDate(from)),"to",UtcDateUtils.formatDateTime(fixDate(profile.getRulesUpdatedAt()))));
  eventCache.createEvent(context.getResource(),String.format("Changes in %s",profileLabel(profile)),null,EventCategory.PROFILE,data);
}
