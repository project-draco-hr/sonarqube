{
  Event event=new Event();
  event.setName(String.format("Changes in %s",profileLabel(profile)));
  event.setCategory(Event.CATEGORY_PROFILE);
  String data=KeyValueFormat.format(ImmutableSortedMap.of("key",profile.getKey(),"from",UtcDateUtils.formatDateTime(previousProfile.getRulesUpdatedAt()),"to",UtcDateUtils.formatDateTime(profile.getRulesUpdatedAt())));
  event.setData(data);
  persistenceManager.saveEvent(context.getResource(),event);
}
