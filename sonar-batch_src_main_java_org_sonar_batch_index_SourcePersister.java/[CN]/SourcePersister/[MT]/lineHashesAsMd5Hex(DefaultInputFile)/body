{
  if (inputFile.lines() == 0) {
    return null;
  }
  StringBuilder result=new StringBuilder(inputFile.lines() * (32 + 1));
  for (  byte[] lineHash : inputFile.lineHashes()) {
    if (result.length() > 0) {
      result.append("\n");
    }
    result.append(lineHash != null ? Hex.encodeHexString(lineHash) : "");
  }
  return result.toString();
}
