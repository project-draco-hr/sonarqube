{
  Optional<UpdateCenter> updatePluginCenter=centerClient.getUpdateCenter(refreshUpdateCenter);
  if (updatePluginCenter.isPresent()) {
    return Optional.of(updatePluginCenter.get().setInstalledSonarVersion(sonarVersion).registerInstalledPlugins(installedPluginReferentialFactory.getInstalledPluginReferential()).setDate(centerClient.getLastRefreshDate()));
  }
  return Optional.absent();
}
