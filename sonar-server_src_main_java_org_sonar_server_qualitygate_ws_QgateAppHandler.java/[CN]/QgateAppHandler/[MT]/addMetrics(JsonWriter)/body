{
  writer.name("metrics").beginArray();
  for (  Metric metric : qualityGates.gateMetrics()) {
    writer.beginObject().prop("id",metric.getId()).prop("key",metric.getKey()).prop("name",metric.getName()).prop("type",metric.getType().toString()).prop("domain",metric.getDomain()).prop("hidden",BooleanUtils.isNotFalse(metric.isHidden())).endObject();
  }
  writer.endArray();
}
