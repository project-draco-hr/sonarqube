{
  Query query=getSession().createQuery("SELECT rol.id FROM " + classname + " rol "+ " WHERE rol.resourceId IS NOT NULL AND NOT EXISTS(FROM "+ ResourceModel.class.getSimpleName()+ " r WHERE r.id=rol.resourceId)");
  List<Integer> roleIds=query.getResultList();
  PurgeUtils.executeQuery(getSession(),"",roleIds,"delete from " + classname + " rol where rol.id in (:ids)");
}
