{
  uuid=issue.uuid();
  createdAt=issue.createdAt();
  componentKey(issue.componentKey());
  ruleKey(issue.ruleKey());
  ruleRepositoryKey(issue.ruleRepositoryKey());
  severity(issue.severity());
  message(issue.message());
  line(issue.line());
  cost(issue.cost());
