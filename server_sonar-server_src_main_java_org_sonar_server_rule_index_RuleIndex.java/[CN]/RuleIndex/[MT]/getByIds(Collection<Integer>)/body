{
  SearchResponse response=getClient().prepareSearch(this.getIndexName()).setTypes(this.getIndexType()).setQuery(QueryBuilders.termsQuery(RuleNormalizer.RuleField.ID.field(),ids)).get();
  List<Rule> rules=newArrayList();
  for (  SearchHit hit : response.getHits()) {
    rules.add(toDoc(hit.getSource()));
  }
  return rules;
}
