{
  AbstractSurefireParser parser=newParser();
  SensorContext context=mockContext();
  parser.collect(new Project("foo"),context,getDir("nestedInnerClasses"));
  verify(context).saveMeasure(argThat(new IsResource(Scopes.FILE,Qualifiers.FILE,"org.sonar.plugins.surefire.NestedInnerTest")),eq(CoreMetrics.TESTS),eq(3.0));
}
