{
  writer.beginObject().name("aString").valueObject("stringValue").name("aBoolean").valueObject(true).name("aInt").valueObject(42).name("aFloat").valueObject(3.14).name("aLong").valueObject(42L).name("aList").valueObject(Arrays.asList("one",2,"three")).name("aMap").valueObject(ImmutableMap.of("hello","world","good","bye")).endObject().close();
  expect("{\"aString\":\"stringValue\",\"aBoolean\":true,\"aInt\":42,\"aFloat\":3.14,\"aLong\":42,\"aList\":[\"one\",2,\"three\"],\"aMap\":{\"hello\":\"world\",\"good\":\"bye\"}}");
}
