{
  writer.setSerializeNulls(true);
  writer.beginObject().prop("nullNumber",(Number)null).prop("nullString",(String)null).name("nullNumber").value((Number)null).name("nullString").value((String)null).name("nullDate").valueDate(null).name("nullDateTime").valueDate(null).endObject().close();
  expect("{\"nullNumber\":null,\"nullString\":null,\"nullNumber\":null,\"nullString\":null,\"nullDate\":null,\"nullDateTime\":null}");
}
