{
  writer.beginObject().prop("aBoolean",true).prop("aInt",123).prop("aLong",1000L).prop("aDouble",3.14).prop("aNumber",new AtomicInteger(123456789)).prop("aString","bar").endObject().close();
  expect("{\"aBoolean\":true,\"aInt\":123,\"aLong\":1000,\"aDouble\":3.14,\"aNumber\":123456789,\"aString\":\"bar\"}");
}
