{
  List<ProcessRef> processRefs=new ArrayList<>(javaCommands.size());
  for (  JavaCommand command : javaCommands) {
    try {
      ProcessRef processRef=launcher.launch(command);
      monitor(processRef);
      processRefs.add(processRef);
    }
 catch (    RuntimeException e) {
      stop();
      throw e;
    }
  }
  return processRefs;
}
