{
  WebService.NewAction action=controller.createAction("show").setDescription("Get the list of test cases of a test plan. Require Browse permission on file's project").setSince("4.4").setResponseExample(Resources.getResource(getClass(),"tests-example-show.json")).setHandler(this);
  action.createParam(KEY).setRequired(true).setDescription("Test plan key").setExampleValue("my_project:/src/test/BarTest.java");
}
