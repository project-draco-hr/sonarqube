{
  detectors.add(new EndWithDetector(0.95,'}',';','{'));
  detectors.add(new KeywordsDetector(0.7,"||","&&"));
  detectors.add(new KeywordsDetector(0.3,"public","abstract","class","implements","extends","return","throw","private","protected","enum","continue","assert","package","synchronized","boolean","this","double","instanceof","final","interface","static","void","long","int","float","super","true","case:"));
  detectors.add(new ContainsDetector(0.95,"++","for(","if(","while(","catch(","switch(","try{","else{"));
  detectors.add(new CamelCaseDetector(0.5));
}
