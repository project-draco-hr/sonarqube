{
  Date now=new Date();
  es.putDocuments(SourceLineIndexDefinition.INDEX,SourceLineIndexDefinition.TYPE,new SourceLineDoc().setProjectUuid("project-uuid").setScmDate(now).setLine(25).setFileUuid("file-uuid"),new SourceLineDoc().setProjectUuid("project-uuid").setScmDate(DateUtils.addDays(now,-1)).setLine(24).setFileUuid("file-uuid"));
  Date returnedDate=index.lastCommitDateOnProject("project-uuid");
  assertThat(returnedDate).isEqualTo(now);
}
