{
  es.putDocuments(SourceLineIndexDefinition.INDEX,SourceLineIndexDefinition.TYPE,this.getClass(),"file1_line1.json","file1_line2.json","file2_line1.json","file2_line2.json","file2_line3.json");
  assertThat(index.countLines("file1")).isEqualTo(2);
  assertThat(index.countLines("file2")).isEqualTo(3);
}
