{
  if (prefix != null) {
    int len=prefix.length();
    if (len == 0) {
      prefix=null;
    }
 else     if (prefix.charAt(len - 1) != '/') {
      prefix+='/';
    }
  }
  for (  File child : file.listFiles()) {
    String name=prefix != null ? prefix + child.getName() : child.getName();
    _zip(name,child,out);
  }
}
