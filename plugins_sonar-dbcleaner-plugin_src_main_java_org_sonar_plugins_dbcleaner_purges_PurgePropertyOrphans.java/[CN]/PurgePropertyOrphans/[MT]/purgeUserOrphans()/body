{
  Query query=getSession().createQuery("SELECT p.id FROM " + Property.class.getSimpleName() + " p WHERE p.userId IS NOT NULL AND NOT EXISTS(FROM "+ User.class.getSimpleName()+ " u WHERE u.id=p.userId)");
  List<Integer> idsToDelete=query.getResultList();
  if (!idsToDelete.isEmpty()) {
    PurgeUtils.executeQuery(getSession(),"",idsToDelete,"DELETE FROM " + Property.class.getSimpleName() + " WHERE id in (:ids)");
  }
}
