{
  Query query=getSession().createQuery("SELECT p.id FROM " + Property.class.getSimpleName() + " p WHERE p.resourceId IS NOT NULL AND NOT EXISTS(FROM "+ ResourceModel.class.getSimpleName()+ " r WHERE r.id=p.resourceId)");
  List<Integer> idsToDelete=query.getResultList();
  if (!idsToDelete.isEmpty()) {
    PurgeUtils.executeQuery(getSession(),"",idsToDelete,"DELETE FROM " + Property.class.getSimpleName() + " WHERE id in (:ids)");
  }
}
