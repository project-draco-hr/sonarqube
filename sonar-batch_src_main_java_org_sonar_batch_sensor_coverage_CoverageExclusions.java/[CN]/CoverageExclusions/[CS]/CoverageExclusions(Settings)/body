{
  this.settings=settings;
  this.coverageMetrics=ImmutableSet.<Metric>builder().add(CoreMetrics.COVERAGE).addAll(CoverageConstants.COVERAGE_METRICS).add(CoreMetrics.LINE_COVERAGE).addAll(CoverageConstants.LINE_COVERAGE_METRICS).add(CoreMetrics.BRANCH_COVERAGE).addAll(CoverageConstants.BRANCH_COVERAGE_METRICS).build();
  initPatterns();
}
