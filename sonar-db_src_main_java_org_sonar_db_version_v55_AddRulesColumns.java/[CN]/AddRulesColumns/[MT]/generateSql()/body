{
  return new AddColumnsBuilder(getDialect(),"rules").addColumn(newBigIntegerColumnDefBuilder().setColumnName("created_at_ms").setIsNullable(true).build()).addColumn(newBigIntegerColumnDefBuilder().setColumnName("updated_at_ms").setIsNullable(true).build()).addColumn(new TinyIntColumnDef.Builder().setColumnName("rule_type").setIsNullable(true).build()).build();
}
