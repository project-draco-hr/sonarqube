{
  return new AddColumnsBuilder(getDialect(),"rules").addColumn(newBigDecimalColumnDefBuilder().setColumnName("created_at_ms").setIsNullable(true).build()).addColumn(newBigDecimalColumnDefBuilder().setColumnName("updated_at_ms").setIsNullable(true).build()).addColumn(new TinyIntColumnDef.Builder().setColumnName("rule_type").setIsNullable(true).build()).build();
}
