{
  NewIssue newIssue=NewIssue.create().component("Action.java").cost(4.2).description("the desc").line(123).rule("squid:AvoidCycle").severity("BLOCKER").attribute("JIRA","FOO-123");
  assertThat(newIssue.urlParams()).hasSize(7).includes(entry("component","Action.java"),entry("cost",4.2),entry("desc","the desc"),entry("line",123),entry("rule","squid:AvoidCycle"),entry("severity","BLOCKER"),entry("attr[JIRA]","FOO-123"));
}
