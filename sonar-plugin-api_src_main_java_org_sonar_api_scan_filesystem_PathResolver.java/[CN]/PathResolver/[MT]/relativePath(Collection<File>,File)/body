{
  List<String> stack=Lists.newArrayList();
  File cursor=file;
  while (cursor != null) {
    File parentDir=parentDir(dirs,cursor);
    if (parentDir != null) {
      return new RelativePath(parentDir,Joiner.on("/").join(stack));
    }
    stack.add(0,cursor.getName());
    cursor=cursor.getParentFile();
  }
  return null;
}
