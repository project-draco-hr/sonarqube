{
  boolean destroyed=false;
  if (isAlive(process)) {
    try {
      process.destroy();
      while (isAlive(process)) {
        Thread.sleep(100L);
      }
      destroyed=true;
    }
 catch (    Exception e) {
      LoggerFactory.getLogger(ProcessUtils.class).error("Fail to destroy " + process);
    }
  }
  return destroyed;
}
