{
  RubyModule jdbcConnection=((RubyModule)(runtime.getModule("ActiveRecord").getConstant("ConnectionAdapters"))).defineClassUnder("JdbcConnection",runtime.getObject(),runtime.getObject().getAllocator());
  jdbcConnection.defineAnnotatedMethods(JdbcAdapterInternalService.class);
  RubyModule jdbcSpec=runtime.getOrCreateModule("JdbcSpec");
  rubyApi=JavaEmbedUtils.newObjectAdapter();
  JdbcMySQLSpec.load(jdbcSpec);
  JdbcDerbySpec.load(jdbcSpec,rubyApi);
  return true;
}
