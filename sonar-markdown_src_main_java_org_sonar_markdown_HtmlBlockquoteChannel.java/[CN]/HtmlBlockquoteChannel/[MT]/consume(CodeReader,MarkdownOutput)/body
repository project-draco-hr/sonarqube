{
  try {
    if (code.getColumnPosition() == 0) {
      if (quotedLineElement.consume(code,output)) {
        while (endOfLine.consume(code,output) && quotedLineElement.consume(code,output)) {
        }
        output.append("</blockquote>");
        return true;
      }
    }
    return false;
  }
  finally {
    pendingBlockConstruction=false;
  }
}
