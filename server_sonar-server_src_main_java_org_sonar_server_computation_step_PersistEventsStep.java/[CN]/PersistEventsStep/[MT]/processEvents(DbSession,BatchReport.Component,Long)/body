{
  List<BatchReport.Event> events=component.getEventList();
  if (!events.isEmpty()) {
    for (    BatchReport.Event event : component.getEventList()) {
      dbClient.eventDao().insert(session,createBaseEvent(component,analysisDate).setName(event.getName()).setCategory(convertCategory(event.getCategory())).setDescription(event.hasDescription() ? event.getDescription() : null).setData(event.hasEventData() ? event.getEventData() : null));
    }
  }
}
