{
  Function<Event,EventDto> eventToEventDto=new Function<Event,EventDto>(){
    @Override public EventDto apply(    @Nonnull Event event){
      return newBaseEvent(component,analysisDate).setName(event.getName()).setCategory(convertCategory(event.getCategory())).setDescription(event.getDescription()).setData(event.getData());
    }
  }
;
  for (  EventDto batchEventDto : transform(eventRepository.getEvents(component),eventToEventDto)) {
    dbClient.eventDao().insert(session,batchEventDto);
  }
}
