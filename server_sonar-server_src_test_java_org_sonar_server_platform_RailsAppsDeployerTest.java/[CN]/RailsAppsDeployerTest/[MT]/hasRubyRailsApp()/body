{
  ClassLoader classLoader=new URLClassLoader(new URL[]{getClass().getResource("/org/sonar/server/platform/RailsAppsDeployerTest/FakeRubyRailsApp.jar").toURI().toURL()},null);
  assertTrue(RailsAppsDeployer.hasRailsApp("fake",classLoader));
  assertFalse(RailsAppsDeployer.hasRailsApp("other",classLoader));
}
