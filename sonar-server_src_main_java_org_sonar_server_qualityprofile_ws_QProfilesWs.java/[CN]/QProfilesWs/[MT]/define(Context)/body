{
  NewController controller=context.createController("api/qprofiles").setDescription("Quality profiles management");
  NewAction restoreDefault=controller.createAction("restore_default").setDescription("Restore default profiles").setSince("4.4").setHandler(qProfileBackupWsHandler);
  restoreDefault.createParam("language").setDescription("Restore default profiles for this language");
  controller.done();
}
