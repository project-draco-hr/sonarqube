{
  rules.updateRule(new RuleOperations.RuleChange().setRuleKey(RuleKey.parse((String)params.get("ruleKey"))).setDebtCharacteristicKey(Strings.emptyToNull((String)params.get("debtCharacteristicKey"))).setDebtRemediationFunction((String)params.get("debtRemediationFunction")).setDebtRemediationCoefficient(Strings.emptyToNull((String)params.get("debtRemediationCoefficient"))).setDebtRemediationOffset(Strings.emptyToNull((String)params.get("debtRemediationOffset"))));
}
