{
  if (hasNext) {
    return;
  }
  if (currentIterator == null) {
    if (root == null) {
    }
 else {
      if (transformer == null) {
        findNext(root);
      }
 else {
        findNext(transformer.transform(root));
      }
      root=null;
    }
  }
 else {
    findNextByIterator(currentIterator);
  }
}
