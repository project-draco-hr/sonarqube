{
  String createTableStmt=new CreateTableBuilder(dbTester.getDbClient().getDatabase().getDialect(),"TABLE_2").addPkColumn(newBigIntegerColumnDefBuilder().setColumnName("bg_col_1").setIsNullable(false).build()).addColumn(newVarcharColumnDefBuilder().setColumnName("varchar_col_2").setLimit(40).setIsNullable(false).build()).build().iterator().next();
  dbTester.executeDdl(createTableStmt);
}
