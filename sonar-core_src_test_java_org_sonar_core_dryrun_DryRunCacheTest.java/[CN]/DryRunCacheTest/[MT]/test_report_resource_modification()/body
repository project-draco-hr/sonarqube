{
  when(resourceDao.getRootProjectByComponentKey("foo")).thenReturn(new ResourceDto().setId(456L));
  dryRunCache.reportResourceModification("foo");
  verify(propertiesDao).setProperty(new PropertyDto().setKey(DryRunCache.SONAR_DRY_RUN_CACHE_LAST_UPDATE_KEY).setValue(anyString()).setResourceId(456L));
}
