{
  if (unitTest) {
    return false;
  }
  String fileKey=getKey();
  if (!fileKey.endsWith(".java")) {
    fileKey+=".java";
  }
  if (StringUtils.substringAfterLast(antPattern,"/").indexOf(".") < 0) {
    antPattern+=".*";
  }
  WildcardPattern matcher=WildcardPattern.create(antPattern,".");
  return matcher.match(fileKey);
}
