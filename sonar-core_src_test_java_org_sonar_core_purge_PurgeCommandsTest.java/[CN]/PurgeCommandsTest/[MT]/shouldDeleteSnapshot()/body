{
  setupData("shouldDeleteSnapshot");
  SqlSession session=getMyBatis().openSession();
  try {
    new PurgeCommands(session).deleteSnapshots(PurgeSnapshotQuery.create().setId(5L));
  }
  finally {
    MyBatis.closeQuietly(session);
  }
  checkTables("shouldDeleteSnapshot","snapshots","project_measures","measure_data","rule_failures","snapshot_sources","duplications_index","events","dependencies","snapshot_data");
}
