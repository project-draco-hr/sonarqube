{
  assertThat(matcher.computeGitHubURL("core.properties",null),is("https://raw.github.com/SonarSource/sonar/master/plugins/sonar-l10n-en-plugin/src/main/resources/org/sonar/l10n/core.properties"));
  assertThat(matcher.computeGitHubURL("core.properties","2.11-SNAPSHOT"),is("https://raw.github.com/SonarSource/sonar/master/plugins/sonar-l10n-en-plugin/src/main/resources/org/sonar/l10n/core.properties"));
  assertThat(matcher.computeGitHubURL("core.properties","2.10"),is("https://raw.github.com/SonarSource/sonar/2.10/plugins/sonar-l10n-en-plugin/src/main/resources/org/sonar/l10n/core.properties"));
}
