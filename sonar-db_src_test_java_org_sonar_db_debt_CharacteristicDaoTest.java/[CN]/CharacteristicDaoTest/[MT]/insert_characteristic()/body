{
  db.truncateTables();
  CharacteristicDto dto=new CharacteristicDto().setKey("COMPILER_RELATED_PORTABILITY").setName("Compiler related portability").setOrder(1).setEnabled(true).setCreatedAt(DateUtils.parseDate("2013-11-20"));
  dao.insert(dto);
  db.getSession().commit();
  db.assertDbUnit(getClass(),"insert_characteristic-result.xml",EXCLUDED_COLUMNS,"characteristics");
}
