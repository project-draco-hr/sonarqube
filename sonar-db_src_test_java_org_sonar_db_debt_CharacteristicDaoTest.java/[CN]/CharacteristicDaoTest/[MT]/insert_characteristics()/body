{
  dao.insert(session,new CharacteristicDto().setKey("COMPILER_RELATED_PORTABILITY").setName("Compiler related portability").setOrder(1).setEnabled(true).setCreatedAt(DateUtils.parseDate("2013-11-20")),new CharacteristicDto().setKey("PORTABILITY").setName("portability").setOrder(2).setEnabled(true).setCreatedAt(DateUtils.parseDate("2013-11-20")));
  session.commit();
  assertThat(db.countRowsOfTable("characteristics")).isEqualTo(2);
}
