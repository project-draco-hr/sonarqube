{
  WSUtils utils=WSUtils.getINSTANCE();
  Profile profile=new Profile();
  Boolean defaultProfile=utils.getBoolean(json,"default");
  profile.setLanguage(utils.getString(json,"language")).setName(utils.getString(json,"name")).setDefaultProfile(defaultProfile != null ? defaultProfile : false).setParentName(utils.getString(json,"parent"));
  parseRules(utils,profile,json);
  return profile;
}
