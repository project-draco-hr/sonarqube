{
  when(client.request("/batch_bootstrap/properties?project=struts")).thenReturn(JSON_RESPONSE);
  BatchSettings batchSettings=new BatchSettings(bootstrapSettings,new PropertyDefinitions(),client,deprecatedConf,new BootstrapProperties(Collections.<String,String>emptyMap()),reactor);
  assertThat(batchSettings.getBoolean("sonar.cpd.cross")).isTrue();
}
