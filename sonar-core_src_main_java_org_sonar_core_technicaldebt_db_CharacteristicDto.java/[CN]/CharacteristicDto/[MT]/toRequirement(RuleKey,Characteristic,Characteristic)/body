{
  return new Requirement().setId(id).setRuleKey(ruleKey).setCharacteristic(characteristic).setRootCharacteristic(rootCharacteristic).setFunction(functionKey).setFactor(WorkUnit.create(factorValue,factorUnit)).setOffset(WorkUnit.create(offsetValue,offsetUnit)).setCreatedAt(createdAt).setUpdatedAt(updatedAt);
}
