{
  Characteristic parent=requirement.characteristic();
  return new CharacteristicDto().setRuleId(ruleId).setParentId(parent != null ? parent.id() : null).setFunction(requirement.function()).setFactorValue(requirement.factor().getValue()).setFactorUnit(requirement.factor().getUnit()).setOffsetValue(requirement.offset().getValue()).setOffsetUnit(requirement.offset().getUnit()).setEnabled(true).setCreatedAt(requirement.createdAt()).setUpdatedAt(requirement.updatedAt());
}
