{
  return new DefaultRequirement().setId(id).setRuleKey(ruleKey).setCharacteristic(characteristic).setRootCharacteristic(rootCharacteristic).setFunction(functionKey).setFactorValue(factorValue.intValue()).setFactorUnit(toUnit(factorUnit)).setOffsetValue(offsetValue.intValue()).setOffsetUnit(toUnit(offsetUnit)).setCreatedAt(createdAt).setUpdatedAt(updatedAt);
}
