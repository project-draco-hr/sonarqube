{
  profiler.start("foo");
  clock.sleep(10);
  profiler.stop();
  profiler.reset();
  profiler.start("bar");
  clock.sleep(5);
  profiler.stop();
  profiler.start("foo");
  clock.sleep(8);
  profiler.stop();
  profiler.dump(50,ps);
  String content=baos.toString();
  assertThat(content).contains("foo: 8ms");
  assertThat(content).contains("bar: 5ms");
}
