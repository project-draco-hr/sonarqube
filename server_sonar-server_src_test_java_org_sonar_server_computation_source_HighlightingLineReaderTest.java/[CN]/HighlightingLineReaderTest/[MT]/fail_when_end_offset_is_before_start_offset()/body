{
  HighlightingLineReader highlightingLineReader=new HighlightingLineReader(newArrayList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(1).setStartOffset(4).setEndOffset(2).build()).setType(Constants.HighlightingType.ANNOTATION).build()).iterator());
  try {
    highlightingLineReader.read(line1);
    failBecauseExceptionWasNotThrown(IllegalArgumentException.class);
  }
 catch (  IllegalArgumentException e) {
    assertThat(e).hasMessage("End offset 2 cannot be defined before start offset 4 on line 1");
  }
}
