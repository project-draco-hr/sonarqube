{
  TextRange.Builder textRangeBuilder=TextRange.newBuilder();
  ScannerReport.SyntaxHighlightingRule.Builder ruleBuilder=ScannerReport.SyntaxHighlightingRule.newBuilder();
  syntaxHighlightingList.add(ruleBuilder.setRange(textRangeBuilder.setStartLine(startLine).setEndLine(endLine).setStartOffset(startOffset).setEndOffset(endOffset).build()).setType(type).build());
}
