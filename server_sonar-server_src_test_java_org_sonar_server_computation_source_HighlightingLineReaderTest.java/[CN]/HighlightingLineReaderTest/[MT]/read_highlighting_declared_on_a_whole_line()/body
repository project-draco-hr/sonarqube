{
  HighlightingLineReader highlightingLineReader=new HighlightingLineReader(newArrayList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(2).setStartOffset(0).setEndOffset(0).build()).setType(Constants.HighlightingType.ANNOTATION).build()).iterator());
  highlightingLineReader.read(line1);
  highlightingLineReader.read(line2);
  highlightingLineReader.read(line3);
  assertThat(line1.getHighlighting()).isEqualTo("0,5,a");
  assertThat(line2.getHighlighting()).isEmpty();
  assertThat(line3.getHighlighting()).isEmpty();
}
