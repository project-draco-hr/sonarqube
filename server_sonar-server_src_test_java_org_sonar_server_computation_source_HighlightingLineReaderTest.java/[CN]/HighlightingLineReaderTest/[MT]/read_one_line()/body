{
  HighlightingLineReader highlightingLineReader=new HighlightingLineReader(newArrayList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(1).setStartOffset(2).setEndOffset(4).build()).setType(Constants.HighlightingType.ANNOTATION).build()).iterator());
  FileSourceDb.Line.Builder lineBuilder=FileSourceDb.Data.newBuilder().addLinesBuilder().setSource("line1").setLine(1);
  highlightingLineReader.read(lineBuilder);
  assertThat(lineBuilder.getHighlighting()).isEqualTo("2,4,a");
}
