{
  HighlightingLineReader highlightingLineReader=new HighlightingLineReader(newArrayList(BatchReport.SyntaxHighlighting.newBuilder().setRange(BatchReport.Range.newBuilder().setStartLine(1).setEndLine(1).setStartOffset(2).setEndOffset(10).build()).setType(Constants.HighlightingType.ANNOTATION).build()).iterator());
  try {
    highlightingLineReader.read(line1);
    failBecauseExceptionWasNotThrown(IllegalArgumentException.class);
  }
 catch (  IllegalArgumentException e) {
    assertThat(e).hasMessage("End offset 10 is defined outside the length (5) of the line 1");
  }
}
