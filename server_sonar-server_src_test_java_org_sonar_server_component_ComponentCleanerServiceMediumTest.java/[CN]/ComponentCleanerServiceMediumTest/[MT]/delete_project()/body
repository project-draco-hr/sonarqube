{
  ComponentDto project=new ComponentDto().setId(1L).setKey("MyProject").setScope(Scopes.PROJECT).setQualifier(Qualifiers.PROJECT).setProjectId(1L);
  db.componentDao().insert(session,project);
  session.commit();
  service.delete(project.getKey());
  assertThat(db.componentDao().getNullableByKey(session,project.key())).isNull();
}
