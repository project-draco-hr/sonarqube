{
  long id=row.getLong(1);
  long personId=row.getLong(2);
  String developerUuid=componentUuidById.get(personId);
  if (developerUuid == null) {
    LOG.trace("No UUID found for personId={}",personId);
    return false;
  }
  update.setString(1,developerUuid);
  update.setLong(2,id);
  return true;
}
