{
  WebService.NewAction action=controller.createAction(ACTION).setDescription("Logs of a task. Format of response is plain text. HTTP code 404 is returned if the task does not " + "exist or if logs are not available. Requires system administration permission.").setResponseExample(getClass().getResource("logs-example.log")).setInternal(true).setSince("5.2").setHandler(this);
  action.createParam(PARAM_TASK_UUID).setRequired(true).setDescription("Id of task").setExampleValue(Uuids.UUID_EXAMPLE_01);
}
