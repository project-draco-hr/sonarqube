{
  GetResponse activeRuleResponse=index.client().prepareGet(INDEX_RULES,TYPE_ACTIVE_RULE,Integer.toString(activeRuleId)).setFields(FIELD_SOURCE,FIELD_PARENT).execute().actionGet();
  Map<String,Object> activeRuleSource=activeRuleResponse.getSourceAsMap();
  Map<String,Object> ruleSource=index.client().prepareGet(INDEX_RULES,TYPE_RULE,(String)activeRuleResponse.getField(FIELD_PARENT).getValue()).execute().actionGet().getSourceAsMap();
  return new QProfileRule(ruleSource,activeRuleSource);
}
