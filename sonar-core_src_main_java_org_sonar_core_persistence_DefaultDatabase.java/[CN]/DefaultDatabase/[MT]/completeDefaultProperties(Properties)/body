  completeDefaultProperty(props,DatabaseProperties.PROP_PASSWORD,DatabaseProperties.PROP_PASSWORD_DEFAULT_VALUE);
  completeDefaultProperty(props,DatabaseProperties.PROP_HIBERNATE_HBM2DLL,VALIDATE);
}
