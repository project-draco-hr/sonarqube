{
  if (fieldsToReturn == null) {
    fieldsToReturn=keep;
  }
 else {
    fieldsToReturn=Sets.filter(fieldsToReturn,new Predicate<String>(){
      @Override public boolean apply(      @Nullable String input){
        return input != null && keep.contains(input);
      }
    }
);
  }
  return this;
}
