{
  SettingsPage page=nav.logIn().asAdmin().openSettings(null).openCategory("DEV").assertSettingDisplayed("sonar.test.jira.servers");
  page.getPropertySetInput("sonar.test.jira.servers").setFieldValue("key","jira1").setFieldValue("url","http://jira").setFieldValue("port","12345").save();
  assertPropertySet("sonar.test.jira.servers",asList(entry("key","jira1"),entry("url","http://jira"),entry("port","12345")));
}
