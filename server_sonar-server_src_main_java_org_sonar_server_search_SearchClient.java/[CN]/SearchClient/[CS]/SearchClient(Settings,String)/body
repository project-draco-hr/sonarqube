{
  super(ImmutableSettings.settingsBuilder().put("network.bind_host","localhost").put("node.rack_id",StringUtils.defaultIfEmpty(settings.getString(IndexProperties.NODE_NAME),"unknown")).put("cluster.name",StringUtils.defaultIfBlank(settings.getString(IndexProperties.CLUSTER_NAME),"sonarqube")).build());
  initLogging();
  this.addTransportAddress(new InetSocketTransportAddress("localhost",settings.getInt(IndexProperties.NODE_PORT)));
  this.settings=settings;
  this.healthTimeout=healthTimeout;
  this.profiling=new Profiling(settings);
}
