{
  when(system2.now()).thenReturn(DateUtils.parseDate("2014-06-18").getTime());
  setupData("empty");
  ComponentDto componentDto=new ComponentDto().setId(1L).setKey("org.struts:struts-core:src/org/struts/RequestContext.java").setName("RequestContext.java").setLongName("org.struts.RequestContext").setQualifier("FIL").setScope("FIL").setLanguage("java").setPath("src/org/struts/RequestContext.java").setSubProjectId(3L).setEnabled(true);
  dao.insert(session,componentDto);
  session.commit();
  assertThat(componentDto.getId()).isNotNull();
  checkTables("insert","projects");
}
