{
  return new RuleDto().setRuleKey(ruleKey.rule()).setRepositoryKey(ruleKey.repository()).setName("Rule " + ruleKey.rule()).setDescription("Description " + ruleKey.rule()).setDescriptionFormat(Format.HTML).setStatus(RuleStatus.READY).setConfigKey("InternalKey" + ruleKey.rule()).setSeverity(Severity.INFO).setIsTemplate(false).setTags(ImmutableSet.of("tag1","tag2")).setSystemTags(ImmutableSet.of("systag1","systag2")).setLanguage("js").setRemediationFunction("LINEAR").setDefaultRemediationFunction("LINEAR_OFFSET").setRemediationCoefficient("1h").setDefaultRemediationCoefficient("5d").setDefaultRemediationOffset("10h").setEffortToFixDescription(ruleKey.repository() + "." + ruleKey.rule()+ ".effortToFix").setType(RuleType.CODE_SMELL);
}
