{
  QProfileService profileService=mock(QProfileService.class);
  RuleService ruleService=mock(RuleService.class);
  I18n i18n=mock(I18n.class);
  xoo1=createLanguage("xoo1");
  xoo2=createLanguage("xoo2");
  Languages languages=new Languages(xoo1,xoo2);
  controller=new WsTester(new QProfilesWs(new QProfileRestoreBuiltInAction(mock(QProfileService.class)),new RuleActivationActions(profileService),new BulkRuleActivationActions(profileService,ruleService,i18n),new ProjectAssociationActions(null,null,null,languages))).controller(QProfilesWs.API_ENDPOINT);
}
