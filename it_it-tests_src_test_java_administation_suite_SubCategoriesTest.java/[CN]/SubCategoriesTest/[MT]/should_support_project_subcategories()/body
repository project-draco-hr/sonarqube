{
  orchestrator.executeBuild(SonarRunner.create(projectDir("shared/xoo-sample")));
  Selenese selenese=Selenese.builder().setHtmlTestsInClasspath("subcategories","/administration/subcategories/project-subcategories.html","/administration/subcategories/project-subcategories-no-default.html").build();
  orchestrator.executeSelenese(selenese);
  assertThat(getProperty("prop3","sample")).isEqualTo("myValue2");
}
