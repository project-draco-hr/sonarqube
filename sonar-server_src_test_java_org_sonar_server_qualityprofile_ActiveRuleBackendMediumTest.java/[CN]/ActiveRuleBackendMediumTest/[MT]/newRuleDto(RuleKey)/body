{
  return new RuleDto().setRuleKey(ruleKey.rule()).setRepositoryKey(ruleKey.repository()).setName("Rule " + ruleKey.rule()).setDescription("Description " + ruleKey.rule()).setStatus(RuleStatus.READY).setConfigKey("InternalKey" + ruleKey.rule()).setSeverity(Severity.INFO).setIsTemplate(false).setLanguage("js").setRemediationFunction(DebtRemediationFunction.Type.LINEAR.toString()).setDefaultRemediationFunction(DebtRemediationFunction.Type.LINEAR_OFFSET.toString()).setRemediationCoefficient("1h").setDefaultRemediationCoefficient("5d").setRemediationOffset("5min").setDefaultRemediationOffset("10h").setEffortToFixDescription(ruleKey.repository() + "." + ruleKey.rule()+ ".effortToFix");
}
