{
  NewRepository repository=context.createRepository("xoo","xoo").setName("Xoo Repo");
  repository.createRule("x1").setName("x1 name").setHtmlDescription("x1 desc").setSeverity(Severity.MINOR).createParam("max").setDefaultValue("10").setType(RuleParamType.INTEGER).setDescription("Maximum");
  repository.done();
}
