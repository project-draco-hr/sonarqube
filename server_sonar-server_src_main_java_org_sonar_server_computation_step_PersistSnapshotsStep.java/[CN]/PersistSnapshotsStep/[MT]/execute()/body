{
  DbSession session=dbClient.openSession(false);
  try {
    new PathAwareCrawler<>(new PersistSnapshotsPathAwareVisitor(session,analysisMetadataHolder.getAnalysisDate(),dbIdsRepository)).visit(treeRootHolder.getRoot());
    session.commit();
  }
  finally {
    dbClient.closeSession(session);
  }
}
