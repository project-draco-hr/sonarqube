{
  if (bean != null) {
    Method method=getReadMethod(name);
    if (method != null) {
      try {
        return method.invoke(bean,NULL_ARGUMENTS);
      }
 catch (      IllegalAccessException e) {
        logWarn(e);
      }
catch (      IllegalArgumentException e) {
        logWarn(e);
      }
catch (      InvocationTargetException e) {
        logWarn(e);
      }
catch (      NullPointerException e) {
        logWarn(e);
      }
    }
  }
  return null;
}
