{
  CreateWsResponse.Builder response=CreateWsResponse.newBuilder();
  WsProjectLinks.Link.Builder linkBuilder=WsProjectLinks.Link.newBuilder().setId(String.valueOf(link.getId())).setName(link.getName()).setUrl(link.getHref());
  String type=link.getType();
  if (type != null) {
    linkBuilder.setType(type);
  }
  response.setLink(linkBuilder);
  return response.build();
}
