{
  Query query=session.createQuery("from " + Snapshot.class.getSimpleName() + " s where s.last=true and s.resourceId=(select r.id from "+ ResourceModel.class.getSimpleName()+ " r where r.key=:key)");
  query.setParameter("key",resource.getEffectiveKey());
  return session.getSingleResult(query,null);
}
