{
  Preconditions.checkArgument(!Strings.isNullOrEmpty(url),"Empty url");
  this.url=url;
  this.urlToMatch=url.replaceAll("/?\\*","");
  if ("/*".equals(url)) {
    code=1;
  }
 else   if (url.startsWith("*")) {
    code=2;
  }
 else   if (url.endsWith("*")) {
    code=3;
  }
 else {
    code=4;
  }
}
