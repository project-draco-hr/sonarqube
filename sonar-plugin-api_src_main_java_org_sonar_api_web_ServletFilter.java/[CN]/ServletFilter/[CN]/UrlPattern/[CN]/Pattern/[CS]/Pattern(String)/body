{
  this.url=url;
  this.urlToMatch=url.replaceAll("/?\\*","");
  if (MATCH_ALL_URL.equals(url)) {
    this.urlType=MATCH_ALL;
  }
 else   if (url.startsWith("*")) {
    this.urlType=START_WITH_STAR;
  }
 else   if (url.endsWith("*")) {
    this.urlType=END_WITH_START;
  }
 else {
    this.urlType=Type.ANY;
  }
}
