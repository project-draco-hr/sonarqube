{
  DefaultMeasure m=(DefaultMeasure)object;
  DefaultInputFile inputFile=(DefaultInputFile)m.inputFile();
  if (inputFile != null) {
    value.putString(inputFile.moduleKey());
    value.putString(inputFile.relativePath());
  }
 else {
    value.putNull();
  }
  value.putUTF(m.metric().key());
  value.put(m.value());
}
