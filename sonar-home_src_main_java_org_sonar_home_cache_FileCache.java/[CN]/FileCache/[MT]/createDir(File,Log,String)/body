{
  if (!dir.isDirectory() || !dir.exists()) {
    log.debug("Create : " + dir.getAbsolutePath());
    try {
      FileUtils.forceMkdir(dir);
    }
 catch (    IOException e) {
      throw new IllegalStateException("Unable to create " + debugTitle + dir.getAbsolutePath(),e);
    }
  }
  return dir;
}
