{
  if (nextObjectSet) {
    clearNextObject();
    if (!setPreviousObject()) {
      return false;
    }
 else {
      clearPreviousObject();
    }
  }
  while (iterator.hasPrevious()) {
    Object object=iterator.previous();
    if (predicate.evaluate(object)) {
      previousObject=object;
      previousObjectSet=true;
      return true;
    }
  }
  return false;
}
