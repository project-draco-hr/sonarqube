{
  if (previousObjectSet) {
    clearPreviousObject();
    if (!setNextObject()) {
      return false;
    }
 else {
      clearNextObject();
    }
  }
  while (iterator.hasNext()) {
    Object object=iterator.next();
    if (predicate.evaluate(object)) {
      nextObject=object;
      nextObjectSet=true;
      return true;
    }
  }
  return false;
}
