{
  if (list.isEmpty()) {
    throw new NoSuchElementException("There are no elements for this iterator to loop on");
  }
  if (iterator.hasPrevious() == false) {
    Object result=null;
    while (iterator.hasNext()) {
      result=iterator.next();
    }
    iterator.previous();
    return result;
  }
 else {
    return iterator.previous();
  }
}
