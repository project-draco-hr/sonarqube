{
  scanSampleWithDate("2012-01-01");
  Selenese selenese=Selenese.builder().setHtmlTestsInClasspath("project-settings","/administration/suite/ProjectAdministrationTest/project-settings/override-global-settings.html","/administration/suite/ProjectAdministrationTest/project-settings/only-on-project-settings.html").build();
  new SeleneseTest(selenese).runOn(orchestrator);
  assertThat(orchestrator.getServer().getAdminWsClient().find(PropertyQuery.createForResource("sonar.skippedModules","sample")).getValue()).isEqualTo("my-excluded-module");
}
