{
  when(db.getDialect()).thenReturn(new PostgreSql());
  answerSql(asList(new String[]{TABLE_ISSUES,COLUMN_KEE,"utf8"},new String[]{TABLE_PROJECTS,COLUMN_KEE,"latin"},new String[]{TABLE_PROJECTS,COLUMN_NAME,"latin"}));
  expectedException.expect(MessageException.class);
  expectedException.expectMessage("Database columns [projects.kee, projects.name] must have UTF8 charset.");
  underTest.start();
}
