{
  assertThat(toEntries(measureRepository.getAddedRawMeasures(componentRef))).containsOnly(MeasureRepoEntry.entryOf(DEVELOPMENT_COST_KEY,newMeasureBuilder().create(Long.toString(expectedDevCost))),MeasureRepoEntry.entryOf(SQALE_DEBT_RATIO_KEY,newMeasureBuilder().create(expectedDebtRatio * 100.0)),MeasureRepoEntry.entryOf(SQALE_RATING_KEY,createSqaleRatingMeasure(expectedRating)));
}
