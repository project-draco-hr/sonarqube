{
  if (projectUuid != null) {
    dbClient.issueAuthorizationDao().synchronizeAfter(session,index.get(IssueAuthorizationIndex.class).getLastSynchronization(),ImmutableMap.of(IssueAuthorizationDao.PROJECT_UUID,projectUuid));
  }
 else {
    LOG.info("Try to synchronize issues permissions on a project without UUID, ignore");
  }
}
