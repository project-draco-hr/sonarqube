{
  userSession.checkLoggedIn();
  change.validate();
  boolean changed;
  if (change.user() != null) {
    changed=applyChangeOnUser(session,operation,change);
  }
 else {
    changed=applyChangeOnGroup(session,operation,change);
  }
  if (changed) {
    session.commit();
    if (change.component() != null) {
      indexProjectPermissions();
    }
  }
}
