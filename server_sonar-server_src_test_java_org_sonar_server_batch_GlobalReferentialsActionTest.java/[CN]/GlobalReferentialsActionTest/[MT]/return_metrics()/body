{
  when(metricDao.findEnabled(session)).thenReturn(newArrayList(MetricDto.createFor("coverage").setDescription("Coverage by unit tests").setValueType("PERCENT").setQualitative(true).setWorstValue(0d).setBestValue(100d).setOptimizedBestValue(false).setDirection(1).setEnabled(true)));
  WsTester.TestRequest request=tester.newGetRequest("batch","global");
  request.execute().assertJson(getClass(),"return_global_referentials.json");
}
