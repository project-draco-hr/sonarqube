{
  RulesProfile profile=RulesProfile.create("Default","java");
  Rule rule=Rule.create("repo","key1","name1").setSeverity(RulePriority.CRITICAL);
  profile.activateRule(rule,null);
  try {
    profile.activateRule(rule,null);
    fail();
  }
 catch (  Exception e) {
    assertThat(e).isInstanceOf(IllegalStateException.class).hasMessage("The definition of the profile 'Default' (language 'java') contains multiple occurrences of the 'repo:key1' rule. The plugin which declares this profile should fix this.");
  }
}
