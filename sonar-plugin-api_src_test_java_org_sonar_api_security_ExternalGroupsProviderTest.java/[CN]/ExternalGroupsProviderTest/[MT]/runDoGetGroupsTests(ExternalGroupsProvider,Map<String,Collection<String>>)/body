{
  for (  Map.Entry<String,Collection<String>> userGroupMapEntry : userGroupsMap.entrySet()) {
    Collection<String> groups=groupsProvider.doGetGroups(new ExternalGroupsProvider.Context(userGroupMapEntry.getKey(),mock(HttpServletRequest.class)));
    assertThat(groups).isEqualTo(userGroupMapEntry.getValue());
  }
}
