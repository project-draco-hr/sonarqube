{
  File basedir=temp.newFolder();
  File otherDir=temp.newFolder();
  File srcFile=new File(otherDir,"src/main/java/foo/Bar.java");
  FileUtils.touch(srcFile);
  when(fs.baseDir()).thenReturn(basedir);
  InputFileBuilder builder=new InputFileBuilder("struts",new PathResolver(),langDetection,statusDetection,fs,new Settings(),new FileMetadata());
  DefaultInputFile inputFile=builder.create(srcFile);
  assertThat(inputFile).isNull();
}
