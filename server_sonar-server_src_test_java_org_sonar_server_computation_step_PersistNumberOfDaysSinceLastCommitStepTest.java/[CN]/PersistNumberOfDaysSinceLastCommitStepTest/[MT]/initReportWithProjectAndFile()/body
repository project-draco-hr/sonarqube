{
  treeRootHolder.setRoot(new DumbComponent(Component.Type.PROJECT,1,"project-uuid",null,new DumbComponent(Component.Type.FILE,2,"file-uuid",null)));
  reportReader.setMetadata(BatchReport.Metadata.newBuilder().setSnapshotId(1000).build());
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(1).setType(Constants.ComponentType.PROJECT).addChildRef(2).build());
  reportReader.putComponent(BatchReport.Component.newBuilder().setRef(2).setType(Constants.ComponentType.FILE).build());
}
