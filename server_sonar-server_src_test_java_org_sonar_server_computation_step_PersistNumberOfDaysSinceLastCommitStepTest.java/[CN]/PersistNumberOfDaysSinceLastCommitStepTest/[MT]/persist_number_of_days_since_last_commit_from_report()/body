{
  long threeDaysAgo=DateUtils.addDays(new Date(),-3).getTime();
  initReportWithProjectAndFile();
  reportReader.putChangesets(BatchReport.Changesets.newBuilder().setComponentRef(2).addChangeset(BatchReport.Changesets.Changeset.newBuilder().setDate(threeDaysAgo)).build());
  ComputationContext context=new ComputationContext(ComponentTreeBuilders.from(DumbComponent.DUMB_PROJECT));
  sut.execute(context);
  db.assertDbUnit(getClass(),"insert-from-report-result.xml",new String[]{"id"},"project_measures");
}
