{
  CaseInsensitiveKeywordsTokenizer tokenizer=new CaseInsensitiveKeywordsTokenizer("<k>","</k>","PROCEDURE");
  assertThat(highlight("procedure name",tokenizer),is("<k>procedure</k> name"));
  assertThat(highlight("Procedure name",tokenizer),is("<k>Procedure</k> name"));
  assertThat(highlight("PROCEDURE name",tokenizer),is("<k>PROCEDURE</k> name"));
}
