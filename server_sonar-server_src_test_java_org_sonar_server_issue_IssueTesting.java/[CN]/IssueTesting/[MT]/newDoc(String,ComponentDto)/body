{
  return newDoc().setKey(key).setComponentUuid(componentDto.uuid()).setModuleUuid(!componentDto.scope().equals(Scopes.PROJECT) ? componentDto.moduleUuid() : componentDto.uuid()).setModuleUuidPath(componentDto.moduleUuidPath()).setProjectUuid(componentDto.projectUuid()).setFilePath(!componentDto.scope().equals(Scopes.PROJECT) ? componentDto.path() : null);
}
