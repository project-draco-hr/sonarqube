{
  return new IssueDto().setKee(UUID.randomUUID().toString()).setRule(rule).setComponent(file).setProject(project).setStatus(Issue.STATUS_OPEN).setResolution(null).setSeverity(Severity.MAJOR).setDebt(10L).setIssueCreationDate(DateUtils.parseDate("2014-09-04")).setIssueUpdateDate(DateUtils.parseDate("2014-12-04"));
}
