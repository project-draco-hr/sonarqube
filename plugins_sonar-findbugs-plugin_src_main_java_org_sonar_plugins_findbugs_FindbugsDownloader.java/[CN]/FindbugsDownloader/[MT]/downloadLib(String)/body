{
  try {
    URI uri=new URI(url);
    File temp=File.createTempFile("findbugs",".jar");
    FileUtils.forceDeleteOnExit(temp);
    downloader.download(uri,temp);
    return temp;
  }
 catch (  URISyntaxException e) {
    throw new SonarException(e);
  }
catch (  IOException e) {
    throw new SonarException(e);
  }
}
