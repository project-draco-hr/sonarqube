{
  List<ActiveRule> deprecatedActiveRules=session.createQuery("from " + ActiveRule.class.getSimpleName() + " where rule.enabled=:enabled").setParameter("enabled",false).getResultList();
  for (  ActiveRule deprecatedActiveRule : deprecatedActiveRules) {
    session.removeWithoutFlush(deprecatedActiveRule);
  }
  if (!deprecatedActiveRules.isEmpty()) {
    session.commit();
  }
}
