{
  return new RuleDto().setCardinality(rule.getCardinality()).setConfigKey(rule.getConfigKey()).setDescription(rule.getDescription()).setLanguage(rule.getLanguage()).setName(rule.getName()).setRepositoryKey(rule.getRepositoryKey()).setRuleKey(rule.getKey()).setSeverity(rule.getSeverity().ordinal()).setStatus(rule.getStatus());
}
