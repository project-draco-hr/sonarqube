{
  LanguagePack englishPack=null;
  for (  LanguagePack pack : languagePacks) {
    if (pack.getLocales().contains(Locale.ENGLISH)) {
      englishPack=pack;
      break;
    }
  }
  if (englishPack == null) {
    throw new SonarException("The I18n English Pack was not found.");
  }
  return englishPack;
}
