{
  setupData("shared");
  List<ReviewDto> reviewDtos=dao.selectByQuery(ReviewQuery.create().addResolution(ReviewDto.RESOLUTION_FALSE_POSITIVE).addResolution(ReviewDto.RESOLUTION_FIXED));
  assertThat(reviewDtos.size(),is(2));
  for (  ReviewDto reviewDto : reviewDtos) {
    assertThat(reviewDto.getId(),anyOf(is(101L),is(104L)));
  }
}
