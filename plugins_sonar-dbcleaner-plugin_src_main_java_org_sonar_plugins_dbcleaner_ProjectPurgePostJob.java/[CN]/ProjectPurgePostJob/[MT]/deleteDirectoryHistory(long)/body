{
  try {
    LOG.debug("Delete historical data of directories");
    PurgeSnapshotQuery query=PurgeSnapshotQuery.create().setRootProjectId(projectId).setIslast(false).setScopes(new String[]{Scopes.DIRECTORY});
    purgeDao.deleteSnapshots(query);
  }
 catch (  Exception e) {
    LOG.error("Fail to delete historical data of directories",e);
  }
}
