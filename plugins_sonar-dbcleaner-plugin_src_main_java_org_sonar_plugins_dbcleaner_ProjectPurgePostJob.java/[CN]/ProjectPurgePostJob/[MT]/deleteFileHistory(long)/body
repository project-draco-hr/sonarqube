{
  try {
    LOG.debug("Delete historical data of files");
    PurgeSnapshotQuery query=PurgeSnapshotQuery.create().setRootProjectId(projectId).setIslast(false).setScopes(new String[]{Scopes.FILE});
    purgeDao.deleteSnapshots(query);
  }
 catch (  Exception e) {
    LOG.error("Fail to delete historical data of files",e);
  }
}
