{
  call(new PostRequest(path(ACTION_ADD_PROJECT)).setParam(PARAM_LANGUAGE,request.getLanguage()).setParam(PARAM_PROFILE_NAME,request.getProfileName()).setParam(PARAM_PROFILE_KEY,request.getProfileKey()).setParam(PARAM_PROJECT_KEY,request.getProjectKey()).setParam(PARAM_PROJECT_UUID,request.getProjectUuid()));
}
