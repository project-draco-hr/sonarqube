{
  if (!result.actionPlans().isEmpty()) {
    json.name("actionPlans").beginArray();
    for (    ActionPlan actionPlan : result.actionPlans()) {
      Date deadLine=actionPlan.deadLine();
      Date updatedAt=actionPlan.updatedAt();
      json.beginObject().prop("key",actionPlan.key()).prop("name",actionPlan.name()).prop("status",actionPlan.status()).prop("project",actionPlan.projectKey()).prop("userLogin",actionPlan.userLogin()).prop("deadLine",isoDate(deadLine)).prop("fDeadLine",formatDate(deadLine)).prop("createdAt",isoDate(actionPlan.createdAt())).prop("fCreatedAt",formatDate(actionPlan.createdAt())).prop("updatedAt",isoDate(actionPlan.updatedAt())).prop("fUpdatedAt",formatDate(updatedAt)).endObject();
    }
    json.endArray();
  }
}
