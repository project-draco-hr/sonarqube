{
  ruleDao.insert(newRuleDto(RuleKey.of("javascript","S001")).setDefaultRemediationCoefficient("DefaultCoef").setDefaultRemediationFunction("DefaultFunction").setDefaultRemediationCoefficient("DefaultCoef").setDefaultSubCharacteristicId(1),session);
