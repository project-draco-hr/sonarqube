{
  if (!SystemUtils.IS_OS_WINDOWS) {
    orchestrator=Orchestrator.builderEnv().setSonarVersion(System.getProperty("sonar.runtimeVersion")).setServerProperty("sonar.web.dev","true").build();
    orchestrator.start();
    orchestrator.getServer().adminWsClient().systemClient().restart();
    assertThat(FileUtils.readFileToString(orchestrator.getServer().getLogs())).contains("Restart server").contains("Server restarted");
  }
}
