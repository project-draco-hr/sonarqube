{
  if (!SystemUtils.IS_OS_WINDOWS) {
    orchestrator=Orchestrator.builderEnv().setSonarVersion(System.getProperty("sonar.runtimeVersion")).build();
    orchestrator.start();
    try {
      orchestrator.getServer().adminWsClient().systemClient().restart();
      fail();
    }
 catch (    Exception e) {
      assertThat(e.getMessage()).contains("403");
    }
  }
}
