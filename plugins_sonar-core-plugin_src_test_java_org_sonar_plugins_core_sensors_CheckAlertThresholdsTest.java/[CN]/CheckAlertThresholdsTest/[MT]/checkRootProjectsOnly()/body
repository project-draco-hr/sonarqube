{
  when(project.getQualifier()).thenReturn(Resource.QUALIFIER_FILE);
  when(projectAlerts.all()).thenReturn(Arrays.asList(new Alert(null,CoreMetrics.CLASSES,Alert.OPERATOR_GREATER,null,"20"),new Alert(null,CoreMetrics.COVERAGE,Alert.OPERATOR_GREATER,null,"35.0")));
  decorator.decorate(project,context);
  verify(context,never()).saveMeasure(any(Measure.class));
}
