{
  when(project.getQualifier()).thenReturn(Project.QUALIFIER_FILE);
  when(profile.getAlerts()).thenReturn(Arrays.asList(new Alert(null,CoreMetrics.CLASSES,Alert.OPERATOR_GREATER,null,"20"),new Alert(null,CoreMetrics.COVERAGE,Alert.OPERATOR_GREATER,null,"35.0")));
  decorator.decorate(project,context);
  verify(context,never()).saveMeasure((Measure)anyObject());
}
