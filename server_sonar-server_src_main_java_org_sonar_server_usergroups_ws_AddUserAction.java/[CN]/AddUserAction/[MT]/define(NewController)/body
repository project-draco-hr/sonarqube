{
  NewAction action=context.createAction("add_user").setDescription("Add a user to a group.").setHandler(this).setPost(true).setSince("5.2");
  action.createParam(PARAM_ID).setDescription("ID of the group").setExampleValue("42").setRequired(true);
  action.createParam(PARAM_LOGIN).setDescription("Login of the user.").setExampleValue("g.hopper").setRequired(true);
}
