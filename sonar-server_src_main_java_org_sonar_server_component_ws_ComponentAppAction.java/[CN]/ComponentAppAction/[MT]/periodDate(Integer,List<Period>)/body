{
  if (periodIndex != null) {
    Period period=Iterables.find(periodList,new Predicate<Period>(){
      @Override public boolean apply(      @Nullable Period input){
        return input != null && periodIndex.equals(input.index());
      }
    }
,null);
    return period != null ? period.date() : null;
  }
  return null;
}
