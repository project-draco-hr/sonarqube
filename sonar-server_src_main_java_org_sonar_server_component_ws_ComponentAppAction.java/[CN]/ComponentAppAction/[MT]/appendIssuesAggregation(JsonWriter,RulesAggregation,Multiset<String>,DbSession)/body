{
  json.name("severities").beginArray();
  for (  String severity : severitiesAggregation.elementSet()) {
    json.beginArray().value(severity).value(i18n.message(UserSession.get().locale(),"severity." + severity,null)).value(severitiesAggregation.count(severity)).endArray();
  }
  json.endArray();
  json.name("rules").beginArray();
  for (  RulesAggregation.Rule rule : rulesAggregation.rules()) {
    json.beginArray().value(rule.ruleKey().toString()).value(rule.name()).value(rulesAggregation.countRule(rule)).endArray();
  }
  json.endArray();
}
