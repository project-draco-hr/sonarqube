{
  ComponentDto component=componentFinder.getByUuidOrKey(dbSession,request.getId(),request.getKey(),ParamNames.ID_AND_KEY);
  String projectUuid=firstNonNull(component.projectUuid(),component.uuid());
  if (!userSession.hasPermission(GlobalPermissions.SYSTEM_ADMIN) && !userSession.hasComponentUuidPermission(UserRole.ADMIN,projectUuid) && !userSession.hasComponentUuidPermission(UserRole.USER,projectUuid)) {
    throw insufficientPrivilegesException();
  }
  return component;
}
