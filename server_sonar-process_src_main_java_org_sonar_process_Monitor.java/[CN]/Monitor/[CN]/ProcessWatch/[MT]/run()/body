{
  for (  ProcessWrapper process : processes) {
    try {
      long time=process.getProcessMXBean().ping();
      LOGGER.debug("PINGED '{}'",process.getName());
      pings.put(process.getName(),time);
    }
 catch (    Exception e) {
      LOGGER.error("Error while pinging {}",process.getName(),e);
    }
  }
}
