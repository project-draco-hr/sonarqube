{
  if (metricType == null) {
    return Collections.emptySet();
  }
 else {
switch (metricType) {
case BOOL:
      return BOOLEAN_OPERATORS;
case LEVEL:
    return LEVEL_OPERATORS;
case STRING:
  return STRING_OPERATORS;
case INT:
case FLOAT:
case PERCENT:
case MILLISEC:
case RATING:
return NUMERIC_OPERATORS;
default :
return Collections.emptySet();
}
}
}
