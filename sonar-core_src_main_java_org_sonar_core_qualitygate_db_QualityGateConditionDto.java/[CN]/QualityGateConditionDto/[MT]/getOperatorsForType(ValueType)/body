{
  Collection<String> operators=Collections.emptySet();
  if (metricType != null) {
switch (metricType) {
case BOOL:
      operators=BOOLEAN_OPERATORS;
    break;
case LEVEL:
  operators=LEVEL_OPERATORS;
break;
case STRING:
operators=STRING_OPERATORS;
break;
case INT:
case FLOAT:
case PERCENT:
case MILLISEC:
case RATING:
case WORK_DUR:
operators=NUMERIC_OPERATORS;
break;
default :
operators=Collections.emptySet();
}
}
return operators;
}
