{
  ws.define(context);
  RequestHandler handler=context.controller("api/rules").action("search").handler();
  SimpleRequest request=new SimpleRequest();
  SimpleResponse response=new SimpleResponse();
  handler.handle(request,response);
  String json=response.outputAsString();
  JSONAssert.assertEquals(IOUtils.toString(getClass().getResource("/org/sonar/server/rule/RuleWebServiceTest/search.json")),json,true);
}
