{
  if (value == null) {
    for (Entry pos=sentinel.next; pos != sentinel; pos=pos.next) {
      if (pos.getValue() == null)       return true;
    }
  }
 else {
    for (Entry pos=sentinel.next; pos != sentinel; pos=pos.next) {
      if (value.equals(pos.getValue()))       return true;
    }
  }
  return false;
}
