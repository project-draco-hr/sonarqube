{
  Entry pos=sentinel;
  if (index < 0) {
    throw new ArrayIndexOutOfBoundsException(index + " < 0");
  }
  int i=-1;
  while (i < (index - 1) && pos.next != sentinel) {
    i++;
    pos=pos.next;
  }
  if (pos.next == sentinel) {
    throw new ArrayIndexOutOfBoundsException(index + " >= " + (i + 1));
  }
  return pos.next;
}
