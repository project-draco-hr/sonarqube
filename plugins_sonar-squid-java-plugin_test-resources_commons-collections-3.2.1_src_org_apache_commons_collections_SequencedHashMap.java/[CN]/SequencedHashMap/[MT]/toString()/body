{
  StringBuffer buf=new StringBuffer();
  buf.append('[');
  for (Entry pos=sentinel.next; pos != sentinel; pos=pos.next) {
    buf.append(pos.getKey());
    buf.append('=');
    buf.append(pos.getValue());
    if (pos.next != sentinel) {
      buf.append(',');
    }
  }
  buf.append(']');
  return buf.toString();
}
