{
  super.doStartTag();
  if (!write) {
    return (EVAL_BODY_TAG);
  }
  String results=null;
  if (value != null) {
    results=TagUtils.getInstance().filter(value);
  }
 else {
    Object value=TagUtils.getInstance().lookup(pageContext,name,property,null);
    if (value == null) {
      results="";
    }
 else {
      results=TagUtils.getInstance().filter(value.toString());
    }
  }
  TagUtils.getInstance().write(pageContext,results);
  return (EVAL_BODY_TAG);
}
