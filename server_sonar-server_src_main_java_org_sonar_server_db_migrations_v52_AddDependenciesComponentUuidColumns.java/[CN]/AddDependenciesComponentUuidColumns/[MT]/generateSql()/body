{
  return new AddColumnsBuilder(db.getDialect(),"dependencies").addColumn(new AddColumnsBuilder.ColumnDef().setName("from_component_uuid").setType(STRING).setLimit(50).setNullable(true)).addColumn(new AddColumnsBuilder.ColumnDef().setName("to_component_uuid").setType(STRING).setLimit(50).setNullable(true)).build();
}
