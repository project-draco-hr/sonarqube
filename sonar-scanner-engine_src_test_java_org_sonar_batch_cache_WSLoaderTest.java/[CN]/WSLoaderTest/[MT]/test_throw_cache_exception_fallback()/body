{
  turnServerOffline();
  when(cache.getString(ID)).thenThrow(new NullPointerException());
  WSLoader loader=new WSLoader(LoadStrategy.SERVER_FIRST,cache,ws);
  try {
    loader.loadString(ID);
    fail("NPE expected");
  }
 catch (  NullPointerException e) {
    assertUsedServer(1);
    assertUsedCache(1);
  }
}
