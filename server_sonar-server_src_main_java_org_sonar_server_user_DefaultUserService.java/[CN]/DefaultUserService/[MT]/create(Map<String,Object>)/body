{
  NewUser newUser=NewUser.create().setLogin((String)params.get("login")).setName((String)params.get("name")).setEmail((String)params.get("email")).setScmAccounts(RubyUtils.toStrings(params.get("scm_accounts"))).setPassword((String)params.get("password")).setPasswordConfirmation((String)params.get("password_confirmation"));
  return userUpdater.create(newUser);
}
