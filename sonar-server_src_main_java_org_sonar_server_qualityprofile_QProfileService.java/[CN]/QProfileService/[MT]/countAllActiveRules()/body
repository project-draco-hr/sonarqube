{
  Map<QualityProfileKey,Long> counts=new HashMap<QualityProfileKey,Long>();
  for (  Map.Entry<String,Long> entry : index.get(ActiveRuleIndex.class).countByField(ActiveRuleNormalizer.ActiveRuleField.PROFILE_KEY).entrySet()) {
    counts.put(QualityProfileKey.parse(entry.getKey()),entry.getValue());
  }
  return counts;
}
