{
  if (list == null) {
    throw new IllegalArgumentException("List must not be null");
  }
  if (list.isEmpty()) {
    return new SetUniqueList(list,new HashSet());
  }
 else {
    List temp=new ArrayList(list);
    list.clear();
    SetUniqueList sl=new SetUniqueList(list,new HashSet());
    sl.addAll(temp);
    return sl;
  }
}
