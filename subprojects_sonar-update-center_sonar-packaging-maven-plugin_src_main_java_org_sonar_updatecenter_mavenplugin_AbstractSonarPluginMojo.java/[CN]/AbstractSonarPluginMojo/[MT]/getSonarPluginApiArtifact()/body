{
  Set<Artifact> dependencies=getDependencyArtifacts();
  if (dependencies != null) {
    for (    Artifact dep : dependencies) {
      if (SONAR_GROUPID.equals(dep.getGroupId()) && SONAR_PLUGIN_API_ARTIFACTID.equals(dep.getArtifactId()) && SONAR_PLUGIN_API_TYPE.equals(dep.getType())) {
        return dep;
      }
    }
  }
  return null;
}
