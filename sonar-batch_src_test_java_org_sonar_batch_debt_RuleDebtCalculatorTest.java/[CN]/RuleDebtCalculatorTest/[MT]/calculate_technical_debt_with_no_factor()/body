{
  RuleKey ruleKey=RuleKey.of("squid","AvoidCycle");
  DefaultIssue issue=new DefaultIssue().setKey("ABCDE").setRuleKey(ruleKey);
  DefaultRequirement requirement=new DefaultRequirement().setFunction("constant_issue").setOffsetValue(5).setOffsetUnit(WorkDuration.UNIT.MINUTES);
  when(model.requirementsByRule(ruleKey)).thenReturn(requirement);
  assertThat(calculator.calculateTechnicalDebt(issue.ruleKey(),issue.effortToFix())).isEqualTo(WorkDuration.createFromValueAndUnit(5,WorkDuration.UNIT.MINUTES,HOURS_IN_DAY));
}
