{
  LOG.info("Store analysis results in memory for later assertions in medium test");
  for (  Issue issue : container.getComponentByType(AnalyzerIssueCache.class).all()) {
    issues.add(issue);
  }
  for (  Measure<?> measure : container.getComponentByType(AnalyzerMeasureCache.class).all()) {
    measures.add(measure);
  }
  storeFs(container);
  storeComponentData(container);
  storeDuplication(container);
  storeTestCases(container);
  storeCoveragePerTest(container);
  storeDependencies(container);
}
