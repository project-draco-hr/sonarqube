{
  WebService.NewAction action=controller.createAction("changelog").setDescription("Display changelog of an issue").setSince("4.1").setHandler(RailsHandler.INSTANCE).setResponseExample(Resources.getResource(this.getClass(),"example-changelog.json"));
  action.createParam("issue").setDescription("Key of the issue").setRequired(true).setExampleValue("5bccd6e8-f525-43a2-8d76-fcb13dde79ef");
  RailsHandler.addFormatParam(action);
}
