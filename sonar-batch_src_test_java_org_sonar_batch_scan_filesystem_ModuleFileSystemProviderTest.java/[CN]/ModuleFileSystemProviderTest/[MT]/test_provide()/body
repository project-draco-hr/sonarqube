{
  ModuleFileSystemProvider provider=new ModuleFileSystemProvider();
  ProjectDefinition module=ProjectDefinition.create().setBaseDir(temp.newFolder()).setWorkDir(temp.newFolder());
  ModuleFileSystem fs=provider.provide(module,new PathResolver(),new TempDirectories(),mock(LanguageFileFilters.class),new Settings(),new FileFilter[0]);
  assertThat(fs).isNotNull();
}
