{
  int maxSize=10;
  List<Entry<String,Long>> result=new ArrayList<Entry<String,Long>>(maxSize);
  int i=0;
  for (  Entry<String,Long> item : sortedFullList) {
    if (i++ >= maxSize || item.getValue() == 0) {
      return result;
    }
 else {
      result.add(item);
    }
  }
  return result;
}
