{
  DbSession dbSession=dbClient.openSession(false);
  long maxDate;
  try {
    IssueResultSetIterator rowIt=IssueResultSetIterator.create(dbClient,dbSession,lastUpdatedAt,projectUuid);
    maxDate=doIndex(bulk,rowIt);
    rowIt.close();
    return maxDate;
  }
  finally {
    dbSession.close();
  }
}
