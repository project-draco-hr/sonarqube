{
  if (component.getType().isDeeperThan(this.visitor.getMaxDepth())) {
    return;
  }
  if (this.visitor.getOrder() == ComponentVisitor.Order.PRE_ORDER) {
    visitNode(component);
  }
  visitChildren(component);
  if (this.visitor.getOrder() == ComponentVisitor.Order.POST_ORDER) {
    visitNode(component);
  }
}
