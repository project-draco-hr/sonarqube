{
  int matchers=0;
  if (toUpperCase) {
    line=line.toUpperCase(Locale.getDefault());
  }
  StringTokenizer tokenizer=new StringTokenizer(line," \t(),{}");
  while (tokenizer.hasMoreTokens()) {
    String word=tokenizer.nextToken();
    if (keywords.contains(word)) {
      matchers++;
    }
  }
  return matchers;
}
