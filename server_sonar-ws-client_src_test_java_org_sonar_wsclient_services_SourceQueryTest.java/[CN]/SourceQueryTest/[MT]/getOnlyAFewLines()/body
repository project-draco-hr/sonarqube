{
  assertThat(SourceQuery.create("myproject:org.foo.Bar").setFromLineToLine(10,30).getUrl(),is("/api/sources?resource=myproject%3Aorg.foo.Bar&from=10&to=30&"));
  assertThat(SourceQuery.create("myproject:org.foo.Bar").setLinesFromLine(10,20).getUrl(),is("/api/sources?resource=myproject%3Aorg.foo.Bar&from=10&to=30&"));
}
