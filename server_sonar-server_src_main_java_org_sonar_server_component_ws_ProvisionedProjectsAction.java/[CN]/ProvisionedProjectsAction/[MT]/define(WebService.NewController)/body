{
  WebService.NewAction action=controller.createAction("provisioned").setDescription("Get the list of provisioned projects.<br /> " + "Require admin role.").setSince("5.2").setResponseExample(Resources.getResource(getClass(),"projects-example-provisioned.json")).setHandler(this).addPagingParams(100).addFieldsParam(POSSIBLE_FIELDS);
