{
  List<String> qualifiers=newArrayList();
  if (annotation.project()) {
    qualifiers.add(Qualifiers.PROJECT);
  }
  if (annotation.module()) {
    qualifiers.add(Qualifiers.MODULE);
  }
  return PropertyDefinition.build(annotation.key()).name(annotation.name()).defaultValue(annotation.defaultValue()).description(annotation.description()).global(annotation.global()).qualifiers(qualifiers).category(annotation.category()).type(annotation.type()).options(annotation.options()).multiValues(annotation.multiValues()).propertySetKey(annotation.propertySetKey()).fields(PropertyFieldDefinition.create(annotation.fields())).deprecatedKey(annotation.deprecatedKey()).index(annotation.index()).build();
}
