{
  if (isBlank(value)) {
    return Result.SUCCESS;
  }
switch (type) {
case BOOLEAN:
    return validateBoolean(value);
case INTEGER:
case LONG:
  return validateInteger(value);
case FLOAT:
return validateFloat(value);
case SINGLE_SELECT_LIST:
if (!options.contains(value)) {
return Result.newError("notInOptions");
}
default :
return Result.SUCCESS;
}
}
