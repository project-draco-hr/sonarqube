{
  try (ByteArrayOutputStream out=new ByteArrayOutputStream();LineReturnEnforcedPrintStream printStream=new LineReturnEnforcedPrintStream(out)){
    error.printStackTrace(printStream);
    printStream.flush();
    return out.toString();
  }
 catch (  IOException e) {
    Logger.getLogger(InternalCeQueueImpl.class).debug("Failed to getStacktrace out of error",e);
    return null;
  }
}
