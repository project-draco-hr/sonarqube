{
  DefaultPluginMetadata metadata=DefaultPluginMetadata.create(new File("sonar-checkstyle-plugin.jar")).setPathsToInternalDeps(new String[]{"META-INF/lib/commons-lang.jar","META-INF/lib/commons-io.jar"});
  assertThat(metadata.getPathsToInternalDeps()).containsOnly("META-INF/lib/commons-lang.jar","META-INF/lib/commons-io.jar");
}
