{
  DefaultPluginMetadata checkstyle=DefaultPluginMetadata.create(new File("sonar-checkstyle-plugin.jar")).setKey("checkstyle").setName("Checkstyle");
  DefaultPluginMetadata pmd=DefaultPluginMetadata.create(new File("sonar-pmd-plugin.jar")).setKey("pmd").setName("PMD");
  List<DefaultPluginMetadata> plugins=Arrays.asList(pmd,checkstyle);
  assertThat(natural().sortedCopy(plugins)).extracting("key").containsExactly("checkstyle","pmd");
}
