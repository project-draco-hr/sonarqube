{
  PluginMetadata checkstyle=DefaultPluginMetadata.create(new File("sonar-checkstyle-plugin.jar")).setKey("checkstyle").setName("Checkstyle");
  PluginMetadata pmd=DefaultPluginMetadata.create(new File("sonar-pmd-plugin.jar")).setKey("pmd").setName("PMD");
  PluginMetadata[] array={pmd,checkstyle};
  Arrays.sort(array);
  assertThat(array[0].getKey(),is("checkstyle"));
  assertThat(array[1].getKey(),is("pmd"));
}
