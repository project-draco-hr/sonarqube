{
  WebService.NewAction action=controller.createAction("covered_files").setDescription("Get the list of source files covered by a test. Require Browse permission on test file's project").setSince("4.4").setResponseExample(Resources.getResource(getClass(),"tests-example-covered-files.json")).setHandler(this).addPagingParams(100);
  action.createParam(TEST_ID).setRequired(true).setDescription("Test ID").setExampleValue(Uuids.UUID_EXAMPLE_01);
}
