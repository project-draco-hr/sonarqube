{
  DefaultRequirement requirement=new DefaultRequirement().setRuleKey(RuleKey.of(rule.getRepositoryKey(),rule.getRuleKey())).setCharacteristic(characteristic.toDefaultCharacteristic()).setRootCharacteristic(rootCharacteristic.toDefaultCharacteristic());
  for (  RequirementPropertyDto property : properties) {
    requirement.addProperty(property.toDefaultRequirementProperty());
  }
  return requirement;
}
