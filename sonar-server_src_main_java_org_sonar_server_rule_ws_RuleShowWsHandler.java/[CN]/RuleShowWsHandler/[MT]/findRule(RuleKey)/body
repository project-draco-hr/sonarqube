{
  if (ruleKey.repository().equals(Rule.MANUAL_REPOSITORY_KEY)) {
    org.sonar.api.rules.Rule rule=ruleFinder.findByKey(ruleKey);
    if (rule != null) {
      RulePriority severity=rule.getSeverity();
      return new Rule.Builder().setKey(rule.getKey()).setRepositoryKey(rule.getRepositoryKey()).setName(rule.getName()).setDescription(rule.getDescription()).setSeverity(severity != null ? severity.name() : null).setStatus(rule.getStatus()).setCreatedAt(rule.getCreatedAt()).setUpdatedAt(rule.getUpdatedAt()).build();
    }
    return null;
  }
 else {
    return rules.findByKey(ruleKey);
  }
}
