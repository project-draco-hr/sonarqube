{
  if (ruleKey.repository().equals("manual")) {
    org.sonar.api.rules.Rule rule=ruleFinder.findByKey(ruleKey);
    if (rule != null) {
      return new Rule(rule.getKey(),rule.getName(),rule.getDescription(),rule.getRepositoryKey(),rule.getSeverity().name(),rule.getStatus(),rule.getCreatedAt(),rule.getUpdatedAt());
    }
    return null;
  }
 else {
    return rules.findByKey(ruleKey);
  }
}
