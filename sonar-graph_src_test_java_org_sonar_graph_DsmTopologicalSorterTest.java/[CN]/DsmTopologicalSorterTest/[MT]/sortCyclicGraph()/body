{
  StringPrintWriter textDsm=new StringPrintWriter();
  textDsm.println("  | A | B | C | D |");
  textDsm.println("A |   |   |   |   |");
  textDsm.println("B | 3 |   | 4 |   |");
  textDsm.println("C | 1 | 1 |   |   |");
  textDsm.println("D |   | 2 |   |   |");
  Dsm<String> dsm=DsmScanner.scan(textDsm.toString());
  DsmTopologicalSorter.sort(dsm);
}
