{
  dbTester.prepareDbUnit(getClass(),"update.xml");
  ResourceDto project=new ResourceDto().setKey("org.struts:struts").setDeprecatedKey("deprecated key").setScope(Scopes.PROJECT).setQualifier(Qualifiers.PROJECT).setName("Struts").setLongName("Apache Struts").setLanguage("java").setDescription("MVC Framework").setPath("/foo/bar").setId(1L);
  dao.insertOrUpdate(project);
  assertThat(project.getId()).isNotNull();
  dbTester.assertDbUnit(getClass(),"update-result.xml","projects");
}
