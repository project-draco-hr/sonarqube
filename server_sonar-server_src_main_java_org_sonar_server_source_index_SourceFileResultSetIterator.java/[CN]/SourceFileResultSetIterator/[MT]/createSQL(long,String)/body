{
  String sql=SQL_ALL;
  if (afterDate > 0L || projectUuid != null) {
    sql+="WHERE ";
    boolean isFirst=true;
    if (afterDate > 0L) {
      sql+=AFTER_DATE_FILTER;
      isFirst=false;
    }
    if (projectUuid != null) {
      if (!isFirst) {
        sql+="AND ";
      }
      sql+=PROJECT_FILTER;
    }
  }
  return sql;
}
