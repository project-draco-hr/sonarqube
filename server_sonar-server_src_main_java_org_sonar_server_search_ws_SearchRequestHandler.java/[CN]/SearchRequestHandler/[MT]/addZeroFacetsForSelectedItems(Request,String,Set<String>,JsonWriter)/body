{
  List<String> requestParams=request.paramAsStrings(facetName);
  if (requestParams != null) {
    for (    String param : requestParams) {
      if (!itemsFromFacets.contains(param)) {
        json.beginObject();
        json.prop("val",param);
        json.prop("count",0);
        json.endObject();
      }
    }
  }
}
