{
  try {
    if (!started && isConnectedToDatabase()) {
      TimeProfiler profiler=new TimeProfiler().start("Start services");
      startCoreComponents();
      startServiceComponents();
      executeStartupTasks();
      started=true;
      profiler.stop();
    }
  }
 catch (  Exception e) {
    throw new ServerStartException(e);
  }
}
