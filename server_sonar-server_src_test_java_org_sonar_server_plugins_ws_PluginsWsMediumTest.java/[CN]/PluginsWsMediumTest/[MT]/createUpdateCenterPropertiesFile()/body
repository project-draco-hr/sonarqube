{
  try {
    temporaryFolder.create();
    String content=FileUtils.readFileToString(getFile("update-center.properties"),ENCODING).replace("[[decoy.10.jar]]",getFileUrl("sonar-decoy-plugin-1.0.jar").toString()).replace("[[decoy.11.jar]]",getFileUrl("sonar-decoy-plugin-1.1.jar").toString()).replace("[[foo.10.jar]]",getFileUrl("sonar-foo-plugin-1.0.jar").toString());
    File res=temporaryFolder.newFile("update-center.properties");
    FileUtils.write(res,content,"UTF-8");
    return res.toURI().toURL();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
