{
  BootstrapProperties bootstrapProps=new BootstrapProperties(ImmutableMap.of(CoreProperties.ANALYSIS_MODE,CoreProperties.ANALYSIS_MODE_PREVIEW,CoreProperties.PREVIEW_READ_TIMEOUT_SEC,"80"));
  AnalysisMode mode=new AnalysisMode(bootstrapProps);
  assertThat(mode.getPreviewReadTimeoutSec()).isEqualTo(80);
}
