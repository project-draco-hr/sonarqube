{
  GetRequestBuilder request=getClient().prepareGet().setIndex(RuleIndexDefinition.INDEX).setType(RuleIndexDefinition.TYPE_ACTIVE_RULE).setId(key.toString()).setFetchSource(true).setRouting(key.ruleKey().repository());
  GetResponse response=request.get();
  if (response.isExists()) {
    return new ActiveRuleDoc(response.getSource());
  }
  return null;
}
