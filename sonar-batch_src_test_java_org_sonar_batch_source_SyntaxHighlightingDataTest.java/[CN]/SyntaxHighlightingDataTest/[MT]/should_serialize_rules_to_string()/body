{
  List<SyntaxHighlightingRule> orderedHighlightingRules=Lists.newArrayList(SyntaxHighlightingRule.create(0,10,"cd"),SyntaxHighlightingRule.create(10,12,"k"),SyntaxHighlightingRule.create(12,20,"cd"),SyntaxHighlightingRule.create(24,38,"k"),SyntaxHighlightingRule.create(24,65,"cppd"),SyntaxHighlightingRule.create(42,50,"k"));
  String serializedRules=new SyntaxHighlightingData(orderedHighlightingRules).writeString();
  assertThat(serializedRules).isEqualTo("0,10,cd;10,12,k;12,20,cd;24,38,k;24,65,cppd;42,50,k;");
}
