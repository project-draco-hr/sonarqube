{
  Date lastSync=cacheStatus.getSyncStatus();
  if (lastSync != null) {
    if (!force) {
      LOG.info("Found cache [{}]",lastSync);
      return;
    }
 else {
      LOG.info("-- Found cache [{}], synchronizing data..",lastSync);
    }
  }
 else {
    LOG.info("-- Cache not found, synchronizing data..");
  }
  loadData();
  cacheStatus.save();
  LOG.info("-- Succesfully synchronized cache");
}
