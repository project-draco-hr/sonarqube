{
  Date lastSync=cacheStatus.getSyncStatus(null);
  if (lastSync != null) {
    if (!force) {
      LOG.info("Found cache [{}]",lastSync);
      return;
    }
 else {
      LOG.info("-- Found cache [{}], synchronizing data..",lastSync);
    }
    cacheStatus.delete(null);
  }
 else {
    LOG.info("-- Cache not found, synchronizing data..");
  }
  loadData();
  saveStatus();
}
