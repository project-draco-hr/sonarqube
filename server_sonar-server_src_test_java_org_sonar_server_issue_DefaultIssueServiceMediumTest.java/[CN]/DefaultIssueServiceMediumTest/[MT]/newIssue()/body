{
  return new IssueDto().setIssueCreationDate(DateUtils.parseDate("2014-09-04")).setIssueUpdateDate(DateUtils.parseDate("2014-12-04")).setRule(rule).setDebt(10L).setRootComponent(project).setComponent(file).setStatus(Issue.STATUS_OPEN).setResolution(null).setSeverity(Severity.MAJOR).setKee(UUID.randomUUID().toString());
}
