{
  String host=settings.getString("sonar.web.host");
  int port=settings.getInt("sonar.web.port");
  String context=settings.getString("sonar.web.context");
  StringBuilder res=new StringBuilder();
  res.append("http://");
  appendHost(host,res);
  appendPort(port,res);
  appendContext(context,res);
  return res.toString();
}
