{
  List<ActionPlan> plans=Arrays.asList(shortTerm,longTerm);
  Collections.sort(plans,new ActionPlanDeadlineComparator());
  assertThat(plans).containsSequence(shortTerm,longTerm);
  plans=Arrays.asList(longTerm,shortTerm);
  Collections.sort(plans,new ActionPlanDeadlineComparator());
  assertThat(plans).containsSequence(shortTerm,longTerm);
}
