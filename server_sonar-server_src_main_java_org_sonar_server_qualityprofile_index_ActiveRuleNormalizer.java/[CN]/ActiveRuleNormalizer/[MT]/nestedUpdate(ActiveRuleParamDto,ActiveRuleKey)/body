{
  Preconditions.checkNotNull(key);
  Map<String,Object> newParam=new HashMap<>();
  newParam.put(ActiveRuleParamField.NAME.field(),param.getKey());
  newParam.put(ActiveRuleParamField.VALUE.field(),param.getValue());
  return ImmutableList.of(new UpdateRequest().routing(key.ruleKey().toString()).id(key.toString()).script(ProcessProperties.ES_PLUGIN_LISTUPDATE_SCRIPT_NAME).addScriptParam(ProcessProperties.ES_PLUGIN_LISTUPDATE_FIELD,ActiveRuleField.PARAMS.field()).addScriptParam(ProcessProperties.ES_PLUGIN_LISTUPDATE_VALUE,newParam).addScriptParam(ProcessProperties.ES_PLUGIN_LISTUPDATE_ID_FIELD,ActiveRuleParamField.NAME.field()).addScriptParam(ProcessProperties.ES_PLUGIN_LISTUPDATE_ID_VALUE,param.getKey()));
}
