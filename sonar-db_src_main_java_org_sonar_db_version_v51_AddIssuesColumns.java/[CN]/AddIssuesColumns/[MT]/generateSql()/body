{
  return new AddColumnsBuilder(db.getDialect(),"issues").addColumn(new ColumnDef().setName("issue_creation_date_ms").setType(ColumnDef.Type.BIG_INTEGER).setNullable(true)).addColumn(new ColumnDef().setName("issue_update_date_ms").setType(ColumnDef.Type.BIG_INTEGER).setNullable(true)).addColumn(new ColumnDef().setName("issue_close_date_ms").setType(ColumnDef.Type.BIG_INTEGER).setNullable(true)).addColumn(new ColumnDef().setName("tags").setType(ColumnDef.Type.STRING).setLimit(4000).setNullable(true)).addColumn(new ColumnDef().setName("component_uuid").setType(ColumnDef.Type.STRING).setLimit(50).setNullable(true)).addColumn(new ColumnDef().setName("project_uuid").setType(ColumnDef.Type.STRING).setLimit(50).setNullable(true)).build();
}
